$ ITEMS_LIST = "LOGIN_FAILURES/MASTER_PID/MODE"
$ ITEM_DESCRIPTION = "Number of Logins failed/Process pid/Current process Mode"
$ I=0
$MOREINFO:
$ ITEM = F$ELEMENT(I,"/",ITEMS_LIST)
$ IF ITEM .EQS. "/" THEN GOTO EXIT_LOOP
$ VALUE = F$GETJPI("","''ITEM'")
$ DESCRIPTION = F$ELEMENT(I,"/",ITEM_DESCRIPTION)
$ SAY "''DESCRIPTION':  ''VALUE'"
$ I=I+1 
$ GOTO MOREINFO
$EXIT_LOOP:
$ SAY "SCRIPT FINISHED"