C
	INTEGER*4 OMAXTRPRW
	PARAMETER (OMAXTRPRW = 16)
C
	INTEGER*4 ODTRSTS,ODTRWEK,ODTRDAT,ODTRBSD,ODTRESD,ODTRCTM,ODTRTIM
	INTEGER*4 ODTRSAL(NUMTOT),ODTRPAD,ODTRPRG,ODTRREF,ODTRWON,ODTRPOL(2),ODTRTPL
	INTEGER*4 ODTRTBK,ODTRBRK(2),ODTRABW,ODTRODS(MAXTRPTI),ODTRWIN(4,3)
	INTEGER*4 ODTRTAX,ODTROTX(NUMTOT),ODTRHLD(4,3)
	INTEGER*4 ODTRMID(NUMTOT),ODTRUTX(NUMTOT),ODTRHST(MAXTRPTI),ODTRORM(2)
	INTEGER*4 ODTRMNM(TRPENM_LEN/4),ODTRENM(TRPENM_LEN/4,3)
	INTEGER*4 ODTRDES(TRPDES_LEN/4,3)
	INTEGER*4 ODTRNMS(TRPNMS_LEN/4,OMAXTRPRW,3)
	INTEGER*4 ODTRREV,ODTRPUP,ODTRUPD,ODTRSTA(OMAXTRPRW,3),ODTREST(3)
	INTEGER*4 ODTREVD(3),ODTREVT(3)
	INTEGER*4 ODTRSBR(OMAXTRPRW,3),ODTRPFN(5),ODTRERF,ODTRTER,ODTRDRW
	INTEGER*4 ODTRSER,ODTRPRF,ODTRSPR,ODTRPRC
	INTEGER*4 ODTRRWS(3),ODTRWCP(MAXTRPTI),ODTRWBT(NUMTOT,MAXTRPTI)
        INTEGER*4 ODTRWRO(NUMTOT,2),ODTRWRA(NUMTOT,2),ODTRWPO(NUMTOT,2)
        INTEGER*4 ODTRWPA(NUMTOT,2),ODTRWPR(NUMTOT,2)
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
	INTEGER*4 ODTRSTS_OFF, ODTRWEK_OFF, ODTRDAT_OFF, ODTRDRW_OFF
	PARAMETER(ODTRSTS_OFF = 1)					!001
	PARAMETER(ODTRWEK_OFF = ODTRSTS_OFF + 1)				!002
	PARAMETER(ODTRDAT_OFF = ODTRWEK_OFF + 1)				!003
	PARAMETER(ODTRDRW_OFF = ODTRDAT_OFF + 1)				!004
C
	INTEGER*4 ODTRBSD_OFF, ODTRESD_OFF, ODTRPUP_OFF, ODTRUPD_OFF
	PARAMETER(ODTRBSD_OFF = ODTRDRW_OFF + 1)				!005
	PARAMETER(ODTRESD_OFF = ODTRBSD_OFF + 1)				!006
	PARAMETER(ODTRPUP_OFF = ODTRESD_OFF + 1)				!007
	PARAMETER(ODTRUPD_OFF = ODTRPUP_OFF + 1)				!008
C
	INTEGER*4 ODTRCTM_OFF, ODTRTIM_OFF, ODTRSER_OFF, ODTRSAL_OFF
	PARAMETER(ODTRCTM_OFF = ODTRUPD_OFF + 1)				!009
	PARAMETER(ODTRTIM_OFF = ODTRCTM_OFF + 1)				!010
	PARAMETER(ODTRSER_OFF = ODTRTIM_OFF + 1)				!011
	PARAMETER(ODTRSAL_OFF = ODTRSER_OFF + 1)			        !012
C
	INTEGER*4 ODTRPAD_OFF, ODTRPRG_OFF, ODTRPRF_OFF, ODTRREF_OFF
	PARAMETER(ODTRPAD_OFF = ODTRSAL_OFF + NUMTOT)	                !014
	PARAMETER(ODTRPRG_OFF = ODTRPAD_OFF + 1)				!015
	PARAMETER(ODTRPRF_OFF = ODTRPRG_OFF + 1)				!016
	PARAMETER(ODTRREF_OFF = ODTRPRF_OFF + 1)				!017
C
	INTEGER*4 ODTRERF_OFF, ODTRTER_OFF, ODTRWON_OFF, ODTRPOL_OFF
	PARAMETER(ODTRERF_OFF = ODTRREF_OFF + 1)				!018
	PARAMETER(ODTRTER_OFF = ODTRERF_OFF + 1)				!019
	PARAMETER(ODTRWON_OFF = ODTRTER_OFF + 1)				!020
	PARAMETER(ODTRPOL_OFF = ODTRWON_OFF + 1)				!021
C
	INTEGER*4 ODTRTPL_OFF, ODTRTBK_OFF, ODTRBRK_OFF, ODTRABW_OFF
	PARAMETER(ODTRTPL_OFF = ODTRPOL_OFF + 2)				!023
	PARAMETER(ODTRTBK_OFF = ODTRTPL_OFF + 1)				!024
	PARAMETER(ODTRBRK_OFF = ODTRTBK_OFF + 1)				!025
	PARAMETER(ODTRABW_OFF = ODTRBRK_OFF + 2)				!027
C
	INTEGER*4 ODTRODS_OFF, ODTRWIN_OFF, ODTRHLD_OFF, ODTRTAX_OFF
	PARAMETER(ODTRODS_OFF = ODTRABW_OFF + 1)				!028
	PARAMETER(ODTRWIN_OFF = ODTRODS_OFF + MAXTRPTI)			!094
	PARAMETER(ODTRHLD_OFF = ODTRWIN_OFF + 12)                         !106
	PARAMETER(ODTRTAX_OFF = ODTRHLD_OFF + 12)				!118

C
	INTEGER*4 ODTROTX_OFF, ODTRMID_OFF, ODTRUTX_OFF, ODTRHST_OFF
	PARAMETER(ODTROTX_OFF = ODTRTAX_OFF + 1)				!119
	PARAMETER(ODTRMID_OFF = ODTROTX_OFF + NUMTOT)			!121
	PARAMETER(ODTRUTX_OFF = ODTRMID_OFF + NUMTOT)			!123
	PARAMETER(ODTRHST_OFF = ODTRUTX_OFF + NUMTOT)			!125
C
	INTEGER*4 ODTRORM_OFF, ODTRMNM_OFF, ODTRENM_OFF, ODTRDES_OFF
        INTEGER*4 ODTRNMS_OFF
	PARAMETER(ODTRORM_OFF = ODTRHST_OFF + MAXTRPTI)                   !189
	PARAMETER(ODTRMNM_OFF = ODTRORM_OFF + NUMTOT)			!191
	PARAMETER(ODTRENM_OFF = ODTRMNM_OFF + TRPENM_LEN/4)               !198
	PARAMETER(ODTRDES_OFF = ODTRENM_OFF + (TRPENM_LEN/4)*3)		!247
	PARAMETER(ODTRNMS_OFF = ODTRDES_OFF + (TRPDES_LEN/4)*3)		!268
C
	INTEGER*4 ODTRSTA_OFF, ODTRSBR_OFF, ODTRREV_OFF, ODTRPFN_OFF
	INTEGER*4 ODTREST_OFF, ODTREVD_OFF, ODTREVT_OFF
	PARAMETER(ODTRSTA_OFF = ODTRNMS_OFF + (TRPNMS_LEN/4)*OMAXTRPRW*3)	!412
	PARAMETER(ODTREST_OFF = ODTRSTA_OFF + OMAXTRPRW*3)                 !460
	PARAMETER(ODTREVD_OFF = ODTREST_OFF + 3)				!463
	PARAMETER(ODTREVT_OFF = ODTREVD_OFF + 3)				!466
	PARAMETER(ODTRSBR_OFF = ODTREVT_OFF + 3)				!469
	PARAMETER(ODTRREV_OFF = ODTRSBR_OFF + OMAXTRPRW*3)			!517
	PARAMETER(ODTRPFN_OFF = ODTRREV_OFF + 1)				!518
C
	INTEGER*4 ODTRPRC_OFF, ODTRSPR_OFF
	PARAMETER(ODTRPRC_OFF = ODTRPFN_OFF + 5)				!523
	PARAMETER(ODTRSPR_OFF = ODTRPRC_OFF + 1)				!524

        INTEGER*4 ODTRWRO_OFF, ODTRWRA_OFF, ODTRWPO_OFF, ODTRWPA_OFF
        INTEGER*4 ODTRWPR_OFF, ODTRRWS_OFF, ODTRWCP_OFF
	INTEGER*4 ODTRWBT_OFF
	PARAMETER(ODTRRWS_OFF = ODTRSPR_OFF + 1)				!525
	PARAMETER(ODTRWCP_OFF = ODTRRWS_OFF + 3)				!528
	PARAMETER(ODTRWBT_OFF = ODTRWCP_OFF + MAXTRPTI)			!592
        PARAMETER(ODTRWRO_OFF = ODTRWBT_OFF + NUMTOT*MAXTRPTI)		!720
        PARAMETER(ODTRWRA_OFF = ODTRWRO_OFF + 2*NUMTOT)			!724
        PARAMETER(ODTRWPO_OFF = ODTRWRA_OFF + 2*NUMTOT)			!728
        PARAMETER(ODTRWPA_OFF = ODTRWPO_OFF + 2*NUMTOT)			!732
        PARAMETER(ODTRWPR_OFF = ODTRWPA_OFF + 2*NUMTOT)			!736

	INTEGER*4 ODTRFRE_OFF
        PARAMETER(ODTRFRE_OFF = ODTRWPR_OFF + 2*NUMTOT)

C
C CALCULATE FREE SPACE BASED ON MACHINE DEPENDENT SECTOR SIZES.
C SHOULD ALLWAYS ADD UP TO A EVEN SECTOR SIZE BYTES (VAX 512).
C NOTE! IN CALCULATIONS, FRE_OFF-2 IS DONE TO TAKE CARE OF WHEN THE DATA SIZE
C       ENDS ON A EVEN SECTOR SIZE.
C
        INTEGER*4 ODTRSEC_VAX, ODTRSEC_NUM
        PARAMETER (ODTRSEC_VAX = SECSIZE/4)
	PARAMETER (ODTRSEC_NUM = NINT(FLOAT((ODTRFRE_OFF-2)/ODTRSEC_VAX)+.5))
C
        INTEGER*4 ODTRLEN, ODTRSEC
        PARAMETER(ODTRLEN = ODTRSEC_VAX * ODTRSEC_NUM)        ! even sector size
        PARAMETER(ODTRSEC = ODTRLEN/64)
C
        INTEGER*4 ODTRREC(ODTRLEN)
C
	EQUIVALENCE(ODTRSTS,ODTRREC(ODTRSTS_OFF))
	EQUIVALENCE(ODTRWEK,ODTRREC(ODTRWEK_OFF))
	EQUIVALENCE(ODTRDAT,ODTRREC(ODTRDAT_OFF))
	EQUIVALENCE(ODTRDRW,ODTRREC(ODTRDRW_OFF))
	EQUIVALENCE(ODTRBSD,ODTRREC(ODTRBSD_OFF))
	EQUIVALENCE(ODTRESD,ODTRREC(ODTRESD_OFF))
	EQUIVALENCE(ODTRPUP,ODTRREC(ODTRPUP_OFF))
	EQUIVALENCE(ODTRUPD,ODTRREC(ODTRUPD_OFF))
	EQUIVALENCE(ODTRCTM,ODTRREC(ODTRCTM_OFF))
	EQUIVALENCE(ODTRTIM,ODTRREC(ODTRTIM_OFF))
	EQUIVALENCE(ODTRSER,ODTRREC(ODTRSER_OFF))
	EQUIVALENCE(ODTRSAL,ODTRREC(ODTRSAL_OFF))
	EQUIVALENCE(ODTRPAD,ODTRREC(ODTRPAD_OFF))
	EQUIVALENCE(ODTRPRG,ODTRREC(ODTRPRG_OFF))
	EQUIVALENCE(ODTRPRF,ODTRREC(ODTRPRF_OFF))
	EQUIVALENCE(ODTRREF,ODTRREC(ODTRREF_OFF))
	EQUIVALENCE(ODTRERF,ODTRREC(ODTRERF_OFF))
	EQUIVALENCE(ODTRTER,ODTRREC(ODTRTER_OFF))
	EQUIVALENCE(ODTRWON,ODTRREC(ODTRWON_OFF))
	EQUIVALENCE(ODTRPOL,ODTRREC(ODTRPOL_OFF))
	EQUIVALENCE(ODTRTPL,ODTRREC(ODTRTPL_OFF))
	EQUIVALENCE(ODTRTBK,ODTRREC(ODTRTBK_OFF))
	EQUIVALENCE(ODTRBRK,ODTRREC(ODTRBRK_OFF))
	EQUIVALENCE(ODTRABW,ODTRREC(ODTRABW_OFF))
	EQUIVALENCE(ODTRODS,ODTRREC(ODTRODS_OFF))
	EQUIVALENCE(ODTRWIN,ODTRREC(ODTRWIN_OFF))
	EQUIVALENCE(ODTRHLD,ODTRREC(ODTRHLD_OFF))
	EQUIVALENCE(ODTRTAX,ODTRREC(ODTRTAX_OFF))
	EQUIVALENCE(ODTROTX,ODTRREC(ODTROTX_OFF))
	EQUIVALENCE(ODTRMID,ODTRREC(ODTRMID_OFF))
	EQUIVALENCE(ODTRUTX,ODTRREC(ODTRUTX_OFF))
	EQUIVALENCE(ODTRHST,ODTRREC(ODTRHST_OFF))
	EQUIVALENCE(ODTRORM,ODTRREC(ODTRORM_OFF))
	EQUIVALENCE(ODTRMNM,ODTRREC(ODTRMNM_OFF))
	EQUIVALENCE(ODTRENM,ODTRREC(ODTRENM_OFF))
	EQUIVALENCE(ODTRDES,ODTRREC(ODTRDES_OFF))
	EQUIVALENCE(ODTRNMS,ODTRREC(ODTRNMS_OFF))
	EQUIVALENCE(ODTRSTA,ODTRREC(ODTRSTA_OFF))
	EQUIVALENCE(ODTREST,ODTRREC(ODTREST_OFF))
	EQUIVALENCE(ODTREVD,ODTRREC(ODTREVD_OFF))
	EQUIVALENCE(ODTREVT,ODTRREC(ODTREVT_OFF))
	EQUIVALENCE(ODTRSBR,ODTRREC(ODTRSBR_OFF))
	EQUIVALENCE(ODTRREV,ODTRREC(ODTRREV_OFF))
	EQUIVALENCE(ODTRPFN,ODTRREC(ODTRPFN_OFF))
	EQUIVALENCE(ODTRPRC,ODTRREC(ODTRPRC_OFF))
	EQUIVALENCE(ODTRSPR,ODTRREC(ODTRSPR_OFF))
	EQUIVALENCE(ODTRRWS,ODTRREC(ODTRRWS_OFF))
	EQUIVALENCE(ODTRWCP,ODTRREC(ODTRWCP_OFF))
	EQUIVALENCE(ODTRWBT,ODTRREC(ODTRWBT_OFF))
        EQUIVALENCE(ODTRWRO,ODTRREC(ODTRWRO_OFF))
        EQUIVALENCE(ODTRWRA,ODTRREC(ODTRWRA_OFF))
        EQUIVALENCE(ODTRWPO,ODTRREC(ODTRWPO_OFF))
        EQUIVALENCE(ODTRWPA,ODTRREC(ODTRWPA_OFF))
        EQUIVALENCE(ODTRWPR,ODTRREC(ODTRWPR_OFF))
