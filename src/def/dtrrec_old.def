C DTRREC.DEF
C
C V01 11-MAY-1999 UXN Initial release.
C  
C  
C TODAY'S TRIO GAME DATA FILE RECORD LAYOUT (DISK)
C
C
C
C     ODTRSTS                         ; GAME STATUS
C     ODTRWEK                         ; WEEK NUMBER
C     ODTRDAT                         ; DRAW DATE
C     ODTRDRW                         ; DRAW NUMBER
C     ODTRBSD                         ; BEGINNING SALES DATE
C     ODTRESD                         ; ENDING SALES DATE
C     ODTRPUP                         ; LAST PRUGE UPDATE  (FILE ONLY)
C     ODTRUPD                         ; LAST FILE UPDATE   (FILE ONLY)
C     ODTRCTM                         ; ACTUAL TIME WHEN CLOSED
C     ODTRTIM                         ; TIME WHEN GAME SHOULD CLOSE
C     ODTRSER                         ; LAST SERIAL NUMBER
C     ODTRSAL(NUMTOT)                 ; TOTAL SALES (COUNT , AMOUNT)
C     ODTRPAD                         ; PRIZES PAID + REFUNDS
C     ODTRPRG                         ; PRIZES PURGED
C     ODTRPRF                         ; PAID REFUNDS
C     ODTRREF                         ; TOTAL REFUND AMOUNT
C     ODTRERF                         ; EARLY REFUNDS PAID
C     ODTRTER                         ; TOTAL EARLY REFUNDS (LIAB)
C     ODTRWON                         ; PRIZES WON
C     ODTRPOL(2)                      ; ROLL POOL
C     ODTRTPL                         ; TOTAL PRIZE POOL
C     ODTRTBK                         ; TOTAL BREAKAGE
C     ODTRBRK(2)                      ; WINNING ROUNDING POT
C     ODTRABW                         ; AMOUNT BET ON WINNING SCORE
C     ODTRODS(MAXTRPTI)               ; WINNING ODDS
C     ODTRWIN(4,3)                    ; WINNING RESULTS (MAX 4 WINNERS)
C     ODTRHLD(4,3)                    ; WINNING RESULTS HOLD (MAX 4 WINNERS)
C     ODTRTAX                         ; WINNING TAXES
C     ODTROTX(NUMTOT)                 ; OVER TAX LIMIT
C     ODTRMID(NUMTOT)                 ; >REDMIN <REDMAX
C     ODTRUTX(NUMTOT)                 ; UNDER TAX LIMIT
C     ODTRHST(MAXTRPTI)               ; HIGHEST WINNER PER WINING COMB
C     ODTRORM(NUMTOT)                 ; OVER REDMAX
C     ODTRMNM(TRPENM_LEN/4)           ; MASTER EVENT NAME
C     ODTRENM(TRPENM_LEN/4,3)         ; EVENT NAME
C     ODTRDES(TRPDES_LEN/4,3)         ; EVENT DESCRIPTION
C     ODTRNMS(TRPNMS_LEN/4,OMAXTRPRW,3); ROW NAMES
C     ODTRSTA(OMAXTRPRW,3)             ; ROW STATUS'S
C     ODTREST(3)			     ; EVENT STATUS
C     ODTREVD(3)			     ; EVENT BEGIN DATE
C     ODTRETT(3)			     ; EVENT BEGIN TIME
C     ODTRSBR(OMAXTRPRW,3)             ; SALES BY ROW TABLE
C     ODTRREV                         ; REV #
C     ODTRPFN(5)                      ; POOL FILE NAME
C     ODTRPRC                         ; BASE PRICE
C     ODTRSPR                         ; POOL PERCENTAGE
C     ODTRRWS(3)			     ; ACTUAL # OF ROWS USED.
C     ODTRWCP(MAXTRPTI)		     ; # WINNING COUPONS PLAYED
C     ODTRWBT(NUMTOT,MAXTRPTI)        ; CNT, AMT WINNERS PLAYED
C     ODTRWRO(NUMTOT,2)               ; WINNING: COUNT,AMOUNT REFUNDS ONLY
C     ODTRWRA(NUMTOT,2)               ; WINNING: COUNT,AMOUNT REFUNDS ALTOGETHER
C     ODTRWPO(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES ONLY
C     ODTRWPA(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES ALTOGETHER
C     ODTRWPR(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES AND REFUNDS
C                                    ; (1=PRWON,2=PRPAD)
C
C WINNERS GAME FILE PARAMETERS
C
C
C
C+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
