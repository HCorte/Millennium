	INTEGER*4	MAXLUNARRAY
	PARAMETER	(MAXLUNARRAY=100)
C
	INTEGER*4	FABFILSZ(MAXLUNARRAY)
	INTEGER*4	LUNARRAY(MAXLUNARRAY)
	INTEGER*4	XSTAT
