	INTEGER*4	 FDB_IOSB
	PARAMETER	(FDB_IOSB  = 3)		!2 WORDS FOR IO STATUS BLOCK
 
	INTEGER*4	 FDB_IOLEN
	PARAMETER	(FDB_IOLEN = 5)		!ACTUAL I/O LEN
	INTEGER*4	 FDB_EVNT
	PARAMETER	(FDB_EVNT  = 6)		!EVENT FLAG USED
	INTEGER*4	 FDB_BYTES
	PARAMETER	(FDB_BYTES = 7)		!# OF BYTES PER I/O
