C
C   LAST PART OF RECORD TO BE DEFINED IS THE AGENT TRAINING AND
C   INSTALLATION FIELDS. THIS INFORMATION WILL BE STORED ONLY IN
C   THE GTRACK SYSTEM AND NOT PART OF THE ONLINE DATA FOR CENTRAL.
C
C------------------------ DECLARE ALL VARIABLES ----------------------
C
C-----TRAINING/INSTALLATION
C
	INTEGER*4 LPRES,SPRES,EPRES          !PRESITE SCHEDULE(DATE)
	INTEGER*4 LPREA,SPREA,EPREA          !PRESITE ACTUAL(DATE)
	INTEGER*4 LINDP,SINDP,EINDP          !INSTALLATION DEPOT
	INTEGER*4 LINSC,SINSC,EINSC          !INSTALLATION SCHEDULE
	INTEGER*4 LINAC,SINAC,EINAC          !INSTALLATION ACTUAL
	INTEGER*4 LLNGE,SLNGE,ELNGE          !LANGUAGE CODE
	INTEGER*4 LTRLC,STRLC,ETRLC          !TRAINING LOCATION CODE
	INTEGER*4 LTRSC,STRSC,ETRSC          !TRAINING SCHEDUE(DATE)
	INTEGER*4 LTRTM,STRTM,ETRTM          !TRAINING CLASS CODE
	INTEGER*4 LTRCO,STRCO,ETRCO          !TRAINING CONFIRMATION
	INTEGER*4 LTRAC,STRAC,ETRAC          !TRAINING ACTUAL(DATE)
	INTEGER*4 LTRAT,STRAT,ETRAT          !TRAINING ACTUAL CLASS
	INTEGER*4 LTRNO,STRNO,ETRNO          !NUMBER TRAINED
	INTEGER*4 LTRNA,STRNA,ETRNA          !NAME TRAINED
	INTEGER*4 LTRD2,STRD2,ETRD2          !ADDITION TRAINING(DATE)
	INTEGER*4 LTRT2,STRT2,ETRT2          !ADDITION TRAINING(CLASS)
	INTEGER*4 LTRN2,STRN2,ETRN2          !ADDITIONAL NUMBER TRAINED
	INTEGER*4 LTRNM,STRNM,ETRNM          !ADDITIONAL NAMES TRAINED
C
	INTEGER*4 GLENGTH             !GTRACK AGENT INFO. RECORD(BYTES)
	INTEGER*4 MLENGTH             !MODIFY AGENT INFORMATION REC. LEN
C
C------------------------ DEFINE FIELD LENGTHS -----------------------
C
C-----TRAINING/INSTALLATION
C
	PARAMETER(LPRES =  6)                !PRESITE SCHEDULE(DATE)
	PARAMETER(LPREA =  6)                !PRESITE ACTUAL(DATE)
	PARAMETER(LINDP =  2)                !INSTALLATION DEPOT
	PARAMETER(LINSC =  6)                !INSTALLATION SCHEDULE
	PARAMETER(LINAC =  6)                !INSTALLATION ACTUAL
	PARAMETER(LLNGE =  2)                !LANGUAGE CODE
	PARAMETER(LTRLC =  2)                !TRAINING LOCATION CODE
	PARAMETER(LTRSC =  6)                !TRAINING SCHEDUE(DATE)
	PARAMETER(LTRTM =  1)                !TRAINING CLASS CODE
	PARAMETER(LTRCO =  1)                !TRAINING CONFIRMATION
	PARAMETER(LTRAC =  6)                !TRAINING ACTUAL(DATE)
	PARAMETER(LTRAT =  1)                !TRAINING ACTUAL CLASS
	PARAMETER(LTRNO =  1)                !NUMBER TRAINED
	PARAMETER(LTRNA = 30)                !NAME TRAINED
	PARAMETER(LTRD2 =  6)                !ADDITION TRAINING(DATE)
	PARAMETER(LTRT2 =  1)                !ADDITION TRAINING(CLASS)
	PARAMETER(LTRN2 =  1)                !ADDITIONAL NUMBER TRAINED
	PARAMETER(LTRNM = 30)                !ADDITIONAL NAMES TRAINED
C
	PARAMETER(GLENGTH = 626)      !GTRACK AGENT INFO. RECORD(BYTES)
	PARAMETER(MLENGTH = 627)      !MODIFY AGENT INFORMATION REC. LEN
C
C------------- DEFINE START AND END OF FIELDS IN THE RECORD ----------
C
	PARAMETER(SPRES = EFREE + 1, EPRES = SPRES + LPRES - 1)
	PARAMETER(SPREA = EPRES + 1, EPREA = SPREA + LPREA - 1)
	PARAMETER(SINDP = EPREA + 1, EINDP = SINDP + LINDP - 1)
	PARAMETER(SINSC = EINDP + 1, EINSC = SINSC + LINSC - 1)
	PARAMETER(SINAC = EINSC + 1, EINAC = SINAC + LINAC - 1)
	PARAMETER(SLNGE = EINAC + 1, ELNGE = SLNGE + LLNGE - 1)
	PARAMETER(STRLC = ELNGE + 1, ETRLC = STRLC + LTRLC - 1)
	PARAMETER(STRSC = ETRLC + 1, ETRSC = STRSC + LTRSC - 1)
	PARAMETER(STRTM = ETRSC + 1, ETRTM = STRTM + LTRTM - 1)
	PARAMETER(STRCO = ETRTM + 1, ETRCO = STRCO + LTRCO - 1)
	PARAMETER(STRAC = ETRCO + 1, ETRAC = STRAC + LTRAC - 1)
	PARAMETER(STRAT = ETRAC + 1, ETRAT = STRAT + LTRAT - 1)
	PARAMETER(STRNO = ETRAT + 1, ETRNO = STRNO + LTRNO - 1)
	PARAMETER(STRNA = ETRNO + 1, ETRNA = STRNA + LTRNA - 1)
	PARAMETER(STRD2 = ETRNA + 1, ETRD2 = STRD2 + LTRD2 - 1)
	PARAMETER(STRT2 = ETRD2 + 1, ETRT2 = STRT2 + LTRT2 - 1)
	PARAMETER(STRN2 = ETRT2 + 1, ETRN2 = STRN2 + LTRN2 - 1)
	PARAMETER(STRNM = ETRN2 + 1, ETRNM = STRNM + LTRNM - 1)
C
C DEFINITION OF STORE HOURS IN DETAIL
C
	INTEGER*4 SMONO,STUEO,SWEDO,STHUO,SFRIO,SSATO,SSUNO
	INTEGER*4 SMONC,STUEC,SWEDC,STHUC,SFRIC,SSATC,SSUNC
C
C
	PARAMETER (SMONO=SHOUR)
	PARAMETER (STUEO=SMONO+4)
	PARAMETER (SWEDO=STUEO+4)
	PARAMETER (STHUO=SWEDO+4)
	PARAMETER (SFRIO=STHUO+4)
	PARAMETER (SSATO=SFRIO+4)
	PARAMETER (SSUNO=SSATO+4)
	PARAMETER (SMONC=SSUNO+4)
	PARAMETER (STUEC=SMONC+4)
	PARAMETER (SWEDC=STUEC+4)
	PARAMETER (STHUC=SWEDC+4)
	PARAMETER (SFRIC=STHUC+4)
	PARAMETER (SSATC=SFRIC+4)
	PARAMETER (SSUNC=SSATC+4)
C
C
	INTEGER*4  EMONO, ETUEO, EWEDO, ETHUO, EFRIO, ESATO, ESUNO
	INTEGER*4  EMONC, ETUEC, EWEDC, ETHUC, EFRIC, ESATC, ESUNC
C
	PARAMETER (EMONO=SHOUR+3)
	PARAMETER (ETUEO=EMONO+4)
	PARAMETER (EWEDO=ETUEO+4)
	PARAMETER (ETHUO=EWEDO+4)
	PARAMETER (EFRIO=ETHUO+4)
	PARAMETER (ESATO=EFRIO+4)
	PARAMETER (ESUNO=ESATO+4)
	PARAMETER (EMONC=ESUNO+4)
	PARAMETER (ETUEC=EMONC+4)
	PARAMETER (EWEDC=ETUEC+4)
	PARAMETER (ETHUC=EWEDC+4)
	PARAMETER (EFRIC=ETHUC+4)
	PARAMETER (ESATC=EFRIC+4)
	PARAMETER (ESUNC=ESATC+4)
C
C
C   DEFINE ARRAY OF OFFSETS OF BYTE FIELDS AND A CHARACTER ARRAY
C   TO HOLD VALUES WHENEVER THEY ARE NEEDED.
C
C	CHARACTER CDATA(2),CDROP,CFLAG
C	EQUIVALENCE (CDATA(1),CDROP), (CDATA(2),CFLAG)
