C
C
	INTEGER*4 DSCSTS,DSCWEK,DSCDAT,DSCBSD,DSCESD,DSCCTM,DSCTIM
	INTEGER*4 DSCSAL,DSCPAD,DSCPRG,DSCREF,DSCWON,DSCPOL(2),DSCTPL
	INTEGER*4 DSCTBK,DSCBRK(2),DSCABW,DSCODS,DSCWIN(2),DSCHLD(2)
	INTEGER*4 DSCTAX,DSCFUL(NUMTOT,MAXFUL),DSCOTX(NUMTOT)
	INTEGER*4 DSCMID(NUMTOT),DSCUTX(NUMTOT),DSCHST,DSCORM(2)
	INTEGER*4 DSCNM1(SNMS_LEN/4),DSCNM2(SNMS_LEN/4),DSCPFN(5)
	INTEGER*4 DSCDRW,DSCSER,DSCPRF
	INTEGER*4 DSCREV,DSCTYP,DSCPRC,DSCPUP,DSCUPD
	INTEGER*4 DSCSPR,DSCDES(SDES_LEN/4),DSCTVC(STVC_LEN/4)
C
	BYTE	  DSCMDS(MAXMLTD_AVL)
C
        INTEGER*4 DSCWRO(NUMTOT,2),DSCWRA(NUMTOT,2),DSCWPO(NUMTOT,2)
        INTEGER*4 DSCWPA(NUMTOT,2),DSCWPR(NUMTOT,2)
	INTEGER*4 DSCLAT(NUMLAT)
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
	INTEGER*4 DSCSTS_OFF, DSCWEK_OFF, DSCDAT_OFF, DSCDRW_OFF
	PARAMETER(DSCSTS_OFF = 1)					!001
	PARAMETER(DSCWEK_OFF = DSCSTS_OFF + 1)				!002
	PARAMETER(DSCDAT_OFF = DSCWEK_OFF + 1)				!003
	PARAMETER(DSCDRW_OFF = DSCDAT_OFF + 1)				!004
C
	INTEGER*4 DSCBSD_OFF, DSCESD_OFF, DSCPUP_OFF, DSCUPD_OFF
	PARAMETER(DSCBSD_OFF = DSCDRW_OFF + 1)				!005
	PARAMETER(DSCESD_OFF = DSCBSD_OFF + 1)				!006
	PARAMETER(DSCPUP_OFF = DSCESD_OFF + 1)				!007
	PARAMETER(DSCUPD_OFF = DSCPUP_OFF + 1)				!008
C
	INTEGER*4 DSCCTM_OFF, DSCTIM_OFF, DSCSER_OFF, DSCSAL_OFF
	PARAMETER(DSCCTM_OFF = DSCUPD_OFF + 1)				!009
	PARAMETER(DSCTIM_OFF = DSCCTM_OFF + 1)				!010
	PARAMETER(DSCSER_OFF = DSCTIM_OFF + 1)				!011
	PARAMETER(DSCSAL_OFF = DSCSER_OFF + 1)				!012
C
	INTEGER*4 DSCPAD_OFF, DSCPRG_OFF, DSCPRF_OFF, DSCREF_OFF
	PARAMETER(DSCPAD_OFF = DSCSAL_OFF + 1)				!013
	PARAMETER(DSCPRG_OFF = DSCPAD_OFF + 1)				!014
	PARAMETER(DSCPRF_OFF = DSCPRG_OFF + 1)				!015
	PARAMETER(DSCREF_OFF = DSCPRF_OFF + 1)				!016
C
	INTEGER*4 DSCWON_OFF, DSCPOL_OFF, DSCTPL_OFF, DSCTBK_OFF
	PARAMETER(DSCWON_OFF = DSCREF_OFF + 1)				!017
	PARAMETER(DSCPOL_OFF = DSCWON_OFF + 1)				!018
	PARAMETER(DSCTPL_OFF = DSCPOL_OFF + 2)				!020
	PARAMETER(DSCTBK_OFF = DSCTPL_OFF + 1)				!021
C
	INTEGER*4 DSCBRK_OFF, DSCABW_OFF, DSCODS_OFF, DSCWIN_OFF
	PARAMETER(DSCBRK_OFF = DSCTBK_OFF + 1)				!022
	PARAMETER(DSCABW_OFF = DSCBRK_OFF + 2)				!023
	PARAMETER(DSCODS_OFF = DSCABW_OFF + 1)				!024
	PARAMETER(DSCWIN_OFF = DSCODS_OFF + 1)				!025
C
	INTEGER*4 DSCHLD_OFF, DSCTAX_OFF, DSCFUL_OFF, DSCOTX_OFF
	PARAMETER(DSCHLD_OFF = DSCWIN_OFF + 2)				!027
	PARAMETER(DSCTAX_OFF = DSCHLD_OFF + 2)				!029
	PARAMETER(DSCFUL_OFF = DSCTAX_OFF + 1)				!030
	PARAMETER(DSCOTX_OFF = DSCFUL_OFF + (NUMTOT*MAXFUL))		!230
C
	INTEGER*4 DSCMID_OFF, DSCUTX_OFF, DSCHST_OFF, DSCORM_OFF
	PARAMETER(DSCMID_OFF = DSCOTX_OFF + NUMTOT)			!232
	PARAMETER(DSCUTX_OFF = DSCMID_OFF + NUMTOT)			!234
	PARAMETER(DSCHST_OFF = DSCUTX_OFF + NUMTOT)			!236
	PARAMETER(DSCORM_OFF = DSCHST_OFF + 1)				!237
C
	INTEGER*4 DSCNM1_OFF, DSCNM2_OFF, DSCREV_OFF, DSCTYP_OFF
	PARAMETER(DSCNM1_OFF = DSCORM_OFF + NUMTOT)			!239
	PARAMETER(DSCNM2_OFF = DSCNM1_OFF + (SNMS_LEN/4))		!243
	PARAMETER(DSCREV_OFF = DSCNM2_OFF + (SNMS_LEN/4))		!247
	PARAMETER(DSCTYP_OFF = DSCREV_OFF + 1)				!248
C
	INTEGER*4 DSCPFN_OFF, DSCPRC_OFF, DSCSPR_OFF, DSCDES_OFF
	PARAMETER(DSCPFN_OFF = DSCTYP_OFF + 1)				!249
	PARAMETER(DSCPRC_OFF = DSCPFN_OFF + 5)				!254
	PARAMETER(DSCSPR_OFF = DSCPRC_OFF + 1)				!255
	PARAMETER(DSCDES_OFF = DSCSPR_OFF + 1)			        !256
C
	INTEGER*4 DSCTVC_OFF, DSCMDS_OFF 
	PARAMETER(DSCTVC_OFF = DSCDES_OFF + (SDES_LEN/4))		!286
	PARAMETER(DSCMDS_OFF = DSCTVC_OFF + (STVC_LEN/4))		!287
C
        INTEGER*4 DSCWRO_OFF, DSCWRA_OFF, DSCWPO_OFF, DSCWPA_OFF
        INTEGER*4 DSCWPR_OFF
        PARAMETER(DSCWRO_OFF = DSCMDS_OFF + (MAXMLTD_AVL/4))
        PARAMETER(DSCWRA_OFF = DSCWRO_OFF + 2*NUMTOT)
        PARAMETER(DSCWPO_OFF = DSCWRA_OFF + 2*NUMTOT)
        PARAMETER(DSCWPA_OFF = DSCWPO_OFF + 2*NUMTOT)
        PARAMETER(DSCWPR_OFF = DSCWPA_OFF + 2*NUMTOT)
	INTEGER*4 DSCLAT_OFF
	PARAMETER(DSCLAT_OFF = DSCWPR_OFF + 2*NUMTOT)

        INTEGER*4 DSCFRE_OFF
        PARAMETER(DSCFRE_OFF = DSCLAT_OFF + NUMLAT)
C
C CALCULATE FREE SPACE BASED ON MACHINE DEPENDENT SECTOR SIZES.
C SHOULD ALLWAYS ADD UP TO A EVEN SECTOR SIZE BYTES (VAX 512).
C NOTE! IN CALCULATIONS, FRE_OFF-2 IS DONE TO TAKE CARE OF WHEN THE DATA SIZE
C       ENDS ON A EVEN SECTOR SIZE.
C
        INTEGER*4 DSCSEC_VAX, DSCSEC_NUM
        PARAMETER (DSCSEC_VAX = SECSIZE/4)
	PARAMETER (DSCSEC_NUM = NINT(FLOAT((DSCFRE_OFF-2)/DSCSEC_VAX)+.5))
C
        INTEGER*4 DSCLEN, DSCSEC
        PARAMETER(DSCLEN = DSCSEC_VAX * DSCSEC_NUM)        ! even sector size
        PARAMETER(DSCSEC = DSCLEN/64)
C
        INTEGER*4 DSCREC(DSCLEN)
C
	EQUIVALENCE(DSCSTS,DSCREC(DSCSTS_OFF))
	EQUIVALENCE(DSCWEK,DSCREC(DSCWEK_OFF))
	EQUIVALENCE(DSCDAT,DSCREC(DSCDAT_OFF))
	EQUIVALENCE(DSCDRW,DSCREC(DSCDRW_OFF))
	EQUIVALENCE(DSCBSD,DSCREC(DSCBSD_OFF))
	EQUIVALENCE(DSCESD,DSCREC(DSCESD_OFF))
	EQUIVALENCE(DSCPUP,DSCREC(DSCPUP_OFF))
	EQUIVALENCE(DSCUPD,DSCREC(DSCUPD_OFF))
	EQUIVALENCE(DSCCTM,DSCREC(DSCCTM_OFF))
	EQUIVALENCE(DSCTIM,DSCREC(DSCTIM_OFF))
	EQUIVALENCE(DSCSER,DSCREC(DSCSER_OFF))
	EQUIVALENCE(DSCSAL,DSCREC(DSCSAL_OFF))
	EQUIVALENCE(DSCPAD,DSCREC(DSCPAD_OFF))
	EQUIVALENCE(DSCPRG,DSCREC(DSCPRG_OFF))
	EQUIVALENCE(DSCPRF,DSCREC(DSCPRF_OFF))
	EQUIVALENCE(DSCREF,DSCREC(DSCREF_OFF))
	EQUIVALENCE(DSCWON,DSCREC(DSCWON_OFF))
	EQUIVALENCE(DSCPOL,DSCREC(DSCPOL_OFF))
	EQUIVALENCE(DSCTPL,DSCREC(DSCTPL_OFF))
	EQUIVALENCE(DSCTBK,DSCREC(DSCTBK_OFF))
	EQUIVALENCE(DSCBRK,DSCREC(DSCBRK_OFF))
	EQUIVALENCE(DSCABW,DSCREC(DSCABW_OFF))
	EQUIVALENCE(DSCODS,DSCREC(DSCODS_OFF))
	EQUIVALENCE(DSCWIN,DSCREC(DSCWIN_OFF))
	EQUIVALENCE(DSCHLD,DSCREC(DSCHLD_OFF))
	EQUIVALENCE(DSCTAX,DSCREC(DSCTAX_OFF))
	EQUIVALENCE(DSCFUL,DSCREC(DSCFUL_OFF))
	EQUIVALENCE(DSCOTX,DSCREC(DSCOTX_OFF))
	EQUIVALENCE(DSCMID,DSCREC(DSCMID_OFF))
	EQUIVALENCE(DSCUTX,DSCREC(DSCUTX_OFF))
	EQUIVALENCE(DSCHST,DSCREC(DSCHST_OFF))
	EQUIVALENCE(DSCORM,DSCREC(DSCORM_OFF))
	EQUIVALENCE(DSCNM1,DSCREC(DSCNM1_OFF))
	EQUIVALENCE(DSCNM2,DSCREC(DSCNM2_OFF))
	EQUIVALENCE(DSCREV,DSCREC(DSCREV_OFF))
	EQUIVALENCE(DSCTYP,DSCREC(DSCTYP_OFF))
	EQUIVALENCE(DSCPFN,DSCREC(DSCPFN_OFF))
        EQUIVALENCE(DSCPRC,DSCREC(DSCPRC_OFF))
	EQUIVALENCE(DSCSPR,DSCREC(DSCSPR_OFF))
	EQUIVALENCE(DSCDES,DSCREC(DSCDES_OFF))
	EQUIVALENCE(DSCTVC,DSCREC(DSCTVC_OFF))
	EQUIVALENCE(DSCMDS,DSCREC(DSCMDS_OFF))
        EQUIVALENCE(DSCWRO,DSCREC(DSCWRO_OFF))
        EQUIVALENCE(DSCWRA,DSCREC(DSCWRA_OFF))
        EQUIVALENCE(DSCWPO,DSCREC(DSCWPO_OFF))
        EQUIVALENCE(DSCWPA,DSCREC(DSCWPA_OFF))
        EQUIVALENCE(DSCWPR,DSCREC(DSCWPR_OFF))
        EQUIVALENCE(DSCLAT,DSCREC(DSCLAT_OFF))
