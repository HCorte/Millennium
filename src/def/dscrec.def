C DSCREC.DEF
C
C V07 30-JUN-2000 UXN DSCLAT added.
C V06 18-DEC-1993 HXK ADDED FIELDS FOR COUNTS AND AMOUNTS FOR 
C                     PRIZES/REFUNDS WON AND PAID.
C V05 06-JUL-1993 GXA Adjusted dimension of DSCNMS# to use SNMS_LEN parameters.
C V04 17-JUN-1993 GXA Released for Finland Dec Conversion / Oddset.
C V03 21-JAN-1993 DAB Initial Release
C                     Based on Netherlands Bible, 12/92, and Comm 1/93 update
C                     DEC Baseline
C V02 07-OCT-1991 MTK INITAL RELEASE FOR NETHERLANDS
C V01 01-AUG-1990 XXX RELEASED FOR VAX
C
C
C SCORE GAME DATA FILE RECORD LAYOUT (DISK)
C
C
C
C     DSCSTS                         ; GAME STATUS
C     DSCWEK                         ; WEEK NUMBER
C     DSCDAT                         ; DRAW DATE
C     DSCDRW                         ; DRAW NUMBER
C     DSCBSD                         ; BEGINNING SALES DATE
C     DSCESD                         ; ENDING SALES DATE
C     DSCPUP                         ; LAST PRUGE UPDATE  (FILE ONLY)
C     DSCUPD                         ; LAST FILE UPDATE   (FILE ONLY)
C     DSCCTM                         ; ACTUAL TIME WHEN CLOSED
C     DSCTIM                         ; TIME WHEN GAME SHOULD CLOSE
C     DSCSER                         ; LAST SERIAL NUMBER
C     DSCSAL                         ; TOTAL SALES
C     DSCPAD                         ; PRIZES PAID + REFUNDS
C     DSCPRG                         ; PRIZES PURGED
C     DSCPRF                         ; PAID REFUNDS
C     DSCREF                         ; TOTAL REFUND AMOUNT
C     DSCWON                         ; PRIZES WON
C     DSCPOL(2)                      ; ROLL POOL
C     DSCTPL                         ; TOTAL PRIZE POOL
C     DSCTBK                         ; TOTAL BREAKAGE
C     DSCBRK(2)                      ; WINNING ROUNDING POT
C     DSCABW                         ; AMOUNT BET ON WINNING SCORE
C     DSCODS                         ; WINNING ODDS
C     DSCWIN(2)                      ; WINNING RESULTS
C     DSCHLD(2)                      ; WINNING RESULTS HOLD
C     DSCTAX                         ; WINNING TAXES
C     DSCFUL(NUMTOT,MAXFUL)          ; FULL SYS SALES CNT AMT
C     DSCOTX(NUMTOT)                 ; OVER TAX LIMIT
C     DSCMID(NUMTOT)                 ; >REDMIN <REDSCX
C     DSCUTX(NUMTOT)                 ; UNDER TAX LIMIT
C     DSCHST                         ; HIGHEST WINNER FOR THIS GAME
C     DSCORM(NUMTOT)                 ; OVER REDSCX
C     DSCNM1(SNMS_LEN/4)             ; TEAM 1 NAME
C     DSCNM2(SNMS_LEN/4)             ; TEAM 2 NAME
C     DSCREV                         ; REV #
C     DSCTYP                         ; GAME TYPE 1=SOC. 2=HOCK.
C     DSCPFN(5)                      ; POOL FILE NAME
C     DSCPRC                         ; BASE PRICE
C     DSCSPR                         ; POOL PERCENTAGE
C     DSCDES(SDES_LEN/4)	     ; GAME DESCRIPTION
C     DSCTVC(STVC_LEN/4)             ; TV-CHANEL NAME
C     DSCMDS(MAXMLTD_AVL)	     ; MULTI DRAW SELECTED TABLE
C                                      (52 BYTE / 4 = 13)
C     DSCWRO(NUMTOT,2)               ; WINNING: COUNT,AMOUNT REFUNDS ONLY
C     DSCWRA(NUMTOT,2)               ; WINNING: COUNT,AMOUNT REFUNDS ALTOGETHER
C     DSCWPO(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES ONLY
C     DSCWPA(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES ALTOGETHER
C     DSCWPR(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES AND REFUNDS
C                                    ; (1=PRWON,2=PRPAD)
C     DSCLAT(NUMLAT)                 ; EVENT STARTED BEFORE GAME WAS CLOSED
C                                    ; TICKETS PLAYED LATER SHOULD BE REFUNDED.
C
C SCORE GAME FILE PARAMETERS
C
C+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
