C
C BINGO DATA FILE RECORD LAYOUT (DISK)
C
C     DBNSTS                         !STATUS
C     DBNCTM                         !ACTUAL TIME WHEN CLOSED
C     DBNTIM                         !TIME WHEN GAME SHOULD CLOSE
C     DBNDRW                         !DRAW NUMBER
C     DBNBSD                         !BINGO BEGGINING DRAW DATE
C     DBNESD                         !BINGO ENDING DRAW DATE
C     DBNPUP                         !LAST PURGE UPDATE
C     DBNUPD                         !LAST FILE UPDATE
C     DBNDAT(DATLEN)                 !DRAW DATES
C     DBNADV(NUMADV)                 !ADVANCE DRAW DATES
C     DBNSAL(BGOENT)                 !SALES DATA
C     DBNSHV(BGODIV,BGOSUB)          !SHARE VALUES
C     DBNSHR(BGODIV,BGOSUB)          !SHARES
C     DBNPOL(BGODIV,BGOSUB)          !POOL CARRIED OVER
C     DBNPAD(BGODIV,BGOSUB)          !AMOUNT PAID
C     DBNPRG(BGODIV,BGOSUB)          !AMOUNT PURGED
C     DBNANU(BGODIV,BGOSUB)          !ANNUITY PRIZES
C     DBNBRK(BGODIV,BGOSUB)          !BREAKAGE AMOUNT BY POOL
C     DBNOSV(BGODIV,BGOSUB)          !OLD SHARE VALUES
C     DBNFRZ(BGODIV,BGOSUB)          !DIVISION PAYOUT FROZEN FLAGS
C     DBNWIN(BGONBR)                 !WINNING RESULTS (FULL HOUSE)
C     DBNHLD(BGONBR)                 !WINNING RESULTS HOLD (FULL HOUSE)
C     DBNWAB(BGONAB)                 !WINNING RESULTS (A,B)
C     DBNHAB(BGONAB)                 !WINNING RESULTS HOLD (A,B)
C     DBNWLN(BGONLN)                 !WINNING RESULTS (LUCKY)
C     DBNHLN(BGONLN)                 !WINNING RESULTS HOLD (LUCKY)
C     DBNFAC                         !ANNUITY FACTOR
C     DBNCUT                         !ANNUITY CUT-IN
C     DBNTAX                         !LOTTERY TAX
C     DBNRES(2)                      !RESERVE POOL
C     DBNAPL                         !ADDITIONAL POOL
C     DBNMIN                         !MINIMUM POOL
C     DBNSER                         !LAST SERIAL NUMBER
C     DBNOPA                         !OVER-RIDE POOL AMOUNT
C     DBNPRC                         !PRICE/SINGLE COMBINATION
C     DBNMAX                         !MAXIMUM NUMBER OF EVENTS (ROWS)
C     DBNMLT                         !MULTI-DRAW ENABLE FLAG
C     DBNMDS(MAXMLTD_AVL)            !MULTI-DRAW SELECTED TABLE (52/4 WORDS)
C     DBNDIV(BGOSUB)                 !# OF DIVISIONS
C     DBNMAT(BGOMAXMAP+2+BGOCOL*BGOROW,BGOSUB) !DIVISION MATCH TABLE
C                    !(row bitmaps + worst + second worst + entire 5x5 panel)
C     DBNPER(BGODIV,BGOSUB)          !DIVISION PERCENTAGES
C     DBNSPR                         !SALES PERCENTAGE
C     DBNTSR(BGODIV,BGOSUB)          !TOTAL SHARE COUNT
C     DBNREV                         !BINGO REVISION NUMBER
C     DBNASH(BGODIV,BGOSUB)          !ADDITIONAL POOL BY DIV.
C     DBNFRG                         !FLAG FOR FREE GAME (PRICE = 0.00)
C     DBNWRF(POSTED)                 !WIN RESERVE FUND
C     DBNBST                         !BEST MATCH FLAG (ADJUST DIVISIONS)
C     DBNBAL(MAXDRW)                 !BALANCE SALES INFO. BY DRAW
C     DBNSED                         !FULL HOUSE SEED
C     DBNBAS                         !LUCKY NUMBER BASE
C     DBNPHS(BGOPHS)                 !#'s DRAWN PER PHASE (FH)
C     DBNPHSH(BGOPHS)                !#'s DRAWN PER PHASE (FH) HOLD
C     DBN1ST(BGO1ST)                 !#'s DRAWN FOR 1st BINGO,DOUBLE,etc. (FH)
C     DBNWST                         !#'s MATCHED FOR WORST WIN (FH)
C     DBNWSTH                        !#'s MATCHED FOR WORST WIN (FH) HOLD
C     DBNMAP(BGOMAXMAP,BGOSUB)       !MAX BITMAPS TO WIN ON
C     DBNMPF(BGOMAXMAP,BGOSUB)       !WIN ALLOWED FOR BITMAP OVERLAP FLAG
C     DBNAMP(BGODIV,BGOMAXMAP,BGOSUB) !# OF ADDITIONAL BITMAPS FOR DIVS WON
C     DBNPMT(3,BGOPHS)               !# OF (1=BITMAPS,2=NUMBERS,3=FIGURES)
C                                    !MATCH A PHASE TO COMPLETE
C     DBNLOB                         !NUMBER OF LAST BINGO WITH AB BOARDS
C     DBNWS2                         !#'s MATCHED FOR SECOND WORST WIN (FH)
C     DBNWS2H                        !#'s MATCHED FOR SECOND WORST WIN (FH) HOLD
C     DBNNDF(BGODIV)                 !#'s DRAWN FOR FIRST MATCH (FH)
C     DBNNDFH(BGODIV)                !#'s DRAWN FOR FIRST MATCH (FH) HOLD
C     DBNFST(BGODIV,BGOSUB)          !FLAGS SET FOR DIVISIONS OF FIRST MATCH
C     DBNOTH(BGODIV,BGOSUB)          !OTHER DIV WHICH WINS WITH SAME BITMAPS
C     DBNDNR(BGODIV)                 !DIV NUMBERS IN LOTTERY 1..20 SCALE (FH)
C     DBNNSP                         !NUMBER OF SUBPHASES(INCL.FULLHOUSE)
C     DBNSPH(BGOSPH)                 !#'s DRAWN PER SUBPHASE (FH) 
C     DBNSPHH(BGOSPH)                !#'s DRAWN PER SUBPHASE (FH) HOLD
C
C BINGO GAME FILE PARAMETERS
C
C
