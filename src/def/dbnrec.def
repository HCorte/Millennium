C
C RECORD OFFSET PARAMETERS
C
        INTEGER*4 DBNSTS,DBNCTM,DBNTIM,DBNDRW,DBNBSD,DBNESD
        INTEGER*4 DBNPUP,DBNUPD,DBNDAT(DATLEN),DBNADV(NUMADV)
        INTEGER*4 DBNSAL(BGOENT)
        INTEGER*4 DBNSHV(BGODIV,BGOSUB),DBNSHR(BGODIV,BGOSUB)
        INTEGER*4 DBNPOL(BGODIV,BGOSUB)
        INTEGER*4 DBNPAD(BGODIV,BGOSUB),DBNPRG(BGODIV,BGOSUB)
        INTEGER*4 DBNANU(BGODIV,BGOSUB),DBNBRK(BGODIV,BGOSUB)
        INTEGER*4 DBNOSV(BGODIV,BGOSUB)
        INTEGER*4 DBNFRZ(BGODIV,BGOSUB),DBNWIN(BGONBR),DBNHLD(BGONBR)
        INTEGER*4 DBNWAB(BGONAB),DBNHAB(BGONAB),DBNWLN(BGONLN)
        INTEGER*4 DBNHLN(BGONLN)
        INTEGER*4 DBNFAC,DBNCUT
        INTEGER*4 DBNTAX,DBNRES(2),DBNAPL,DBNMIN,DBNSER,DBNOPA,DBNPRC
        INTEGER*4 DBNMAX,DBNMLT
        BYTE      DBNMDS(MAXMLTD_AVL)
        INTEGER*4 DBNDIV(BGOSUB)
        INTEGER*4 DBNMAT(BGOMAXMAP+2+BGOCOL*BGOROW,BGOSUB)
        INTEGER*4 DBNPER(BGODIV,BGOSUB)
        INTEGER*4 DBNSPR,DBNTSR(BGODIV,BGOSUB),DBNREV
        INTEGER*4 DBNASH(BGODIV,BGOSUB),DBNFRG,DBNWRF(POSTED),DBNBST
        INTEGER*4 DBNBAL(MAXDRW),DBNSED,DBNBAS,DBNPHS(BGOPHS)
        INTEGER*4 DBNPHSH(BGOPHS),DBN1ST(BGO1ST),DBNWST,DBNWSTH
        INTEGER*4 DBNMAP(BGOMAXMAP,BGOSUB)
        INTEGER*4 DBNMPF(BGOMAXMAP,BGOSUB)
        INTEGER*4 DBNAMP(BGODIV,BGOMAXMAP,BGOSUB)
        INTEGER*4 DBNPMT(3,BGOPHS)
        INTEGER*4 DBNLOB
        INTEGER*4 DBNWS2
        INTEGER*4 DBNWS2H
        INTEGER*4 DBNNDF(BGODIV)
        INTEGER*4 DBNNDFH(BGODIV)
        INTEGER*4 DBNFST(BGODIV,BGOSUB)
        INTEGER*4 DBNOTH(BGODIV,BGOSUB)
        INTEGER*4 DBNDNR(BGODIV)
        INTEGER*4 DBNNSP,DBNSPH(BGOSPH),DBNSPHH(BGOSPH)
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
        INTEGER*4 DBNSTS_OFF, DBNCTM_OFF, DBNTIM_OFF, DBNDRW_OFF
        PARAMETER(DBNSTS_OFF = 1)                                       !0001
        PARAMETER(DBNCTM_OFF = DBNSTS_OFF + 1)                          !0002
        PARAMETER(DBNTIM_OFF = DBNCTM_OFF + 1)                          !0003
        PARAMETER(DBNDRW_OFF = DBNTIM_OFF + 1)                          !0004
C
        INTEGER*4 DBNBSD_OFF, DBNESD_OFF, DBNPUP_OFF, DBNUPD_OFF
        PARAMETER(DBNBSD_OFF = DBNDRW_OFF + 1)                          !0005
        PARAMETER(DBNESD_OFF = DBNBSD_OFF + 1)                          !0006
        PARAMETER(DBNPUP_OFF = DBNESD_OFF + 1)                          !0007
        PARAMETER(DBNUPD_OFF = DBNPUP_OFF + 1)                          !0008   
C
        INTEGER*4 DBNDAT_OFF, DBNADV_OFF, DBNSAL_OFF, DBNSHV_OFF
        PARAMETER(DBNDAT_OFF = DBNUPD_OFF + 1)                          !0009
        PARAMETER(DBNADV_OFF = DBNDAT_OFF + DATLEN)                     !0029
        PARAMETER(DBNSAL_OFF = DBNADV_OFF + NUMADV)                     !0036
        PARAMETER(DBNSHV_OFF = DBNSAL_OFF + BGOENT)                     !0061
C
        INTEGER*4 DBNSHR_OFF, DBNPOL_OFF, DBNPAD_OFF, DBNPRG_OFF
        PARAMETER(DBNSHR_OFF = DBNSHV_OFF + BGODIV*BGOSUB)
        PARAMETER(DBNPOL_OFF = DBNSHR_OFF + BGODIV*BGOSUB)      !0077
        PARAMETER(DBNPAD_OFF = DBNPOL_OFF + BGODIV*BGOSUB)      !0085
        PARAMETER(DBNPRG_OFF = DBNPAD_OFF + BGODIV*BGOSUB)      !0093
C
        INTEGER*4 DBNANU_OFF, DBNBRK_OFF, DBNOSV_OFF, DBNFRZ_OFF
        PARAMETER(DBNANU_OFF = DBNPRG_OFF + BGODIV*BGOSUB)      !0101
        PARAMETER(DBNBRK_OFF = DBNANU_OFF + BGODIV*BGOSUB)      !0109
        PARAMETER(DBNOSV_OFF = DBNBRK_OFF + BGODIV*BGOSUB)      !0117
        PARAMETER(DBNFRZ_OFF = DBNOSV_OFF + BGODIV*BGOSUB)      !0125
C
        INTEGER*4 DBNWIN_OFF, DBNHLD_OFF
        PARAMETER(DBNWIN_OFF = DBNFRZ_OFF + BGODIV*BGOSUB)              !0133
        PARAMETER(DBNHLD_OFF = DBNWIN_OFF + BGONBR)                     !0146
C
        INTEGER*4 DBNWAB_OFF, DBNHAB_OFF, DBNWLN_OFF, DBNHLN_OFF
        PARAMETER(DBNWAB_OFF = DBNHLD_OFF + BGONBR)                     !
        PARAMETER(DBNHAB_OFF = DBNWAB_OFF + BGONAB)                     !
        PARAMETER(DBNWLN_OFF = DBNHAB_OFF + BGONAB)                     !
        PARAMETER(DBNHLN_OFF = DBNWLN_OFF + BGONLN)                     !
C
        INTEGER*4 DBNFAC_OFF, DBNCUT_OFF
        PARAMETER(DBNFAC_OFF = DBNHLN_OFF + BGONLN)
        PARAMETER(DBNCUT_OFF = DBNFAC_OFF + 1)                          !0160
C
        INTEGER*4 DBNTAX_OFF, DBNRES_OFF, DBNAPL_OFF, DBNMIN_OFF
        PARAMETER(DBNTAX_OFF = DBNCUT_OFF + 1)                          !0161
        PARAMETER(DBNRES_OFF = DBNTAX_OFF + 1)                          !0162
        PARAMETER(DBNAPL_OFF = DBNRES_OFF + 2)                          !0164
        PARAMETER(DBNMIN_OFF = DBNAPL_OFF + 1)                          !0165
C
        INTEGER*4 DBNSER_OFF, DBNOPA_OFF, DBNPRC_OFF, DBNMAX_OFF
        PARAMETER(DBNSER_OFF = DBNMIN_OFF + 1)                          !0166
        PARAMETER(DBNOPA_OFF = DBNSER_OFF + 1)                          !0167
        PARAMETER(DBNPRC_OFF = DBNOPA_OFF + 1)                          !0168
        PARAMETER(DBNMAX_OFF = DBNPRC_OFF + 1)                          !0169
C
        INTEGER*4 DBNMLT_OFF, DBNMDS_OFF, DBNDIV_OFF, DBNMAT_OFF
        PARAMETER(DBNMLT_OFF = DBNMAX_OFF + 1)                          !0170
        PARAMETER(DBNMDS_OFF = DBNMLT_OFF + 1)                          !0171
        PARAMETER(DBNDIV_OFF = DBNMDS_OFF + (MAXMLTD_AVL/4))            !0184
        PARAMETER(DBNMAT_OFF = DBNDIV_OFF + BGOSUB)
C
        INTEGER*4 DBNPER_OFF, DBNSPR_OFF, DBNTSR_OFF, DBNREV_OFF
        PARAMETER(DBNPER_OFF = DBNMAT_OFF + 
     *                         (BGOMAXMAP+2+BGOCOL*BGOROW)*BGOSUB)
        PARAMETER(DBNSPR_OFF = DBNPER_OFF + BGODIV*BGOSUB)
        PARAMETER(DBNTSR_OFF = DBNSPR_OFF + 1)                          !0202
        PARAMETER(DBNREV_OFF = DBNTSR_OFF + BGODIV*BGOSUB)              !0210
C
        INTEGER*4 DBNASH_OFF, DBNFRG_OFF
        PARAMETER(DBNASH_OFF = DBNREV_OFF + 1)                          !0340
        PARAMETER(DBNFRG_OFF = DBNASH_OFF + BGODIV*BGOSUB)      !0348
C
        INTEGER*4 DBNWRF_OFF, DBNBST_OFF, DBNBAL_OFF
        PARAMETER(DBNWRF_OFF = DBNFRG_OFF + 1)                          !0349
        PARAMETER(DBNBST_OFF = DBNWRF_OFF + POSTED)                     !0359
        PARAMETER(DBNBAL_OFF = DBNBST_OFF + 1)                          !0360
C
        INTEGER*4 DBNSED_OFF, DBNBAS_OFF, DBNPHS_OFF
        INTEGER*4 DBNPHSH_OFF,DBN1ST_OFF
        PARAMETER(DBNSED_OFF = DBNBAL_OFF + MAXDRW)                
        PARAMETER(DBNBAS_OFF = DBNSED_OFF + 1)                     
        PARAMETER(DBNPHS_OFF = DBNBAS_OFF + 1)
        PARAMETER(DBNPHSH_OFF= DBNPHS_OFF + BGOPHS)
        PARAMETER(DBN1ST_OFF = DBNPHSH_OFF + BGOPHS)               

        INTEGER*4 DBNWST_OFF, DBNWSTH_OFF, DBNMAP_OFF, DBNMPF_OFF
        INTEGER*4 DBNAMP_OFF
        PARAMETER(DBNWST_OFF = DBN1ST_OFF + BGO1ST)
        PARAMETER(DBNWSTH_OFF= DBNWST_OFF + 1)
        PARAMETER(DBNMAP_OFF = DBNWSTH_OFF + 1)                     !0380
        PARAMETER(DBNMPF_OFF = DBNMAP_OFF + BGOMAXMAP*BGOSUB)  
        PARAMETER(DBNAMP_OFF = DBNMPF_OFF + BGOMAXMAP*BGOSUB)
        
        INTEGER*4 DBNPMT_OFF
        PARAMETER(DBNPMT_OFF = DBNAMP_OFF + BGODIV*BGOMAXMAP*BGOSUB)
        INTEGER*4 DBNLOB_OFF
        INTEGER*4 DBNWS2_OFF,DBNWS2H_OFF,DBNNDF_OFF,DBNNDFH_OFF
        INTEGER*4 DBNFST_OFF,DBNOTH_OFF,DBNDNR_OFF
        PARAMETER(DBNLOB_OFF = DBNPMT_OFF + 3*BGOPHS)
        PARAMETER(DBNWS2_OFF = DBNLOB_OFF + 1)
        PARAMETER(DBNWS2H_OFF = DBNWS2_OFF + 1)
        PARAMETER(DBNNDF_OFF = DBNWS2H_OFF + 1)
        PARAMETER(DBNNDFH_OFF = DBNNDF_OFF + BGODIV)
        PARAMETER(DBNFST_OFF = DBNNDFH_OFF + BGODIV)
        PARAMETER(DBNOTH_OFF = DBNFST_OFF + BGODIV*BGOSUB)
        PARAMETER(DBNDNR_OFF = DBNOTH_OFF + BGODIV*BGOSUB)

        INTEGER*4 DBNNSP_OFF, DBNSPH_OFF, DBNSPHH_OFF
        PARAMETER(DBNNSP_OFF = DBNDNR_OFF + BGODIV)
        PARAMETER(DBNSPH_OFF = DBNNSP_OFF + 1)
        PARAMETER(DBNSPHH_OFF = DBNSPH_OFF + BGOSPH)

        INTEGER*4  DBNFRE_OFF
        PARAMETER(DBNFRE_OFF = DBNSPHH_OFF + BGOSPH)
C
C CALCULATE FREE SPACE BASED ON MACHINE DEPENDENT SECTOR SIZES.
C SHOULD ALLWAYS ADD UP TO A EVEN SECTOR SIZE BYTES (VAX 512).
C NOTE! IN CALCULATIONS, FRE_OFF-2 IS DONE TO TAKE CARE OF WHEN THE DATA SIZE
C       ENDS ON A EVEN SECTOR SIZE.
C
        INTEGER*4 DBNSEC_VAX, DBNSEC_NUM
        PARAMETER (DBNSEC_VAX = SECSIZE/4)
        PARAMETER (DBNSEC_NUM = 
     *             NINT(FLOAT((DBNFRE_OFF-2)/DBNSEC_VAX)+.5))
C
        INTEGER*4 DBNLEN, DBNSEC
        PARAMETER (DBNLEN = DBNSEC_VAX*DBNSEC_NUM)         !EVEN SEC SIZE
        PARAMETER (DBNSEC = DBNLEN / 64)
C
        INTEGER*4 DBNREC(DBNLEN)        
C
        EQUIVALENCE(DBNSTS,DBNREC(DBNSTS_OFF))
        EQUIVALENCE(DBNCTM,DBNREC(DBNCTM_OFF))
        EQUIVALENCE(DBNTIM,DBNREC(DBNTIM_OFF))
        EQUIVALENCE(DBNDRW,DBNREC(DBNDRW_OFF))
        EQUIVALENCE(DBNBSD,DBNREC(DBNBSD_OFF))
        EQUIVALENCE(DBNESD,DBNREC(DBNESD_OFF))
        EQUIVALENCE(DBNPUP,DBNREC(DBNPUP_OFF))
        EQUIVALENCE(DBNUPD,DBNREC(DBNUPD_OFF))
        EQUIVALENCE(DBNDAT,DBNREC(DBNDAT_OFF))
        EQUIVALENCE(DBNADV,DBNREC(DBNADV_OFF))
        EQUIVALENCE(DBNSAL,DBNREC(DBNSAL_OFF))
        EQUIVALENCE(DBNSHV,DBNREC(DBNSHV_OFF))
        EQUIVALENCE(DBNSHR,DBNREC(DBNSHR_OFF))
        EQUIVALENCE(DBNPOL,DBNREC(DBNPOL_OFF))
        EQUIVALENCE(DBNPAD,DBNREC(DBNPAD_OFF))
        EQUIVALENCE(DBNPRG,DBNREC(DBNPRG_OFF))
        EQUIVALENCE(DBNANU,DBNREC(DBNANU_OFF))
        EQUIVALENCE(DBNBRK,DBNREC(DBNBRK_OFF))
        EQUIVALENCE(DBNOSV,DBNREC(DBNOSV_OFF))
        EQUIVALENCE(DBNFRZ,DBNREC(DBNFRZ_OFF))
        EQUIVALENCE(DBNWIN,DBNREC(DBNWIN_OFF))
        EQUIVALENCE(DBNHLD,DBNREC(DBNHLD_OFF))
        EQUIVALENCE(DBNWAB,DBNREC(DBNWAB_OFF))
        EQUIVALENCE(DBNHAB,DBNREC(DBNHAB_OFF))
        EQUIVALENCE(DBNWLN,DBNREC(DBNWLN_OFF))
        EQUIVALENCE(DBNHLN,DBNREC(DBNHLN_OFF))
        EQUIVALENCE(DBNFAC,DBNREC(DBNFAC_OFF))
        EQUIVALENCE(DBNCUT,DBNREC(DBNCUT_OFF))
        EQUIVALENCE(DBNTAX,DBNREC(DBNTAX_OFF))
        EQUIVALENCE(DBNRES,DBNREC(DBNRES_OFF))
        EQUIVALENCE(DBNAPL,DBNREC(DBNAPL_OFF))
        EQUIVALENCE(DBNMIN,DBNREC(DBNMIN_OFF))
        EQUIVALENCE(DBNSER,DBNREC(DBNSER_OFF))
        EQUIVALENCE(DBNOPA,DBNREC(DBNOPA_OFF))
        EQUIVALENCE(DBNPRC,DBNREC(DBNPRC_OFF))
        EQUIVALENCE(DBNMAX,DBNREC(DBNMAX_OFF))
        EQUIVALENCE(DBNMLT,DBNREC(DBNMLT_OFF))
        EQUIVALENCE(DBNMDS,DBNREC(DBNMDS_OFF))
        EQUIVALENCE(DBNDIV,DBNREC(DBNDIV_OFF))
        EQUIVALENCE(DBNMAT,DBNREC(DBNMAT_OFF))
        EQUIVALENCE(DBNPER,DBNREC(DBNPER_OFF))
        EQUIVALENCE(DBNSPR,DBNREC(DBNSPR_OFF))
        EQUIVALENCE(DBNTSR,DBNREC(DBNTSR_OFF))
        EQUIVALENCE(DBNREV,DBNREC(DBNREV_OFF))
        EQUIVALENCE(DBNASH,DBNREC(DBNASH_OFF))
        EQUIVALENCE(DBNFRG,DBNREC(DBNFRG_OFF))
        EQUIVALENCE(DBNWRF,DBNREC(DBNWRF_OFF))
        EQUIVALENCE(DBNBST,DBNREC(DBNBST_OFF))
        EQUIVALENCE(DBNBAL,DBNREC(DBNBAL_OFF))
        EQUIVALENCE(DBNSED,DBNREC(DBNSED_OFF))
        EQUIVALENCE(DBNBAS,DBNREC(DBNBAS_OFF))
        EQUIVALENCE(DBNPHS,DBNREC(DBNPHS_OFF))
        EQUIVALENCE(DBNPHSH,DBNREC(DBNPHSH_OFF))
        EQUIVALENCE(DBN1ST,DBNREC(DBN1ST_OFF))
        EQUIVALENCE(DBNWST,DBNREC(DBNWST_OFF))
        EQUIVALENCE(DBNWSTH,DBNREC(DBNWSTH_OFF))
        EQUIVALENCE(DBNMAP,DBNREC(DBNMAP_OFF))
        EQUIVALENCE(DBNMPF,DBNREC(DBNMPF_OFF))
        EQUIVALENCE(DBNAMP,DBNREC(DBNAMP_OFF))
        EQUIVALENCE(DBNPMT,DBNREC(DBNPMT_OFF))
        EQUIVALENCE(DBNLOB,DBNREC(DBNLOB_OFF))
        EQUIVALENCE(DBNWS2,DBNREC(DBNWS2_OFF))
        EQUIVALENCE(DBNWS2H,DBNREC(DBNWS2H_OFF))
        EQUIVALENCE(DBNNDF,DBNREC(DBNNDF_OFF))
        EQUIVALENCE(DBNNDFH,DBNREC(DBNNDFH_OFF))
        EQUIVALENCE(DBNFST,DBNREC(DBNFST_OFF))
        EQUIVALENCE(DBNOTH,DBNREC(DBNOTH_OFF))
        EQUIVALENCE(DBNDNR,DBNREC(DBNDNR_OFF))
        EQUIVALENCE(DBNNSP,DBNREC(DBNNSP_OFF))
        EQUIVALENCE(DBNSPH,DBNREC(DBNSPH_OFF))
        EQUIVALENCE(DBNSPHH,DBNREC(DBNSPHH_OFF))
