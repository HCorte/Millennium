	INTEGER*4  CRSLEN
	PARAMETER (CRSLEN=100)
	INTEGER*4  CRSBUF
	DIMENSION CRSBUF(CRSLEN)
C
C QUOTA OFFSETS
C
	INTEGER*4 MAXQTA
	PARAMETER (MAXQTA=10)
C
	INTEGER*4 NBRENT, NXTGAM, QTGAME, OORDER, QUOTAM
	PARAMETER (NBRENT=1)			!NUMBER OF ENTRIES
	PARAMETER (NXTGAM=2)			!NEXT GAME TO REQUEST
	PARAMETER (QTGAME=3)			!GAME ID   (03-12)
	PARAMETER (OORDER=QTGAME+MAXQTA)	!ON ORDER  (13-22)
	PARAMETER (QUOTAM=OORDER+MAXQTA)	!QUOTA MAX (23-32)
C
	INTEGER*4 UNSOLD, REORDE
	PARAMETER (UNSOLD=QUOTAM+MAXQTA)	!UNSOLD    (33-42)
	PARAMETER (REORDE=UNSOLD+MAXQTA)	!REORDER   (43-52)
C
C INVENTORY OFFSETS
C
	INTEGER*4 MAXINV
	PARAMETER (MAXINV=10)
C
	INTEGER*4 NXTPCK, INGAME, INPACK
C						!NUMBER OF ENTRIES
C						!NEXT GAME TO REQUEST
	PARAMETER (NXTPCK=3)			!NEXT PACK TO REQUEST
	PARAMETER (INGAME=4)			!GAME ID     (04-13)
	PARAMETER (INPACK=INGAME+MAXINV)	!PACK NUMBER (14-23)
C
	INTEGER*4 INSTAT
	PARAMETER (INSTAT=INPACK+MAXINV)	!STATUS      (24-33)
C
C FINANCIAL REPORT OFFSETS
C
	INTEGER*4 NXTAGT, STRSAL
	PARAMETER (NXTAGT=1)			!NEXT AGENT TO REQUEST
	PARAMETER (STRSAL=2)			!START SALES DATA
C
C MENU OFFSETS
C
	INTEGER*4 MAXSHT, MAXLNG
	PARAMETER (MAXSHT=10)
	PARAMETER (MAXLNG=5)
C
C ADDRESS OFFSETS
C
	INTEGER*4 MNUADD
C						!NUMBER OF ENTRIES
	PARAMETER (MNUADD=2)			!START ADDRESS
C
C LONG AND SHORT OFFSETS
C
	INTEGER*4 MNUQTA, MNUITM
	PARAMETER (MNUQTA=2)			!QUOTA		(2-11)
	PARAMETER (MNUITM=MNUQTA+MAXSHT)	!ITEM NUMBER	(12-21)
C
	INTEGER*4 MNUDSC1, MNUDSC2, MNUDSC3, MNUDSC4, MNUDSC5
	PARAMETER (MNUDSC1=MNUITM+MAXSHT)	!DESCRIPTION1	(22-31)
	PARAMETER (MNUDSC2=MNUDSC1+MAXSHT)	!DESCRIPTION2	(32-41)
	PARAMETER (MNUDSC3=MNUDSC2+MAXSHT)	!DESCRIPTION3	(42-51)
	PARAMETER (MNUDSC4=MNUDSC3+MAXSHT)	!DESCRIPTION4	(52-61)
	PARAMETER (MNUDSC5=MNUDSC4+MAXSHT)	!DESCRIPTION5	(62-71)
C
C SUPPLY ORDER OFFSETS
C
	INTEGER*4 ORDNUM, ORDINF
	PARAMETER (ORDNUM=1)			!ORDER NUMBER
	PARAMETER (ORDINF=2)			!ORDER INFO
C
C GAME TABLE OFFSETS (CLASS=1) AND GAME PARAMETERS
C
	INTEGER*4 MAXGTA,MAXGTB
	PARAMETER (MAXGTB=4)                   !AVAILABLE FOR ORDERING
	PARAMETER (MAXGTA=40)                   !ACTIVE GAMES
C
	INTEGER*4 GTBCHK,GTBGAM,GTBNM1,GTBNM2,GTBNM3,GTBNM4,GTBNM5,GTBNM6,GTBNM7
	INTEGER*4 GTBFLG,TKTPRC,TKTPCK,GTBRCL
	PARAMETER (GTBCHK=3)			!CHECKSUM
	PARAMETER (GTBRCL=4)			!RETAILER CREDIT LIMIT
	PARAMETER (GTBGAM=5)			!ACTIVE GAME NUMBERS (05-44)
	PARAMETER (GTBNM1=GTBGAM+MAXGTA)        !GAME NAME 1         (45-48)
	PARAMETER (GTBNM2=GTBNM1+MAXGTB)	!GAME NAME 2	     (49-52)
	PARAMETER (GTBNM3=GTBNM2+MAXGTB)	!GAME NAME 3	     (53-56)
	PARAMETER (GTBNM4=GTBNM3+MAXGTB)	!GAME NAME 4	     (57-60)
	PARAMETER (GTBNM5=GTBNM4+MAXGTB)	!GAME NAME 5	     (61-64)
	PARAMETER (GTBNM6=GTBNM5+MAXGTB)	!GAME NAME 6	     (65-68)
	PARAMETER (GTBNM7=GTBNM6+MAXGTB)	!GAME NAME 7	     (69-72)
	PARAMETER (GTBFLG=GTBNM7+MAXGTB)        !GAME FLAGS          (73-76)
	PARAMETER (TKTPRC=GTBFLG+MAXGTB)        !TICKET COST         (77-80)
	PARAMETER (TKTPCK=TKTPRC+MAXGTB)        !TICKETS PER PACK    (81-84)
C
C GAME TABLE OFFSETS (CLASS=2)
C
	INTEGER*4 MAXTBL
	PARAMETER (MAXTBL=2)
C
	INTEGER*4 GTBGNM, GTBPRZ1, GTBPRZ2, GTBPRZ3 
	PARAMETER (GTBGNM=1)			!GAME NUMBER	(01-02)
	PARAMETER (GTBPRZ1=GTBGNM+MAXTBL)	!PRIZE ONE	(03-04)
	PARAMETER (GTBPRZ2=GTBPRZ1+MAXTBL)	!PRIZE TWO	(05-06)
	PARAMETER (GTBPRZ3=GTBPRZ2+MAXTBL)	!PRIZE THREE	(07-08)
C
	INTEGER*4 GTBPRZ4, GTBPRZ5, GTBPRZ6, GTBPRZ7, GTBPRZ8
	PARAMETER (GTBPRZ4=GTBPRZ3+MAXTBL)	!PRIZE FOUR	(10-11)
	PARAMETER (GTBPRZ5=GTBPRZ4+MAXTBL)	!PRIZE FIVE	(12-13)
	PARAMETER (GTBPRZ6=GTBPRZ5+MAXTBL)	!PRIZE SIX	(14-15)
	PARAMETER (GTBPRZ7=GTBPRZ6+MAXTBL)	!PRIZE SEVEN	(16-17)
	PARAMETER (GTBPRZ8=GTBPRZ7+MAXTBL)	!PRIZE EIGHT	(18-19)
C
	INTEGER*4 GTBPRZ9, GTBPRZ10, GTBPRZ11, GTBPRZ12, GTBPRZ13
	PARAMETER (GTBPRZ9=GTBPRZ8+MAXTBL)	!PRIZE NINE	(20-21)
        PARAMETER (GTBPRZ10=GTBPRZ9+MAXTBL)     !PRIZE TEN      (22-23)
        PARAMETER (GTBPRZ11=GTBPRZ10+MAXTBL)    !PRIZE ELEVEN   (24-25)
        PARAMETER (GTBPRZ12=GTBPRZ11+MAXTBL)    !PRIZE TWELVE   (26-27)
        PARAMETER (GTBPRZ13=GTBPRZ12+MAXTBL)    !PRIZE THIRTEEN (28-29)
C
        INTEGER*4 GTBPRZ14, GTBPRZ15, GTBPRZ16, GTBPRZ17, GTBPRZ18
        PARAMETER (GTBPRZ14=GTBPRZ13+MAXTBL)    !PRIZE FOURTEEN (30-31)
        PARAMETER (GTBPRZ15=GTBPRZ14+MAXTBL)    !PRIZE FIFTEEN  (32-33)
        PARAMETER (GTBPRZ16=GTBPRZ15+MAXTBL)    !PRIZE SIXTEEN  (34-35)
        PARAMETER (GTBPRZ17=GTBPRZ16+MAXTBL)    !PRIZE SEVENTEEN(36-37)
        PARAMETER (GTBPRZ18=GTBPRZ17+MAXTBL)    !PRIZE EIGHTEEN (38-39)
C
        INTEGER*4 GTBPRZ19, GTBPRZ20
        PARAMETER (GTBPRZ19=GTBPRZ18+MAXTBL)    !PRIZE NINETEENN (40-41)
        PARAMETER (GTBPRZ20=GTBPRZ19+MAXTBL)    !PRIZE TWENTY    (42-43)
C
        INTEGER*4 GTBNAME1, GTBNAME2, GTBNAME3
        PARAMETER (GTBNAME1=GTBPRZ20+MAXTBL)    !GAME NAME       (44-45)
        PARAMETER (GTBNAME2=GTBNAME1+MAXTBL)    !GAME NAME       (46-47)
        PARAMETER (GTBNAME3=GTBNAME2+MAXTBL)    !GAME NAME       (48-49)
C
C PACK SETTLED REPORT
C
        INTEGER*4  MAXPCK
        PARAMETER (MAXPCK=7)                    !MAXIMUM NUMBER OF PACKS
C
                                                !NUMBER OF ENTRIES
                                                !NEXT GAME TO REQUEST
                                                !NEXT PACK TO REQUEST
C
        INTEGER*4  NXTRET,BEGMON, BEGDAY, BEGYEAR, ENDMON, ENDDAY, ENDYEAR
        PARAMETER (NXTRET=4)                    !NEXT AGT TO REQUEST FOR STORE
        PARAMETER (BEGMON=5)                    !BEGIN INVOICE DATE MONTH
        PARAMETER (BEGDAY=6)                    !BEGIN INVOICE DATE DAY
        PARAMETER (BEGYEAR=7)                   !BEGIN INVOICE DATE YEAR
        PARAMETER (ENDMON=8)                    !END INVOICE DATE MONTH
        PARAMETER (ENDDAY=9)                    !END INVOICE DATE DAY
        PARAMETER (ENDYEAR=10)                  !END INVOICE DATE YEAR
C
        INTEGER*4  PCKAMT
        PARAMETER (PCKAMT=11)                   !PACK SETTLED AMOUNT
C
        INTEGER*4  PCKNUM, ORDNUM1, ORDNUM2, GAMNUM, AUTFLG
        PARAMETER (PCKNUM=PCKAMT+1)             !PACK NUMBER         (12-21)
        PARAMETER (GAMNUM=PCKNUM+MAXPCK)        !GAME NUMBER         (22-31)
        PARAMETER (ORDNUM1=GAMNUM+MAXPCK)       !ORDER NUMBER 1      (32-41)
        PARAMETER (ORDNUM2=ORDNUM1+MAXPCK)      !ORDER NUMBER 2      (42-51)
        PARAMETER (AUTFLG=ORDNUM2+MAXPCK)       !AUTO SETTLED FLAG   (52-61)
C
        INTEGER*4  PCKMON, PCKDAY, PCKYEAR
        PARAMETER (PCKMON=AUTFLG+MAXPCK)        !DATE MONTH          (62-71)
        PARAMETER (PCKDAY=PCKMON+MAXPCK)        !DATE DAY            (72-81)
        PARAMETER (PCKYEAR=PCKDAY+MAXPCK)       !DATE YEAR           (82-91)
C
C ADJUSTMENT REPORT
C
        INTEGER*4  MAXADJ
        PARAMETER (MAXADJ=7)                   !MAXIMUM NUMBER OF ADJUSTMENTS 
        INTEGER*4  NXTDAT, NXTTIM, NXTTYP
                                                !NUMBER OF ENTRIES
        PARAMETER (NXTDAT=2)                    !NEXT DATE (YYYYMMDD) TO REQUEST
        PARAMETER (NXTTIM=3)                    !NEXT TIME (HHMMSS) TO REQUEST
        PARAMETER (NXTTYP=4)                    !NEXT ADJUSTMENT TYPE REQUEST
                                                !BEGIN INVOICE DATE MONTH
                                                !BEGIN INVOICE DATE DAY
                                                !BEGIN INVOICE DATE YEAR
                                                !END INVOICE DATE MONTH
                                                !END INVOICE DATE DAY
                                                !END INVOICE DATE YEAR    
C
C
        INTEGER*4 BEGCDC, ENDCDC, MFLAG, ADJTOT
        PARAMETER (BEGCDC = 5)                  !BEGIN INVOICE CDC
        PARAMETER (ENDCDC = 6)                  !END INVOICE CDC
        PARAMETER (MFLAG = 7)                   !MOREFLAG
        PARAMETER (ADJTOT=8)                    !TOTAL ADJUSTMENT AMOUNT
        INTEGER*4 ADJTXT1, ADJTXT2, ADJTXT3, ADJTXT4, ADJCDC, ADJAMT    !V02
        PARAMETER (ADJTXT1=ADJTOT+MAXADJ)       !ADJUSTMENT TEXT 1 (11-20)
        PARAMETER (ADJTXT2=ADJTXT1+MAXADJ)      !ADJUSTMENT TEXT 2 (21-30)
        PARAMETER (ADJTXT3=ADJTXT2+MAXADJ)      !ADJUSTMENT TEXT 3 (31-40)
        PARAMETER (ADJTXT4=ADJTXT3+MAXADJ)      !V02
        PARAMETER (ADJCDC=ADJTXT4+MAXADJ)       !ADJUSTMENT CDC
        PARAMETER (ADJAMT=ADJCDC+MAXADJ)        !ADJUSTMENT AMOUNT
