C
C SCORE GAME VERIFY DATA FILE RECORD LAYOUT (DISK)
C
C
C
C     TSCSTS                         ; GAME STATUS
C     TSCWEK                         ; WEEK NUMBER
C     TSCDAT                         ; DRAW DATE
C     TSCDRW                         ; DRAW NUMBER
C     TSCBSD                         ; BEGINNING SALES DATE
C     TSCESD                         ; ENDING SALES DATE
C     TSCPUP                         ; LAST PRUGE UPDATE  (FILE ONLY)
C     TSCUPD                         ; LAST FILE UPDATE   (FILE ONLY)
C     TSCCTM                         ; ACTUAL TIME WHEN CLOSED
C     TSCTIM                         ; TIME WHEN GAME SHOULD CLOSE
C     TSCSER                         ; LAST SERIAL NUMBER
C     TSCSAL                         ; TOTAL SALES
C     TSCPAD                         ; PRIZES PAID + REFUNDS
C     TSCPRG                         ; PRIZES PURGED
C     TSCPRF                         ; PAID REFUNDS
C     TSCREF                         ; TOTAL REFUND AMOUNT
C     TSCWON                         ; PRIZES WON
C     TSCPOL(2)                      ; ROLL POOL
C     TSCTPL                         ; TOTAL PRIZE POOL
C     TSCTBK                         ; TOTAL BREAKAGE
C     TSCBRK(2)                      ; WINNING ROUNDING POT
C     TSCABW                         ; AMOUNT BET ON WINNING SCORE
C     TSCODS                         ; WINNING ODDS
C     TSCWIN(2)                      ; WINNING RESULTS
C     TSCHLD(2)                      ; WINNING RESULTS HOLD
C     TSCTAX                         ; WINNING TAXES
C     TSCFUL(NUMTOT,MAXFUL)          ; FULL SYS SALES CNT AMT
C     TSCOTX(NUMTOT)                 ; OVER TAX LIMIT
C     TSCMID(NUMTOT)                 ; >REDMIN <REDMAX
C     TSCUTX(NUMTOT)                 ; UNDER TAX LIMIT
C     TSCHST                         ; HIGHEST WINNER FOR THIS GAME
C     TSCORM(NUMTOT)                 ; OVER REDMAX
C     TSCNM1(SNMS_LEN/4)             ; TEAM 1 NAME
C     TSCNM2(SNMS_LEN/4)             ; TEAM 2 NAME
C     TSCREV                         ; REV #
C     TSCTYP                         ; GAME TYPE 1=SOC. 2=HOCK.
C     TSCPFN(5)                      ; POOL FILE NAME
C     TSCPRC                         ; BASE PRICE
C     TSCSPR                         ; POOL PERCENTAGE
C     TSCDES(SDES_LEN/4)	     ; GAME DESCRIPTION
C     TSCTVC(STVC_LEN/4)             ; TV-CHANEL NAME
C     TSCMDS(MAXMLTD_AVL)	     ; MULTI DRAW SELECTED TABLE
C                                      (52 BYTE / 4 = 13)
C     TSCWRO(NUMTOT,2)               ; WINNING: COUNT,AMOUNT REFUNDS ONLY
C     TSCWRA(NUMTOT,2)               ; WINNING: COUNT,AMOUNT REFUNDS ALTOGETHER
C     TSCWPO(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES ONLY
C     TSCWPA(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES ALTOGETHER
C     TSCWPR(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES AND REFUNDS
C                                    ; (1=PRWON,2=PRPAD)
C
C SCORE GAME FILE PARAMETERS
C
C
