	INTEGER*4 TSCSTS,TSCWEK,TSCDAT,TSCBSD,TSCESD,TSCCTM,TSCTIM
	INTEGER*4 TSCSAL,TSCPAD,TSCPRG,TSCREF,TSCWON,TSCPOL(2),TSCTPL
	INTEGER*4 TSCTBK,TSCBRK(2),TSCABW,TSCODS,TSCWIN(2),TSCHLD(2)
	INTEGER*4 TSCTAX,TSCFUL(NUMTOT,MAXFUL),TSCOTX(NUMTOT)
	INTEGER*4 TSCMID(NUMTOT),TSCUTX(NUMTOT),TSCHST,TSCORM(2)
	INTEGER*4 TSCNM1(SNMS_LEN/4),TSCNM2(SNMS_LEN/4),TSCPFN(5)
	INTEGER*4 TSCDRW,TSCSER,TSCPRF
	INTEGER*4 TSCREV,TSCTYP,TSCPRC,TSCPUP,TSCUPD
	INTEGER*4 TSCSPR,TSCDES(SDES_LEN/4),TSCTVC(STVC_LEN/4)
C
	BYTE	  TSCMDS(MAXMLTD_AVL)
C
        INTEGER*4 TSCWRO(NUMTOT,2),TSCWRA(NUMTOT,2),TSCWPO(NUMTOT,2)
        INTEGER*4 TSCWPA(NUMTOT,2),TSCWPR(NUMTOT,2)
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
	INTEGER*4 TSCSTS_OFF, TSCWEK_OFF, TSCDAT_OFF, TSCDRW_OFF
	PARAMETER(TSCSTS_OFF = 1)					!001
	PARAMETER(TSCWEK_OFF = TSCSTS_OFF + 1)				!002
	PARAMETER(TSCDAT_OFF = TSCWEK_OFF + 1)				!003
	PARAMETER(TSCDRW_OFF = TSCDAT_OFF + 1)				!004
C
	INTEGER*4 TSCBSD_OFF, TSCESD_OFF, TSCPUP_OFF, TSCUPD_OFF
	PARAMETER(TSCBSD_OFF = TSCDRW_OFF + 1)				!005
	PARAMETER(TSCESD_OFF = TSCBSD_OFF + 1)				!006
	PARAMETER(TSCPUP_OFF = TSCESD_OFF + 1)				!007
	PARAMETER(TSCUPD_OFF = TSCPUP_OFF + 1)				!008
C
	INTEGER*4 TSCCTM_OFF, TSCTIM_OFF, TSCSER_OFF, TSCSAL_OFF
	PARAMETER(TSCCTM_OFF = TSCUPD_OFF + 1)				!009
	PARAMETER(TSCTIM_OFF = TSCCTM_OFF + 1)				!010
	PARAMETER(TSCSER_OFF = TSCTIM_OFF + 1)				!011
	PARAMETER(TSCSAL_OFF = TSCSER_OFF + 1)				!012
C
	INTEGER*4 TSCPAD_OFF, TSCPRG_OFF, TSCPRF_OFF, TSCREF_OFF
	PARAMETER(TSCPAD_OFF = TSCSAL_OFF + 1)				!013
	PARAMETER(TSCPRG_OFF = TSCPAD_OFF + 1)				!014
	PARAMETER(TSCPRF_OFF = TSCPRG_OFF + 1)				!015
	PARAMETER(TSCREF_OFF = TSCPRF_OFF + 1)				!016
C
	INTEGER*4 TSCWON_OFF, TSCPOL_OFF, TSCTPL_OFF, TSCTBK_OFF
	PARAMETER(TSCWON_OFF = TSCREF_OFF + 1)				!017
	PARAMETER(TSCPOL_OFF = TSCWON_OFF + 1)				!018
	PARAMETER(TSCTPL_OFF = TSCPOL_OFF + 2)				!020
	PARAMETER(TSCTBK_OFF = TSCTPL_OFF + 1)				!021
C
	INTEGER*4 TSCBRK_OFF, TSCABW_OFF, TSCODS_OFF, TSCWIN_OFF
	PARAMETER(TSCBRK_OFF = TSCTBK_OFF + 1)				!022
	PARAMETER(TSCABW_OFF = TSCBRK_OFF + 2)				!023
	PARAMETER(TSCODS_OFF = TSCABW_OFF + 1)				!024
	PARAMETER(TSCWIN_OFF = TSCODS_OFF + 1)				!025
C
	INTEGER*4 TSCHLD_OFF, TSCTAX_OFF, TSCFUL_OFF, TSCOTX_OFF
	PARAMETER(TSCHLD_OFF = TSCWIN_OFF + 2)				!027
	PARAMETER(TSCTAX_OFF = TSCHLD_OFF + 2)				!029
	PARAMETER(TSCFUL_OFF = TSCTAX_OFF + 1)				!030
	PARAMETER(TSCOTX_OFF = TSCFUL_OFF + (NUMTOT*MAXFUL))		!230
C
	INTEGER*4 TSCMID_OFF, TSCUTX_OFF, TSCHST_OFF, TSCORM_OFF
	PARAMETER(TSCMID_OFF = TSCOTX_OFF + NUMTOT)			!232
	PARAMETER(TSCUTX_OFF = TSCMID_OFF + NUMTOT)			!234
	PARAMETER(TSCHST_OFF = TSCUTX_OFF + NUMTOT)			!236
	PARAMETER(TSCORM_OFF = TSCHST_OFF + 1)				!237
C
	INTEGER*4 TSCNM1_OFF, TSCNM2_OFF, TSCREV_OFF, TSCTYP_OFF
	PARAMETER(TSCNM1_OFF = TSCORM_OFF + NUMTOT)			!239
	PARAMETER(TSCNM2_OFF = TSCNM1_OFF + (SNMS_LEN/4))		!243
	PARAMETER(TSCREV_OFF = TSCNM2_OFF + (SNMS_LEN/4))		!247
	PARAMETER(TSCTYP_OFF = TSCREV_OFF + 1)				!248
C
	INTEGER*4 TSCPFN_OFF, TSCPRC_OFF, TSCSPR_OFF, TSCDES_OFF
	PARAMETER(TSCPFN_OFF = TSCTYP_OFF + 1)				!249
	PARAMETER(TSCPRC_OFF = TSCPFN_OFF + 5)				!254
	PARAMETER(TSCSPR_OFF = TSCPRC_OFF + 1)				!255
	PARAMETER(TSCDES_OFF = TSCSPR_OFF + 1)			        !256
C
	INTEGER*4 TSCTVC_OFF, TSCMDS_OFF 
	PARAMETER(TSCTVC_OFF = TSCDES_OFF + (SDES_LEN/4))		!286
	PARAMETER(TSCMDS_OFF = TSCTVC_OFF + (STVC_LEN/4))		!287
C
        INTEGER*4 TSCWRO_OFF, TSCWRA_OFF, TSCWPO_OFF, TSCWPA_OFF
        INTEGER*4 TSCWPR_OFF
        PARAMETER(TSCWRO_OFF = TSCMDS_OFF + (MAXMLTD_AVL/4))
        PARAMETER(TSCWRA_OFF = TSCWRO_OFF + NUMTOT*2)
        PARAMETER(TSCWPO_OFF = TSCWRA_OFF + NUMTOT*2)
        PARAMETER(TSCWPA_OFF = TSCWPO_OFF + NUMTOT*2)
        PARAMETER(TSCWPR_OFF = TSCWPA_OFF + NUMTOT*2)

        INTEGER*4 TSCFRE_OFF
        PARAMETER(TSCFRE_OFF = TSCWPR_OFF + NUMTOT*2)

C
C CALCULATE FREE SPACE BASED ON MACHINE DEPENDENT SECTOR SIZES.
C SHOULD ALLWAYS ADD UP TO A EVEN SECTOR SIZE BYTES (VAX 512).
C NOTE! IN CALCULATIONS, FRE_OFF-2 IS DONE TO TAKE CARE OF WHEN THE DATA SIZE
C       ENDS ON A EVEN SECTOR SIZE.
C
        INTEGER*4 TSCSEC_VAX, TSCSEC_NUM
        PARAMETER (TSCSEC_VAX = SECSIZE/4)
	PARAMETER (TSCSEC_NUM = NINT(FLOAT((TSCFRE_OFF-2)/TSCSEC_VAX)+.5))
C
        INTEGER*4 TSCLEN, TSCSEC
        PARAMETER(TSCLEN = TSCSEC_VAX * TSCSEC_NUM)        ! even sector size
        PARAMETER(TSCSEC = TSCLEN/64)
C
        INTEGER*4 TSCREC(TSCLEN)
C
	EQUIVALENCE(TSCSTS,TSCREC(TSCSTS_OFF))
	EQUIVALENCE(TSCWEK,TSCREC(TSCWEK_OFF))
	EQUIVALENCE(TSCDAT,TSCREC(TSCDAT_OFF))
	EQUIVALENCE(TSCDRW,TSCREC(TSCDRW_OFF))
	EQUIVALENCE(TSCBSD,TSCREC(TSCBSD_OFF))
	EQUIVALENCE(TSCESD,TSCREC(TSCESD_OFF))
	EQUIVALENCE(TSCPUP,TSCREC(TSCPUP_OFF))
	EQUIVALENCE(TSCUPD,TSCREC(TSCUPD_OFF))
	EQUIVALENCE(TSCCTM,TSCREC(TSCCTM_OFF))
	EQUIVALENCE(TSCTIM,TSCREC(TSCTIM_OFF))
	EQUIVALENCE(TSCSER,TSCREC(TSCSER_OFF))
	EQUIVALENCE(TSCSAL,TSCREC(TSCSAL_OFF))
	EQUIVALENCE(TSCPAD,TSCREC(TSCPAD_OFF))
	EQUIVALENCE(TSCPRG,TSCREC(TSCPRG_OFF))
	EQUIVALENCE(TSCPRF,TSCREC(TSCPRF_OFF))
	EQUIVALENCE(TSCREF,TSCREC(TSCREF_OFF))
	EQUIVALENCE(TSCWON,TSCREC(TSCWON_OFF))
	EQUIVALENCE(TSCPOL,TSCREC(TSCPOL_OFF))
	EQUIVALENCE(TSCTPL,TSCREC(TSCTPL_OFF))
	EQUIVALENCE(TSCTBK,TSCREC(TSCTBK_OFF))
	EQUIVALENCE(TSCBRK,TSCREC(TSCBRK_OFF))
	EQUIVALENCE(TSCABW,TSCREC(TSCABW_OFF))
	EQUIVALENCE(TSCODS,TSCREC(TSCODS_OFF))
	EQUIVALENCE(TSCWIN,TSCREC(TSCWIN_OFF))
	EQUIVALENCE(TSCHLD,TSCREC(TSCHLD_OFF))
	EQUIVALENCE(TSCTAX,TSCREC(TSCTAX_OFF))
	EQUIVALENCE(TSCFUL,TSCREC(TSCFUL_OFF))
	EQUIVALENCE(TSCOTX,TSCREC(TSCOTX_OFF))
	EQUIVALENCE(TSCMID,TSCREC(TSCMID_OFF))
	EQUIVALENCE(TSCUTX,TSCREC(TSCUTX_OFF))
	EQUIVALENCE(TSCHST,TSCREC(TSCHST_OFF))
	EQUIVALENCE(TSCORM,TSCREC(TSCORM_OFF))
	EQUIVALENCE(TSCNM1,TSCREC(TSCNM1_OFF))
	EQUIVALENCE(TSCNM2,TSCREC(TSCNM2_OFF))
	EQUIVALENCE(TSCREV,TSCREC(TSCREV_OFF))
	EQUIVALENCE(TSCTYP,TSCREC(TSCTYP_OFF))
	EQUIVALENCE(TSCPFN,TSCREC(TSCPFN_OFF))
        EQUIVALENCE(TSCPRC,TSCREC(TSCPRC_OFF))
	EQUIVALENCE(TSCSPR,TSCREC(TSCSPR_OFF))
	EQUIVALENCE(TSCDES,TSCREC(TSCDES_OFF))
	EQUIVALENCE(TSCTVC,TSCREC(TSCTVC_OFF))
	EQUIVALENCE(TSCMDS,TSCREC(TSCMDS_OFF))
        EQUIVALENCE(TSCWRO,TSCREC(TSCWRO_OFF))
        EQUIVALENCE(TSCWRA,TSCREC(TSCWRA_OFF))
        EQUIVALENCE(TSCWPO,TSCREC(TSCWPO_OFF))
        EQUIVALENCE(TSCWPA,TSCREC(TSCWPA_OFF))
        EQUIVALENCE(TSCWPR,TSCREC(TSCWPR_OFF))
