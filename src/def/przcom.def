C
C PRZCOM.DEF
C
C DETAIL PRIZE DATA COMMON FOR PAST 100 DRAWS
C
C
C LPZSHV(LTGDIV,2,100,NUMLTO)        ;LOTTO SHARE VALUES        2*3200
C LPZDAT(100,NUMLTO)                 ;LOTTO DRAW DATES           200
C
C SPZSHV(SPGDIV,PRZDRW,NUMSPT)   ;SPORTS SHARE VALUES       1600
C SPZDAT(PRZDRW,NUMSPT)          ;SPORTS DRAW DATES          200
C SPZMAT(SPGDIV,PRZDRW,NUMSPT)   ;SPORTS MATCH TABLE        1600
C
C TGZSHV(TGGDIV,PRZDRW,NUMTGL)   ;RESULTS SHARE VALUES       1600
C TGZDAT(PRZDRW,NUMTGL)          ;RESULTS DRAW DATES          200
C TGZMAT(TGGDIV,PRZDRW,NUMTGL)   ;RESULTS MATCH TABLE        1600
C
C KPZSHV(KIGDIV,100,NUMKIK)          ;KICKER SHARE VALUES       1200
C KPZDAT(100,NUMKIK)                 ;KICKER DRAW DATES          100
C
C BPZSHV(BGODIV,BGOSUB,100,NUMBGO)   ;BINGO SHARE VALUES        2400
C BPZDAT(100,NUMBGO)                 ;BINGO DRAW DATES           100
C BPZLOT(BGODIV,100,NUMBGO)          ;CORRESPOND BINGO DIVS IN
C                                     LOTTERY SCALE             1500   
