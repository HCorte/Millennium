C
C
        INTEGER*4 PRZDRW
        PARAMETER(PRZDRW = 100)                 !Max # of draws to Store
C            
C DEFINE OFFSETS
C
        INTEGER*4 LPZSHV_OFF, LPZDAT_OFF
        PARAMETER(LPZSHV_OFF = 1)
        PARAMETER(LPZDAT_OFF = LPZSHV_OFF + (2*LTGDIV*PRZDRW*NUMLTO))
C
        INTEGER*4 SPZSHV_OFF, SPZDAT_OFF, SPZMAT_OFF
        PARAMETER(SPZSHV_OFF = LPZDAT_OFF + (PRZDRW*NUMLTO))
        PARAMETER(SPZDAT_OFF = SPZSHV_OFF + (SPGDIV*PRZDRW*NUMSPT))
        PARAMETER(SPZMAT_OFF = SPZDAT_OFF + (PRZDRW*NUMSPT))
C
        INTEGER*4 TGZSHV_OFF, TGZDAT_OFF, TGZMAT_OFF
        PARAMETER(TGZSHV_OFF = SPZMAT_OFF + (SPGDIV*PRZDRW*NUMSPT))
        PARAMETER(TGZDAT_OFF = TGZSHV_OFF + (TGGDIV*PRZDRW*NUMTGL))
        PARAMETER(TGZMAT_OFF = TGZDAT_OFF + (PRZDRW*NUMTGL))
C
        INTEGER*4 KPZSHV_OFF, KPZDAT_OFF
        PARAMETER(KPZSHV_OFF = TGZMAT_OFF + (TGGDIV*PRZDRW*NUMTGL))
        PARAMETER(KPZDAT_OFF = KPZSHV_OFF + (KIGDIV*PRZDRW*NUMKIK))
C
        INTEGER*4 BPZSHV_OFF, BPZDAT_OFF, BPZLOT_OFF
        PARAMETER(BPZSHV_OFF = KPZDAT_OFF + (PRZDRW*NUMKIK))
        PARAMETER(BPZDAT_OFF = BPZSHV_OFF + 
     *                         (BGODIV*BGOSUB*PRZDRW*NUMBGO))
        PARAMETER(BPZLOT_OFF = BPZDAT_OFF + (PRZDRW*NUMBGO))
C
        INTEGER*4 PRZFRE_OFF
        PARAMETER(PRZFRE_OFF = BPZLOT_OFF + (BGODIV*PRZDRW*NUMBGO))
C
C CALCULATE LENGTH OF COMMON
C
        INTEGER*4 PRZSEC_SIZE, PRZSEC_NUM, PRZLEN
        PARAMETER(PRZSEC_SIZE = 2048)
        PARAMETER(PRZSEC_NUM = NINT(FLOAT((PRZFRE_OFF-2)/
     *                         PRZSEC_SIZE)+.5))
        PARAMETER(PRZLEN = PRZSEC_SIZE * PRZSEC_NUM)
C
C DEFINE COMMON
C
        COMMON /PRZCOM/
     *          LPZSHV(LTGDIV,2,PRZDRW,NUMLTO),
     *          LPZDAT(PRZDRW,NUMLTO)
C
C
        COMMON /PRZCOM/
     *          SPZSHV(SPGDIV,PRZDRW,NUMSPT),
     *          SPZDAT(PRZDRW,NUMSPT),
     *          SPZMAT(SPGDIV,PRZDRW,NUMSPT)

        COMMON /PRZCOM/
     *          TGZSHV(TGGDIV,PRZDRW,NUMTGL),
     *          TGZDAT(PRZDRW,NUMTGL),
     *          TGZMAT(TGGDIV,PRZDRW,NUMTGL)
C
C
        COMMON /PRZCOM/
     *          KPZSHV(KIGDIV,PRZDRW,NUMKIK),
     *          KPZDAT(PRZDRW,NUMKIK)
C
C
        COMMON /PRZCOM/
     *          BPZSHV(BGODIV,BGOSUB,PRZDRW,NUMBGO),
     *          BPZDAT(PRZDRW,NUMBGO),
     *          BPZLOT(BGODIV,PRZDRW,NUMBGO)
C

C
C
C DECLARE VARIABLES
C
        INTEGER*4 LPZSHV, LPZDAT
        INTEGER*4 SPZSHV, SPZDAT, SPZMAT
        INTEGER*4 TGZSHV, TGZDAT, TGZMAT
        INTEGER*4 KPZSHV, KPZDAT
        INTEGER*4 BPZSHV, BPZDAT, BPZLOT
C
C EQUIVALENCE FOR CHECKPOINT
C
        INTEGER*4 PRZCSB
        PARAMETER(PRZCSB = PRZSEC_NUM)
C
        INTEGER*4 PRZBLK(2048,PRZCSB)
        EQUIVALENCE(PRZBLK(1,1),LPZSHV(1,1,1,1))
C
C
        INTEGER*4        FRST_PRZCOM(2)
        EQUIVALENCE     (FRST_PRZCOM, PRZBLK(1,1))
        INTEGER*4        LAST_PRZCOM
        EQUIVALENCE     (LAST_PRZCOM, PRZBLK(2048,PRZCSB))
C
        VOLATILE /PRZCOM/
C

