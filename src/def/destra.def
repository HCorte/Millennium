        PARAMETER (TIGSW_MIDH=TIGSW_MIDL+1)        !TERMINAL MESSAGE ID (HIGH 4 BYTES)               (32)
        PARAMETER (TIGSW_XGID=TIGSW_MIDH+1)        !ABP GAME ID                                      (33)

        INTEGER*4 TIGSW_STID, TIGSW_USTK, TIGSW_TBET
        PARAMETER (TIGSW_STID=TIGSW_XGID+1)        !SUBTYPE ID                                       (34)
        PARAMETER (TIGSW_USTK=TIGSW_STID+1)        !UNIT STAKE OF THE BET                            (35)
        PARAMETER (TIGSW_TBET=TIGSW_USTK+1)        !TOTAL BETS/NUMBER OF SELECTIONS                  (36)

        INTEGER*4 TIGSW_WRDY, TIGSW_WRDM, TIGSW_WRDD, TIGSW_WRGM
        INTEGER*4 TIGSW_WRSL, TIGSW_WRSH, TIGSW_WRCD
        PARAMETER (TIGSW_WRDY=TIGSW_TBET+1)        !BET REFERENCE DATE YEAR(YY, LAST TWO DIGITS ONLY)(37)
        PARAMETER (TIGSW_WRDM=TIGSW_WRDY+1)        !BET REFERENCE DATE MONTH(MM)                     (38)
        PARAMETER (TIGSW_WRDD=TIGSW_WRDM+1)        !BET REFERENCE DATE DAY(DD)                       (39)
        PARAMETER (TIGSW_WRGM=TIGSW_WRDD+1)        !BET REFERENCE GAME                               (40)
        PARAMETER (TIGSW_WRSL=TIGSW_WRGM+1)        !BET REFERENCE SERIAL NUMBER (LOW FOUR BYTES)     (41)
        PARAMETER (TIGSW_WRSH=TIGSW_WRSL+1)        !BET REFERENCE SERIAL NUMBER (HIGH ONE BYTE)      (42)
        PARAMETER (TIGSW_WRCD=TIGSW_WRSH+1)        !BET REFERENCE CHECK DIGITS                       (43)

        INTEGER*4 TIGSW_WCDY, TIGSW_WCDM, TIGSW_WCDD, TIGSW_WCTH
        INTEGER*4 TIGSW_WCTM, TIGSW_WCTS
        PARAMETER (TIGSW_WCDY=TIGSW_WRCD+1)        !BET CREATION DATE YEAR(YYYY)                     (44)
        PARAMETER (TIGSW_WCDM=TIGSW_WCDY+1)        !BET CREATION DATE MONTH(MM)                      (45)
        PARAMETER (TIGSW_WCDD=TIGSW_WCDM+1)        !BET CREATION DATE DAY(DD)                        (46)
        PARAMETER (TIGSW_WCTH=TIGSW_WCDD+1)        !BET CREATION TIME HOUR(HH)                       (47)
        PARAMETER (TIGSW_WCTM=TIGSW_WCTH+1)        !BET CREATION TIME MINUTE(MI)                     (48)
        PARAMETER (TIGSW_WCTS=TIGSW_WCTM+1)        !BET CREATION TIME SECOND(SS)                     (49)

        INTEGER*4 TIGSW_LEDY, TIGSW_LEDM, TIGSW_LEDD, TIGSW_TSTK, TIGSW_MAXR
        PARAMETER (TIGSW_LEDY=TIGSW_WCTS+1)        !BET LAST EVENT DATE YEAR(YYYY)                   (50)
        PARAMETER (TIGSW_LEDM=TIGSW_LEDY+1)        !BET LAST EVENT DATE MONTH(MM)                    (51)
        PARAMETER (TIGSW_LEDD=TIGSW_LEDM+1)        !BET LAST EVENT DATE DAY(DD)                      (52)
        PARAMETER (TIGSW_TSTK=TIGSW_LEDD+1)        !BET TOTAL STAKE                                  (53)
        PARAMETER (TIGSW_MAXR=TIGSW_TSTK+1)        !BET MAXIMUM POSSIBLE RETURNS                     (54)
        
        INTEGER*4 TIGSW_PNIF
        PARAMETER (TIGSW_PNIF=TIGSW_MAXR+1)        !PORTUGUESE PLAYER NIF NUMBER                     (55)       
C
C IF TRANSACTION IS CANCELLATION
C
        INTEGER*4 TIGSC_MIDL, TIGSC_MIDH
        PARAMETER (TIGSC_MIDL=TIGS_XERR+XERR_I4LEN)!TERMINAL MESSAGE ID (LOW 4 BYTES)                (31)
        PARAMETER (TIGSC_MIDH=TIGSC_MIDL+1)        !TERMINAL MESSAGE ID (HIGH 4 BYTES)               (32)

        INTEGER*4 TIGSC_WRDY, TIGSC_WRDM, TIGSC_WRDD
        PARAMETER (TIGSC_WRDY=TIGSC_MIDH+1)        !BET REFERENCE DATE YEAR(YY, LAST TWO DIGITS ONLY)(33)
        PARAMETER (TIGSC_WRDM=TIGSC_WRDY+1)        !BET REFERENCE DATE MONTH(MM)                     (34)
        PARAMETER (TIGSC_WRDD=TIGSC_WRDM+1)        !BET REFERENCE DATE DAY(DD)                       (35)

        INTEGER*4 TIGSC_WRGM, TIGSC_WRSL, TIGSC_WRSH, TIGSC_WRCD
        PARAMETER (TIGSC_WRGM=TIGSC_WRDD+1)        !BET REFERENCE GAME                               (36)
        PARAMETER (TIGSC_WRSL=TIGSC_WRGM+1)        !BET REFERENCE SERIAL NUMBER (LOW FOUR BYTES)     (37)
        PARAMETER (TIGSC_WRSH=TIGSC_WRSL+1)        !BET REFERENCE SERIAL NUMBER (HIGH ONE BYTE)      (38)
        PARAMETER (TIGSC_WRCD=TIGSC_WRSH+1)        !BET REFERENCE CHECK DIGITS                       (39)

        INTEGER*4 TIGSC_CRDY, TIGSC_CRDM, TIGSC_CRDD
        PARAMETER (TIGSC_CRDY=TIGSC_WRCD+1)        !CANCEL REF DATE YEAR(YY, LAST TWO DIGITS ONLY)   (41)
        PARAMETER (TIGSC_CRDM=TIGSC_CRDY+1)        !CANCEL REF DATE MONTH(MM)                        (42)
        PARAMETER (TIGSC_CRDD=TIGSC_CRDM+1)        !CANCEL REF DATE DAY(DD)                          (43)

        INTEGER*4 TIGSC_CRGM, TIGSC_CRSL, TIGSC_CRSH, TIGSC_CRCD
        PARAMETER (TIGSC_CRGM=TIGSC_CRDD+1)        !CANCEL REFERENCE GAME                            (44)
        PARAMETER (TIGSC_CRSL=TIGSC_CRGM+1)        !CANCEL REFERENCE SERIAL NUMBER (LOW FOUR BYTES)  (45)
        PARAMETER (TIGSC_CRSH=TIGSC_CRSL+1)        !CANCEL REFERENCE SERIAL NUMBER (HIGH ONE BYTE)   (46)
        PARAMETER (TIGSC_CRCD=TIGSC_CRSH+1)        !CANCEL REFERENCE CHECK DIGITS                    (47)

        INTEGER*4 TIGSC_WCDY, TIGSC_WCDM, TIGSC_WCDD
        PARAMETER (TIGSC_WCDY=TIGSC_CRCD+1)        !WAGER CANCEL DATE YEAR(YYYY)                     (48)
        PARAMETER (TIGSC_WCDM=TIGSC_WCDY+1)        !WAGER CANCEL DATE MONTH(MM)                      (49)
        PARAMETER (TIGSC_WCDD=TIGSC_WCDM+1)        !WAGER CANCEL DATE DAY(DD)                        (50)

        INTEGER*4 TIGSC_WCTH, TIGSC_WCTM, TIGSC_WCTS, TIGSC_CAMT
        PARAMETER (TIGSC_WCTH=TIGSC_WCDD+1)        !WAGER CANCEL TIME HOUR(HH)                       (51)
        PARAMETER (TIGSC_WCTM=TIGSC_WCTH+1)        !WAGER CANCEL TIME MINUTE(MI)                     (52)
        PARAMETER (TIGSC_WCTS=TIGSC_WCTM+1)        !WAGER CANCEL TIME SECOND(SS)                     (53)
        PARAMETER (TIGSC_CAMT=TIGSC_WCTS+1)        !CANCEL AMOUNT (WAGER UNITS)                      (54)
C
C IF TRANSACTION IS VALIDATION(INQUIRY)
C
        INTEGER*4 TIGSV_MIDL, TIGSV_MIDH
        PARAMETER (TIGSV_MIDL=TIGS_XERR+XERR_I4LEN)!TERMINAL MESSAGE ID (LOW 4 BYTES)                (31)
        PARAMETER (TIGSV_MIDH=TIGSV_MIDL+1)        !TERMINAL MESSAGE ID (HIGH 4 BYTES)               (32)

        INTEGER*4 TIGSV_WRDY, TIGSV_WRDM, TIGSV_WRDD
        PARAMETER (TIGSV_WRDY=TIGSV_MIDH+1)        !BET REFERENCE DATE YEAR(YY, LAST TWO DIGITS ONLY)(33)
        PARAMETER (TIGSV_WRDM=TIGSV_WRDY+1)        !BET REFERENCE DATE MONTH (MM)                    (34)
        PARAMETER (TIGSV_WRDD=TIGSV_WRDM+1)        !BET REFERENCE DATE DAY (DD)                      (35)

        INTEGER*4 TIGSV_WRGM, TIGSV_WRSL, TIGSV_WRSH, TIGSV_WRCD
        PARAMETER (TIGSV_WRGM=TIGSV_WRDD+1)        !BET REFERENCE GAME                               (36)
        PARAMETER (TIGSV_WRSL=TIGSV_WRGM+1)        !BET REFERENCE SERIAL NUMBER (LOW FOUR BYTES)     (37)
        PARAMETER (TIGSV_WRSH=TIGSV_WRSL+1)        !BET REFERENCE SERIAL NUMBER (HIGH ONE BYTE)      (38)
        PARAMETER (TIGSV_WRCD=TIGSV_WRSH+1)        !BET REFERENCE CHECK DIGITS                       (39)

        INTEGER*4 TIGSV_WVDY, TIGSV_WVDM, TIGSV_WVDD
        PARAMETER (TIGSV_WVDY=TIGSV_WRCD+1)        !WAGER VALIDATION DATE YEAR (YYYY)                (41)
        PARAMETER (TIGSV_WVDM=TIGSV_WVDY+1)        !WAGER VALIDATION DATE MONTH (MM)                 (42)
        PARAMETER (TIGSV_WVDD=TIGSV_WVDM+1)        !WAGER VALIDATION DATE DAY (DD)                   (43)

        INTEGER*4 TIGSV_WVTH, TIGSV_WVTM, TIGSV_WVTS
        PARAMETER (TIGSV_WVTH=TIGSV_WVDD+1)        !WAGER VALIDATION TIME HOUR (HH)                  (44)
        PARAMETER (TIGSV_WVTM=TIGSV_WVTH+1)        !WAGER VALIDATION TIME MINUTE (MI)                (45)
        PARAMETER (TIGSV_WVTS=TIGSV_WVTM+1)        !WAGER VALIDATION TIME SECOND (SS)                (46)

        INTEGER*4 TIGSV_TPRZ, TIGSV_TTAX, TIGSV_NPRZ, TIGSV_PMOD
        PARAMETER (TIGSV_TPRZ=TIGSV_WVTS+1)        !TOTAL PRIZE AMOUNT (VALIDATION UNITS)            (47)
        PARAMETER (TIGSV_TTAX=TIGSV_TPRZ+1)        !TOTAL TAX AMOUNT (VALIDATION UNITS)              (48)
        PARAMETER (TIGSV_NPRZ=TIGSV_TTAX+1)        !NET PRIZE AMOUNT (VALIDATION UNITS)              (49)
        PARAMETER (TIGSV_PMOD=TIGSV_NPRZ+1)        !PAYMENT MODE                                     (50)
        
        INTEGER*4 TIGSV_FNIF, TIGSV_PNIF 
        PARAMETER (TIGSV_FNIF=TIGSV_PMOD+1)        !NIF CONFIRMATION NEEDED FLAG                     (51)
        PARAMETER (TIGSV_PNIF=TIGSV_FNIF+1)        !PORTUGUESE PLAYER NIF NUMBER                     (52)
C
C IF TRANSACTION IS PAYMENT
C
        INTEGER*4 TIGSP_MIDL, TIGSP_MIDH
        PARAMETER (TIGSP_MIDL=TIGS_XERR+XERR_I4LEN)!TERMINAL MESSAGE ID (LOW 4 BYTES)                (31)
        PARAMETER (TIGSP_MIDH=TIGSP_MIDL+1)        !TERMINAL MESSAGE ID (HIGH 4 BYTES)               (32)

        INTEGER*4 TIGSP_WRDY, TIGSP_WRDM, TIGSP_WRDD
        PARAMETER (TIGSP_WRDY=TIGSP_MIDH+1)        !BET REFERENCE DATE YEAR(YY, LAST TWO DIGITS ONLY)(33)
        PARAMETER (TIGSP_WRDM=TIGSP_WRDY+1)        !BET REFERENCE DATE MONTH (MM)                    (34)
        PARAMETER (TIGSP_WRDD=TIGSP_WRDM+1)        !BET REFERENCE DATE DAY (DD)                      (35)

        INTEGER*4 TIGSP_WRGM, TIGSP_WRSL, TIGSP_WRSH, TIGSP_WRCD
        PARAMETER (TIGSP_WRGM=TIGSP_WRDD+1)        !BET REFERENCE GAME                               (36)
        PARAMETER (TIGSP_WRSL=TIGSP_WRGM+1)        !BET REFERENCE SERIAL NUMBER (LOW FOUR BYTES)     (37)
        PARAMETER (TIGSP_WRSH=TIGSP_WRSL+1)        !BET REFERENCE SERIAL NUMBER (HIGH ONE BYTE)      (38)
        PARAMETER (TIGSP_WRCD=TIGSP_WRSH+1)        !BET REFERENCE CHECK DIGITS                       (39)

        INTEGER*4 TIGSP_PMOD, TIGSP_IDTY, TIGSP_PYID
        PARAMETER (TIGSP_PMOD=TIGSP_WRCD+1)        !PAYMENT MODE                                     (40)
        PARAMETER (TIGSP_IDTY=TIGSP_PMOD+1)        !PLAYER ID TYPE (*)                               (41)
        PARAMETER (TIGSP_PYID=TIGSP_IDTY+1)        !PLAYER ID (*)                                    (42)

        INTEGER*4 TIGSP_NIBB, TIGSP_NIBO, TIGSP_NIA1, TIGSP_NIA2, TIGSP_NICD
        PARAMETER (TIGSP_NIBB=TIGSP_PYID+1)        !NIB BRANCH (*)                                   (43)
        PARAMETER (TIGSP_NIBO=TIGSP_NIBB+1)        !NIB OFFICE (*)                                   (44)
        PARAMETER (TIGSP_NIA1=TIGSP_NIBO+1)        !NIB ACCOUNT NUMBER PART 1 (*)                    (45)
        PARAMETER (TIGSP_NIA2=TIGSP_NIA1+1)        !NIB ACCOUNT NUMBER PART 2 (*)                    (46)
        PARAMETER (TIGSP_NICD=TIGSP_NIA2+1)        !NIB CHECK DIGITS (*)                             (47)
C       (*) This data is set if payment mode is a bank transfer

        INTEGER*4 TIGSP_PRDY, TIGSP_PRDM, TIGSP_PRDD
        PARAMETER (TIGSP_PRDY=TIGSP_NICD+1)        !PAYMENT REF DATE YEAR(YY, LAST TWO DIGITS ONLY)  (49)
        PARAMETER (TIGSP_PRDM=TIGSP_PRDY+1)        !PAYMENT REF DATE MONTH (MM)                      (50)
        PARAMETER (TIGSP_PRDD=TIGSP_PRDM+1)        !PAYMENT REF DATE DAY (DD)                        (51)

        INTEGER*4 TIGSP_PRGM, TIGSP_PRSL, TIGSP_PRSH, TIGSP_PRCD
        PARAMETER (TIGSP_PRGM=TIGSP_PRDD+1)        !PAYMENT REFERENCE GAME                           (52)
        PARAMETER (TIGSP_PRSL=TIGSP_PRGM+1)        !PAYMENT REFERENCE SERIAL NUMBER (LOW FOUR BYTES) (53)
        PARAMETER (TIGSP_PRSH=TIGSP_PRSL+1)        !PAYMENT REFERENCE SERIAL NUMBER (HIGH ONE BYTE)  (54)
        PARAMETER (TIGSP_PRCD=TIGSP_PRSH+1)        !PAYMENT REFERENCE CHECK DIGITS                   (55)

        INTEGER*4 TIGSP_PPDY, TIGSP_PPDM, TIGSP_PPDD
        PARAMETER (TIGSP_PPDY=TIGSP_PRCD+1)        !PRIZE PAYMENT DATE YEAR (YYYY)                   (56)
        PARAMETER (TIGSP_PPDM=TIGSP_PPDY+1)        !PRIZE PAYMENT DATE MONTH (MM)                    (57)
        PARAMETER (TIGSP_PPDD=TIGSP_PPDM+1)        !PRIZE PAYMENT DATE DAY (DD)                      (58)

        INTEGER*4 TIGSP_PPTH, TIGSP_PPTM, TIGSP_PPTS
        PARAMETER (TIGSP_PPTH=TIGSP_PPDD+1)        !PRIZE PAYMENT TIME HOUR (HH)                     (59)
        PARAMETER (TIGSP_PPTM=TIGSP_PPTH+1)        !PRIZE PAYMENT TIME MINUTE (MI)                   (60)
        PARAMETER (TIGSP_PPTS=TIGSP_PPTM+1)        !PRIZE PAYMENT TIME SECOND (SS)                   (61)

        INTEGER*4 TIGSP_TPRZ, TIGSP_TTAX, TIGSP_NPRZ
        PARAMETER (TIGSP_TPRZ=TIGSP_PPTS+1)        !TOTAL PRIZE AMOUNT                               (62)
        PARAMETER (TIGSP_TTAX=TIGSP_TPRZ+1)        !TOTAL TAX AMOUNT                                 (63)
        PARAMETER (TIGSP_NPRZ=TIGSP_TTAX+1)        !NET PRIZE AMOUNT                                 (64)

        INTEGER*4 TIGSP_PNIF
        PARAMETER (TIGSP_PNIF=TIGSP_NPRZ+1)        !PORTUGUESE PLAYER NIF NUMBER                     (65)
C
C IF TRANSACTION IS GAME PROGRAMME REPORT
C
        INTEGER*4 TIGSR_MIDL, TIGSR_MIDH
        PARAMETER (TIGSR_MIDL=TIGS_XERR+XERR_I4LEN)!TERMINAL MESSAGE ID (LOW 4 BYTES)                (31)
        PARAMETER (TIGSR_MIDH=TIGSR_MIDL+1)        !TERMINAL MESSAGE ID (HIGH 4 BYTES)               (32)

        INTEGER*4 TIGSR_SEGN, TIGSR_MEID, TIGSR_PTID
        PARAMETER (TIGSR_SEGN=TIGSR_MIDH+1)        !SEGMENT NUMBER REQUESTED                         (33)
        PARAMETER (TIGSR_MEID=TIGSR_SEGN+1)        !MEDIA ID                                         (34)
        PARAMETER (TIGSR_PTID=TIGSR_MEID+1)        !PROGRAMME TEMPLATE ID                            (35)

        INTEGER*4 TIGSR_MVRL, TIGSR_MVRH
        PARAMETER (TIGSR_MVRL=TIGSR_PTID+1)        !MEDIA VERSION (LOW 4 BYTES)
        PARAMETER (TIGSR_MVRH=TIGSR_MVRL+1)        !MEDIA VERSION (HIGH 4 BYTES)

        INTEGER*4 TIGSR_PRDY, TIGSR_PRDM, TIGSR_PRDD
        PARAMETER (TIGSR_PRDY=TIGSR_MVRH+1)        !PROGRAMME REPORT DATE YEAR (YYYY)                (36)
        PARAMETER (TIGSR_PRDM=TIGSR_PRDY+1)        !PROGRAMME REPORT DATE MONTH (MM)                 (37)
        PARAMETER (TIGSR_PRDD=TIGSR_PRDM+1)        !PROGRAMME REPORT DATE DAY (DD)                   (38)

        INTEGER*4 TIGSR_PRTH, TIGSR_PRTM, TIGSR_PRTS, TIGSR_TSEG
        PARAMETER (TIGSR_PRTH=TIGSR_PRDD+1)        !PROGRAMME REPORT TIME HOUR (HH)                  (39)
        PARAMETER (TIGSR_PRTM=TIGSR_PRTH+1)        !PROGRAMME REPORT TIME MINUTE (MI)                (40)
        PARAMETER (TIGSR_PRTS=TIGSR_PRTM+1)        !PROGRAMME REPORT TIME SECOND (SS)                (41)
        PARAMETER (TIGSR_TSEG=TIGSR_PRTS+1)        !TOTAL SEGMENTS                                   (42)



