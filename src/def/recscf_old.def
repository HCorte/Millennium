        INTEGER*4 OMAXTYP                                                       !V17
        PARAMETER(OMAXTYP = 18)
C
C       INTEGER*4  OSCFLEN, OSCFSEC
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
        INTEGER*4  OSCFPAROFF, OSCFGTNOFF, OSCFGNTOFF, OSCFLGNOFF
        INTEGER*4  OSCFSGNOFF, OSCFGFNOFF, OSCFSFNOFF, OSCFFSZOFF
        INTEGER*4  OSCFKGNOFF, OSCFGSZOFF, OSCFCOGOFF, OSCFCOTOFF
        INTEGER*4  OSCFTKCOFF, OSCFPRGOFF, OSCFREDOFF, OSCFDLNAMOFF
        INTEGER*4  OSCFSTPOFF, OSCFGVNOFF, OSCFGVSOFF, OSCFHVLOFF
        INTEGER*4  OSCFHVROFF, OSCFTALOFF, OSCFTAROFF, OSCFRMIOFF
        INTEGER*4  OSCFCTXOFF, OSCFFRCOFF, OSCFIVCOFF
        INTEGER*4  OSCF_OPNIDOFF, OSCF_OPNSEEDOFF, OSCF_OPNDATEOFF
        INTEGER*4  OSCFFREOFF, OSCFDLTABOFF
        INTEGER*4  OSCF_HLDLIMOFF, OSCF_HLDDAYOFF
        INTEGER*4  OSCF_TCPPORTSOFF, OSCF_TCPPREFIXOFF, OSCF_TCPSUFFIXOFF
        INTEGER*4  OSCF_ORDEROFF                                                !V14
        INTEGER*4  OSCFRETCOMOFF
C
        PARAMETER (OSCFPAROFF = 1)
        PARAMETER (OSCFGTNOFF = OSCFPAROFF + NUMPAR)
        PARAMETER (OSCFGNTOFF = OSCFGTNOFF + OMAXTYP*MAXIND)
        PARAMETER (OSCFKGNOFF = OSCFGNTOFF + 2*MAXGAM)
        PARAMETER (OSCFLGNOFF = OSCFKGNOFF + MAXGAM)
        PARAMETER (OSCFSGNOFF = OSCFLGNOFF + 4*MAXGAM)
        PARAMETER (OSCFGFNOFF = OSCFSGNOFF + MAXGAM)
        PARAMETER (OSCFSFNOFF = OSCFGFNOFF + 5*MAXGAM)
        PARAMETER (OSCFFSZOFF = OSCFSFNOFF + 5*MAXFIL)
        PARAMETER (OSCFGSZOFF = OSCFFSZOFF + MAXFIL)
        PARAMETER (OSCFCOGOFF = OSCFGSZOFF + MAXGAM)
        PARAMETER (OSCFCOTOFF = OSCFCOGOFF + MAXGAM)
        PARAMETER (OSCFTKCOFF = OSCFCOTOFF + NUMFIN)
        PARAMETER (OSCFPRGOFF = OSCFTKCOFF + MAXGAM)
        PARAMETER (OSCFREDOFF = OSCFPRGOFF + MAXGAM)
        PARAMETER (OSCFDLNAMOFF = OSCFREDOFF + MAXGAM)
        PARAMETER (OSCFDLTABOFF = OSCFDLNAMOFF + MAXLOADS)
        PARAMETER (OSCFSTPOFF = OSCFDLTABOFF + (5*MAXLOADS/2))
        PARAMETER (OSCFGVNOFF = OSCFSTPOFF + MAXIND)
        PARAMETER (OSCFGVSOFF = OSCFGVNOFF + 5*MAXGAM)
        PARAMETER (OSCFHVLOFF = OSCFGVSOFF + MAXGAM)
        PARAMETER (OSCFHVROFF = OSCFHVLOFF + 1)
        PARAMETER (OSCFTALOFF = OSCFHVROFF + 1)
        PARAMETER (OSCFTAROFF = OSCFTALOFF + 1)
        PARAMETER (OSCFRMIOFF = OSCFTAROFF + 1)
        PARAMETER (OSCFCTXOFF = OSCFRMIOFF + MAXGAM)
        PARAMETER (OSCFFRCOFF = OSCFCTXOFF + 1)
        PARAMETER (OSCFIVCOFF = OSCFFRCOFF + MAXGAM)
        PARAMETER (OSCF_OPNIDOFF = OSCFIVCOFF + 1)
        PARAMETER (OSCF_OPNSEEDOFF = OSCF_OPNIDOFF + PRM_NUMOPN)
        PARAMETER (OSCF_OPNDATEOFF = OSCF_OPNSEEDOFF + PRM_NUMOPN)
        PARAMETER (OSCF_HLDLIMOFF = OSCF_OPNDATEOFF + PRM_ENDDAT*PRM_NUMOPN)
        PARAMETER (OSCF_HLDDAYOFF = OSCF_HLDLIMOFF + 3*MAXGAM)
        PARAMETER (OSCF_TCPPORTSOFF = OSCF_HLDDAYOFF + 3*MAXGAM)
        PARAMETER (OSCF_TCPPREFIXOFF = OSCF_TCPPORTSOFF + 2*2)
        PARAMETER (OSCF_TCPSUFFIXOFF = OSCF_TCPPREFIXOFF + 2)
        PARAMETER (OSCF_ORDEROFF = OSCF_TCPSUFFIXOFF + 2*4)                     !V14
        PARAMETER (OSCFRETCOMOFF = OSCF_ORDEROFF + MAXGAM)
        PARAMETER (OSCFFREOFF = OSCFRETCOMOFF + NUMPAS)
C
        INTEGER*4 OSCFLEN
        PARAMETER(OSCFLEN = OSCFFREOFF-1)
C
        INTEGER*4 IOSCFSEC
        PARAMETER(IOSCFSEC = OSCFLEN/128 + 1)
C
        INTEGER*4 OSCFSEC
        PARAMETER(OSCFSEC = IOSCFSEC*2)
C
        INTEGER*4 OSCFFRESIZ
        PARAMETER(OSCFFRESIZ = OSCFSEC * 64 - OSCFLEN)
C
        INTEGER*4 OSCFREC(OSCFLEN+OSCFFRESIZ)
C
        INTEGER*4  OSCFPAR(NUMPAR)
        INTEGER*4  OSCFGTN(OMAXTYP,MAXIND)
        INTEGER*4  OSCFGNT(2,MAXGAM)
        INTEGER*4  OSCFKGN(MAXGAM)
        INTEGER*4  OSCFLGN(4,MAXGAM)
        INTEGER*4  OSCFSGN(MAXGAM)
        INTEGER*4  OSCFGFN(5,MAXGAM)
        INTEGER*4  OSCFSFN(5,MAXFIL)
        INTEGER*4  OSCFFSZ(MAXFIL)
        INTEGER*4  OSCFGSZ(MAXGAM)
        INTEGER*4  OSCFCOG(MAXGAM)
        INTEGER*4  OSCFCOT(NUMFIN)
        INTEGER*4  OSCFTKC(MAXGAM)
        INTEGER*4  OSCFPRG(MAXGAM)
        INTEGER*4  OSCFRED(MAXGAM)
        INTEGER*4  OSCFDLNAM(MAXLOADS)
        INTEGER*2  OSCFDLTAB(MAXLOADS,5)                                        !  <<<  I*2
        INTEGER*4  OSCFSTP(MAXIND)
        INTEGER*4  OSCFGVN(5,MAXGAM)
        INTEGER*4  OSCFGVS(MAXGAM)
        INTEGER*4  OSCFHVL
        INTEGER*4  OSCFHVR
        INTEGER*4  OSCFTAL
        INTEGER*4  OSCFTAR
        INTEGER*4  OSCFRMI(MAXGAM)
        INTEGER*4  OSCFCTX
        INTEGER*4  OSCFFRC(MAXGAM)
        INTEGER*4  OSCFIVC
        INTEGER*4  OSCF_OPNID(PRM_NUMOPN)
        INTEGER*4  OSCF_OPNSEED(PRM_NUMOPN)
        INTEGER*4  OSCF_OPNDATE(PRM_ENDDAT,PRM_NUMOPN)
        INTEGER*4  OSCF_HLDLIM(MAXGAM,3)
        INTEGER*4  OSCF_HLDDAY(MAXGAM,3)
        INTEGER*4  OSCF_TCPPORTS(2,2)
        INTEGER*4  OSCF_TCPPREFIX(2)
        INTEGER*4  OSCF_TCPSUFFIX(2,4)
        INTEGER*4  OSCF_ORDER(MAXGAM)
        INTEGER*4  OSCFRETCOM(NUMPAS)
        INTEGER*4  OSCFFRE(OSCFFRESIZ)
C
C
        EQUIVALENCE (OSCFPAR(1),          OSCFREC(OSCFPAROFF))
        EQUIVALENCE (OSCFGTN(1,1),        OSCFREC(OSCFGTNOFF))
        EQUIVALENCE (OSCFGNT(1,1),        OSCFREC(OSCFGNTOFF))
        EQUIVALENCE (OSCFKGN(1),          OSCFREC(OSCFKGNOFF))
        EQUIVALENCE (OSCFLGN(1,1),        OSCFREC(OSCFLGNOFF))
        EQUIVALENCE (OSCFSGN(1),          OSCFREC(OSCFSGNOFF))
        EQUIVALENCE (OSCFGFN(1,1),        OSCFREC(OSCFGFNOFF))
        EQUIVALENCE (OSCFSFN(1,1),        OSCFREC(OSCFSFNOFF))
        EQUIVALENCE (OSCFFSZ(1),          OSCFREC(OSCFFSZOFF))
        EQUIVALENCE (OSCFGSZ(1),          OSCFREC(OSCFGSZOFF))
        EQUIVALENCE (OSCFCOG(1),          OSCFREC(OSCFCOGOFF))
        EQUIVALENCE (OSCFCOT(1),          OSCFREC(OSCFCOTOFF))
        EQUIVALENCE (OSCFTKC(1),          OSCFREC(OSCFTKCOFF))
        EQUIVALENCE (OSCFPRG(1),          OSCFREC(OSCFPRGOFF))
        EQUIVALENCE (OSCFRED(1),          OSCFREC(OSCFREDOFF))
        EQUIVALENCE (OSCFDLNAM(1),        OSCFREC(OSCFDLNAMOFF))
        EQUIVALENCE (OSCFDLTAB(1,1),      OSCFREC(OSCFDLTABOFF))
        EQUIVALENCE (OSCFSTP(1),          OSCFREC(OSCFSTPOFF))
        EQUIVALENCE (OSCFGVN(1,1),        OSCFREC(OSCFGVNOFF))
        EQUIVALENCE (OSCFGVS(1),          OSCFREC(OSCFGVSOFF))
        EQUIVALENCE (OSCFHVL,             OSCFREC(OSCFHVLOFF))
        EQUIVALENCE (OSCFHVR,             OSCFREC(OSCFHVROFF))
        EQUIVALENCE (OSCFTAL,             OSCFREC(OSCFTALOFF))
        EQUIVALENCE (OSCFTAR,             OSCFREC(OSCFTAROFF))
        EQUIVALENCE (OSCFRMI,             OSCFREC(OSCFRMIOFF))
        EQUIVALENCE (OSCFCTX,             OSCFREC(OSCFCTXOFF))
        EQUIVALENCE (OSCFFRC(1),          OSCFREC(OSCFFRCOFF))
        EQUIVALENCE (OSCFIVC,             OSCFREC(OSCFIVCOFF))
        EQUIVALENCE (OSCF_OPNID(1),       OSCFREC(OSCF_OPNIDOFF))
        EQUIVALENCE (OSCF_OPNSEED(1),     OSCFREC(OSCF_OPNSEEDOFF))
        EQUIVALENCE (OSCF_OPNDATE(1),     OSCFREC(OSCF_OPNDATEOFF))
        EQUIVALENCE (OSCF_HLDLIM(1),      OSCFREC(OSCF_HLDLIMOFF))
        EQUIVALENCE (OSCF_HLDDAY(1),      OSCFREC(OSCF_HLDDAYOFF))
        EQUIVALENCE (OSCF_TCPPORTS(1,1),  OSCFREC(OSCF_TCPPORTSOFF))
        EQUIVALENCE (OSCF_TCPPREFIX(1),   OSCFREC(OSCF_TCPPREFIXOFF))
        EQUIVALENCE (OSCF_TCPSUFFIX(1,1), OSCFREC(OSCF_TCPSUFFIXOFF))
        EQUIVALENCE (OSCF_ORDER(1),       OSCFREC(OSCF_ORDEROFF))
        EQUIVALENCE (OSCFRETCOM(1),       OSCFREC(OSCFRETCOMOFF))
        EQUIVALENCE (OSCFFRE(1),          OSCFREC(OSCFFREOFF))
C
C END RECSCF_OLD.DEF
C
