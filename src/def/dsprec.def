C
C RECORD OFFSET PARAMETERS
C
	INTEGER*4 DSPSTS,DSPCTM,DSPTIM,DSPDRW,DSPBSD,DSPESD
	INTEGER*4 DSPPUP,DSPUPD,DSPDAT(DATLEN),DSPSAL(SPGENT)
	INTEGER*4 DSPSHV(SPGDIV),DSPSHR(SPGDIV),DSPPOL(SPGDIV)
	INTEGER*4 DSPPAD(SPGDIV),DSPPRG(SPGDIV),DSPADV(NUMADV)
	INTEGER*4 DSPAFD(SPGDIV),DSPCSP(SPGDIV),DSPOSV(SPGDIV)
	INTEGER*4 DSPOSH(SPGDIV),DSPWIN(SPGNBR),DSPHLD(SPGNBR)
	INTEGER*4 DSPPRP,DSPPRN,DSPMAT(SPGDIV),DSPNMS(SPNMS_LEN/4,2,SPGNBR)
	INTEGER*4 DSPTAX,DSPRES(2),DSPAPL,DSPMIN,DSPSER,DSPOPA,DSPPRC
	INTEGER*4 DSPMAX,DSPMLT,DSPDIV,DSPCLB(25)
	INTEGER*4 DSPPER(SPGDIV)
	INTEGER*4 DSPSPR,DSPTSR(SPGDIV),DSPREV
	INTEGER*4 DSPASH(SPGDIV),DSPFRG,DSPWRF(POSTED),DSPBST
	INTEGER*4 DSPBAL(MAXDRW),DSPEVN(SPEVN_LEN/4)
	INTEGER*4 DSPEXT,DSPRGT(2),DSPROD(2)
        INTEGER*4  DSPECD(SPGNBR), DSPMCE, DSPDCD, DSPRWD
C
	BYTE      DSPMDS(MAXMLTD_AVL)
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
	INTEGER*4 DSPSTS_OFF, DSPCTM_OFF, DSPTIM_OFF, DSPDRW_OFF
	PARAMETER(DSPSTS_OFF = 1)					
	PARAMETER(DSPCTM_OFF = DSPSTS_OFF + 1)				
	PARAMETER(DSPTIM_OFF = DSPCTM_OFF + 1)				
	PARAMETER(DSPDRW_OFF = DSPTIM_OFF + 1)				
C
	INTEGER*4 DSPBSD_OFF, DSPESD_OFF, DSPPUP_OFF, DSPUPD_OFF
	PARAMETER(DSPBSD_OFF = DSPDRW_OFF + 1)				
	PARAMETER(DSPESD_OFF = DSPBSD_OFF + 1)				
	PARAMETER(DSPPUP_OFF = DSPESD_OFF + 1)				
	PARAMETER(DSPUPD_OFF = DSPPUP_OFF + 1)				
C
	INTEGER*4 DSPDAT_OFF, DSPADV_OFF, DSPSAL_OFF, DSPSHV_OFF
	PARAMETER(DSPDAT_OFF = DSPUPD_OFF + 1)				
	PARAMETER(DSPADV_OFF = DSPDAT_OFF + DATLEN)			
	PARAMETER(DSPSAL_OFF = DSPADV_OFF + NUMADV)			
	PARAMETER(DSPSHV_OFF = DSPSAL_OFF + SPGENT)			
C
	INTEGER*4 DSPSHR_OFF, DSPPOL_OFF, DSPPAD_OFF, DSPPRG_OFF
	PARAMETER(DSPSHR_OFF = DSPSHV_OFF + SPGDIV)			
	PARAMETER(DSPPOL_OFF = DSPSHR_OFF + SPGDIV)
        PARAMETER(DSPPAD_OFF = DSPPOL_OFF + SPGDIV)			
	PARAMETER(DSPPRG_OFF = DSPPAD_OFF + SPGDIV)			
C
	INTEGER*4 DSPAFD_OFF, DSPCSP_OFF, DSPOSV_OFF, DSPOSH_OFF
	PARAMETER(DSPAFD_OFF = DSPPRG_OFF + SPGDIV)			
	PARAMETER(DSPCSP_OFF = DSPAFD_OFF + SPGDIV)			
	PARAMETER(DSPOSV_OFF = DSPCSP_OFF + SPGDIV)			
	PARAMETER(DSPOSH_OFF = DSPOSV_OFF + SPGDIV)			
C
	INTEGER*4 DSPWIN_OFF, DSPHLD_OFF, DSPPRP_OFF, DSPPRN_OFF
	PARAMETER(DSPWIN_OFF = DSPOSH_OFF + SPGDIV)			
	PARAMETER(DSPHLD_OFF = DSPWIN_OFF + SPGNBR)			
	PARAMETER(DSPPRP_OFF = DSPHLD_OFF + SPGNBR)			
	PARAMETER(DSPPRN_OFF = DSPPRP_OFF + 1)				
C
	INTEGER*4 DSPTAX_OFF, DSPRES_OFF, DSPAPL_OFF, DSPMIN_OFF
	PARAMETER(DSPTAX_OFF = DSPPRN_OFF + 1)				
	PARAMETER(DSPRES_OFF = DSPTAX_OFF + 1)				
	PARAMETER(DSPAPL_OFF = DSPRES_OFF + 2)				
	PARAMETER(DSPMIN_OFF = DSPAPL_OFF + 1)				
C
	INTEGER*4 DSPSER_OFF, DSPOPA_OFF, DSPPRC_OFF, DSPMAX_OFF
	PARAMETER(DSPSER_OFF = DSPMIN_OFF + 1)				
	PARAMETER(DSPOPA_OFF = DSPSER_OFF + 1)				
	PARAMETER(DSPPRC_OFF = DSPOPA_OFF + 1)				
	PARAMETER(DSPMAX_OFF = DSPPRC_OFF + 1)				
C
	INTEGER*4 DSPMLT_OFF, DSPMDS_OFF, DSPDIV_OFF, DSPMAT_OFF
	PARAMETER(DSPMLT_OFF = DSPMAX_OFF + 1)				
	PARAMETER(DSPMDS_OFF = DSPMLT_OFF + 1)				
	PARAMETER(DSPDIV_OFF = DSPMDS_OFF + (MAXMLTD_AVL/4))		
	PARAMETER(DSPMAT_OFF = DSPDIV_OFF + 1)				
C
	INTEGER*4 DSPPER_OFF, DSPSPR_OFF, DSPTSR_OFF, DSPREV_OFF
	PARAMETER(DSPPER_OFF = DSPMAT_OFF + SPGDIV)			
	PARAMETER(DSPSPR_OFF = DSPPER_OFF + SPGDIV)			
	PARAMETER(DSPTSR_OFF = DSPSPR_OFF + 1)				
	PARAMETER(DSPREV_OFF = DSPTSR_OFF + SPGDIV)			
C
	INTEGER*4 DSPNMS_OFF, DSPCLB_OFF, DSPASH_OFF, DSPFRG_OFF
	PARAMETER(DSPNMS_OFF = DSPREV_OFF + 1)				
	PARAMETER(DSPCLB_OFF = DSPNMS_OFF + ((SPNMS_LEN/4)*2*SPGNBR))	
	PARAMETER(DSPASH_OFF = DSPCLB_OFF + 25)				
	PARAMETER(DSPFRG_OFF = DSPASH_OFF + SPGDIV)			
C
	INTEGER*4 DSPWRF_OFF, DSPBST_OFF, DSPBAL_OFF, DSPEVN_OFF
	PARAMETER(DSPWRF_OFF = DSPFRG_OFF + 1)				
	PARAMETER(DSPBST_OFF = DSPWRF_OFF + POSTED)			
	PARAMETER(DSPBAL_OFF = DSPBST_OFF + 1)				
	PARAMETER(DSPEVN_OFF = DSPBAL_OFF + MAXDRW)
C
        INTEGER*4 DSPEXT_OFF
        INTEGER*4 DSPRGT_OFF
	INTEGER*4 DSPROD_OFF
        PARAMETER(DSPEXT_OFF = DSPEVN_OFF + SPEVN_LEN/4)
        PARAMETER(DSPRGT_OFF = DSPEXT_OFF + 1)
	PARAMETER(DSPROD_OFF = DSPRGT_OFF + 2)
C
        INTEGER * 4 DSPECD_OFF
        INTEGER * 4 DSPMCE_OFF
        INTEGER * 4 DSPDCD_OFF
        INTEGER * 4 DSPRWD_OFF
C
        PARAMETER(DSPECD_OFF = DSPROD_OFF  + 2)
        PARAMETER(DSPMCE_OFF = DSPECD_OFF + SPGNBR)
        PARAMETER(DSPDCD_OFF = DSPMCE_OFF + 1)
        PARAMETER(DSPRWD_OFF = DSPDCD_OFF + 1)
C
	INTEGER*4 DSPFRE_OFF
	PARAMETER(DSPFRE_OFF = DSPRWD_OFF + 1)
C
C CALCULATE FREE SPACE BASED ON MACHINE DEPENDENT SECTOR SIZES.
C SHOULD ALLWAYS ADD UP TO A EVEN SECTOR SIZE BYTES (VAX 512).
C NOTE! IN CALCULATIONS, FRE_OFF-2 IS DONE TO TAKE CARE OF WHEN THE DATA SIZE
C       ENDS ON A EVEN SECTOR SIZE.
C
        INTEGER*4 DSPSEC_VAX, DSPSEC_NUM
        PARAMETER (DSPSEC_VAX = SECSIZE/4)
	PARAMETER (DSPSEC_NUM = NINT(FLOAT((DSPFRE_OFF-2)/DSPSEC_VAX)+.5))
C
        INTEGER*4 DSPLEN, DSPSEC
        PARAMETER (DSPLEN = DSPSEC_VAX*DSPSEC_NUM)         !EVEN SEC SIZE
        PARAMETER (DSPSEC = DSPLEN / 64)
C
	INTEGER*4 DSPREC(DSPLEN)	
C
	EQUIVALENCE(DSPSTS,DSPREC(DSPSTS_OFF))
	EQUIVALENCE(DSPCTM,DSPREC(DSPCTM_OFF))
	EQUIVALENCE(DSPTIM,DSPREC(DSPTIM_OFF))
	EQUIVALENCE(DSPDRW,DSPREC(DSPDRW_OFF))
	EQUIVALENCE(DSPBSD,DSPREC(DSPBSD_OFF))
	EQUIVALENCE(DSPESD,DSPREC(DSPESD_OFF))
	EQUIVALENCE(DSPPUP,DSPREC(DSPPUP_OFF))
	EQUIVALENCE(DSPUPD,DSPREC(DSPUPD_OFF))
	EQUIVALENCE(DSPDAT,DSPREC(DSPDAT_OFF))
	EQUIVALENCE(DSPADV,DSPREC(DSPADV_OFF))
	EQUIVALENCE(DSPSAL,DSPREC(DSPSAL_OFF))
	EQUIVALENCE(DSPSHV,DSPREC(DSPSHV_OFF))
	EQUIVALENCE(DSPSHR,DSPREC(DSPSHR_OFF))
	EQUIVALENCE(DSPPOL,DSPREC(DSPPOL_OFF))
	EQUIVALENCE(DSPPAD,DSPREC(DSPPAD_OFF))
	EQUIVALENCE(DSPPRG,DSPREC(DSPPRG_OFF))
	EQUIVALENCE(DSPAFD,DSPREC(DSPAFD_OFF))
	EQUIVALENCE(DSPCSP,DSPREC(DSPCSP_OFF))
	EQUIVALENCE(DSPOSV,DSPREC(DSPOSV_OFF))
	EQUIVALENCE(DSPOSH,DSPREC(DSPOSH_OFF))
	EQUIVALENCE(DSPWIN,DSPREC(DSPWIN_OFF))
	EQUIVALENCE(DSPHLD,DSPREC(DSPHLD_OFF))
	EQUIVALENCE(DSPPRP,DSPREC(DSPPRP_OFF))
	EQUIVALENCE(DSPPRN,DSPREC(DSPPRN_OFF))
	EQUIVALENCE(DSPTAX,DSPREC(DSPTAX_OFF))
	EQUIVALENCE(DSPRES,DSPREC(DSPRES_OFF))
	EQUIVALENCE(DSPAPL,DSPREC(DSPAPL_OFF))
	EQUIVALENCE(DSPMIN,DSPREC(DSPMIN_OFF))
	EQUIVALENCE(DSPSER,DSPREC(DSPSER_OFF))
	EQUIVALENCE(DSPOPA,DSPREC(DSPOPA_OFF))
	EQUIVALENCE(DSPPRC,DSPREC(DSPPRC_OFF))
	EQUIVALENCE(DSPMAX,DSPREC(DSPMAX_OFF))
	EQUIVALENCE(DSPMLT,DSPREC(DSPMLT_OFF))
	EQUIVALENCE(DSPMDS,DSPREC(DSPMDS_OFF))
	EQUIVALENCE(DSPDIV,DSPREC(DSPDIV_OFF))
	EQUIVALENCE(DSPMAT,DSPREC(DSPMAT_OFF))
	EQUIVALENCE(DSPPER,DSPREC(DSPPER_OFF))
	EQUIVALENCE(DSPSPR,DSPREC(DSPSPR_OFF))
	EQUIVALENCE(DSPTSR,DSPREC(DSPTSR_OFF))
	EQUIVALENCE(DSPREV,DSPREC(DSPREV_OFF))
	EQUIVALENCE(DSPNMS,DSPREC(DSPNMS_OFF))
	EQUIVALENCE(DSPCLB,DSPREC(DSPCLB_OFF))
	EQUIVALENCE(DSPASH,DSPREC(DSPASH_OFF))
	EQUIVALENCE(DSPFRG,DSPREC(DSPFRG_OFF))
	EQUIVALENCE(DSPWRF,DSPREC(DSPWRF_OFF))
	EQUIVALENCE(DSPBST,DSPREC(DSPBST_OFF))
	EQUIVALENCE(DSPBAL,DSPREC(DSPBAL_OFF))
	EQUIVALENCE(DSPEVN,DSPREC(DSPEVN_OFF))
        EQUIVALENCE(DSPEXT,DSPREC(DSPEXT_OFF))
        EQUIVALENCE(DSPRGT,DSPREC(DSPRGT_OFF))
	EQUIVALENCE(DSPROD,DSPREC(DSPROD_OFF))
	EQUIVALENCE(DSPECD,DSPREC(DSPECD_OFF))
	EQUIVALENCE(DSPMCE,DSPREC(DSPMCE_OFF))
	EQUIVALENCE(DSPDCD,DSPREC(DSPDCD_OFF))
	EQUIVALENCE(DSPRWD,DSPREC(DSPRWD_OFF))
