C
C SPORT DATA FILE RECORD LAYOUT (DISK)
C
C
C
C     DSPSTS                         ;STATUS
C     DSPCTM                         ;ACTUAL TIME WHEN CLOSED
C     DSPTIM                         ;TIME WHEN GAME SHOULD CLOSE
C     DSPDRW                         ;DRAW NUMBER
C     DSPBSD                         ;SPORT BEGGINING DRAW DATE
C     DSPESD                         ;SPORT ENDING DRAW DATE
C     DSPPUP                         ;LAST PURGE UPDATE
C     DSPUPD                         ;LAST FILE UPDATE
C     DSPDAT(DATLEN)                 ;DRAW DATES
C     DSPADV(NUMADV)                 ;ADVANCE DRAW DATES
C     DSPSAL(SPGENT)                 ;SALES DATA
C     DSPSHV(SPGDIV)                 ;SHARE VALUES
C     DSPSHR(SPGDIV)                 ;SHARES
C     DSPPOL(SPGDIV)                 ;POOL CARRIED OVER
C     DSPPAD(SPGDIV)                 ;AMOUNT PAID
C     DSPPRG(SPGDIV)                 ;AMOUNT PURGED
C     DSPAFD(SPGDIV)                 ;AMOUNTS FROM OTHERS DRAWS/GAMES (EXTRA)
C     DSPCSP(SPGDIV)                 ;CALCULATED SHARE CALCULATIONS POOLS
C     DSPOSV(SPGDIV)                 ;OFF LINE AMOUNT TO PRIZES BY DIVISION
C     DSPOSH(SPGDIV)                 ;OFF LINE SHARE
C     DSPWIN(SPGNBR)                 ;WINNING RESULTS
C     DSPHLD(SPGNBR)                 ;WINNING RESULTS HOLD
C     DSPPRP                         ;PERCENTAGE OF THE PROMISED AMOUNT
C     DSPPRN                         ;NUM. OF WEEKS WITHOUT DIV1 WINNERS
C     DSPTAX                         ;LOTTERY TAX
C     DSPRES(2)                      ;RESERVE POOL
C     DSPAPL                         ;ADDITIONAL POOL
C     DSPMIN                         ;MINIMUM POOL
C     DSPSER                         ;LAST SERIAL NUMBER
C     DSPOPA                         ;OVER-RIDE POOL AMOUNT
C     DSPPRC                         ;PRICE/SINGLE COMBINATION
C     DSPMAX                         ;MAXIMUM NUMBER OF EVENTS (ROWS)
C     DSPMLT                         ;MULTI-DRAW ENABLE FLAG
C     DSPMDS(MAXMLTD_AVL)            ;MULTI-DRAW SELECTED TABLE (52/4 WORDS)
C     DSPDIV                         ;# OF DIVISIONS
C     DSPMAT(SPGDIV)                 ;DIVISION MATCH TABLE
C     DSPPER(SPGDIV)                 ;DIVISION PERCENTAGES
C     DSPSPR                         ;SALES PERCENTAGE
C     DSPTSR(SPGDIV)                 ;TOTAL SHARE COUNT
C     DSPREV                         ;SPORT REVISION NUMBER
C     DSPNMS(SPNMS_LEN/4,2,SPGNBR)   ;TEAM NAMES
C     DSPCLB(25)                     ;CLUB VOTING COUNT
C     DSPASH(SPGDIV)                 ;ADDITIONAL POOL BY DIV.
C     DSPFRG                         ;NUMBER OF RESULTS GAMES
C     DSPWRF(POSTED)                 ;WIN RESERVE FUND
C     DSPBST                         ;BEST MATCH FLAG (ADJUST DIVISIONS)
C     DSPBAL(MAXDRW)		     ;BALANCE SALES INFO. BY DRAW
C     DSPEVN(SPEVN_LEN/4)            ;EVENT NAME
C     DSPEXT                         ;EXTRAORDINARY DRAW
C     DSPRGT(2)                      ;WHERE ROLLOVER GO TO (1:GAME, 2:DRW)
C     DSPROD(2)			     ;WHERE ROLLOVER CAME FROM (1:GAME, 2:DRW)
C     DSPECD(SPGNBR)                 ;EVENT CANCEL DATE AND TIME
C     DSPMCE                         ;MAXIMUN NUMBER OF EVENTS CANCELLED TO CANCEL THE DRAW
C     DSPDCD                         ;DRAW CANCEL DATE AND TIME
C     DSPRWD                         ;REFUND WINNING DIVISION
C
C SPORT GAME FILE PARAMETERS
C
