C
C DESCRIPTION PARAMETERS DEFINITION
C
C
C     DTGSTS                         ; STATUS
C     DTGCTM                         ; ACTUAL TIME WHEN CLOSED
C     DTGTIM                         ; TIME WHEN GAME SHOULD CLOSE
C     DTGDRW                         ; DRAW NUMBER
C     DTGBSD                         ; RESULTS BEGGINING DRAW DATE
C     DTGESD                         ; RESULTS ENDING DRAW DATE
C     DTGPUP                         ; LAST PURGE UPDATE
C     DTGUPD                         ; LAST FILE UPDATE
C     DTGDAT(DATLEN)                 ; DRAW DATES
C     DTGADV(NUMADV)                 ; ADVANCE DRAW DATES
C     DTGSAL(TGGENT)                 ; SALES DATA
C     DTGSHV(TGGDIV)                 ; SHARE VALUES
C     DTGSHR(TGGDIV)                 ; SHARES
C     DTGPOL(TGGDIV)                 ; POOL CARRIED OVER
C     DTGPAD(TGGDIV)                 ; AMOUNT PAID
C     DTGPRG(TGGDIV)                 ; AMOUNT PURGED
C     DTGAFD(TGGDIV)                 ; AMOUNTS FROM OTHERS DRAWS/GAMES (EXTRA)
C     DTGCSP(TGGDIV)                 ; CALCULATED SHARE CALCULATIONS POOLS
C     DTGOSV(TGGDIV)                 ; OFF LINE AMOUNT TO PRIZES BY DIVISION
C     DTGOSH(TGGDIV)                 ; OFF LINE SHARE
C     DTGWIN(2, TGGNBR)              ; WINNING RESULTS
C     DTGHLD(2, TGGNBR)              ; WINNING RESULTS HOLD
C     DTGPRP                         ; PERCENTAGE OF THE PROMISED AMOUNT
C     DTGPRN                         ; MAX NUM. OF WINNERS FOR PROMISED AMOUNT,
C                                    ; IF DTGPRN=-1, IT MEANS 'ERIKOISPOTTI'
C     DTGTAX                         ; LOTTERY TAX
C     DTGRES(2)                      ; RESERVE POOL
C     DTGAPL                         ; ADDITIONAL POOL
C     DTGMIN                         ; MINIMUM POOL
C     DTGSER                         ; LAST SERIAL NUMBER
C     DTGOPA                         ; OVER-RIDE POOL AMOUNT
C     DTGPRC                         ; PRICE/SINGLE COMBINATION
C     DTGMAX                         ; MAXIMUM NUMBER OF EVENTS (ROWS)
C     DTGMLT                         ; MULTI-DRAW ENABLE FLAG
C     DTGMDS(MAXMLTD_AVL)            ; MULTI-DRAW SELECTED TABLE (52/4 WORDS)
C     DTGDIV                         ; # OF DIVISIONS
C     DTGMAT(TGGDIV)                 ; DIVISION MATCH TABLE
C     DTGPER(TGGDIV)                 ; DIVISION PERCENTAGES
C     DTGSPR                         ; SALES PERCENTAGE
C     DTGTSR(TGGDIV)                 ; TOTAL SHARE COUNT
C     DTGREV                         ; RESULTS REVISION NUMBER
C
C     DTGNMS(TGNMS_LEN / 4, 2, TGGNBR)   ; TEAM NAMES
C
C     DTGCLB(25)                     ; CLUB VOTING COUNT
C     DTGASH(TGGDIV)                 ; ADDITIONAL POOL BY DIV.
C     DTGFRG                         ; FLAG FOR FREE GAME (PRICE = 0.00)
C     DTGWRF(POSTED)                 ; WIN RESERVE FUND
C     DTGBST                         ; BEST MATCH FLAG (ADJUST DIVISIONS)
C     DTGBAL(MAXDRW)                 ; BALANCE SALES INFO. BY DRAW
C     DTGEVN(TGEVN_LEN / 4)          ; EVENT NAME
C     DTGEXT                         ; EXTRAORDINARY DRAW
C     DTGRGT(2)                      : WHERE ROLLOVER GO TO (1:GAME, 2:DRW)
C     DTGROD(2)                      ; WHERE ROLLOVER CAME FROM (1:GAME, 2:DRW)
C
C
C RESULTS DRAW FILE PARAMETERS DEFINICION
C
      INTEGER * 4 DTGSTS
      INTEGER * 4 DTGCTM
      INTEGER * 4 DTGTIM
      INTEGER * 4 DTGDRW
      INTEGER * 4 DTGBSD
      INTEGER * 4 DTGESD
      INTEGER * 4 DTGPUP
      INTEGER * 4 DTGUPD
      INTEGER * 4 DTGDAT(DATLEN)
      INTEGER * 4 DTGSAL(TGGENT)
      INTEGER * 4 DTGSHV(TGGDIV)
      INTEGER * 4 DTGSHR(TGGDIV)
      INTEGER * 4 DTGPOL(TGGDIV)
      INTEGER * 4 DTGPAD(TGGDIV)
      INTEGER * 4 DTGPRG(TGGDIV)
      INTEGER * 4 DTGADV(NUMADV)
      INTEGER * 4 DTGAFD(TGGDIV)
      INTEGER * 4 DTGCSP(TGGDIV)
      INTEGER * 4 DTGOSV(TGGDIV)
      INTEGER * 4 DTGOSH(TGGDIV)
      INTEGER * 4 DTGWIN(2, TGGNBR)
      INTEGER * 4 DTGHLD(2, TGGNBR)
      INTEGER * 4 DTGPRP
      INTEGER * 4 DTGPRN
      INTEGER * 4 DTGMAT(TGGDIV)
      INTEGER * 4 DTGNMS(TGNMS_LEN / 4, 2, TGGNBR)
      INTEGER * 4 DTGTAX
      INTEGER * 4 DTGRES(2)
      INTEGER * 4 DTGAPL
      INTEGER * 4 DTGMIN
      INTEGER * 4 DTGSER
      INTEGER * 4 DTGOPA
      INTEGER * 4 DTGPRC
      INTEGER * 4 DTGMAX
      INTEGER * 4 DTGMLT
      INTEGER * 4 DTGDIV
      INTEGER * 4 DTGCLB(25)
      INTEGER * 4 DTGPER(TGGDIV)
      INTEGER * 4 DTGSPR
      INTEGER * 4 DTGTSR(TGGDIV)
      INTEGER * 4 DTGREV
      INTEGER * 4 DTGASH(TGGDIV)
      INTEGER * 4 DTGFRG
      INTEGER * 4 DTGWRF(POSTED)
      INTEGER * 4 DTGBST
      INTEGER * 4 DTGBAL(MAXDRW)
      INTEGER * 4 DTGEVN(TGEVN_LEN / 4)
      INTEGER * 4 DTGEXT
      INTEGER * 4 DTGRGT(2)
      INTEGER * 4 DTGROD(2)
C
      BYTE DTGMDS(MAXMLTD_AVL)
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
      INTEGER * 4 DTGSTS_OFF, DTGCTM_OFF, DTGTIM_OFF, DTGDRW_OFF
C
      PARAMETER(DTGSTS_OFF = 1)
      PARAMETER(DTGCTM_OFF = DTGSTS_OFF + 1)
      PARAMETER(DTGTIM_OFF = DTGCTM_OFF + 1)
      PARAMETER(DTGDRW_OFF = DTGTIM_OFF + 1)
C
      INTEGER * 4 DTGBSD_OFF, DTGESD_OFF, DTGPUP_OFF, DTGUPD_OFF
C
      PARAMETER(DTGBSD_OFF = DTGDRW_OFF + 1)
      PARAMETER(DTGESD_OFF = DTGBSD_OFF + 1)
      PARAMETER(DTGPUP_OFF = DTGESD_OFF + 1)
      PARAMETER(DTGUPD_OFF = DTGPUP_OFF + 1)
C
      INTEGER * 4 DTGDAT_OFF, DTGADV_OFF, DTGSAL_OFF, DTGSHV_OFF
C        
      PARAMETER(DTGDAT_OFF = DTGUPD_OFF + 1)
      PARAMETER(DTGADV_OFF = DTGDAT_OFF + DATLEN)
      PARAMETER(DTGSAL_OFF = DTGADV_OFF + NUMADV)
      PARAMETER(DTGSHV_OFF = DTGSAL_OFF + TGGENT)
C
      INTEGER * 4 DTGSHR_OFF, DTGPOL_OFF, DTGPAD_OFF, DTGPRG_OFF
C        
      PARAMETER(DTGSHR_OFF = DTGSHV_OFF + TGGDIV)
      PARAMETER(DTGPOL_OFF = DTGSHR_OFF + TGGDIV)
      PARAMETER(DTGPAD_OFF = DTGPOL_OFF + TGGDIV)
      PARAMETER(DTGPRG_OFF = DTGPAD_OFF + TGGDIV)
C
      INTEGER * 4 DTGAFD_OFF, DTGCSP_OFF, DTGOSV_OFF, DTGOSH_OFF
C        
      PARAMETER(DTGAFD_OFF = DTGPRG_OFF + TGGDIV)
      PARAMETER(DTGCSP_OFF = DTGAFD_OFF + TGGDIV)
      PARAMETER(DTGOSV_OFF = DTGCSP_OFF + TGGDIV)
      PARAMETER(DTGOSH_OFF = DTGOSV_OFF + TGGDIV)
C
      INTEGER * 4 DTGWIN_OFF, DTGHLD_OFF, DTGPRP_OFF, DTGPRN_OFF
C        
      PARAMETER(DTGWIN_OFF = DTGOSH_OFF + TGGDIV)
      PARAMETER(DTGHLD_OFF = DTGWIN_OFF + (2 * TGGNBR))
      PARAMETER(DTGPRP_OFF = DTGHLD_OFF + (2 * TGGNBR))
      PARAMETER(DTGPRN_OFF = DTGPRP_OFF + 1)
C
      INTEGER * 4 DTGTAX_OFF, DTGRES_OFF, DTGAPL_OFF, DTGMIN_OFF
C        
      PARAMETER(DTGTAX_OFF = DTGPRN_OFF + 1)
      PARAMETER(DTGRES_OFF = DTGTAX_OFF + 1)
      PARAMETER(DTGAPL_OFF = DTGRES_OFF + 2)
      PARAMETER(DTGMIN_OFF = DTGAPL_OFF + 1)
C
      INTEGER * 4 DTGSER_OFF, DTGOPA_OFF, DTGPRC_OFF, DTGMAX_OFF
C
      PARAMETER(DTGSER_OFF = DTGMIN_OFF + 1)
      PARAMETER(DTGOPA_OFF = DTGSER_OFF + 1)
      PARAMETER(DTGPRC_OFF = DTGOPA_OFF + 1)
      PARAMETER(DTGMAX_OFF = DTGPRC_OFF + 1)
C
      INTEGER * 4 DTGMLT_OFF, DTGMDS_OFF, DTGDIV_OFF, DTGMAT_OFF
C        
      PARAMETER(DTGMLT_OFF = DTGMAX_OFF + 1)
      PARAMETER(DTGMDS_OFF = DTGMLT_OFF + 1)
      PARAMETER(DTGDIV_OFF = DTGMDS_OFF + (MAXMLTD_AVL / 4))
      PARAMETER(DTGMAT_OFF = DTGDIV_OFF + 1)
C
      INTEGER * 4 DTGPER_OFF, DTGSPR_OFF, DTGTSR_OFF, DTGREV_OFF
C
      PARAMETER(DTGPER_OFF = DTGMAT_OFF + TGGDIV)
      PARAMETER(DTGSPR_OFF = DTGPER_OFF + TGGDIV)
      PARAMETER(DTGTSR_OFF = DTGSPR_OFF + 1)
      PARAMETER(DTGREV_OFF = DTGTSR_OFF + TGGDIV)
C
      INTEGER * 4 DTGNMS_OFF, DTGCLB_OFF, DTGASH_OFF, DTGFRG_OFF
C
      PARAMETER(DTGNMS_OFF = DTGREV_OFF + 1)
      PARAMETER(DTGCLB_OFF = DTGNMS_OFF + ((TGNMS_LEN / 4) * 2 * TGGNBR))
      PARAMETER(DTGASH_OFF = DTGCLB_OFF + 25)
      PARAMETER(DTGFRG_OFF = DTGASH_OFF + TGGDIV)
C
      INTEGER * 4 DTGWRF_OFF, DTGBST_OFF, DTGBAL_OFF, DTGEVN_OFF
C        
      PARAMETER(DTGWRF_OFF = DTGFRG_OFF + 1)
      PARAMETER(DTGBST_OFF = DTGWRF_OFF + POSTED)
      PARAMETER(DTGBAL_OFF = DTGBST_OFF + 1)
      PARAMETER(DTGEVN_OFF = DTGBAL_OFF + MAXDRW)
C
      INTEGER * 4 DTGEXT_OFF
      INTEGER * 4 DTGRGT_OFF
      INTEGER * 4 DTGROD_OFF
C     
      PARAMETER(DTGEXT_OFF = DTGEVN_OFF + TGEVN_LEN / 4)
      PARAMETER(DTGRGT_OFF = DTGEXT_OFF + 1)
      PARAMETER(DTGROD_OFF = DTGRGT_OFF + 2)
C
      INTEGER * 4 DTGFRE_OFF
C
      PARAMETER(DTGFRE_OFF = DTGROD_OFF + 2)
C
C CALCULATE FREE SPACE BASED ON MACHINE DEPENDENT SECTOR SIZES.
C SHOULD ALLWAYS ADD UP TO A EVEN SECTOR SIZE BYTES (VAX 512).
C
C NOTE 
C       IN CALCULATIONS, FRE_OFF - 2 IS DONE TO TAKE CARE OF WHEN THE DATA SIZE
C       ENDS ON A EVEN SECTOR SIZE.
C
C
      INTEGER * 4 DTGSEC_VAX, DTGSEC_NUM
C
      PARAMETER(DTGSEC_VAX = SECSIZE / 4)
      PARAMETER(DTGSEC_NUM = NINT(FLOAT((DTGFRE_OFF - 2) / DTGSEC_VAX) + .5))
C
      INTEGER * 4 DTGLEN, DTGSEC
C
      PARAMETER (DTGLEN = DTGSEC_VAX * DTGSEC_NUM)     ! EVEN SEC SIZE
      PARAMETER (DTGSEC = DTGLEN / 64)
C
C RECORD DEFINITION
C
      INTEGER * 4 DTGREC(DTGLEN)
C
C EQUIVALENCE DRAW RECORD DEFINITION
C
      EQUIVALENCE(DTGSTS, DTGREC(DTGSTS_OFF))
      EQUIVALENCE(DTGCTM, DTGREC(DTGCTM_OFF))
      EQUIVALENCE(DTGTIM, DTGREC(DTGTIM_OFF))
      EQUIVALENCE(DTGDRW, DTGREC(DTGDRW_OFF))
      EQUIVALENCE(DTGBSD, DTGREC(DTGBSD_OFF))
      EQUIVALENCE(DTGESD, DTGREC(DTGESD_OFF))
      EQUIVALENCE(DTGPUP, DTGREC(DTGPUP_OFF))
      EQUIVALENCE(DTGUPD, DTGREC(DTGUPD_OFF))
      EQUIVALENCE(DTGDAT, DTGREC(DTGDAT_OFF))
      EQUIVALENCE(DTGADV, DTGREC(DTGADV_OFF))
      EQUIVALENCE(DTGSAL, DTGREC(DTGSAL_OFF))
      EQUIVALENCE(DTGSHV, DTGREC(DTGSHV_OFF))
      EQUIVALENCE(DTGSHR, DTGREC(DTGSHR_OFF))
      EQUIVALENCE(DTGPOL, DTGREC(DTGPOL_OFF))
      EQUIVALENCE(DTGPAD, DTGREC(DTGPAD_OFF))
      EQUIVALENCE(DTGPRG, DTGREC(DTGPRG_OFF))
      EQUIVALENCE(DTGAFD, DTGREC(DTGAFD_OFF))
      EQUIVALENCE(DTGCSP, DTGREC(DTGCSP_OFF))
      EQUIVALENCE(DTGOSV, DTGREC(DTGOSV_OFF))
      EQUIVALENCE(DTGOSH, DTGREC(DTGOSH_OFF))
      EQUIVALENCE(DTGWIN, DTGREC(DTGWIN_OFF))
      EQUIVALENCE(DTGHLD, DTGREC(DTGHLD_OFF))
      EQUIVALENCE(DTGPRP, DTGREC(DTGPRP_OFF))
      EQUIVALENCE(DTGPRN, DTGREC(DTGPRN_OFF))
      EQUIVALENCE(DTGTAX, DTGREC(DTGTAX_OFF))
      EQUIVALENCE(DTGRES, DTGREC(DTGRES_OFF))
      EQUIVALENCE(DTGAPL, DTGREC(DTGAPL_OFF))
      EQUIVALENCE(DTGMIN, DTGREC(DTGMIN_OFF))
      EQUIVALENCE(DTGSER, DTGREC(DTGSER_OFF))
      EQUIVALENCE(DTGOPA, DTGREC(DTGOPA_OFF))
      EQUIVALENCE(DTGPRC, DTGREC(DTGPRC_OFF))
      EQUIVALENCE(DTGMAX, DTGREC(DTGMAX_OFF))
      EQUIVALENCE(DTGMLT, DTGREC(DTGMLT_OFF))
      EQUIVALENCE(DTGMDS, DTGREC(DTGMDS_OFF))
      EQUIVALENCE(DTGDIV, DTGREC(DTGDIV_OFF))
      EQUIVALENCE(DTGMAT, DTGREC(DTGMAT_OFF))
      EQUIVALENCE(DTGPER, DTGREC(DTGPER_OFF))
      EQUIVALENCE(DTGSPR, DTGREC(DTGSPR_OFF))
      EQUIVALENCE(DTGTSR, DTGREC(DTGTSR_OFF))
      EQUIVALENCE(DTGREV, DTGREC(DTGREV_OFF))
      EQUIVALENCE(DTGNMS, DTGREC(DTGNMS_OFF))
      EQUIVALENCE(DTGCLB, DTGREC(DTGCLB_OFF))
      EQUIVALENCE(DTGASH, DTGREC(DTGASH_OFF))
      EQUIVALENCE(DTGFRG, DTGREC(DTGFRG_OFF))
      EQUIVALENCE(DTGWRF, DTGREC(DTGWRF_OFF))
      EQUIVALENCE(DTGBST, DTGREC(DTGBST_OFF))
      EQUIVALENCE(DTGBAL, DTGREC(DTGBAL_OFF))
      EQUIVALENCE(DTGEVN, DTGREC(DTGEVN_OFF))
      EQUIVALENCE(DTGEXT, DTGREC(DTGEXT_OFF))
      EQUIVALENCE(DTGRGT, DTGREC(DTGRGT_OFF))
      EQUIVALENCE(DTGROD, DTGREC(DTGROD_OFF))
C
C END OF FILE
C
