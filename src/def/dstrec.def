C DSTREC.DEF
C
C V02 30-JUN-2000 UXN DSTLAT ADDED.
C V01 13-MAY-1999 UXN INITIAL RELEASE.
C 
C SUPER TRIPLE GAME DATA FILE RECORD LAYOUT (DISK)
C
C
C     DSTSTS                         ; GAME STATUS
C     DSTWEK                         ; WEEK NUMBER
C     DSTDAT                         ; DRAW DATE
C     DSTDRW                         ; DRAW NUMBER
C     DSTBSD                         ; BEGINNING SALES DATE
C     DSTESD                         ; ENDING SALES DATE
C     DSTPUP                         ; LAST PRUGE UPDATE  (FILE ONLY)
C     DSTUPD                         ; LAST FILE UPDATE   (FILE ONLY)
C     DSTCTM                         ; ACTUAL TIME WHEN CLOSED
C     DSTTIM                         ; TIME WHEN GAME SHOULD CLOSE
C     DSTSER                         ; LAST SERIAL NUMBER
C     DSTSAL(NUMTOT)                 ; TOTAL SALES (COUNT , AMOUNT)
C     DSTPAD                         ; PRIZES PAID + REFUNDS
C     DSTPRG                         ; PRIZES PURGED
C     DSTPRF                         ; PAID REFUNDS
C     DSTREF                         ; TOTAL REFUND AMOUNT
C     DSTERF                         ; EARLY REFUNDS PAID
C     DSTTER                         ; TOTAL EARLY REFUNDS (LIAB)
C     DSTWON                         ; PRIZES WON
C     DSTPOL(2)                      ; ROLL POOL
C     DSTTPL                         ; TOTAL PRIZE POOL
C     DSTTBK                         ; TOTAL BREAKAGE
C     DSTBRK(2)                      ; WINNING ROUNDING POT
C     DSTABW                         ; AMOUNT BET ON WINNING SCORE
C     DSTODS(MAXSTRTI)               ; WINNING ODDS
C     DSTWIN(3,MAXSTRTI)             ; WINNING RESULTS
C     DSTHLD(3,MAXSTRTI)             ; WINNING RESULTS HOLD
C     DSTTAX                         ; WINNING TAXES
C     DSTOTX(NUMTOT)                 ; OVER TAX LIMIT
C     DSTMID(NUMTOT)                 ; >REDMIN <REDMAX
C     DSTUTX(NUMTOT)                 ; UNDER TAX LIMIT
C     DSTHST(MAXSTRTI)               ; HIGHEST WINNER PER WINING COMB
C     DSTORM(NUMTOT)                 ; OVER REDMAX
C     DSTENM(STRENM_LEN/4)           ; EVENT NAME
C     DSTDES(STRDES_LEN/4)           ; GAME DESCRIPTION
C     DSTNMS(STRNMS_LEN/4,MAXSTRRW)  ; ROW NAMES
C     DSTSTA(MAXSTRRW)               ; ROW STATUS 1 THROUGH MAXTRW
C     DSTSBR(MAXSTRRW)               ; SALES BY ROW TABLE
C     DSTREV                         ; REV #
C     DSTPFN(5)                      ; POOL FILE NAME
C     DSTRTM(MAXSTRRW)               ; ROW CLOSE TIMES
C     DSTPRC                         ; BASE PRICE
C     DSTSPR                         ; POOL PERCENTAGE
C     DSTTVC(STRTVC_LEN/4)    	     ; TV-CHANEL NAME
C     DSTMDS(MAXMLTD_AVL)	     ; MULTI DRAW SELECTED TABLE
C                                      (52 BYTE / 4 = 13)
C     DSTRWS			     ; ACTUAL # OF ROWS USED.
C     DSTWCP(MAXSTRTI)		     ; # WINNING COUPONS PALYED.
C     DSTWBT(NUMTOT,MAXSTRTI)        ; CNT, AMT PLAYED.
C     DSTWRO(NUMTOT,2)               ; WINNING: COUNT,AMOUNT REFUNDS ONLY
C     DSTWRA(NUMTOT,2)               ; WINNING: COUNT,AMOUNT REFUNDS ALTOGETHER
C     DSTWPO(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES ONLY
C     DSTWPA(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES ALTOGETHER
C     DSTWPR(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES AND REFUNDS
C                                    ; (1=PRWON,2=PRPAD)
C     DSTPCC			     ; Partially closed combinations. If this
C                                    ; field is set to 1 then combinations 
C                                    ; xx-16-xx, xx-xx-16, 17-xx-xx, xx-xx-17,
C                                    ; 18-xx-xx, xx-18-xx cannot be played.
C     DSTCMB                         ; NUMBER OF WINNING COMBINATIONS.
C     DSTLAT(NUMLAT)                 ; EVENT STARTED BEFORE GAME WAS CLOSED
C                                    ; TICKETS PLAYED LATER SHOULD BE REFUNDED.
C
C SUPER TRIPLE GAME FILE PARAMETERS
C
C+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
