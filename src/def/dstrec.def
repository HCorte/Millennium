C+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
C
C
	INTEGER*4 DSTSTS,DSTWEK,DSTDAT,DSTBSD,DSTESD,DSTCTM,DSTTIM
	INTEGER*4 DSTSAL(NUMTOT),DSTPAD,DSTPRG,DSTREF,DSTWON,DSTPOL(2)
        INTEGER*4 DSTTPL
	INTEGER*4 DSTTBK,DSTBRK(2),DSTABW
	INTEGER*4 DSTODS(MAXSTRTI),DSTWIN(3,MAXSTRTI)
	INTEGER*4 DSTTAX,DSTOTX(NUMTOT),DSTHLD(3,MAXSTRTI)
	INTEGER*4 DSTMID(NUMTOT),DSTUTX(NUMTOT)
	INTEGER*4 DSTHST(MAXSTRTI),DSTORM(2)
	INTEGER*4 DSTENM(STRENM_LEN/4)
	INTEGER*4 DSTDES(STRDES_LEN/4)
	INTEGER*4 DSTNMS(STRNMS_LEN/4,MAXSTRRW)
	INTEGER*4 DSTREV,DSTPUP,DSTUPD,DSTSTA(MAXSTRRW)
	INTEGER*4 DSTSBR(MAXSTRRW),DSTPFN(5),DSTERF,DSTTER,DSTDRW
	INTEGER*4 DSTRTM(MAXSTRRW),DSTSER,DSTPRF,DSTSPR,DSTPRC
	INTEGER*4 DSTTVC(STRTVC_LEN/4)

	BYTE	  DSTMDS(MAXMLTD_AVL)

	INTEGER*4 DSTRWS,DSTWCP(MAXSTRTI),DSTWBT(NUMTOT,MAXSTRTI)
        INTEGER*4 DSTWRO(NUMTOT,2),DSTWRA(NUMTOT,2),DSTWPO(NUMTOT,2)
        INTEGER*4 DSTWPA(NUMTOT,2),DSTWPR(NUMTOT,2)
	INTEGER*4 DSTPCC,DSTCMB,DSTLAT(NUMLAT)
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
	INTEGER*4 DSTSTS_OFF, DSTWEK_OFF, DSTDAT_OFF, DSTDRW_OFF
	PARAMETER(DSTSTS_OFF = 1)					!001
	PARAMETER(DSTWEK_OFF = DSTSTS_OFF + 1)				!002
	PARAMETER(DSTDAT_OFF = DSTWEK_OFF + 1)				!003
	PARAMETER(DSTDRW_OFF = DSTDAT_OFF + 1)				!004

	INTEGER*4 DSTBSD_OFF, DSTESD_OFF, DSTPUP_OFF, DSTUPD_OFF
	PARAMETER(DSTBSD_OFF = DSTDRW_OFF + 1)				!005
	PARAMETER(DSTESD_OFF = DSTBSD_OFF + 1)				!006
	PARAMETER(DSTPUP_OFF = DSTESD_OFF + 1)				!007
	PARAMETER(DSTUPD_OFF = DSTPUP_OFF + 1)				!008

	INTEGER*4 DSTCTM_OFF, DSTTIM_OFF, DSTSER_OFF, DSTSAL_OFF
	PARAMETER(DSTCTM_OFF = DSTUPD_OFF + 1)				!009
	PARAMETER(DSTTIM_OFF = DSTCTM_OFF + 1)				!010
	PARAMETER(DSTSER_OFF = DSTTIM_OFF + 1)				!011
	PARAMETER(DSTSAL_OFF = DSTSER_OFF + 1)   			!012
C
	INTEGER*4 DSTPAD_OFF, DSTPRG_OFF, DSTPRF_OFF, DSTREF_OFF
	PARAMETER(DSTPAD_OFF = DSTSAL_OFF + NUMTOT)			!014
	PARAMETER(DSTPRG_OFF = DSTPAD_OFF + 1)				!015
	PARAMETER(DSTPRF_OFF = DSTPRG_OFF + 1)				!016
	PARAMETER(DSTREF_OFF = DSTPRF_OFF + 1)				!017
C
	INTEGER*4 DSTERF_OFF, DSTTER_OFF, DSTWON_OFF, DSTPOL_OFF
	PARAMETER(DSTERF_OFF = DSTREF_OFF + 1)				!018
	PARAMETER(DSTTER_OFF = DSTERF_OFF + 1)				!019
	PARAMETER(DSTWON_OFF = DSTTER_OFF + 1)				!020
	PARAMETER(DSTPOL_OFF = DSTWON_OFF + 1)				!021
C
	INTEGER*4 DSTTPL_OFF, DSTTBK_OFF, DSTBRK_OFF, DSTABW_OFF
	PARAMETER(DSTTPL_OFF = DSTPOL_OFF + 2)				!023
	PARAMETER(DSTTBK_OFF = DSTTPL_OFF + 1)				!024
	PARAMETER(DSTBRK_OFF = DSTTBK_OFF + 1)				!025
	PARAMETER(DSTABW_OFF = DSTBRK_OFF + 2)				!027
C
	INTEGER*4 DSTODS_OFF, DSTWIN_OFF, DSTHLD_OFF, DSTTAX_OFF
	PARAMETER(DSTODS_OFF = DSTABW_OFF + 1)				!028
	PARAMETER(DSTWIN_OFF = DSTODS_OFF + MAXSTRTI)				!040
	PARAMETER(DSTHLD_OFF = DSTWIN_OFF + 3*MAXSTRTI)			!052
	PARAMETER(DSTTAX_OFF = DSTHLD_OFF + 3*MAXSTRTI)			!076
C
	INTEGER*4 DSTOTX_OFF, DSTMID_OFF, DSTUTX_OFF, DSTHST_OFF
	PARAMETER(DSTOTX_OFF = DSTTAX_OFF + 1)				!077
	PARAMETER(DSTMID_OFF = DSTOTX_OFF + NUMTOT)			!079
	PARAMETER(DSTUTX_OFF = DSTMID_OFF + NUMTOT)			!081
	PARAMETER(DSTHST_OFF = DSTUTX_OFF + NUMTOT)			!083
C
	INTEGER*4 DSTORM_OFF, DSTENM_OFF, DSTDES_OFF, DSTNMS_OFF
	PARAMETER(DSTORM_OFF = DSTHST_OFF + MAXSTRTI)				!096
	PARAMETER(DSTENM_OFF = DSTORM_OFF + NUMTOT)			!098
	PARAMETER(DSTDES_OFF = DSTENM_OFF + (STRENM_LEN/4))		!104
	PARAMETER(DSTNMS_OFF = DSTDES_OFF + (STRDES_LEN/4))		!134
C
	INTEGER*4 DSTSTA_OFF, DSTSBR_OFF, DSTREV_OFF, DSTPFN_OFF
	PARAMETER(DSTSTA_OFF = DSTNMS_OFF + (STRNMS_LEN/4)*MAXSTRRW)	!278
	PARAMETER(DSTSBR_OFF = DSTSTA_OFF + MAXSTRRW)			!314
	PARAMETER(DSTREV_OFF = DSTSBR_OFF + MAXSTRRW)			!350
	PARAMETER(DSTPFN_OFF = DSTREV_OFF + 1)				!351
C
	INTEGER*4 DSTRTM_OFF, DSTPRC_OFF, DSTSPR_OFF, DSTTVC_OFF
	PARAMETER(DSTRTM_OFF = DSTPFN_OFF + 5)				!356
	PARAMETER(DSTPRC_OFF = DSTRTM_OFF + MAXSTRRW)			!392
	PARAMETER(DSTSPR_OFF = DSTPRC_OFF + 1)				!393
	PARAMETER(DSTTVC_OFF = DSTSPR_OFF + 1)				!394 
C
	INTEGER*4 DSTMDS_OFF
	PARAMETER(DSTMDS_OFF = DSTTVC_OFF + (STRTVC_LEN/4))		
C
        INTEGER*4 DSTWRO_OFF, DSTWRA_OFF, DSTWPO_OFF, DSTWPA_OFF
        INTEGER*4 DSTWPR_OFF, DSTRWS_OFF, DSTWCP_OFF
        INTEGER*4 DSTWBT_OFF
	PARAMETER(DSTRWS_OFF = DSTMDS_OFF + (MAXMLTD_AVL/4))
	PARAMETER(DSTWCP_OFF = DSTRWS_OFF + 1)
	PARAMETER(DSTWBT_OFF = DSTWCP_OFF + MAXSTRTI)
        PARAMETER(DSTWRO_OFF = DSTWBT_OFF + NUMTOT*MAXSTRTI)
        PARAMETER(DSTWRA_OFF = DSTWRO_OFF + 2*NUMTOT)
        PARAMETER(DSTWPO_OFF = DSTWRA_OFF + 2*NUMTOT)
        PARAMETER(DSTWPA_OFF = DSTWPO_OFF + 2*NUMTOT)
        PARAMETER(DSTWPR_OFF = DSTWPA_OFF + 2*NUMTOT) 

	INTEGER*4 DSTPCC_OFF
	PARAMETER(DSTPCC_OFF =  DSTWPR_OFF + 2*NUMTOT) 

	INTEGER*4 DSTCMB_OFF
	PARAMETER(DSTCMB_OFF = DSTPCC_OFF + 1)

	INTEGER*4 DSTLAT_OFF
	PARAMETER(DSTLAT_OFF = DSTCMB_OFF + 1)

	INTEGER*4 DSTFRE_OFF
	PARAMETER(DSTFRE_OFF = DSTLAT_OFF + NUMLAT)
C
C CALCULATE FREE SPACE BASED ON MACHINE DEPENDENT SECTOR SIZES.
C SHOULD ALLWAYS ADD UP TO A EVEN SECTOR SIZE BYTES (VAX 512).
C NOTE! IN CALCULATIONS, FRE_OFF-2 IS DONE TO TAKE CARE OF WHEN THE DATA SIZE
C       ENDS ON A EVEN SECTOR SIZE.
C
        INTEGER*4 DSTSEC_VAX, DSTSEC_NUM
        PARAMETER (DSTSEC_VAX = SECSIZE/4)
	PARAMETER (DSTSEC_NUM = NINT(FLOAT((DSTFRE_OFF-2)/DSTSEC_VAX)+.5))
C
        INTEGER*4 DSTLEN, DSTSEC
        PARAMETER(DSTLEN = DSTSEC_VAX * DSTSEC_NUM)        ! even sector size
        PARAMETER(DSTSEC = DSTLEN/64)
C
        INTEGER*4 DSTREC(DSTLEN)
C
	EQUIVALENCE(DSTSTS,DSTREC(DSTSTS_OFF))
	EQUIVALENCE(DSTWEK,DSTREC(DSTWEK_OFF))
	EQUIVALENCE(DSTDAT,DSTREC(DSTDAT_OFF))
	EQUIVALENCE(DSTDRW,DSTREC(DSTDRW_OFF))
	EQUIVALENCE(DSTBSD,DSTREC(DSTBSD_OFF))
	EQUIVALENCE(DSTESD,DSTREC(DSTESD_OFF))
	EQUIVALENCE(DSTPUP,DSTREC(DSTPUP_OFF))
	EQUIVALENCE(DSTUPD,DSTREC(DSTUPD_OFF))
	EQUIVALENCE(DSTCTM,DSTREC(DSTCTM_OFF))
	EQUIVALENCE(DSTTIM,DSTREC(DSTTIM_OFF))
	EQUIVALENCE(DSTSER,DSTREC(DSTSER_OFF))
	EQUIVALENCE(DSTSAL,DSTREC(DSTSAL_OFF))
	EQUIVALENCE(DSTPAD,DSTREC(DSTPAD_OFF))
	EQUIVALENCE(DSTPRG,DSTREC(DSTPRG_OFF))
	EQUIVALENCE(DSTPRF,DSTREC(DSTPRF_OFF))
	EQUIVALENCE(DSTREF,DSTREC(DSTREF_OFF))
	EQUIVALENCE(DSTERF,DSTREC(DSTERF_OFF))
	EQUIVALENCE(DSTTER,DSTREC(DSTTER_OFF))
	EQUIVALENCE(DSTWON,DSTREC(DSTWON_OFF))
	EQUIVALENCE(DSTPOL,DSTREC(DSTPOL_OFF))
	EQUIVALENCE(DSTTPL,DSTREC(DSTTPL_OFF))
	EQUIVALENCE(DSTTBK,DSTREC(DSTTBK_OFF))
	EQUIVALENCE(DSTBRK,DSTREC(DSTBRK_OFF))
	EQUIVALENCE(DSTABW,DSTREC(DSTABW_OFF))
	EQUIVALENCE(DSTODS,DSTREC(DSTODS_OFF))
	EQUIVALENCE(DSTWIN,DSTREC(DSTWIN_OFF))
	EQUIVALENCE(DSTHLD,DSTREC(DSTHLD_OFF))
	EQUIVALENCE(DSTTAX,DSTREC(DSTTAX_OFF))
	EQUIVALENCE(DSTOTX,DSTREC(DSTOTX_OFF))
	EQUIVALENCE(DSTMID,DSTREC(DSTMID_OFF))
	EQUIVALENCE(DSTUTX,DSTREC(DSTUTX_OFF))
	EQUIVALENCE(DSTHST,DSTREC(DSTHST_OFF))
	EQUIVALENCE(DSTORM,DSTREC(DSTORM_OFF))
	EQUIVALENCE(DSTENM,DSTREC(DSTENM_OFF))
	EQUIVALENCE(DSTDES,DSTREC(DSTDES_OFF))
	EQUIVALENCE(DSTNMS,DSTREC(DSTNMS_OFF))
	EQUIVALENCE(DSTSTA,DSTREC(DSTSTA_OFF))
	EQUIVALENCE(DSTSBR,DSTREC(DSTSBR_OFF))
	EQUIVALENCE(DSTREV,DSTREC(DSTREV_OFF))
	EQUIVALENCE(DSTPFN,DSTREC(DSTPFN_OFF))
	EQUIVALENCE(DSTRTM,DSTREC(DSTRTM_OFF))
	EQUIVALENCE(DSTPRC,DSTREC(DSTPRC_OFF))
	EQUIVALENCE(DSTSPR,DSTREC(DSTSPR_OFF))
	EQUIVALENCE(DSTTVC,DSTREC(DSTTVC_OFF))
	EQUIVALENCE(DSTMDS,DSTREC(DSTMDS_OFF))
	EQUIVALENCE(DSTRWS,DSTREC(DSTRWS_OFF))
	EQUIVALENCE(DSTWCP,DSTREC(DSTWCP_OFF))
	EQUIVALENCE(DSTWBT,DSTREC(DSTWBT_OFF))
        EQUIVALENCE(DSTWRO,DSTREC(DSTWRO_OFF))
        EQUIVALENCE(DSTWRA,DSTREC(DSTWRA_OFF))
        EQUIVALENCE(DSTWPO,DSTREC(DSTWPO_OFF))
        EQUIVALENCE(DSTWPA,DSTREC(DSTWPA_OFF))
        EQUIVALENCE(DSTWPR,DSTREC(DSTWPR_OFF))
        EQUIVALENCE(DSTPCC,DSTREC(DSTPCC_OFF))
        EQUIVALENCE(DSTCMB,DSTREC(DSTCMB_OFF))
        EQUIVALENCE(DSTLAT,DSTREC(DSTLAT_OFF))
