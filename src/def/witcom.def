C
      INTEGER*4 WITSTS_OFF, WITWEK_OFF, WITDAT_OFF, WITDRW_OFF
      INTEGER*4 WITBSD_OFF, WITESD_OFF, WITPUP_OFF, WITUPD_OFF
      INTEGER*4 WITCTM_OFF, WITTIM_OFF, WITSER_OFF, WITSAL_OFF
      PARAMETER(WITSTS_OFF=1)                                             
      PARAMETER(WITWEK_OFF=WITSTS_OFF+NUMWIT)                             
      PARAMETER(WITDAT_OFF=WITWEK_OFF+NUMWIT)                             
      PARAMETER(WITDRW_OFF=WITDAT_OFF+NUMWIT)                             
      PARAMETER(WITBSD_OFF=WITDRW_OFF+NUMWIT)                             
      PARAMETER(WITESD_OFF=WITBSD_OFF+NUMWIT)                             
      PARAMETER(WITPUP_OFF=WITESD_OFF+NUMWIT)                             
      PARAMETER(WITUPD_OFF=WITPUP_OFF+NUMWIT)                             
      PARAMETER(WITCTM_OFF=WITUPD_OFF+NUMWIT)                             
      PARAMETER(WITTIM_OFF=WITCTM_OFF+NUMWIT)                             
      PARAMETER(WITSER_OFF=WITTIM_OFF+NUMWIT)                             
      PARAMETER(WITSAL_OFF=WITSER_OFF+NUMWIT)                             
C
      INTEGER*4 WITPAD_OFF, WITPRG_OFF, WITPRF_OFF, WITREF_OFF
      INTEGER*4 WITERF_OFF, WITTER_OFF, WITWON_OFF, WITPOL_OFF
      INTEGER*4 WITTPL_OFF, WITTBK_OFF, WITBRK_OFF, WITABW_OFF
      PARAMETER(WITPAD_OFF=WITSAL_OFF+NUMWIT)                             
      PARAMETER(WITPRG_OFF=WITPAD_OFF+NUMWIT)                             
      PARAMETER(WITPRF_OFF=WITPRG_OFF+NUMWIT)                             
      PARAMETER(WITREF_OFF=WITPRF_OFF+NUMWIT)                             
      PARAMETER(WITERF_OFF=WITREF_OFF+NUMWIT)                             
      PARAMETER(WITTER_OFF=WITERF_OFF+NUMWIT)                             
      PARAMETER(WITWON_OFF=WITTER_OFF+NUMWIT)                             
      PARAMETER(WITPOL_OFF=WITWON_OFF+NUMWIT)                             
      PARAMETER(WITTPL_OFF=WITPOL_OFF+2*NUMWIT)                           
      PARAMETER(WITTBK_OFF=WITTPL_OFF+NUMWIT)                             
      PARAMETER(WITBRK_OFF=WITTBK_OFF+NUMWIT)                             
      PARAMETER(WITABW_OFF=WITBRK_OFF+2*NUMWIT)                           
C
      INTEGER*4 WITODS_OFF, WITWIN_OFF, WITHLD_OFF, WITTAX_OFF
      INTEGER*4 WITOTX_OFF, WITMID_OFF, WITUTX_OFF, WITHST_OFF
      INTEGER*4 WITORM_OFF, WITENM_OFF, WITDES_OFF, WITNMS_OFF
      INTEGER*4 WITPRC_OFF
      PARAMETER(WITODS_OFF=WITABW_OFF+NUMWIT)                             
      PARAMETER(WITWIN_OFF=WITODS_OFF+4*NUMWIT)                           
      PARAMETER(WITHLD_OFF=WITWIN_OFF+4*NUMWIT)                           
      PARAMETER(WITTAX_OFF=WITHLD_OFF+4*NUMWIT)                           
      PARAMETER(WITOTX_OFF=WITTAX_OFF+NUMWIT)                             
      PARAMETER(WITMID_OFF=WITOTX_OFF+NUMTOT*NUMWIT)                      
      PARAMETER(WITUTX_OFF=WITMID_OFF+NUMTOT*NUMWIT)                      
      PARAMETER(WITHST_OFF=WITUTX_OFF+NUMTOT*NUMWIT)                      
      PARAMETER(WITORM_OFF=WITHST_OFF+NUMWIT)                             
      PARAMETER(WITENM_OFF=WITORM_OFF+NUMTOT*NUMWIT)                      
      PARAMETER(WITDES_OFF=WITENM_OFF + (WENM_LEN/4)*NUMWIT)
      PARAMETER(WITNMS_OFF=WITDES_OFF + (WDES_LEN/4)*NUMWIT)
C
      INTEGER*4 WITSTA_OFF, WITSBR_OFF, WITREV_OFF, WITPFN_OFF
      INTEGER*4 WITRTM_OFF, WITSPR_OFF, WITTVC_OFF 
      PARAMETER(WITSTA_OFF=WITNMS_OFF + ((WNMS_LEN/4)*MAXWRW*NUMWIT))
      PARAMETER(WITSBR_OFF=WITSTA_OFF+MAXWRW*NUMWIT)                      
      PARAMETER(WITREV_OFF=WITSBR_OFF+MAXWRW*NUMWIT)                      
      PARAMETER(WITPFN_OFF=WITREV_OFF+NUMWIT)                             
      PARAMETER(WITRTM_OFF=WITPFN_OFF+5*NUMWIT)                           
      PARAMETER(WITPRC_OFF=WITRTM_OFF+MAXWRW*NUMWIT)
      PARAMETER(WITSPR_OFF=WITPRC_OFF+NUMWIT)
      PARAMETER(WITTVC_OFF=WITSPR_OFF + NUMWIT)
C
      INTEGER*4 WITMDS_OFF
      PARAMETER(WITMDS_OFF = WITTVC_OFF + (WTVC_LEN/4)*NUMWIT)
C
      INTEGER*4 WITWRO_OFF, WITWRA_OFF, WITWPO_OFF, WITWPA_OFF
      INTEGER*4 WITWPR_OFF 
      PARAMETER(WITWRO_OFF = WITMDS_OFF + (MAXMLTD_AVL/4)*NUMWIT)
      PARAMETER(WITWRA_OFF = WITWRO_OFF + NUMTOT*2*NUMWIT)
      PARAMETER(WITWPO_OFF = WITWRA_OFF + NUMTOT*2*NUMWIT)
      PARAMETER(WITWPA_OFF = WITWPO_OFF + NUMTOT*2*NUMWIT)
      PARAMETER(WITWPR_OFF = WITWPA_OFF + NUMTOT*2*NUMWIT)
C
      INTEGER*4 WITLAT_OFF
      PARAMETER(WITLAT_OFF = WITWPR_OFF + NUMTOT*2*NUMWIT)
C
      INTEGER*4 WITFRE_OFF
      PARAMETER(WITFRE_OFF = WITLAT_OFF + NUMLAT*NUMWIT)
C
C
      INTEGER*4 WITSEC_SIZE, WITSEC_NUM, WITLEN
      PARAMETER(WITSEC_SIZE = 2048)
      PARAMETER(WITSEC_NUM = NINT(FLOAT((WITFRE_OFF-2)/
     *			     WITSEC_SIZE)+.5))
      PARAMETER(WITLEN = WITSEC_SIZE * WITSEC_NUM)
C
C
	COMMON /WITCOM/
     *	 WITSTS(NUMWIT),
     *	 WITWEK(NUMWIT),
     *	 WITDAT(NUMWIT),
     *	 WITDRW(NUMWIT),
     *	 WITBSD(NUMWIT),
     *	 WITESD(NUMWIT),
     *	 WITPUP(NUMWIT),
     *	 WITUPD(NUMWIT),
     *	 WITCTM(NUMWIT),
     *	 WITTIM(NUMWIT),
     *	 WITSER(NUMWIT),
     *	 WITSAL(NUMWIT),
     *	 WITPAD(NUMWIT),
     *	 WITPRG(NUMWIT),
     *	 WITPRF(NUMWIT),
     *	 WITREF(NUMWIT)
	COMMON /WITCOM/
     *	 WITERF(NUMWIT),
     *	 WITTER(NUMWIT),
     *	 WITWON(NUMWIT),
     *	 WITPOL(2,NUMWIT),
     *	 WITTPL(NUMWIT),
     *	 WITTBK(NUMWIT),
     *	 WITBRK(2,NUMWIT),
     *	 WITABW(NUMWIT),
     *	 WITODS(4,NUMWIT),
     *	 WITWIN(4,NUMWIT),
     *	 WITHLD(4,NUMWIT),
     *	 WITTAX(NUMWIT)
	COMMON /WITCOM/
     *	 WITOTX(NUMTOT,NUMWIT),
     *	 WITMID(NUMTOT,NUMWIT),
     *	 WITUTX(NUMTOT,NUMWIT),
     *	 WITHST(NUMWIT),
     *	 WITORM(NUMTOT,NUMWIT),
     *	 WITENM(WENM_LEN/4,NUMWIT),
     *	 WITDES(WDES_LEN/4,NUMWIT),
     *	 WITNMS(WNMS_LEN/4,MAXWRW,NUMWIT),
     *	 WITSTA(MAXWRW,NUMWIT),
     *	 WITSBR(MAXWRW,NUMWIT)
	COMMON /WITCOM/
     *	 WITREV(NUMWIT),
     *	 WITPFN(5,NUMWIT),
     *	 WITRTM(MAXWRW,NUMWIT),
     *   WITPRC(NUMWIT),
     *   WITSPR(NUMWIT),
     *   WITTVC(WTVC_LEN/4,NUMWIT),
     *   WITMDS(MAXMLTD_AVL,NUMWIT)
       COMMON /WITCOM/
     *   WITWRO(NUMTOT,2,NUMWIT),
     *   WITWRA(NUMTOT,2,NUMWIT),
     *   WITWPO(NUMTOT,2,NUMWIT),
     *   WITWPA(NUMTOT,2,NUMWIT),
     *   WITWPR(NUMTOT,2,NUMWIT),
     *   WITLAT(NUMLAT,NUMWIT),
     *	 WITFRE(WITLEN - WITFRE_OFF + 1)
C
	COMMON /WITCOM/
     *	 WTPOOL(MAXWRW,VALW,TYPW,NUMWIT)
C
	INTEGER*4 WITSTS,WITWEK,WITDAT,WITBSD,WITESD,WITCTM,WITTIM
	INTEGER*4 WITSAL,WITPAD,WITPRG,WITREF,WITWON,WITPOL,WITTPL
	INTEGER*4 WITTBK,WITBRK,WITABW,WITODS,WITWIN,WITHLD,WITTAX
	INTEGER*4 WITOTX,WITMID,WITUTX,WITHST,WITORM,WITENM
	INTEGER*4 WITDES,WITNMS,WITREV,WITFRE,WITPUP,WITUPD,WITSTA
	INTEGER*4 WITSBR,WITPFN,WITDRW,WITRTM,WITSER,WITPRF,WITERF
	INTEGER*4 WITTER,WITSPR,WITPRC,WITTVC
C
	BYTE	  WITMDS

        INTEGER*4 WITWRO,WITWRA,WITWPO,WITWPA,WITWPR,WITLAT
C
	INTEGER*4 WTPOOL
	INTEGER*4 WTPOOL_LEN, WTPSEC_NUM 
	PARAMETER (WTPOOL_LEN = MAXWRW*VALW*TYPW*NUMWIT)
	PARAMETER (WTPSEC_NUM = NINT(FLOAT((WTPOOL_LEN-1)/
     *                          WITSEC_SIZE)+.5))
C
C EQUIVALENCE FOR CHECKPOINT
C
	INTEGER*4 WITCSB,WITBLK
	PARAMETER(WITCSB = WITSEC_NUM + WTPSEC_NUM)!NUMBER OF WITCOM 2048 BLOCKS
	DIMENSION WITBLK(2048,WITCSB)
	EQUIVALENCE(WITBLK(1,1),WITSTS(1))
C
	VOLATILE /WITCOM/
C
        INTEGER*4        FRST_WITCOM(2)
        EQUIVALENCE     (FRST_WITCOM, WITBLK(1,1))
        INTEGER*4        LAST_WITCOM
        EQUIVALENCE     (LAST_WITCOM, WITBLK(2048,WITCSB))
