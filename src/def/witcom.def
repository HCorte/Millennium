C WITCOM.DEF
C
C V08 30-JUN-2000 UXN WITLAT added.
C V07 08-JUN-2000 UXN WITFUL removed.
C V06 18-DEC-1993 HXK ADDED FIELDS FOR COUNTS AND AMOUNTS FOR PRIZES/REFUNDS 
C                     WON AND PAID.
C V05 15-dec-1993 HXK FIXED BUG in WITNMS length.
C V04 17-JUN01993 GXA Added Multi-Draw Selected table.
C V03 21-JAN-1993 DAB Initial Release
C                     Based on Netherlands Bible, 12/92, and Comm 1/93 update
C                     DEC Baseline
C V02 07-OCT-1991 MTK INITAL RELEASE FOR NETHERLANDS
C V01 01-AUG-1990 XXX RELEASED FOR VAX
C
C
C WINNERS TIP GAME AND POOL COMMONS
C
C
C GAME VARIABLE ARRAYS
C
C     WITSTS(NUMWIT)                 ; GAME STATUS
C     WITWEK(NUMWIT)                 ; WEEK NUMBER
C     WITDAT(NUMWIT)                 ; DRAW DATE
C     WITDRW(NUMWIT)                 ; DRAW NUMBER
C     WITBSD(NUMWIT)                 ; BEGINNING SALES DATE
C     WITESD(NUMWIT)                 ; ENDING SALES DATE
C     WITPUP(NUMWIT)                 ; LAST PURGE UPDATE   (FILE ONLY)
C     WITUPD(NUMWIT)                 ; LAST FILE UPDATE    (FILE ONLY)
C     WITCTM(NUMWIT)                 ; ACTUAL TIME WHEN CLOSED
C     WITTIM(NUMWIT)                 ; TIME WHEN GAME SHOULD CLOSE
C     WITSER(NUMWIT)                 ; LAST SERIAL NUMBER
C     WITSAL(NUMWIT)                 ; TOTAL SALES
C     WITPAD(NUMWIT)                 ; PRIZES PAID + REFUNDS
C     WITPRG(NUMWIT)                 ; PRIZES PURGED
C     WITPRF(NUMWIT)                 ; PAID REFUNDS
C     WITREF(NUMWIT)                 ; TOTAL REFUND AMOUNT (LIAB)
C     WITERF(NUMWIT)                 ; EARLY REFUNDS PAID
C     WITTER(NUMWIT)                 ; TOTAL EARLY REFUNDS (LIAB)
C     WITWON(NUMWIT)                 ; PRIZES WON
C     WITPOL(2,NUMWIT)               ; ROLL POOL
C     WITTPL(NUMWIT)                 ; TOTAL PRIZE POOL
C     WITTBK(NUMWIT)                 ; TOTAL BREAKAGE
C     WITBRK(2,NUMWIT)               ; WINNING ROUNDING POT
C     WITABW(NUMWIT)                 ; AMOUNT BET ON WINNING ROW
C     WITODS(4,NUMWIT)               ; WINNING ODDS
C     WITWIN(4,NUMWIT)               ; WINNING RESULTS
C     WITHLD(4,NUMWIT)               ; WINNING RESULTS HOLD
C     WITTAX(NUMWIT)                 ; WINNING TAXES
C     WITOTX(NUMTOT,NUMWIT)          ; OVER TAX LIMIT
C     WITMID(NUMTOT,NUMWIT)          ; >REDMIN >REDMAX
C     WITUTX(NUMTOT,NUMWIT)          ; UNDER TAX LIMIT
C     WITHST(NUMWIT)                 ; HIGHEST WINNER FOR THIS GAME
C     WITORM(NUMTOT,NUMWIT)          ; WINNERS >REDMAX
C     WITENM(WENM_LEN/4,NUMWIT)      ; EVENT NAME
C     WITDES(WDES_LEN/4,NUMWIT)      ; EVENT DESCRIPTION
C     WITNMS(WNMS_LEN/4,MAXWRW,NUMWIT) ; ROW NAMES
C     WITSTA(MAXWRW,NUMWIT)          ; ROW STATUS 1 THROUGH MAXTRW
C     WITSBR(MAXWRW,NUMWIT)          ; SALES BY ROW
C     WITREV(NUMWIT)                 ; REV #
C     WITPFN(5,NUMWIT)               ; POOL FILE NAME
C     WITRTM(MAXWRW,NUMWIT)          ; ROW CLOSE TIMES
C     WITPRC(NUMWIT)                 ; BASE PRICE
C     WITSPR(NUMWIT)                 ; POOL PERCENTAGE
C     WITTVC(WTVC_LEN/4,NUMWIT)      ; TV-CHANEL NAME
C     WITMDS(MAXMLTD_AVL,NUMWIT)     ; MULTI DRAW SELECTED TABLE
C                                      (52 BYTE / 4 = 13)
C     WITWRO(NUMTOT,2,NUMWIT)        ; WINNING: COUNT,AMOUNT REFUNDS ONLY
C     WITWRA(NUMTOT,2,NUMWIT)        ; WINNING: COUNT,AMOUNT REFUNDS ALTOGETHER
C     WITWPO(NUMTOT,2,NUMWIT)        ; WINNING: COUNT,AMOUNT PRIZES ONLY
C     WITWPA(NUMTOT,2,NUMWIT)        ; WINNING: COUNT,AMOUNT PRIZES ALTOGETHER
C     WITWPR(NUMTOT,2,NUMWIT)        ; WINNING: COUNT,AMOUNT PRIZES AND REFUNDS
C                                    ; 1=PRIZES/REFUNDS WON  (PRWON)
C                                    ; 2=PRIZES/REFUNDS PAID (PRPAD)
C     WITLAT(NUMLAT,NUMWIT)          ; EVENT STARTED BEFORE GAME WAS CLOSED
C                                    ; TICKETS PLAYED LATER SHOULD BE REFUNDED.
C
C     WITFRE(CALCULATED)             ; FREE SPACE
C
C
C WINNERS POOL ARRAYS
C
C     WTPOOL(MAXWRW,VALW,TYPW,NUMWIT) ; POOL ODDS ARRAY
C
C
C
