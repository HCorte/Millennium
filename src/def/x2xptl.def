	PARAMETER (X2ERR_ADRLEN=60)           !BAD STATION ADDRESS LEN
	PARAMETER (X2ERR_SDTU=61)             !BAD STATION DATA TYPE
	PARAMETER (X2ERR_MOFF=62)             !BAD MESSAGE OFFSET
C
C
C     MESSAGE FORMAT FOR MESSAGES BUILT BY X2GETMSG
C     THESE MESSAGES ARE ROUTED TO X2XPRO
C
C     11 hex   CTL/SEQ
C     EE            A0 HEX FOR COMMAND, A1 HEX FOR ERROR
C     CC               01 - TRANSPORT MESSAGE
C                      02 - FRONT - END MESSAGE
C                      03 - STATION MESSAGE
C                      04 - GLOBAL
C     TT       TERMINAL #
C     TT
C     XX       ERROR CODE    AS DEFINED BY X2ERR_...
C     SS       STATION NO
C     SS
C     YY       SAP
C     FF       OFFSET OF THE MESSAGE (MMM)
C     LL       LENGTH OF STATION ADDRESS OR OF VSN NUMBER
C     ad,ad,ad,ad,ad,ad,ad,ad - 8 BYTES OF STATION ADDRESS OR VSN NO
C	       3 BYTES FREE TO USE
C     AA       ADDITIONAL INFORMATION
C     AA
C     MM       MESSAGE IF ANY
C     MM
C     MM
C
	INTEGER*4  X2X_MESTYP_CMD, X2X_MESTYP_ERR
	PARAMETER (X2X_MESTYP_CMD='000011A0'X)   !COMMAND
	PARAMETER (X2X_MESTYP_ERR='000011A1'X)   !ERROR MESSAGE
C
	INTEGER*4  X2X_TRATYP_XPORT, X2X_TRATYP_FE, X2X_TRATYP_STTN
	INTEGER*4  X2X_TRATYP_GLO
	PARAMETER (X2X_TRATYP_XPORT=1)           !TRANSPORT TYPE
	PARAMETER (X2X_TRATYP_FE=2)              !FRONT END TYPE
	PARAMETER (X2X_TRATYP_STTN=3)            !STATION TYPE
	PARAMETER (X2X_TRATYP_GLO=4)             !GLOBAL
C
C     PROCOM BUFFER OFFSETS  (INPUT AREA)
C
	INTEGER*4  X2PRO_MESTYP, X2PRO_TRATYP, X2PRO_TERMINAL
	INTEGER*4  X2PRO_STATUS, X2PRO_STATION, X2PRO_SSAP
	INTEGER*4  X2PRO_OFFSET, X2PRO_MSGLEN, X2PRO_SERIAL
	INTEGER*4  X2PRO_MSGNUM, X2PRO_TRACE_MSG
	INTEGER*4 X2PRO_ADR_LEN, X2PRO_ADR
	PARAMETER (X2PRO_MESTYP=1)        !CONTROL/SEQ, TYP/SUBTYP/ERR
	PARAMETER (X2PRO_TRATYP=3)        !TRANSACTION TYPE
	PARAMETER (X2PRO_TERMINAL=4)      !TERMINAL NO OFFSET
	PARAMETER (X2PRO_STATUS=6)        !MESSAGE STATUS
	PARAMETER (X2PRO_STATION=7)       !STATION NO
	PARAMETER (X2PRO_SSAP=9)          !SAP
	PARAMETER (X2PRO_OFFSET=10)       !POINTER TO MESSAGE
	PARAMETER (X2PRO_MSGLEN=11)       !LENGTH OF TRACE DATA
	PARAMETER (X2PRO_ADR_LEN=13)	  !ADDRESS LENGTH
	PARAMETER (X2PRO_ADR=14)	  !ADRESS 8 BYTES (11-18)
C
C					  !BYTES 20, 21, 22 FREE TO USE
C                                         ;PROGRAM SETS IT TO 11
	PARAMETER (X2PRO_SERIAL=25)       !SERIAL # OF TRACED DATA
	PARAMETER (X2PRO_MSGNUM=29)       !MESSAGE NUM OF TRACED DATA
	PARAMETER (X2PRO_TRACE_MSG=33)    !TRACED MESSAGE OFFSET
C
	INTEGER*4  X2PRO_MAXMES
	PARAMETER (X2PRO_MAXMES=200)       !UP TO 200 BYTES  DATA FOLLOW
 
 
