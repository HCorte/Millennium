C OFFSET PARAMETERS AND EQUIVALENCES
C
	INTEGER*4 DLTSTSOFF, DLTCTMOFF, DLTTIMOFF, DLTDRWOFF
	PARAMETER(DLTSTSOFF = 1)                         !   1
	PARAMETER(DLTCTMOFF = DLTSTSOFF + 1)             !   2
	PARAMETER(DLTTIMOFF = DLTCTMOFF + 1)             !   3
	PARAMETER(DLTDRWOFF = DLTTIMOFF + 1)             !   4
C
	INTEGER*4 DLTBSDOFF, DLTESDOFF, DLTPUPOFF, DLTUPDOFF
	PARAMETER(DLTBSDOFF = DLTDRWOFF + 1)             !   5
	PARAMETER(DLTESDOFF = DLTBSDOFF + 1)             !   6
	PARAMETER(DLTPUPOFF = DLTESDOFF + 1)             !   7
	PARAMETER(DLTUPDOFF = DLTPUPOFF + 1)             !   8
C
	INTEGER*4 DLTDATOFF, DLTADVOFF, DLTSALOFF, DLTSHVOFF
	PARAMETER(DLTDATOFF = DLTUPDOFF + 1)             !   9
	PARAMETER(DLTADVOFF = DLTDATOFF + DATLEN)        !  29
	PARAMETER(DLTSALOFF = DLTADVOFF + NUMADV)        !  36
	PARAMETER(DLTSHVOFF = DLTSALOFF + LTGENT)        !  61
C                                
	INTEGER*4 DLTSHROFF, DLTPADOFF, DLTPRGOFF, DLTANUOFF
	PARAMETER(DLTSHROFF = DLTSHVOFF + LTGDIV*2)      !  77
	PARAMETER(DLTPADOFF = DLTSHROFF + LTGDIV*2)      !  93
	PARAMETER(DLTPRGOFF = DLTPADOFF + LTGDIV*2)      ! 109
	PARAMETER(DLTANUOFF = DLTPRGOFF + LTGDIV*2)      ! 125
C
	INTEGER*4 DLTMSROFF, DLTPOLOFF, DLTBRKOFF, DLTFRZOFF
	PARAMETER(DLTMSROFF = DLTANUOFF + LTGDIV*2)      ! 141
	PARAMETER(DLTPOLOFF = DLTMSROFF + LTGDIV*2)      ! 157
	PARAMETER(DLTBRKOFF = DLTPOLOFF + LTGDIV)        ! 165
	PARAMETER(DLTFRZOFF = DLTBRKOFF + LTGDIV)        ! 173
C
	INTEGER*4 DLTWINOFF, DLTHLDOFF, DLTBNMOFF, DLTBHLOFF
	PARAMETER(DLTWINOFF = DLTFRZOFF + LTGDIV)        ! 181
	PARAMETER(DLTHLDOFF = DLTWINOFF + LTGNBR*MAXBDR) ! 221
	PARAMETER(DLTBNMOFF = DLTHLDOFF + LTGNBR*MAXBDR) ! 261
	PARAMETER(DLTBHLOFF = DLTBNMOFF + LTGBON*MAXBDR) ! 276
C
	INTEGER*4 DLTPRPOFF, DLTPRNOFF, DLTTAXOFF, DLTRESOFF
	PARAMETER(DLTPRPOFF = DLTBHLOFF + LTGBON*MAXBDR) ! 291
	PARAMETER(DLTPRNOFF = DLTPRPOFF + 1)             ! 292
	PARAMETER(DLTTAXOFF = DLTPRNOFF + 1)             ! 293
	PARAMETER(DLTRESOFF = DLTTAXOFF + 1)             ! 294
C
	INTEGER*4 DLTAPLOFF, DLTMINOFF, DLTSEROFF, DLTOPAOFF
	PARAMETER(DLTAPLOFF = DLTRESOFF + 2)             ! 296
	PARAMETER(DLTMINOFF = DLTAPLOFF + 1)             ! 297
	PARAMETER(DLTSEROFF = DLTMINOFF + 1)             ! 298
	PARAMETER(DLTOPAOFF = DLTSEROFF + 1)             ! 299
C
	INTEGER*4 DLTPRCOFF, DLTMAXOFF, DLTNUMOFF, DLTBFLOFF
	PARAMETER(DLTPRCOFF = DLTOPAOFF + 1)             ! 300
	PARAMETER(DLTMAXOFF = DLTPRCOFF + 1)             ! 301
	PARAMETER(DLTNUMOFF = DLTMAXOFF + 1)             ! 302
	PARAMETER(DLTBFLOFF = DLTNUMOFF + 1)             ! 303
C
	INTEGER*4 DLTMLTOFF, DLTDIVOFF, DLTWTBOFF, DLTPEROFF
	PARAMETER(DLTMLTOFF = DLTBFLOFF + 1)             ! 304
	PARAMETER(DLTDIVOFF = DLTMLTOFF + 1)             ! 305
	PARAMETER(DLTWTBOFF = DLTDIVOFF + 1)             ! 306
	PARAMETER(DLTPEROFF = DLTWTBOFF + 
     *            LTGNBR*(LTGBON+1)*LTGBET)              ! 402
C
	INTEGER*4 DLTSPROFF, DLTTSROFF, DLTREVOFF, DLTBDROFF 
	PARAMETER(DLTSPROFF = DLTPEROFF + LTGDIV)        ! 410
	PARAMETER(DLTTSROFF = DLTSPROFF + 1)             ! 411
	PARAMETER(DLTREVOFF = DLTTSROFF + LTGDIV*2)      ! 427
	PARAMETER(DLTBDROFF = DLTREVOFF + 1)             ! 428

	INTEGER*4 DLTLSTOFF,  DLTLSVOFF, DLTADWOFF, DLTLANOFF
	PARAMETER(DLTLSTOFF = DLTBDROFF + 1)             ! 429
	PARAMETER(DLTLSVOFF = DLTLSTOFF + 1)             ! 430
	PARAMETER(DLTADWOFF = DLTLSVOFF + LTGDIV*2)      ! 446
	PARAMETER(DLTLANOFF = DLTADWOFF + 1)             ! 447

	INTEGER*4 DLTBETOFF, DLTWEDOFF, DLTPAWOFF
	PARAMETER(DLTBETOFF = DLTLANOFF + LTGDIV*2)      ! 463
	PARAMETER(DLTWEDOFF = DLTBETOFF + LTGBET)        ! 466
        PARAMETER(DLTPAWOFF = DLTWEDOFF + 2)             ! 468

        INTEGER*4 DLTSTXOFF, DLTASHOFF, DLTWRFOFF
        PARAMETER(DLTSTXOFF = DLTPAWOFF + 1)             ! 471
        PARAMETER(DLTASHOFF = DLTSTXOFF + LTGDIV*2)      ! 487
	PARAMETER(DLTWRFOFF = DLTASHOFF + LTGDIV)        ! 495

        INTEGER*4 DLTESTOFF, DLTMDSOFF, DLTBALOFF
	PARAMETER(DLTESTOFF = DLTWRFOFF + 10)            ! 513
        PARAMETER(DLTMDSOFF = DLTESTOFF + 1)             ! 514
	PARAMETER(DLTBALOFF = DLTMDSOFF + (MAXMLTD_AVL/4)) ! 527

        INTEGER*4 DLTSPLOFF
	PARAMETER(DLTSPLOFF = DLTBALOFF + MAXDRW)
C
        INTEGER*4 DLTLNMOFF, DLTLHLOFF, DLTLFLOFF       !V20...
        INTEGER*4 DLTLNCOFF, DLTLDVOFF
        PARAMETER(DLTLNMOFF = DLTSPLOFF + 1 )
        PARAMETER(DLTLHLOFF = DLTLNMOFF + MAXBDR )
        PARAMETER(DLTLNCOFF = DLTLHLOFF + MAXBDR )
        PARAMETER(DLTLFLOFF = DLTLNCOFF + 2*LTGDIV )
        PARAMETER(DLTLDVOFF = DLTLFLOFF + 1 )
C
        INTEGER*4 DLTJPGOFF
        PARAMETER(DLTJPGOFF = DLTLDVOFF + 1 )           !V20 FJG 
C                
        INTEGER*4 DLTCCCOFF
        PARAMETER(DLTCCCOFF = DLTJPGOFF + 1 )           !V21 HXK 
C
        INTEGER*4 DLTFREOFF
	PARAMETER(DLTFREOFF = DLTCCCOFF + 1)            !...V21


        INTEGER*4 DLTSEC_VAX, DLTSEC_NUM
        PARAMETER (DLTSEC_VAX = SECSIZE/4)
        PARAMETER (DLTSEC_NUM = NINT(FLOAT((DLTFREOFF-2)/
     *                                     DLTSEC_VAX)+.5))


	INTEGER*4 DLTLEN, DLTSEC
	PARAMETER(DLTLEN = DLTSEC_VAX * DLTSEC_NUM)        ! even sector size
	PARAMETER(DLTSEC = DLTLEN/64)

	INTEGER*4 DLTREC(DLTLEN)
C
	INTEGER*4 DLTSTS,DLTCTM,DLTTIM,DLTDRW,DLTBSD,DLTESD
	INTEGER*4 DLTPUP,DLTUPD,DLTDAT(DATLEN),DLTSAL(LTGENT)
	INTEGER*4 DLTSHV(LTGDIV,2),DLTSHR(LTGDIV,2),DLTPOL(LTGDIV)
	INTEGER*4 DLTPAD(LTGDIV,2),DLTPRG(LTGDIV,2),DLTADV(NUMADV)
	INTEGER*4 DLTANU(LTGDIV,2),DLTBRK(LTGDIV),DLTMSR(LTGDIV,2)
	INTEGER*4 DLTFRZ(LTGDIV),DLTWIN(LTGNBR,MAXBDR)
	INTEGER*4 DLTHLD(LTGNBR,MAXBDR)
	INTEGER*4 DLTBNM(LTGBON,MAXBDR),DLTBHL(LTGBON,MAXBDR),DLTPRP
	INTEGER*4 DLTPRN,DLTRES(2),DLTAPL,DLTMIN,DLTSER,DLTOPA,DLTPRC
	INTEGER*4 DLTMAX,DLTNUM,DLTBFL,DLTMLT,DLTDIV
	INTEGER*4 DLTWTB(LTGNBR,LTGBON+1,LTGBET),DLTPER(LTGDIV),DLTBDR
	INTEGER*4 DLTSPR,DLTTAX,DLTTSR(LTGDIV,2),DLTREV,DLTLST
	INTEGER*4 DLTLSV(LTGDIV,2),DLTADW,DLTLAN(LTGDIV,2)
	INTEGER*4 DLTBET(LTGBET),DLTWED(2)
        INTEGER*4 DLTPAW,DLTSTX(LTGDIV,2),DLTASH(LTGDIV)
        INTEGER*4 DLTWRF(10),DLTEST
        BYTE      DLTMDS(MAXMLTD_AVL)   ! MAXMLTD_AVL must be divisible by 4
                                        ! to simplify gamlog, loggam)
	INTEGER*4 DLTBAL(MAXDRW)
	INTEGER*4 DLTSPL
C
        INTEGER*4 DLTLNM(MAXBDR), DLTLHL(MAXBDR), DLTLFL, DLTLNC(2,LTGDIV) !V20
        INTEGER*4 DLTLDV, DLTJPG, DLTCCC                      !V20 FJG, V21 HXK
C
        INTEGER*4 DLTFRE(DLTLEN - DLTFREOFF + 1) 
C
	EQUIVALENCE(DLTSTS,DLTREC(DLTSTSOFF))
