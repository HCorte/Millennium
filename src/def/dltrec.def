	EQUIVALENCE(DLTOPA,DLTREC(DLTOPAOFF))
	EQUIVALENCE(DLTPRC,DLTREC(DLTPRCOFF))
	EQUIVALENCE(DLTMAX,DLTREC(DLTMAXOFF))
	EQUIVALENCE(DLTNUM,DLTREC(DLTNUMOFF))
