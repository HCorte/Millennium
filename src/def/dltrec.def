C
C LOTTO DATA FILE RECORD LAYOUT (DISK)
C
C
C
C     DLTSTS                         ;STATUS
C     DLTCTM                         ;ACTUAL TIME WHEN CLOSED
C     DLTTIM                         ;TIME WHEN GAME SHOULD CLOSE
C     DLTDRW                         ;DRAW NUMBER
C     DLTBSD                         ;LOTTO BEGGINING DRAW DATE
C     DLTESD                         ;LOTTO ENDING DRAW DATE
C     DLTPUP                         ;LAST PURGE UPDATE
C     DLTUPD                         ;LAST FILE UPDATE
C     DLTDAT(DATLEN)                 ;DRAW DATES
C     DLTADV(NUMADV)                 ;ADVANCE DRAW DATES
C     DLTSAL(LTGENT)                 ;SALES DATA
C     DLTSHV(LTGDIV,2)               ;SHARE VALUES      (REGULAR,BONUS)
C     DLTSHR(LTGDIV,2)               ;SHARES            (REGULAR,BONUS) 
C     DLTPAD(LTGDIV,2)               ;AMOUNT PAID       (REGULAR,BONUS)
C     DLTPRG(LTGDIV,2)               ;AMOUNT PURGED     (REGULAR,BONUS)
C     DLTANU(LTGDIV,2)               ;ANNUITY PRIZES    (REGULAR,BONUS)
C     DLTMSR(LTGDIV,2)               ;MAILSUB SHARES    (REGULAR,BONUS)
C     DLTPOL(LTGDIV)                 ;POOL CARRIED OVER
C     DLTBRK(LTGDIV)                 ;BREAKAGE AMOUNT BY POOL
C     DLTFRZ(LTGDIV)                 ;DIVISION PAYOUT FROZEN FLAGS
C     DLTWIN(LTGNBR,MAXBDR)          ;WINNING NUMBERS
C     DLTHLD(LTGNBR,MAXBDR)          ;WINNING NUMBERS HOLD
C     DLTBNM(LTGBON,MAXBDR)          ;WINNING BONUS NUMBER(S)
C     DLTBHL(LTGBON,MAXBDR)          ;BONUS NUMBER(S) HOLD
C     DLTPRP                         ;PERCENTAGE OF THE PROMISED AMOUNT
C     DLTPRN                         ;MAX NUM. OF WINNERS FOR PROMISED AMOUNT
C                                    ;IF DLTPRN=-1, IT MEANS 'ERIKOISPOTTI'
C     DLTTAX                         ;LOTTERY TAX AMOUNT
C     DLTRES(2)                      ;RESERVE POOL
C     DLTAPL                         ;ADDITIONAL POOL
C     DLTMIN                         ;MINIMUM POOL
C     DLTSER                         ;LAST SERIAL NUMBER
C     DLTOPA                         ;OVER-RIDE POOL AMOUNT
C     DLTPRC                         ;PRICE/BOARD
C     DLTMAX                         ;HIGHEST # BET
C     DLTNUM                         ;# OF NUMBERS DRAWN
C     DLTBFL                         ;BONUS NUMBER ENABLE FLAG
C     DLTMLT                         ;MULTI-DRAW ENABLE FLAG
C     DLTDIV                         ;# OF DIVISIONS
C     DLTWTB(LTGNBR,LTGBON+1,LTGBET) ;WINNING DIVISIONS TABLE
C     DLTPER(LTGDIV)                 ;DIVISION PERCENTAGES
C     DLTSPR                         ;SALES PERCENTAGE
C     DLTTSR(LTGDIV,2)               ;TOTAL SHARE COUNT (REGULAR BONUS)
C     DLTREV                         ;LOTTO REVISION NUMBER
C     DLTBDR                         ;# OF BONUS DRAWS ENABLED
C     DLTLST                         ;LAST DRAWS GAME STATUS
C     DLTLSV(LTGDIV,2)               ;LAST DRAWS SHARE VALUES (REG,BONUS)
C     DLTADW                         ;ADVANCE WAGERING FLAG
C     DLTLAN(LTGDIV,2)               ;LAST DRAWS ANUITY VALUES (REG,BONUS)
C     DLTBET(LTGBET)                 ;ACTIVE BET TYPES
C     DLTWED(2)                      ;WEDNESDAY DRAW DATES
C     DLTPAW                         ;PERCENTAGE APPLIED TO WINS
C     DLTSTX(LTGDIV,2)               ;SHARE TAX VALUE(REG,BONUS)    
C     DLTASH(LTGDIV)                 ;ADDITIONAL POOL BY DIV
C     DLTWRF(10)                     ;WIN RESERVE FUND TABLE   
C     DLTEST                         ;ESTIMATED JACKPOT      
C     DLTMDS(MAXMLTD_AVL)            ;MULTI DRAW SELECTED TABLE 
C                                     (52 BYTE / 4 = 13)
C     DLTBAL(MAXDRW)		     ;BALANCE SALES INFO. BY DRAW
C     DLTSPL			     ;SPECIAL POOL, ADDED TO TOTAL POOL - 
C				      NOT TO DIV1 POOL AS OTHER POOL MONEY
C     DLTLNM(MAXBDR)                 ;WINNING LUCKY NUMBER      !V20...
C     DLTLHL(MAXBDR)                 ;LUCKY NUMBER HOLD
C     DLTLNC(2,LTGDIV)               ;LUCKY NUMBER DIVISION CONVERSION TABLE
C     DLTLFL                         ;LUCKY NUMBER ENABLE FLAG  
C     DLTLDV                         ;Lucky number refund division number  !...V20
C     DLTJPG                         ;JACKPOT POST GAME FLAG    !...V20 FJG
C     DLTCCC                         ;TOTOLOTO DRAW COUNTER
C     DLTFRE	                     ;FREE SPACE
C
C LOTTO GAME FILE PARAMETERS
C
