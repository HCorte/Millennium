	EQUIVALENCE(DLTLHL,DLTREC(DLTLHLOFF))
	EQUIVALENCE(DLTLNC,DLTREC(DLTLNCOFF))
	EQUIVALENCE(DLTLFL,DLTREC(DLTLFLOFF))
