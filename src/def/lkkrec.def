        EQUIVALENCE(LKKUPD,LKKREC(LKKUPD_OFF))
        EQUIVALENCE(LKKDAT,LKKREC(LKKDAT_OFF))
        EQUIVALENCE(LKKADV,LKKREC(LKKADV_OFF))
        EQUIVALENCE(LKKSAL,LKKREC(LKKSAL_OFF))
        EQUIVALENCE(LKKSHR,LKKREC(LKKSHR_OFF))
        EQUIVALENCE(LKKPAD,LKKREC(LKKPAD_OFF))
        EQUIVALENCE(LKKPRG,LKKREC(LKKPRG_OFF))
        EQUIVALENCE(LKKSHV,LKKREC(LKKSHV_OFF))
        EQUIVALENCE(LKKPOL,LKKREC(LKKPOL_OFF))
        EQUIVALENCE(LKKBRK,LKKREC(LKKBRK_OFF))
        EQUIVALENCE(LKKFRZ,LKKREC(LKKFRZ_OFF))
        EQUIVALENCE(LKKTAX,LKKREC(LKKTAX_OFF))
        EQUIVALENCE(LKKRES,LKKREC(LKKRES_OFF))
        EQUIVALENCE(LKKWIN,LKKREC(LKKWIN_OFF))
        EQUIVALENCE(LKKHLD,LKKREC(LKKHLD_OFF))
        EQUIVALENCE(LKKSER,LKKREC(LKKSER_OFF))
        EQUIVALENCE(LKKPRC,LKKREC(LKKPRC_OFF))
        EQUIVALENCE(LKKMAX,LKKREC(LKKMAX_OFF))
        EQUIVALENCE(LKKDIV,LKKREC(LKKDIV_OFF))
        EQUIVALENCE(LKKSPR,LKKREC(LKKSPR_OFF))
        EQUIVALENCE(LKKMAT,LKKREC(LKKMAT_OFF))
        EQUIVALENCE(LKKPER,LKKREC(LKKPER_OFF))
        EQUIVALENCE(LKKTSR,LKKREC(LKKTSR_OFF))
        EQUIVALENCE(LKKREV,LKKREC(LKKREV_OFF))
        EQUIVALENCE(LKKSED,LKKREC(LKKSED_OFF))
        EQUIVALENCE(LKKOCT,LKKREC(LKKOCT_OFF))
        EQUIVALENCE(LKKMLT,LKKREC(LKKMLT_OFF))
