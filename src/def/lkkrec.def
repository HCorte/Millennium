C LOGICAL KICKER GAME RECORD
C
C     LKKSTS(NUMKIK)                 ;STATUS
C     LKKCTM(NUMKIK)                 ;ACTUAL TIME CLOSED
C     LKKTIM(NUMKIK)                 ;TIME WHEN GAME SHOULD CLOSE
C     LKKDRW(NUMKIK)                 ;DRAW NUMBER
C     LKKBSD(NUMKIK)                 ;KICKER BEGGINING DRAW DATE
C     LKKESD(NUMKIK)                 ;KICKER ENDING DRAW DATE
C     LKKPUP(NUMKIK)                 ;LAST PURGE UPDATE
C     LKKUPD(NUMKIK)                 ;LAST FILE UPDATE
C     LKKDAT(DATLEN,NUMKIK)          ;DRAW DATES
C     LKKADV(NUMADV,NUMKIK)          ;ADVANCE DRAW DATES
C     LKKSAL(KIGENT,MAXGAM,NUMKIK)   ;SALES DATA
C     LKKSHR(KIGDIV,NUMKIK)          ;SHARES
C     LKKPAD(KIGDIV,NUMKIK)          ;AMOUNT PAID
C     LKKPRG(KIGDIV,NUMKIK)          ;AMOUNT PURGED
C     LKKSHV(KIGDIV,NUMKIK)          ;SHARE VALUES
C     LKKPOL(2,KIGDIV,NUMKIK)        ;POOL
C     LKKBRK(KIGDIV,NUMKIK)          ;BREAKAGE
C     LKKFRZ(KIGDIV,NUMKIK)          ;DIVISION PAYOUT FROZEN FLAGS
C     LKKTAX(NUMKIK)                 ;LOTTERY TAX
C     LKKRES(2,NUMKIK)               ;RESERVE POOL
C     LKKWIN(NUMKIK)                 ;WINNING NUMBER
C     LKKHLD(NUMKIK)                 ;WINNING NUMBER HOLD
C     LKKSER(NUMKIK)                 ;LAST SERIAL NUMBER
C     LKKPRC(NUMKIK)                 ;PRICE/BOARD
C     LKKMAX(NUMKIK)                 ;MAXIMUM NUMBER BET
C     LKKDIV(NUMKIK)                 ;NUMBER OF DIVISIONS
C     LKKSPR(NUMKIK)                 ;SALES POOL PERCENTAGE
C     LKKMAT(3,KIGDIV,NUMKIK)        ;WINNERS MATCH TABLE
C     LKKPER(KIGDIV,NUMKIK)          ;DIVISION POOL PERCENTAGE
C     LKKTSR(KIGDIV,NUMKIK)          ;TOTAL SHARE COUNT
C     LKKREV(NUMKIK)                 ;KICKER REVISION NUMBER
C     LKKSED(2,NUMKIK)               ;KICKER NUMBER SEEDS
C     LKKOCT(NUMKIK)                 ;# OF OCTAL DIGITS (FOR UNIRAN)
C     LKKMLT(NUMKIK)                 ;KICKER MULTI DRAW FLAG
C     LKKWRF(10,NUMKIK)              ;WIN RESERVE FUND TABLE
C     LKKASH(KIGDIV,NUMKIK)          ;ADDITIONAL POOL BY DIV
C     LKKMIN(NUMKIK)                 ;MINIMUM JACKPOT
C     LKKDFF(NUMKIK)                 ;DOUBLE JOKERI FREE FLAG
C     LKKMDS(MAXMLTD_AVL,NUMKIK)     ;MULTI DRAW SELECTED TABLE
C                                     (52 BYTE / 4 = 13)
C     LKKBAL(MAXDRW,NUMKIK)          ;BALANCE SALES INFO. BY DRAWC
C     LKKSPL(NUMKIK)		      ;SPECIAL POOL, ADDED TO TOTAL POOL -
C                                     NOT TO DIV1 POOL AS OTHER POOL MONEY
C
C LOGICAL GAME FILE VARIABLE FIELD OFFSETS
C
C
