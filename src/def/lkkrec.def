        INTEGER*4 LKKSTS_OFF, LKKCTM_OFF, LKKTIM_OFF, LKKDRW_OFF
        PARAMETER(LKKSTS_OFF=1)
        PARAMETER(LKKCTM_OFF=LKKSTS_OFF+NUMKIK)
        PARAMETER(LKKTIM_OFF=LKKCTM_OFF+NUMKIK)
        PARAMETER(LKKDRW_OFF=LKKTIM_OFF+NUMKIK)
C
        INTEGER*4 LKKBSD_OFF, LKKESD_OFF, LKKPUP_OFF, LKKUPD_OFF
        PARAMETER(LKKBSD_OFF=LKKDRW_OFF+NUMKIK)
        PARAMETER(LKKESD_OFF=LKKBSD_OFF+NUMKIK)
        PARAMETER(LKKPUP_OFF=LKKESD_OFF+NUMKIK)
        PARAMETER(LKKUPD_OFF=LKKPUP_OFF+NUMKIK)
C
        INTEGER*4 LKKDAT_OFF, LKKADV_OFF, LKKSAL_OFF, LKKSHR_OFF
        PARAMETER(LKKDAT_OFF=LKKUPD_OFF+NUMKIK)
        PARAMETER(LKKADV_OFF=LKKDAT_OFF+DATLEN*NUMKIK)
        PARAMETER(LKKSAL_OFF=LKKADV_OFF+(NUMADV*NUMKIK))
        PARAMETER(LKKSHR_OFF=LKKSAL_OFF+(KIGENT*MAXGAM*NUMKIK))
C
        INTEGER*4 LKKPAD_OFF, LKKPRG_OFF, LKKSHV_OFF, LKKPOL_OFF
        PARAMETER(LKKPAD_OFF=LKKSHR_OFF+(KIGDIV*NUMKIK))
        PARAMETER(LKKPRG_OFF=LKKPAD_OFF+(KIGDIV*NUMKIK))
        PARAMETER(LKKSHV_OFF=LKKPRG_OFF+(KIGDIV*NUMKIK))
        PARAMETER(LKKPOL_OFF=LKKSHV_OFF+(KIGDIV*NUMKIK))
C
        INTEGER*4 LKKBRK_OFF, LKKFRZ_OFF, LKKTAX_OFF, LKKRES_OFF
        PARAMETER(LKKBRK_OFF=LKKPOL_OFF+(2*KIGDIV*NUMKIK))
        PARAMETER(LKKFRZ_OFF=LKKBRK_OFF+(KIGDIV*NUMKIK))
        PARAMETER(LKKTAX_OFF=LKKFRZ_OFF+(KIGDIV*NUMKIK))
        PARAMETER(LKKRES_OFF=LKKTAX_OFF+NUMKIK)
C
        INTEGER*4 LKKWIN_OFF, LKKHLD_OFF, LKKSER_OFF, LKKPRC_OFF
        PARAMETER(LKKWIN_OFF=LKKRES_OFF+2*NUMKIK)
        PARAMETER(LKKHLD_OFF=LKKWIN_OFF+NUMKIK)
        PARAMETER(LKKSER_OFF=LKKHLD_OFF+NUMKIK)
        PARAMETER(LKKPRC_OFF=LKKSER_OFF+NUMKIK)
C
        INTEGER*4 LKKMAX_OFF, LKKDIV_OFF, LKKSPR_OFF, LKKMAT_OFF
        PARAMETER(LKKMAX_OFF=LKKPRC_OFF+NUMKIK)
        PARAMETER(LKKDIV_OFF=LKKMAX_OFF+NUMKIK)
        PARAMETER(LKKSPR_OFF=LKKDIV_OFF+NUMKIK)
        PARAMETER(LKKMAT_OFF=LKKSPR_OFF+NUMKIK)
C
        INTEGER*4 LKKPER_OFF, LKKTSR_OFF, LKKREV_OFF, LKKSED_OFF
        PARAMETER(LKKPER_OFF=LKKMAT_OFF+(3*KIGDIV*NUMKIK))
        PARAMETER(LKKTSR_OFF=LKKPER_OFF+(KIGDIV*NUMKIK))
        PARAMETER(LKKREV_OFF=LKKTSR_OFF+(KIGDIV*NUMKIK))
        PARAMETER(LKKSED_OFF=LKKREV_OFF+NUMKIK)
C
        INTEGER*4 LKKOCT_OFF, LKKMLT_OFF, LKKWRF_OFF, LKKASH_OFF
        PARAMETER(LKKOCT_OFF = LKKSED_OFF + NUMKIK*2)
        PARAMETER(LKKMLT_OFF = LKKOCT_OFF + NUMKIK)
        PARAMETER(LKKWRF_OFF = LKKMLT_OFF + NUMKIK)
        PARAMETER(LKKASH_OFF = LKKWRF_OFF + 10*NUMKIK)
C
        INTEGER*4 LKKMIN_OFF, LKKDFF_OFF, LKKMDS_OFF, LKKBAL_OFF
        PARAMETER(LKKMIN_OFF = LKKASH_OFF + KIGDIV*NUMKIK)
        PARAMETER(LKKDFF_OFF = LKKMIN_OFF + NUMKIK)
        PARAMETER(LKKMDS_OFF = LKKDFF_OFF + NUMKIK)
        PARAMETER(LKKBAL_OFF = LKKMDS_OFF + (MAXMLTD_AVL/4)*NUMKIK)
C
        INTEGER*4 LKKSPL_OFF
        PARAMETER(LKKSPL_OFF = LKKBAL_OFF + MAXDRW*NUMKIK)

        INTEGER*4 LKKRECLEN
        PARAMETER(LKKRECLEN = LKKSPL_OFF)
C
C LOGICAL RECORD DEFINITION
C
        INTEGER*4 LKKREC(LKKRECLEN)
C
C GAME FILE VARIABLE DEFINITIONS
C
        INTEGER*4 LKKSTS(NUMKIK),LKKCTM(NUMKIK)
        INTEGER*4 LKKTIM(NUMKIK),LKKDRW(NUMKIK)
        INTEGER*4 LKKBSD(NUMKIK),LKKESD(NUMKIK)
        INTEGER*4 LKKPUP(NUMKIK),LKKUPD(NUMKIK)
        INTEGER*4 LKKDAT(DATLEN,NUMKIK),LKKADV(NUMADV,NUMKIK)
        INTEGER*4 LKKSAL(KIGENT,MAXGAM,NUMKIK)
        INTEGER*4 LKKSHR(KIGDIV,NUMKIK)
        INTEGER*4 LKKPAD(KIGDIV,NUMKIK)
        INTEGER*4 LKKPRG(KIGDIV,NUMKIK)
        INTEGER*4 LKKSHV(KIGDIV,NUMKIK)
        INTEGER*4 LKKPOL(2,KIGDIV,NUMKIK)
        INTEGER*4 LKKBRK(KIGDIV,NUMKIK)
        INTEGER*4 LKKFRZ(KIGDIV,NUMKIK)
        INTEGER*4 LKKTAX(NUMKIK),LKKRES(2,NUMKIK)
        INTEGER*4 LKKWIN(NUMKIK),LKKHLD(NUMKIK)
        INTEGER*4 LKKSER(NUMKIK),LKKPRC(NUMKIK)
        INTEGER*4 LKKMAX(NUMKIK),LKKDIV(NUMKIK)
        INTEGER*4 LKKSPR(NUMKIK)
        INTEGER*4 LKKMAT(3,KIGDIV,NUMKIK)
        INTEGER*4 LKKPER(KIGDIV,NUMKIK)
        INTEGER*4 LKKTSR(KIGDIV,NUMKIK)
        INTEGER*4 LKKREV(NUMKIK),LKKSED(2,NUMKIK)
        INTEGER*4 LKKOCT(NUMKIK),LKKMLT(NUMKIK)
        INTEGER*4 LKKWRF(10,NUMKIK)  
        INTEGER*4 LKKASH(KIGDIV,NUMKIK)                 
        INTEGER*4 LKKMIN(NUMKIK)
        INTEGER*4 LKKDFF(NUMKIK)   
C
        BYTE      LKKMDS(MAXMLTD_AVL,NUMKIK)    ! must be divisible by 4        
C
        INTEGER*4 LKKBAL(MAXDRW,NUMKIK)

        INTEGER*4 LKKSPL(NUMKIK)
C
        EQUIVALENCE(LKKSTS,LKKREC(LKKSTS_OFF))
        EQUIVALENCE(LKKCTM,LKKREC(LKKCTM_OFF))
        EQUIVALENCE(LKKTIM,LKKREC(LKKTIM_OFF))
        EQUIVALENCE(LKKDRW,LKKREC(LKKDRW_OFF))
        EQUIVALENCE(LKKBSD,LKKREC(LKKBSD_OFF))
        EQUIVALENCE(LKKESD,LKKREC(LKKESD_OFF))
