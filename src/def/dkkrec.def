C
C KICKER DATA FILE RECORD LAYOUT (DISK)
C
C     DKKSTS                         ;STATUS
C     DKKCTM                         ;ACTUAL TIME CLOSED
C     DKKTIM                         ;TIME WHEN GAME SHOULD CLOSE
C     DKKDRW                         ;DRAW NUMBER
C     DKKBSD                         ;KICKER BEGGINING DRAW DATE
C     DKKESD                         ;KICKER ENDING DRAW DATE
C     DKKPUP                         ;LAST PURGE UPDATE
C     DKKUPD                         ;LAST FILE UPDATE
C     DKKDAT(DATLEN)                 ;DRAW DATES
C     DKKADV(NUMADV)                 ;ADVANCE DRAW DATES
C     DKKSAL(KIGENT,MAXGAM)          ;SALES DATA
C     DKKSHR(KIGDIV)                 ;SHARES
C     DKKPAD(KIGDIV)                 ;AMOUNT PAID
C     DKKPRG(KIGDIV)                 ;AMOUNT PURGED
C     DKKSHV(KIGDIV)                 ;SHARE VALUES
C     DKKPOL(2,KIGDIV)               ;POOL
C     DKKBRK(KIGDIV)                 ;BREAKAGE
C     DKKFRZ(KIGDIV)                 ;DIVISION PAYOUT FROZEN FLAGS
C     DKKTAX                         ;LOTTERY TAX
C     DKKRES(2)                      ;RESERVE AMOUNT
C     DKKWIN                         ;WINNING NUMBERS
C     DKKHLD                         ;WINNING NUMBER HOLD
C     DKKSER                         ;LAST SERIAL NUMBER
C     DKKPRC                         ;PRICE
C     DKKMAX                         ;MAXIMUM NUMBER BET
C     DKKDIV                         ;NUMBER OF DIVISIONS
C     DKKSPR                         ;SALES PERCENTAGE
C     DKKMAT(3,KIGDIV)               ;WINNERS MATCH TABLE
C     DKKPER(KIGDIV)                 ;DIVISION PERCENTAGE
C     DKKTSR(KIGDIV)                 ;TOTAL SHARE COUNT
C     DKKREV                         ;KICKER REVISION NUMBER
C     DKKSED(2)                      ;KICKER NUMBER SEED
C     DKKOCT                         ;# OF OCTAL DIGITS (FOR UNIRAN)
C     DKKMLT                         ;MULTI-DRAW FLAG
C     DKKWRF(10)                     ;WIN RESERVE FUND TABLE                    
C     DKKASH(KIGDIV)                 ;ADDITIONAL POOL BY DIV                    
C     DKKMIN                         ;MINIMUM JACKPOT                           
C     DKKDFF                         ;DOUBLE JOKERI FREE FLAG                   
C     DKKMDS(MAXMLTD_AVL)            ;MULTI DRAW SELECTED TABLE
C                                     (52 BYTE / 4 = 13)
C     DKKBAL(MAXDRW)                 ;BALANCE SALES INFO. BY DRAW
C     DKKSPL			     ;SPECIAL POOL, ADDED TO TOTAL POOL -
C                                     NOT TO DIV1 POOL AS OTHER POOL MONEY
C     DKKFRE                         ;FREE SPACE
C
C KICKER GAME FILE PARAMETERS
C
C
