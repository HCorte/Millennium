C
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
        INTEGER*4 DKKSTSOFF, DKKCTMOFF, DKKTIMOFF, DKKDRWOFF
        PARAMETER(DKKSTSOFF = 1)                            !    1
        PARAMETER(DKKCTMOFF = DKKSTSOFF + 1)                !    2
        PARAMETER(DKKTIMOFF = DKKCTMOFF + 1)                !    3
        PARAMETER(DKKDRWOFF = DKKTIMOFF + 1)                !    4
C
        INTEGER*4 DKKBSDOFF, DKKESDOFF, DKKPUPOFF, DKKUPDOFF
        PARAMETER(DKKBSDOFF = DKKDRWOFF + 1)                !    5
        PARAMETER(DKKESDOFF = DKKBSDOFF + 1)                !    6
        PARAMETER(DKKPUPOFF = DKKESDOFF + 1)                !    7
        PARAMETER(DKKUPDOFF = DKKPUPOFF + 1)                !    8
C
        INTEGER*4 DKKDATOFF, DKKADVOFF, DKKSALOFF, DKKSHROFF
        PARAMETER(DKKDATOFF = DKKUPDOFF + 1)                !    9
        PARAMETER(DKKADVOFF = DKKDATOFF + DATLEN)           !   29
        PARAMETER(DKKSALOFF = DKKADVOFF + NUMADV)           !   36
        PARAMETER(DKKSHROFF = DKKSALOFF + KIGENT*MAXGAM)    !  661
C
        INTEGER*4 DKKPADOFF, DKKPRGOFF, DKKSHVOFF, DKKPOLOFF
        PARAMETER(DKKPADOFF = DKKSHROFF + KIGDIV)           !  667
        PARAMETER(DKKPRGOFF = DKKPADOFF + KIGDIV)           !  673
        PARAMETER(DKKSHVOFF = DKKPRGOFF + KIGDIV)           !  679
        PARAMETER(DKKPOLOFF = DKKSHVOFF + KIGDIV)           !  685
C
        INTEGER*4 DKKBRKOFF, DKKFRZOFF, DKKTAXOFF, DKKRESOFF
        PARAMETER(DKKBRKOFF = DKKPOLOFF + 2*KIGDIV)
        PARAMETER(DKKFRZOFF = DKKBRKOFF + KIGDIV)           !  697
        PARAMETER(DKKTAXOFF = DKKFRZOFF + KIGDIV)           !  703
        PARAMETER(DKKRESOFF = DKKTAXOFF + 1)                !  704
C
        INTEGER*4 DKKWINOFF, DKKHLDOFF, DKKSEROFF, DKKPRCOFF
        PARAMETER(DKKWINOFF = DKKRESOFF + 2)                !  706
        PARAMETER(DKKHLDOFF = DKKWINOFF + 1)                !  707
        PARAMETER(DKKSEROFF = DKKHLDOFF + 1)                !  708
        PARAMETER(DKKPRCOFF = DKKSEROFF + 1)                !  709
C
        INTEGER*4 DKKMAXOFF, DKKDIVOFF, DKKSPROFF, DKKMATOFF
        PARAMETER(DKKMAXOFF = DKKPRCOFF + 1)                !  710
        PARAMETER(DKKDIVOFF = DKKMAXOFF + 1)                !  711
        PARAMETER(DKKSPROFF = DKKDIVOFF + 1)                !  712
        PARAMETER(DKKMATOFF = DKKSPROFF + 1)                !  713
C
        INTEGER*4 DKKPEROFF, DKKTSROFF, DKKREVOFF, DKKSEDOFF
        PARAMETER(DKKPEROFF = DKKMATOFF + 3*KIGDIV)         !  731
        PARAMETER(DKKTSROFF = DKKPEROFF + KIGDIV)           !  737
        PARAMETER(DKKREVOFF = DKKTSROFF + KIGDIV)           !  743
        PARAMETER(DKKSEDOFF = DKKREVOFF + 1)                !  744
C
        INTEGER*4 DKKOCTOFF, DKKMLTOFF, DKKWRFOFF, DKKASHOFF
        PARAMETER(DKKOCTOFF = DKKSEDOFF + 2)                !  746
        PARAMETER(DKKMLTOFF = DKKOCTOFF + 1)                !  747
        PARAMETER(DKKWRFOFF = DKKMLTOFF + 1)                !  748
        PARAMETER(DKKASHOFF = DKKWRFOFF + 10)               !  758

        INTEGER*4 DKKMINOFF, DKKDFFOFF, DKKMDSOFF, DKKBALOFF
        PARAMETER(DKKMINOFF = DKKASHOFF + KIGDIV)           !  764
        PARAMETER(DKKDFFOFF = DKKMINOFF + 1)                !  765
        PARAMETER(DKKMDSOFF = DKKDFFOFF + 1)                !  766
        PARAMETER(DKKBALOFF = DKKMDSOFF + (MAXMLTD_AVL/4))  !  779
C
        INTEGER*4 DKKSPLOFF
        PARAMETER(DKKSPLOFF = DKKBALOFF + MAXDRW)

        INTEGER*4 DKKFREOFF
        PARAMETER(DKKFREOFF = DKKSPLOFF+ 1)


        INTEGER*4 DKKSEC_VAX, DKKSEC_NUM
        PARAMETER (DKKSEC_VAX = SECSIZE/4)
        PARAMETER (DKKSEC_NUM = NINT(FLOAT((DKKFREOFF-2)/
     *                                      DKKSEC_VAX)+.5))

        INTEGER*4 DKKRECLEN, DKKSEC
        PARAMETER(DKKRECLEN = DKKSEC_VAX * DKKSEC_NUM)     ! even sector size
        PARAMETER(DKKSEC    = DKKRECLEN/64)

        INTEGER*4 DKKREC(DKKRECLEN)
C
        INTEGER*4 DKKSTS,DKKCTM
        INTEGER*4 DKKTIM,DKKDRW
        INTEGER*4 DKKBSD,DKKESD
        INTEGER*4 DKKPUP,DKKUPD
        INTEGER*4 DKKDAT(DATLEN),DKKADV(NUMADV)
        INTEGER*4 DKKSAL(KIGENT,MAXGAM)
        INTEGER*4 DKKSHR(KIGDIV)
        INTEGER*4 DKKPAD(KIGDIV)
        INTEGER*4 DKKPRG(KIGDIV)
        INTEGER*4 DKKSHV(KIGDIV)
        INTEGER*4 DKKPOL(2,KIGDIV)
        INTEGER*4 DKKBRK(KIGDIV)
        INTEGER*4 DKKFRZ(KIGDIV)
        INTEGER*4 DKKTAX,DKKRES(2)
        INTEGER*4 DKKWIN,DKKHLD
        INTEGER*4 DKKSER,DKKPRC
        INTEGER*4 DKKMAX,DKKDIV
        INTEGER*4 DKKSPR
        INTEGER*4 DKKMAT(3,KIGDIV)
        INTEGER*4 DKKPER(KIGDIV)
        INTEGER*4 DKKTSR(KIGDIV)
        INTEGER*4 DKKREV,DKKSED(2)
        INTEGER*4 DKKOCT,DKKMLT
        INTEGER*4 DKKWRF(10)  
        INTEGER*4 DKKASH(KIGDIV)                 
        INTEGER*4 DKKMIN
        INTEGER*4 DKKDFF   
        BYTE      DKKMDS(MAXMLTD_AVL)      ! must be divisible by 4        
        INTEGER*4 DKKBAL(MAXDRW)
C
	INTEGER*4 DKKSPL

        INTEGER*4 DKKFRE(DKKRECLEN - DKKFREOFF + 1)
C
        EQUIVALENCE(DKKSTS,DKKREC(DKKSTSOFF))
        EQUIVALENCE(DKKCTM,DKKREC(DKKCTMOFF))
        EQUIVALENCE(DKKTIM,DKKREC(DKKTIMOFF))
        EQUIVALENCE(DKKDRW,DKKREC(DKKDRWOFF))
        EQUIVALENCE(DKKBSD,DKKREC(DKKBSDOFF))
        EQUIVALENCE(DKKESD,DKKREC(DKKESDOFF))
        EQUIVALENCE(DKKPUP,DKKREC(DKKPUPOFF))
        EQUIVALENCE(DKKUPD,DKKREC(DKKUPDOFF))
        EQUIVALENCE(DKKDAT,DKKREC(DKKDATOFF))
        EQUIVALENCE(DKKADV,DKKREC(DKKADVOFF))
        EQUIVALENCE(DKKSAL,DKKREC(DKKSALOFF))
        EQUIVALENCE(DKKSHR,DKKREC(DKKSHROFF))
        EQUIVALENCE(DKKPAD,DKKREC(DKKPADOFF))
        EQUIVALENCE(DKKPRG,DKKREC(DKKPRGOFF))
        EQUIVALENCE(DKKSHV,DKKREC(DKKSHVOFF))
        EQUIVALENCE(DKKPOL,DKKREC(DKKPOLOFF))
        EQUIVALENCE(DKKBRK,DKKREC(DKKBRKOFF))
        EQUIVALENCE(DKKFRZ,DKKREC(DKKFRZOFF))
        EQUIVALENCE(DKKTAX,DKKREC(DKKTAXOFF))
        EQUIVALENCE(DKKRES,DKKREC(DKKRESOFF))
        EQUIVALENCE(DKKWIN,DKKREC(DKKWINOFF))
        EQUIVALENCE(DKKHLD,DKKREC(DKKHLDOFF))
        EQUIVALENCE(DKKSER,DKKREC(DKKSEROFF))
        EQUIVALENCE(DKKPRC,DKKREC(DKKPRCOFF))
        EQUIVALENCE(DKKMAX,DKKREC(DKKMAXOFF))
        EQUIVALENCE(DKKDIV,DKKREC(DKKDIVOFF))
        EQUIVALENCE(DKKSPR,DKKREC(DKKSPROFF))
        EQUIVALENCE(DKKMAT,DKKREC(DKKMATOFF))
        EQUIVALENCE(DKKPER,DKKREC(DKKPEROFF))
        EQUIVALENCE(DKKTSR,DKKREC(DKKTSROFF))
        EQUIVALENCE(DKKREV,DKKREC(DKKREVOFF))
        EQUIVALENCE(DKKSED,DKKREC(DKKSEDOFF))
        EQUIVALENCE(DKKOCT,DKKREC(DKKOCTOFF))
        EQUIVALENCE(DKKMLT,DKKREC(DKKMLTOFF))
        EQUIVALENCE(DKKWRF,DKKREC(DKKWRFOFF))
        EQUIVALENCE(DKKASH,DKKREC(DKKASHOFF))
        EQUIVALENCE(DKKMIN,DKKREC(DKKMINOFF))
        EQUIVALENCE(DKKDFF,DKKREC(DKKDFFOFF)) 
        EQUIVALENCE(DKKMDS,DKKREC(DKKMDSOFF))                         
        EQUIVALENCE(DKKBAL,DKKREC(DKKBALOFF))
        EQUIVALENCE(DKKSPL,DKKREC(DKKSPLOFF))
        EQUIVALENCE(DKKFRE,DKKREC(DKKFREOFF))
