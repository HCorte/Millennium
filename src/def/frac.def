	INTEGER*4 FRA_SIZ,FRA_TKT,FRA_FRC,FRA_QUE,FRA_WRKTAB
	PARAMETER (FRA_SIZ    = WRKTAB+0)  ! size for fractioned tickets
	PARAMETER (FRA_TKT    = WRKTAB+1)  ! number of fractioned tickets
	PARAMETER (FRA_FRC    = WRKTAB+2)  ! serial #s of fractioned tickets
	PARAMETER (FRA_QUE    = WRKTAB+12) ! queue number for second phase 
                                           ! fractions/unfractions
	PARAMETER (FRA_WRKTAB = WRKTAB+13) ! WRKTAB for second phase frac/unfra
