	  PARAMETER  (X2SCRN_KEYLEV1=2)
	  PARAMETER  (X2SCRN_KEYLEV2=3)
	  PARAMETER  (X2SCRN_KEYLEV3=4)
	  PARAMETER  (X2SCRN_INPLEV1=5)
	  PARAMETER  (X2SCRN_INPLEV2=6)
