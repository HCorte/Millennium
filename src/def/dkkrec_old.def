C
C V02 19-FEB-2004 FRP EuroMillions: recalculate record according to MAXGAM=50.
C
C KICKER DATA FILE RECORD LAYOUT (DISK)
C
C     ODKKSTS                         ;STATUS
C     ODKKCTM                         ;ACTUAL TIME CLOSED
C     ODKKTIM                         ;TIME WHEN GAME SHOULD CLOSE
C     ODKKDRW                         ;DRAW NUMBER
C     ODKKBSD                         ;KICKER BEGGINING DRAW DATE
C     ODKKESD                         ;KICKER ENDING DRAW DATE
C     ODKKPUP                         ;LAST PURGE UPDATE
C     ODKKUPD                         ;LAST FILE UPDATE
C     ODKKDAT(DATLEN)                 ;DRAW DATES
C     ODKKADV(NUMADV)                 ;ADVANCE DRAW DATES
C     ODKKSAL(KIGENT,OMAXGAM)         ;SALES DATA
C     ODKKSHR(KIGDIV)                 ;SHARES
C     ODKKPAD(KIGDIV)                 ;AMOUNT PAID
C     ODKKPRG(KIGDIV)                 ;AMOUNT PURGED
C     ODKKSHV(KIGDIV)                 ;SHARE VALUES
C     ODKKPOL(2,KIGDIV)               ;POOL
C     ODKKBRK(KIGDIV)                 ;BREAKAGE
C     ODKKFRZ(KIGDIV)                 ;DIVISION PAYOUT FROZEN FLAGS
C     ODKKTAX                         ;LOTTERY TAX
C     ODKKRES(2)                      ;RESERVE AMOUNT
C     ODKKWIN                         ;WINNING NUMBERS
C     ODKKHLD                         ;WINNING NUMBER HOLD
C     ODKKSER                         ;LAST SERIAL NUMBER
C     ODKKPRC                         ;PRICE
C     ODKKMAX                         ;MAXIMUM NUMBER BET
C     ODKKDIV                         ;NUMBER OF DIVISIONS
C     ODKKSPR                         ;SALES PERCENTAGE
C     ODKKMAT(3,KIGDIV)               ;WINNERS MATCH TABLE
C     ODKKPER(KIGDIV)                 ;DIVISION PERCENTAGE
C     ODKKTSR(KIGDIV)                 ;TOTAL SHARE COUNT
C     ODKKREV                         ;KICKER REVISION NUMBER
C     ODKKSED(2)                      ;KICKER NUMBER SEED
C     ODKKOCT                         ;# OF OCTAL DIGITS (FOR UNIRAN)
C     ODKKMLT                         ;MULTI-DRAW FLAG
C     ODKKWRF(10)                     ;WIN RESERVE FUND TABLE                    
C     ODKKASH(KIGDIV)                 ;ADDITIONAL POOL BY DIV                    
C     ODKKMIN                         ;MINIMUM JACKPOT                           
C     ODKKDFF                         ;DOUBLE JOKERI FREE FLAG                   
C     ODKKMDS(MAXMLTD_AVL)            ;MULTI DRAW SELECTED TABLE
C                                     (52 BYTE / 4 = 13)
C     ODKKBAL(MAXDRW)                 ;BALANCE SALES INFO. BY DRAW
C     ODKKSPL			     ;SPECIAL POOL, ADDED TO TOTAL POOL -
C                                     NOT TO DIV1 POOL AS OTHER POOL MONEY
C     ODKKFRE                         ;FREE SPACE
C
C KICKER GAME FILE PARAMETERS
C
C
