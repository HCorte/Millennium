C
        INTEGER*4 OMAXGAM
        PARAMETER(OMAXGAM=10)
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
        INTEGER*4 ODKKSTSOFF, ODKKCTMOFF, ODKKTIMOFF, ODKKDRWOFF
        PARAMETER(ODKKSTSOFF = 1)                             !    1
        PARAMETER(ODKKCTMOFF = ODKKSTSOFF + 1)                !    2
        PARAMETER(ODKKTIMOFF = ODKKCTMOFF + 1)                !    3
        PARAMETER(ODKKDRWOFF = ODKKTIMOFF + 1)                !    4
C
        INTEGER*4 ODKKBSDOFF, ODKKESDOFF, ODKKPUPOFF, ODKKUPDOFF
        PARAMETER(ODKKBSDOFF = ODKKDRWOFF + 1)                !    5
        PARAMETER(ODKKESDOFF = ODKKBSDOFF + 1)                !    6
        PARAMETER(ODKKPUPOFF = ODKKESDOFF + 1)                !    7
        PARAMETER(ODKKUPDOFF = ODKKPUPOFF + 1)                !    8
C
        INTEGER*4 ODKKDATOFF, ODKKADVOFF, ODKKSALOFF, ODKKSHROFF
        PARAMETER(ODKKDATOFF = ODKKUPDOFF + 1)                !    9
        PARAMETER(ODKKADVOFF = ODKKDATOFF + DATLEN)           !   29
        PARAMETER(ODKKSALOFF = ODKKADVOFF + NUMADV)           !   36
        PARAMETER(ODKKSHROFF = ODKKSALOFF + KIGENT*OMAXGAM)   !  661
C
        INTEGER*4 ODKKPADOFF, ODKKPRGOFF, ODKKSHVOFF, ODKKPOLOFF
        PARAMETER(ODKKPADOFF = ODKKSHROFF + KIGDIV)           !  667
        PARAMETER(ODKKPRGOFF = ODKKPADOFF + KIGDIV)           !  673
        PARAMETER(ODKKSHVOFF = ODKKPRGOFF + KIGDIV)           !  679
        PARAMETER(ODKKPOLOFF = ODKKSHVOFF + KIGDIV)           !  685
C
        INTEGER*4 ODKKBRKOFF, ODKKFRZOFF, ODKKTAXOFF, ODKKRESOFF
        PARAMETER(ODKKBRKOFF = ODKKPOLOFF + 2*KIGDIV)
        PARAMETER(ODKKFRZOFF = ODKKBRKOFF + KIGDIV)           !  697
        PARAMETER(ODKKTAXOFF = ODKKFRZOFF + KIGDIV)           !  703
        PARAMETER(ODKKRESOFF = ODKKTAXOFF + 1)                !  704
C
        INTEGER*4 ODKKWINOFF, ODKKHLDOFF, ODKKSEROFF, ODKKPRCOFF
        PARAMETER(ODKKWINOFF = ODKKRESOFF + 2)                !  706
        PARAMETER(ODKKHLDOFF = ODKKWINOFF + 1)                !  707
        PARAMETER(ODKKSEROFF = ODKKHLDOFF + 1)                !  708
        PARAMETER(ODKKPRCOFF = ODKKSEROFF + 1)                !  709
C
        INTEGER*4 ODKKMAXOFF, ODKKDIVOFF, ODKKSPROFF, ODKKMATOFF
        PARAMETER(ODKKMAXOFF = ODKKPRCOFF + 1)                !  710
        PARAMETER(ODKKDIVOFF = ODKKMAXOFF + 1)                !  711
        PARAMETER(ODKKSPROFF = ODKKDIVOFF + 1)                !  712
        PARAMETER(ODKKMATOFF = ODKKSPROFF + 1)                !  713
C
        INTEGER*4 ODKKPEROFF, ODKKTSROFF, ODKKREVOFF, ODKKSEDOFF
        PARAMETER(ODKKPEROFF = ODKKMATOFF + 3*KIGDIV)         !  731
        PARAMETER(ODKKTSROFF = ODKKPEROFF + KIGDIV)           !  737
        PARAMETER(ODKKREVOFF = ODKKTSROFF + KIGDIV)           !  743
        PARAMETER(ODKKSEDOFF = ODKKREVOFF + 1)                !  744
C
        INTEGER*4 ODKKOCTOFF, ODKKMLTOFF, ODKKWRFOFF, ODKKASHOFF
        PARAMETER(ODKKOCTOFF = ODKKSEDOFF + 2)                !  746
        PARAMETER(ODKKMLTOFF = ODKKOCTOFF + 1)                !  747
        PARAMETER(ODKKWRFOFF = ODKKMLTOFF + 1)                !  748
        PARAMETER(ODKKASHOFF = ODKKWRFOFF + 10)               !  758

        INTEGER*4 ODKKMINOFF, ODKKDFFOFF, ODKKMDSOFF, ODKKBALOFF
        PARAMETER(ODKKMINOFF = ODKKASHOFF + KIGDIV)           !  764
        PARAMETER(ODKKDFFOFF = ODKKMINOFF + 1)                !  765
        PARAMETER(ODKKMDSOFF = ODKKDFFOFF + 1)                !  766
        PARAMETER(ODKKBALOFF = ODKKMDSOFF + (MAXMLTD_AVL/4))  !  779
C
        INTEGER*4 ODKKSPLOFF
        PARAMETER(ODKKSPLOFF = ODKKBALOFF + MAXDRW)

        INTEGER*4 ODKKFREOFF
        PARAMETER(ODKKFREOFF = ODKKSPLOFF+ 1)


        INTEGER*4 ODKKSEC_VAX, ODKKSEC_NUM
        PARAMETER (ODKKSEC_VAX = SECSIZE/4)
        PARAMETER (ODKKSEC_NUM = NINT(FLOAT((ODKKFREOFF-2)/
     *                                      ODKKSEC_VAX)+.5))

        INTEGER*4 ODKKRECLEN, ODKKSEC
        PARAMETER(ODKKRECLEN = ODKKSEC_VAX * ODKKSEC_NUM)     ! even sector size
        PARAMETER(ODKKSEC    = ODKKRECLEN/64)

        INTEGER*4 ODKKREC(ODKKRECLEN)
C
        INTEGER*4 ODKKSTS,ODKKCTM
        INTEGER*4 ODKKTIM,ODKKDRW
        INTEGER*4 ODKKBSD,ODKKESD
        INTEGER*4 ODKKPUP,ODKKUPD
        INTEGER*4 ODKKDAT(DATLEN),ODKKADV(NUMADV)
        INTEGER*4 ODKKSAL(KIGENT,OMAXGAM)
        INTEGER*4 ODKKSHR(KIGDIV)
        INTEGER*4 ODKKPAD(KIGDIV)
        INTEGER*4 ODKKPRG(KIGDIV)
        INTEGER*4 ODKKSHV(KIGDIV)
        INTEGER*4 ODKKPOL(2,KIGDIV)
        INTEGER*4 ODKKBRK(KIGDIV)
        INTEGER*4 ODKKFRZ(KIGDIV)
        INTEGER*4 ODKKTAX,ODKKRES(2)
        INTEGER*4 ODKKWIN,ODKKHLD
        INTEGER*4 ODKKSER,ODKKPRC
        INTEGER*4 ODKKMAX,ODKKDIV
        INTEGER*4 ODKKSPR
        INTEGER*4 ODKKMAT(3,KIGDIV)
        INTEGER*4 ODKKPER(KIGDIV)
        INTEGER*4 ODKKTSR(KIGDIV)
        INTEGER*4 ODKKREV,ODKKSED(2)
        INTEGER*4 ODKKOCT,ODKKMLT
        INTEGER*4 ODKKWRF(10)  
        INTEGER*4 ODKKASH(KIGDIV)                 
        INTEGER*4 ODKKMIN
        INTEGER*4 ODKKDFF   
        BYTE      ODKKMDS(MAXMLTD_AVL)      ! must be divisible by 4        
        INTEGER*4 ODKKBAL(MAXDRW)
C
	INTEGER*4 ODKKSPL

        INTEGER*4 ODKKFRE(ODKKRECLEN - ODKKFREOFF + 1)
C
        EQUIVALENCE(ODKKSTS,ODKKREC(ODKKSTSOFF))
        EQUIVALENCE(ODKKCTM,ODKKREC(ODKKCTMOFF))
        EQUIVALENCE(ODKKTIM,ODKKREC(ODKKTIMOFF))
        EQUIVALENCE(ODKKDRW,ODKKREC(ODKKDRWOFF))
        EQUIVALENCE(ODKKBSD,ODKKREC(ODKKBSDOFF))
        EQUIVALENCE(ODKKESD,ODKKREC(ODKKESDOFF))
        EQUIVALENCE(ODKKPUP,ODKKREC(ODKKPUPOFF))
        EQUIVALENCE(ODKKUPD,ODKKREC(ODKKUPDOFF))
        EQUIVALENCE(ODKKDAT,ODKKREC(ODKKDATOFF))
        EQUIVALENCE(ODKKADV,ODKKREC(ODKKADVOFF))
        EQUIVALENCE(ODKKSAL,ODKKREC(ODKKSALOFF))
        EQUIVALENCE(ODKKSHR,ODKKREC(ODKKSHROFF))
        EQUIVALENCE(ODKKPAD,ODKKREC(ODKKPADOFF))
        EQUIVALENCE(ODKKPRG,ODKKREC(ODKKPRGOFF))
        EQUIVALENCE(ODKKSHV,ODKKREC(ODKKSHVOFF))
        EQUIVALENCE(ODKKPOL,ODKKREC(ODKKPOLOFF))
        EQUIVALENCE(ODKKBRK,ODKKREC(ODKKBRKOFF))
        EQUIVALENCE(ODKKFRZ,ODKKREC(ODKKFRZOFF))
        EQUIVALENCE(ODKKTAX,ODKKREC(ODKKTAXOFF))
        EQUIVALENCE(ODKKRES,ODKKREC(ODKKRESOFF))
        EQUIVALENCE(ODKKWIN,ODKKREC(ODKKWINOFF))
        EQUIVALENCE(ODKKHLD,ODKKREC(ODKKHLDOFF))
        EQUIVALENCE(ODKKSER,ODKKREC(ODKKSEROFF))
        EQUIVALENCE(ODKKPRC,ODKKREC(ODKKPRCOFF))
        EQUIVALENCE(ODKKMAX,ODKKREC(ODKKMAXOFF))
        EQUIVALENCE(ODKKDIV,ODKKREC(ODKKDIVOFF))
        EQUIVALENCE(ODKKSPR,ODKKREC(ODKKSPROFF))
        EQUIVALENCE(ODKKMAT,ODKKREC(ODKKMATOFF))
        EQUIVALENCE(ODKKPER,ODKKREC(ODKKPEROFF))
        EQUIVALENCE(ODKKTSR,ODKKREC(ODKKTSROFF))
        EQUIVALENCE(ODKKREV,ODKKREC(ODKKREVOFF))
        EQUIVALENCE(ODKKSED,ODKKREC(ODKKSEDOFF))
        EQUIVALENCE(ODKKOCT,ODKKREC(ODKKOCTOFF))
        EQUIVALENCE(ODKKMLT,ODKKREC(ODKKMLTOFF))
        EQUIVALENCE(ODKKWRF,ODKKREC(ODKKWRFOFF))
        EQUIVALENCE(ODKKASH,ODKKREC(ODKKASHOFF))
        EQUIVALENCE(ODKKMIN,ODKKREC(ODKKMINOFF))
        EQUIVALENCE(ODKKDFF,ODKKREC(ODKKDFFOFF)) 
        EQUIVALENCE(ODKKMDS,ODKKREC(ODKKMDSOFF))                         
        EQUIVALENCE(ODKKBAL,ODKKREC(ODKKBALOFF))
        EQUIVALENCE(ODKKSPL,ODKKREC(ODKKSPLOFF))
        EQUIVALENCE(ODKKFRE,ODKKREC(ODKKFREOFF))
