C DESCRIPTION LOGICAL PARAMETERS DEFINITION
C
C     LTGSTS(NUMTGL)                 ; STATUS
C     LTGCTM(NUMTGL)                 ; ACTUAL TIME WHEN CLOSED
C     LTGTIM(NUMTGL)                 ; TIME WHEN GAME SHOULD CLOSE
C     LTGDRW(NUMTGL)                 ; DRAW NUMBER
C     LTGBSD(NUMTGL)                 ; RESULTS BEGGINING DRAW DATE
C     LTGESD(NUMTGL)                 ; RESULTS ENDING DRAW DATE
C     LTGPUP(NUMTGL)                 ; LAST PURGE UPDATE
C     LTGUPD(NUMTGL)                 ; LAST FILE UPDATE
C     LTGDAT(DATLEN, NUMTGL)         ; DRAW DATES
C     LTGADV(NUMADV, NUMKIK)         ; ADVANCE DRAW DATES
C     LTGSAL(TGGENT, NUMTGL)         ; SALES DATA
C     LTGSHV(TGGDIV, NUMTGL)         ; SHARE VALUES
C     LTGSHR(TGGDIV, NUMTGL)         ; SHARES
C     LTGPOL(TGGDIV, NUMTGL)         ; POOL CARRIED OVER
C     LTGPAD(TGGDIV, NUMTGL)         ; AMOUNT PAID
C     LTGPRG(TGGDIV, NUMTGL)         ; AMOUNT PURGED
C     LTGAFD(TGGDIV, NUMTGL)         ; AMOUNTS FROM OTHERS DRAWS/GAMES (EXTRA)
C     LTGCSP(TGGDIV, NUMTGL)         ; CALCULATED SHARE CALCULATIONS POOLS
C     LTGOSV(TGGDIV, NUMTGL)         ; OFF LINE AMOUNT TO PRIZES BY DIVISION
C     LTGOSH(TGGDIV, NUMTGL)         ; OFF LINE SHARE
C     LTGWIN(2, TGGNBR, NUMTGL)      ; WINNING RESULTS
C     LTGHLD(2, TGGNBR, NUMTGL)      ; WINNING RESULTS HOLD
C     LTGPRP(NUMTGL)                 ; PERCENTAGE OF THE PROMISED AMOUNT
C     LTGPRN(NUMTGL)                 ; MAX. NUM OF WINNERS FOR PROMISED AMOUNT,
C                                    ; IF LTGPRN=-1, IT MEANS 'ERIKOISPOTTI'
C     LTGTAX(NUMTGL)                 ; LOTTERY TAX
C     LTGRES(2, NUMTGL)              ; RESERVE POOL
C     LTGAPL(NUMTGL)                 ; ADDITIONAL POOL
C     LTGMIN(NUMTGL)                 ; MINIMUM POOL
C     LTGSER(NUMTGL)                 ; LAST SERIAL NUMBER
C     LTGOPA(NUMTGL)                 ; OVER-RIDE POOL AMOUNT
C     LTGPRC(NUMTGL)                 ; PRICE/SINGLE COMBINATION
C     LTGMAX(NUMTGL)                 ; MAXIMUM NUMBER OF EVENTS (ROWS)
C     LTGMLT(NUMTGL)                 ; MULTI-DRAW ENABLE FLAG
C     LTGMDS(MAXMLTD_AVL, NUMTGL)    ; MULTI-DRAW SELECTED TABLE (52/4 WORDS)
C     LTTDIV(NUMTGL)                 ; # OF DIVISIONS
C     LTGMAT(TGGDIV, NUMTGL)         ; DIVISION MATCH TABLE
C     LTGPER(TGGDIV, NUMTGL)         ; DIVISION PERCENTAGES
C     LTGSPR(NUMTGL)                 ; SALES PERCENTAGE
C     LTGTSR(TGGDIV, NUMTGL)         ; TOTAL SHARE COUNT
C     LTGREV(NUMTGL)                 ; RESULTS REVISION NUMBER
C
C     LTGNMS(TGNMS_LEN / 4, 2, TGGNBR, NUMTGL)     ; TEAM NAMES
C
C     LTGCLB(25, NUMTGL)             ; CLUB VOTING COUNTS
C     LTGASH(TGGDIV, NUMTGL)         ; ADDITIONAL POL BY DIV.
C     LTGFRG(NUMTGL)                 ; FLAG FOR FREE GAME (PRICE = 0.00)
C     LTGWRF(POSTED, NUMTGL)         ; WIN RESERVE FUND
C     LTGBST(NUMTGL)                 ; BEST MATCH FLAG (ADJUST DIVISIONS)
C     LTGBAL(MAXDRW, NUMTGL)         ; BALANCE SALES INFO. BY DRAW
C     LTGEVN(TGEVN_LEN / 4, NUMTGL)  ; EVENT NAME
C     LTGEXT(NUMTGL)                 ; EXTRAORDINARY DRAW
C     LTGRGT(2, NUMTGL)              : WHERE ROLLOVER GO TO (1:GAME, 2:DRW)
C     LTGROD(2, NUMTGL)              ; WHERE ROLLOVER CAME FROM (1:GAME, 2:DRW)
C
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
      INTEGER * 4 LTGSTS_OFF, LTGCTM_OFF, LTGTIM_OFF, LTGDRW_OFF
C
      PARAMETER(LTGSTS_OFF = 1)
      PARAMETER(LTGCTM_OFF = LTGSTS_OFF + NUMTGL)
      PARAMETER(LTGTIM_OFF = LTGCTM_OFF + NUMTGL)
      PARAMETER(LTGDRW_OFF = LTGTIM_OFF + NUMTGL)
C
      INTEGER * 4 LTGBSD_OFF, LTGESD_OFF, LTGPUP_OFF, LTGUPD_OFF
C
      PARAMETER(LTGBSD_OFF = LTGDRW_OFF + NUMTGL)
      PARAMETER(LTGESD_OFF = LTGBSD_OFF + NUMTGL)
      PARAMETER(LTGPUP_OFF = LTGESD_OFF + NUMTGL)
      PARAMETER(LTGUPD_OFF = LTGPUP_OFF + NUMTGL)
C
      INTEGER * 4 LTGDAT_OFF, LTGADV_OFF, LTGSAL_OFF, LTGSHV_OFF
C
      PARAMETER(LTGDAT_OFF = LTGUPD_OFF + NUMTGL)
      PARAMETER(LTGADV_OFF = LTGDAT_OFF + DATLEN * NUMTGL)
      PARAMETER(LTGSAL_OFF = LTGADV_OFF + NUMADV * NUMTGL)
      PARAMETER(LTGSHV_OFF = LTGSAL_OFF + TGGENT * NUMTGL)
C
      INTEGER * 4 LTGSHR_OFF, LTGPOL_OFF, LTGPAD_OFF, LTGPRG_OFF
C
      PARAMETER(LTGSHR_OFF = LTGSHV_OFF + TGGDIV * NUMTGL)
      PARAMETER(LTGPOL_OFF = LTGSHR_OFF + TGGDIV * NUMTGL)
      PARAMETER(LTGPAD_OFF = LTGPOL_OFF + TGGDIV * NUMTGL)
      PARAMETER(LTGPRG_OFF = LTGPAD_OFF + TGGDIV * NUMTGL)
C
      INTEGER * 4 LTGAFD_OFF, LTGCSP_OFF, LTGOSV_OFF, LTGOSH_OFF
C
      PARAMETER(LTGAFD_OFF = LTGPRG_OFF + TGGDIV * NUMTGL)
      PARAMETER(LTGCSP_OFF = LTGAFD_OFF + TGGDIV * NUMTGL)
      PARAMETER(LTGOSV_OFF = LTGCSP_OFF + TGGDIV * NUMTGL)
      PARAMETER(LTGOSH_OFF = LTGOSV_OFF + TGGDIV * NUMTGL)
C
      INTEGER * 4 LTGWIN_OFF, LTGHLD_OFF, LTGPRP_OFF, LTGPRN_OFF
C        
      PARAMETER(LTGWIN_OFF = LTGOSH_OFF + TGGDIV * NUMTGL)
      PARAMETER(LTGHLD_OFF = LTGWIN_OFF + 2 * TGGNBR * NUMTGL)
      PARAMETER(LTGPRP_OFF = LTGHLD_OFF + 2 * TGGNBR * NUMTGL)
      PARAMETER(LTGPRN_OFF = LTGPRP_OFF + NUMTGL)
C
      INTEGER * 4 LTGTAX_OFF, LTGRES_OFF, LTGAPL_OFF, LTGMIN_OFF
C
      PARAMETER(LTGTAX_OFF = LTGPRN_OFF + NUMTGL)
      PARAMETER(LTGRES_OFF = LTGTAX_OFF + NUMTGL)
      PARAMETER(LTGAPL_OFF = LTGRES_OFF + 2 * NUMTGL)
      PARAMETER(LTGMIN_OFF = LTGAPL_OFF + NUMTGL)
C
      INTEGER * 4 LTGSER_OFF, LTGOPA_OFF, LTGPRC_OFF, LTGMAX_OFF
C
      PARAMETER(LTGSER_OFF = LTGMIN_OFF + NUMTGL)
      PARAMETER(LTGOPA_OFF = LTGSER_OFF + NUMTGL)
      PARAMETER(LTGPRC_OFF = LTGOPA_OFF + NUMTGL)
      PARAMETER(LTGMAX_OFF = LTGPRC_OFF + NUMTGL)
C
      INTEGER * 4 LTGMLT_OFF, LTTDIV_OFF, LTGMAT_OFF, LTGPER_OFF, LTGMDS_OFF
C
      PARAMETER(LTGMLT_OFF = LTGMAX_OFF + NUMTGL)
      PARAMETER(LTGMDS_OFF = LTGMLT_OFF + NUMTGL)
      PARAMETER(LTTDIV_OFF = LTGMDS_OFF + (MAXMLTD_AVL / 4) * NUMTGL)
      PARAMETER(LTGMAT_OFF = LTTDIV_OFF + NUMTGL)
      PARAMETER(LTGPER_OFF = LTGMAT_OFF + TGGDIV * NUMTGL)
C
      INTEGER * 4 LTGSPR_OFF, LTGTSR_OFF, LTGREV_OFF, LTGNMS_OFF
C
      PARAMETER(LTGSPR_OFF = LTGPER_OFF + TGGDIV * NUMTGL)
      PARAMETER(LTGTSR_OFF = LTGSPR_OFF + NUMTGL)
      PARAMETER(LTGREV_OFF = LTGTSR_OFF + TGGDIV * NUMTGL)
      PARAMETER(LTGNMS_OFF = LTGREV_OFF + NUMTGL)
C
      INTEGER * 4 LTGCLB_OFF, LTGASH_OFF, LTGFRG_OFF, LTGWRF_OFF
C
      PARAMETER(LTGCLB_OFF = LTGNMS_OFF+((TGNMS_LEN / 4) * 2 * TGGNBR * NUMTGL))
      PARAMETER(LTGASH_OFF = LTGCLB_OFF + 25 * NUMTGL)
      PARAMETER(LTGFRG_OFF = LTGASH_OFF + TGGDIV * NUMTGL)
      PARAMETER(LTGWRF_OFF = LTGFRG_OFF + NUMTGL)
C
      INTEGER * 4 LTGBST_OFF, LTGBAL_OFF, LTGEVN_OFF
C
      PARAMETER(LTGBST_OFF = LTGWRF_OFF + POSTED * NUMTGL)
      PARAMETER(LTGBAL_OFF = LTGBST_OFF + NUMTGL)
      PARAMETER(LTGEVN_OFF = LTGBAL_OFF + MAXDRW * NUMTGL)
C
      INTEGER * 4 LTGEXT_OFF
      INTEGER * 4 LTGRGT_OFF
      INTEGER * 4 LTGROD_OFF
C
      PARAMETER(LTGEXT_OFF = LTGEVN_OFF + (TGEVN_LEN / 4) * NUMTGL)
      PARAMETER(LTGRGT_OFF = LTGEXT_OFF + NUMTGL)
      PARAMETER(LTGROD_OFF = LTGRGT_OFF + 2 * NUMTGL)
C
      INTEGER * 4 LTGRECLEN
C
      PARAMETER(LTGRECLEN  = (LTGROD_OFF + 2 * NUMTGL) - 1)
C
C LOGICAL RECORD DEFINITION
C
      INTEGER * 4 LTGREC(LTGRECLEN)
C
C LOGICAL GAME FILE VARIABLE DEFINITIONS
C
      INTEGER * 4 LTGSTS(NUMTGL)
      INTEGER * 4 LTGCTM(NUMTGL)
      INTEGER * 4 LTGTIM(NUMTGL)
      INTEGER * 4 LTGDRW(NUMTGL)
      INTEGER * 4 LTGBSD(NUMTGL)
      INTEGER * 4 LTGESD(NUMTGL)
      INTEGER * 4 LTGDAT(DATLEN, NUMTGL)
      INTEGER * 4 LTGPUP(NUMTGL)
      INTEGER * 4 LTGUPD(NUMTGL)
      INTEGER * 4 LTGADV(NUMADV, NUMTGL)
      INTEGER * 4 LTGSAL(TGGENT, NUMTGL)
      INTEGER * 4 LTGSHV(TGGDIV, NUMTGL)
      INTEGER * 4 LTGSHR(TGGDIV, NUMTGL)
      INTEGER * 4 LTGPOL(TGGDIV, NUMTGL)
      INTEGER * 4 LTGPAD(TGGDIV, NUMTGL)
      INTEGER * 4 LTGPRG(TGGDIV, NUMTGL)
      INTEGER * 4 LTGAFD(TGGDIV, NUMTGL)
      INTEGER * 4 LTGCSP(TGGDIV, NUMTGL)
      INTEGER * 4 LTGOSV(TGGDIV, NUMTGL)
      INTEGER * 4 LTGOSH(TGGDIV, NUMTGL)
      INTEGER * 4 LTGWIN(2, TGGNBR, NUMTGL)
      INTEGER * 4 LTGHLD(2, TGGNBR, NUMTGL)
      INTEGER * 4 LTGPRP(NUMTGL)
      INTEGER * 4 LTGPRN(NUMTGL)
      INTEGER * 4 LTGTAX(NUMTGL)
      INTEGER * 4 LTGRES(2, NUMTGL)
      INTEGER * 4 LTGAPL(NUMTGL)
      INTEGER * 4 LTGMIN(NUMTGL)
      INTEGER * 4 LTGSER(NUMTGL)
      INTEGER * 4 LTGOPA(NUMTGL)
      INTEGER * 4 LTGPRC(NUMTGL)
      INTEGER * 4 LTGMAX(NUMTGL)
      INTEGER * 4 LTGCLB(25, NUMTGL)
      INTEGER * 4 LTGMLT(NUMTGL)
      INTEGER * 4 LTGNMS(TGNMS_LEN / 4, 2, TGGNBR, NUMTGL)
      INTEGER * 4 LTTDIV(NUMTGL)
      INTEGER * 4 LTGMAT(TGGDIV, NUMTGL)
      INTEGER * 4 LTGPER(TGGDIV, NUMTGL)
      INTEGER * 4 LTGSPR(NUMTGL)
      INTEGER * 4 LTGTSR(TGGDIV, NUMTGL)
      INTEGER * 4 LTGREV(NUMTGL)
      INTEGER * 4 LTGASH(TGGDIV, NUMTGL)
      INTEGER * 4 LTGFRG(NUMTGL)
      INTEGER * 4 LTGWRF(POSTED, NUMTGL)
      INTEGER * 4 LTGBST(NUMTGL)
      INTEGER * 4 LTGBAL(MAXDRW, NUMTGL)
      INTEGER * 4 LTGEVN(TGEVN_LEN / 4, NUMTGL)
      INTEGER * 4 LTGEXT(NUMTGL)
      INTEGER * 4 LTGRGT(2, NUMTGL)
      INTEGER * 4 LTGROD(2, NUMTGL)
C
      BYTE LTGMDS(MAXMLTD_AVL, NUMTGL)
C
C EQUIVALENCE LOGICAL RECORD DEFINITION
C
        EQUIVALENCE(LTGSTS, LTGREC(LTGSTS_OFF))
        EQUIVALENCE(LTGCTM, LTGREC(LTGCTM_OFF))
        EQUIVALENCE(LTGTIM, LTGREC(LTGTIM_OFF))
        EQUIVALENCE(LTGDRW, LTGREC(LTGDRW_OFF))
        EQUIVALENCE(LTGBSD, LTGREC(LTGBSD_OFF))
        EQUIVALENCE(LTGESD, LTGREC(LTGESD_OFF))
        EQUIVALENCE(LTGPUP, LTGREC(LTGPUP_OFF))
        EQUIVALENCE(LTGUPD, LTGREC(LTGUPD_OFF))
        EQUIVALENCE(LTGDAT, LTGREC(LTGDAT_OFF))
        EQUIVALENCE(LTGADV, LTGREC(LTGADV_OFF))
        EQUIVALENCE(LTGSAL, LTGREC(LTGSAL_OFF))
        EQUIVALENCE(LTGSHV, LTGREC(LTGSHV_OFF))
        EQUIVALENCE(LTGSHR, LTGREC(LTGSHR_OFF))
        EQUIVALENCE(LTGPOL, LTGREC(LTGPOL_OFF))
        EQUIVALENCE(LTGPAD, LTGREC(LTGPAD_OFF))
        EQUIVALENCE(LTGPRG, LTGREC(LTGPRG_OFF))
        EQUIVALENCE(LTGAFD, LTGREC(LTGAFD_OFF))
        EQUIVALENCE(LTGCSP, LTGREC(LTGCSP_OFF))
        EQUIVALENCE(LTGOSV, LTGREC(LTGOSV_OFF))
        EQUIVALENCE(LTGOSH, LTGREC(LTGOSH_OFF))
        EQUIVALENCE(LTGWIN, LTGREC(LTGWIN_OFF))
        EQUIVALENCE(LTGHLD, LTGREC(LTGHLD_OFF))
        EQUIVALENCE(LTGPRP, LTGREC(LTGPRP_OFF))
        EQUIVALENCE(LTGPRN, LTGREC(LTGPRN_OFF))
        EQUIVALENCE(LTGTAX, LTGREC(LTGTAX_OFF))
        EQUIVALENCE(LTGRES, LTGREC(LTGRES_OFF))
        EQUIVALENCE(LTGAPL, LTGREC(LTGAPL_OFF))
        EQUIVALENCE(LTGMIN, LTGREC(LTGMIN_OFF))
        EQUIVALENCE(LTGSER, LTGREC(LTGSER_OFF))
        EQUIVALENCE(LTGOPA, LTGREC(LTGOPA_OFF))
        EQUIVALENCE(LTGPRC, LTGREC(LTGPRC_OFF))
        EQUIVALENCE(LTGMAX, LTGREC(LTGMAX_OFF))
        EQUIVALENCE(LTGMLT, LTGREC(LTGMLT_OFF))
        EQUIVALENCE(LTGMDS, LTGREC(LTGMDS_OFF))
        EQUIVALENCE(LTTDIV, LTGREC(LTTDIV_OFF))
        EQUIVALENCE(LTGMAT, LTGREC(LTGMAT_OFF))
        EQUIVALENCE(LTGPER, LTGREC(LTGPER_OFF))
        EQUIVALENCE(LTGSPR, LTGREC(LTGSPR_OFF))
        EQUIVALENCE(LTGTSR, LTGREC(LTGTSR_OFF))
        EQUIVALENCE(LTGREV, LTGREC(LTGREV_OFF))
        EQUIVALENCE(LTGNMS, LTGREC(LTGNMS_OFF))
        EQUIVALENCE(LTGCLB, LTGREC(LTGCLB_OFF))
        EQUIVALENCE(LTGASH, LTGREC(LTGASH_OFF))
        EQUIVALENCE(LTGFRG, LTGREC(LTGFRG_OFF))
        EQUIVALENCE(LTGWRF, LTGREC(LTGWRF_OFF))
        EQUIVALENCE(LTGBST, LTGREC(LTGBST_OFF))
        EQUIVALENCE(LTGBAL, LTGREC(LTGBAL_OFF))
        EQUIVALENCE(LTGEVN, LTGREC(LTGEVN_OFF))
        EQUIVALENCE(LTGEXT, LTGREC(LTGEXT_OFF))
        EQUIVALENCE(LTGRGT, LTGREC(LTGRGT_OFF))
        EQUIVALENCE(LTGROD, LTGREC(LTGROD_OFF))
C
