C
C
        INTEGER*4 KIKSTS_OFF, KIKCTM_OFF, KIKTIM_OFF, KIKDRW_OFF
        PARAMETER(KIKSTS_OFF=1)
        PARAMETER(KIKCTM_OFF=KIKSTS_OFF+NUMKIK)
        PARAMETER(KIKTIM_OFF=KIKCTM_OFF+NUMKIK)
        PARAMETER(KIKDRW_OFF=KIKTIM_OFF+NUMKIK)
C
        INTEGER*4 KIKBSD_OFF, KIKESD_OFF, KIKPUP_OFF, KIKUPD_OFF
        PARAMETER(KIKBSD_OFF=KIKDRW_OFF+NUMKIK)
        PARAMETER(KIKESD_OFF=KIKBSD_OFF+NUMKIK)
        PARAMETER(KIKPUP_OFF=KIKESD_OFF+NUMKIK)
        PARAMETER(KIKUPD_OFF=KIKPUP_OFF+NUMKIK)
C
        INTEGER*4 KIKDAT_OFF, KIKADV_OFF, KIKSAL_OFF, KIKSHR_OFF
        PARAMETER(KIKDAT_OFF=KIKUPD_OFF+NUMKIK)
        PARAMETER(KIKADV_OFF=KIKDAT_OFF+DATLEN*NUMKIK)
        PARAMETER(KIKSAL_OFF=KIKADV_OFF+(NUMADV*NUMKIK))
        PARAMETER(KIKSHR_OFF=KIKSAL_OFF+(KIGENT*MAXGAM*NUMKIK))
C
        INTEGER*4 KIKPAD_OFF, KIKPRG_OFF, KIKSHV_OFF, KIKPOL_OFF
        PARAMETER(KIKPAD_OFF=KIKSHR_OFF+(KIGDIV*NUMKIK))
        PARAMETER(KIKPRG_OFF=KIKPAD_OFF+(KIGDIV*NUMKIK))
        PARAMETER(KIKSHV_OFF=KIKPRG_OFF+(KIGDIV*NUMKIK))
        PARAMETER(KIKPOL_OFF=KIKSHV_OFF+(KIGDIV*NUMKIK))
C
        INTEGER*4 KIKBRK_OFF, KIKFRZ_OFF, KIKTAX_OFF, KIKRES_OFF
        PARAMETER(KIKBRK_OFF=KIKPOL_OFF+(2*KIGDIV*NUMKIK))
        PARAMETER(KIKFRZ_OFF=KIKBRK_OFF+(KIGDIV*NUMKIK))
        PARAMETER(KIKTAX_OFF=KIKFRZ_OFF+(KIGDIV*NUMKIK))
        PARAMETER(KIKRES_OFF=KIKTAX_OFF+NUMKIK)
C
        INTEGER*4 KIKWIN_OFF, KIKHLD_OFF, KIKSER_OFF, KIKPRC_OFF
        PARAMETER(KIKWIN_OFF=KIKRES_OFF+2*NUMKIK)
        PARAMETER(KIKHLD_OFF=KIKWIN_OFF+NUMKIK)
        PARAMETER(KIKSER_OFF=KIKHLD_OFF+NUMKIK)
        PARAMETER(KIKPRC_OFF=KIKSER_OFF+NUMKIK)
C
        INTEGER*4 KIKMAX_OFF, KIKDIV_OFF, KIKSPR_OFF, KIKMAT_OFF
        PARAMETER(KIKMAX_OFF=KIKPRC_OFF+NUMKIK)
        PARAMETER(KIKDIV_OFF=KIKMAX_OFF+NUMKIK)
        PARAMETER(KIKSPR_OFF=KIKDIV_OFF+NUMKIK)
        PARAMETER(KIKMAT_OFF=KIKSPR_OFF+NUMKIK)
C
        INTEGER*4 KIKPER_OFF, KIKTSR_OFF, KIKREV_OFF, KIKSED_OFF
        PARAMETER(KIKPER_OFF=KIKMAT_OFF+(3*KIGDIV*NUMKIK))
        PARAMETER(KIKTSR_OFF=KIKPER_OFF+(KIGDIV*NUMKIK))
        PARAMETER(KIKREV_OFF=KIKTSR_OFF+(KIGDIV*NUMKIK))
        PARAMETER(KIKSED_OFF=KIKREV_OFF+NUMKIK)
C
        INTEGER*4 KIKOCT_OFF, KIKMLT_OFF, KIKWRF_OFF, KIKASH_OFF
        PARAMETER(KIKOCT_OFF = KIKSED_OFF + NUMKIK*2)
        PARAMETER(KIKMLT_OFF = KIKOCT_OFF + NUMKIK)
        PARAMETER(KIKWRF_OFF = KIKMLT_OFF + NUMKIK)
        PARAMETER(KIKASH_OFF = KIKWRF_OFF + 10*NUMKIK)
C
        INTEGER*4 KIKMIN_OFF, KIKDFF_OFF, KIKMDS_OFF, KIKBAL_OFF
        PARAMETER(KIKMIN_OFF = KIKASH_OFF + KIGDIV*NUMKIK)
        PARAMETER(KIKDFF_OFF = KIKMIN_OFF + NUMKIK)
        PARAMETER(KIKMDS_OFF = KIKDFF_OFF + NUMKIK)
        PARAMETER(KIKBAL_OFF = KIKMDS_OFF + (MAXMLTD_AVL/4)*NUMKIK)
C
        INTEGER*4 KIKSPL_OFF
        PARAMETER(KIKSPL_OFF = KIKBAL_OFF + MAXDRW*NUMKIK)
C
        INTEGER*4 KIKFRE_OFF
        PARAMETER(KIKFRE_OFF = KIKSPL_OFF + NUMKIK)
C        
        INTEGER*4 KIKSEC_SIZE, KIKSEC_NUM, KIKLEN
        PARAMETER(KIKSEC_SIZE = 2048)
        PARAMETER(KIKSEC_NUM = NINT(FLOAT((KIKFRE_OFF-2)/
     *                         KIKSEC_SIZE)+.5))
        PARAMETER(KIKLEN = KIKSEC_SIZE * KIKSEC_NUM)
C
C
        COMMON /KIKCOM/
     *   KIKSTS(NUMKIK),
     *   KIKCTM(NUMKIK),
     *   KIKTIM(NUMKIK),
     *   KIKDRW(NUMKIK),
     *   KIKBSD(NUMKIK),
     *   KIKESD(NUMKIK),
     *   KIKPUP(NUMKIK),
     *   KIKUPD(NUMKIK),
     *   KIKDAT(DATLEN,NUMKIK),
     *   KIKADV(NUMADV,NUMKIK),
     *   KIKSAL(KIGENT,MAXGAM,NUMKIK),
     *   KIKSHR(KIGDIV,NUMKIK)
        COMMON /KIKCOM/
     *   KIKPAD(KIGDIV,NUMKIK),
     *   KIKPRG(KIGDIV,NUMKIK),
     *   KIKSHV(KIGDIV,NUMKIK),
     *   KIKPOL(2,KIGDIV,NUMKIK),
     *   KIKBRK(KIGDIV,NUMKIK),
     *   KIKFRZ(KIGDIV,NUMKIK),
     *   KIKTAX(NUMKIK),
     *   KIKRES(2,NUMKIK),
     *   KIKWIN(NUMKIK),
     *   KIKHLD(NUMKIK),
     *   KIKSER(NUMKIK),
     *   KIKPRC(NUMKIK),
     *   KIKMAX(NUMKIK)
        COMMON /KIKCOM/
     *   KIKDIV(NUMKIK),
     *   KIKSPR(NUMKIK),
     *   KIKMAT(3,KIGDIV,NUMKIK),
     *   KIKPER(KIGDIV,NUMKIK),
     *   KIKTSR(KIGDIV,NUMKIK),
     *   KIKREV(NUMKIK),
     *   KIKSED(2,NUMKIK),
     *   KIKOCT(NUMKIK),
     *   KIKMLT(NUMKIK),
     *   KIKWRF(10,NUMKIK),
     *   KIKASH(KIGDIV,NUMKIK),
     *   KIKMIN(NUMKIK),
     *   KIKDFF(NUMKIK),
     *   KIKMDS(MAXMLTD_AVL,NUMKIK),
     *   KIKBAL(MAXDRW,NUMKIK)
         COMMON /KIKCOM/
     *   KIKSPL(NUMKIK),
     *   KIKFRE(KIKLEN - KIKFRE_OFF + 1)
C
        INTEGER*4 KIKSTS, KIKCTM, KIKTIM, KIKDRW, KIKBSD, KIKESD
        INTEGER*4 KIKPUP, KIKUPD, KIKDAT, KIKADV, KIKSAL, KIKSHR
        INTEGER*4 KIKPAD, KIKPRG, KIKSHV, KIKPOL, KIKBRK, KIKFRZ
        INTEGER*4 KIKTAX, KIKRES, KIKWIN, KIKHLD, KIKSER, KIKPRC
        INTEGER*4 KIKMAX, KIKDIV, KIKSPR, KIKMAT, KIKPER, KIKTSR
        INTEGER*4 KIKREV, KIKSED, KIKOCT, KIKMLT, KIKDFF, KIKFRE
        INTEGER*4 KIKWRF, KIKASH, KIKMIN, KIKBAL
        INTEGER*4 KIKSPL
        BYTE      KIKMDS
C
C EQUIVALENCE FOR CHECKPOINT
C
        INTEGER*4 KIKCSB
        PARAMETER(KIKCSB=KIKSEC_NUM)
        INTEGER*4 KIKBLK(2048,KIKCSB)
        EQUIVALENCE (KIKBLK(1,1),KIKSTS(1))
C
C
        INTEGER*4        FRST_KIKCOM(2)
        EQUIVALENCE     (FRST_KIKCOM, KIKSTS)
        INTEGER*4        LAST_KIKCOM
        EQUIVALENCE     (LAST_KIKCOM, KIKBLK(2048,KIKCSB))
C
        VOLATILE /KIKCOM/
C
