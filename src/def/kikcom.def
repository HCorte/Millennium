C KICKER GAME COMMON
C
C     KIKSTS(NUMKIK)                 ;STATUS
C     KIKCTM(NUMKIK)                 ;ACTUAL TIME CLOSED
C     KIKTIM(NUMKIK)                 ;TIME WHEN GAME SHOULD CLOSE
C     KIKDRW(NUMKIK)                 ;DRAW NUMBER
C     KIKBSD(NUMKIK)                 ;KICKER BEGGINING DRAW DATE
C     KIKESD(NUMKIK)                 ;KICKER ENDING DRAW DATE
C     KIKPUP(NUMKIK)                 ;LAST PURGE UPDATE
C     KIKUPD(NUMKIK)                 ;LAST FILE UPDATE
C     KIKDAT(DATLEN,NUMKIK)          ;DRAW DATES
C     KIKADV(NUMADV,NUMKIK)          ;ADVANCE DRAW DATES
C     KIKSAL(KIGENT,MAXGAM,NUMKIK)   ;SALES DATA
C     KIKSHR(KIGDIV,NUMKIK)          ;SHARES
C     KIKPAD(KIGDIV,NUMKIK)          ;AMOUNT PAID
C     KIKPRG(KIGDIV,NUMKIK)          ;AMOUNT PURGED
C     KIKSHV(KIGDIV,NUMKIK)          ;SHARE VALUES
C     KIKPOL(2,KIGDIV,NUMKIK)        ;POOL
C     KIKBRK(KIGDIV,NUMKIK)          ;BREAKAGE
C     KIKFRZ(KIGDIV,NUMKIK)          ;DIVISION PAYOUT FROZEN FLAGS
C     KIKTAX(NUMKIK)                 ;LOTTERY TAX
C     KIKRES(2,NUMKIK)               ;RESERVE POOL
C     KIKWIN(NUMKIK)                 ;WINNING NUMBER
C     KIKHLD(NUMKIK)                 ;WINNING NUMBER HOLD
C     KIKSER(NUMKIK)                 ;LAST SERIAL NUMBER
C     KIKPRC(NUMKIK)                 ;PRICE/BOARD
C     KIKMAX(NUMKIK)                 ;MAXIMUM NUMBER BET
C     KIKDIV(NUMKIK)                 ;NUMBER OF DIVISIONS
C     KIKSPR(NUMKIK)                 ;SALES POOL PERCENTAGE
C     KIKMAT(3,KIGDIV,NUMKIK)        ;WINNERS MATCH TABLE
C     KIKPER(KIGDIV,NUMKIK)          ;DIVISION POOL PERCENTAGE
C     KIKTSR(KIGDIV,NUMKIK)          ;TOTAL SHARE COUNT
C     KIKREV(NUMKIK)                 ;KICKER REVISION NUMBER
C     KIKSED(2,NUMKIK)               ;KICKER NUMBER SEEDS
C     KIKOCT(NUMKIK)                 ;# OF OCTAL DIGITS (FOR UNIRAN)
C     KIKMLT(NUMKIK)                 ;KICKER MULTI-DRAW FLAG
C     KIKWRF(10,NUMKIK)              ;WIN RESERVE FUND TABLE
C     KIKASH(KIGDIV,NUMKIK)          ;ADDITIONAL POOL BY DIV
C     KIKMIN(NUMKIK)                 ;MINIMUM JACKPOT
C     KIKDFF(NUMKIK)                 ;DOUBLE JOKERI FREE FLAG                   
C     KIKMDS(MAXMLTD_AVL,NUMKIK)     ;MULTI DRAW SELECTED TABLE
C                                     (52 BYTE / 4 = 13)
C     KIKBAL(MAXDRW,NUMKIK)          ;BALANCE SALES INFO. BY DRAWC
C     KIKSPL(NUMKIK)		      ;SPECIAL POOL, ADDED TO TOTAL POOL -
C                                     NOT TO DIV1 POOL AS OTHER POOL MONEY
C     KIKFRE(CALCULATED)              ;FREE SPACE
C
C
C
C+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
