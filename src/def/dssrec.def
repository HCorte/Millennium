C
C
	INTEGER*4 DSSSTS,DSSWEK,DSSDAT,DSSBSD,DSSESD,DSSCTM,DSSTIM
	INTEGER*4 DSSSAL,DSSPAD,DSSPRG,DSSREF,DSSWON,DSSPOL(2),DSSTPL
	INTEGER*4 DSSTBK,DSSBRK(2),DSSABW,DSSODS,DSSWIN(2,3),DSSHLD(2,3)
	INTEGER*4 DSSTAX,DSSOTX(NUMTOT)
	INTEGER*4 DSSMID(NUMTOT),DSSUTX(NUMTOT),DSSHST,DSSORM(NUMTOT)
	INTEGER*4 DSSMNM(SSNMS_LEN/4),DSSSNM(SSNMS_LEN/4,3),DSSPFN(5)
	INTEGER*4 DSSPOF(5),DSSEST(3)
	INTEGER*4 DSSDRW,DSSSER,DSSPRF
	INTEGER*4 DSSREV,DSSTYP,DSSPRC,DSSPUP,DSSUPD
	INTEGER*4 DSSSPR,DSSDES(SSDES_LEN/4)
        INTEGER*4 DSSWRO(NUMTOT,2),DSSWRA(NUMTOT,2),DSSWPO(NUMTOT,2)
        INTEGER*4 DSSWPA(NUMTOT,2),DSSWPR(NUMTOT,2)
        INTEGER*4 DSSECD(3),DSSECT(3),DSSLAT(NUMLAT)
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
	INTEGER*4 DSSSTS_OFF, DSSWEK_OFF, DSSDAT_OFF, DSSDRW_OFF
	PARAMETER(DSSSTS_OFF = 1)					!001
	PARAMETER(DSSWEK_OFF = DSSSTS_OFF + 1)				!002
	PARAMETER(DSSDAT_OFF = DSSWEK_OFF + 1)				!003
	PARAMETER(DSSDRW_OFF = DSSDAT_OFF + 1)				!004
C
	INTEGER*4 DSSBSD_OFF, DSSESD_OFF, DSSPUP_OFF, DSSUPD_OFF
	PARAMETER(DSSBSD_OFF = DSSDRW_OFF + 1)				!005
	PARAMETER(DSSESD_OFF = DSSBSD_OFF + 1)				!006
	PARAMETER(DSSPUP_OFF = DSSESD_OFF + 1)				!007
	PARAMETER(DSSUPD_OFF = DSSPUP_OFF + 1)				!008
C
	INTEGER*4 DSSCTM_OFF, DSSTIM_OFF, DSSSER_OFF, DSSSAL_OFF
	PARAMETER(DSSCTM_OFF = DSSUPD_OFF + 1)				!009
	PARAMETER(DSSTIM_OFF = DSSCTM_OFF + 1)				!010
	PARAMETER(DSSSER_OFF = DSSTIM_OFF + 1)				!011
	PARAMETER(DSSSAL_OFF = DSSSER_OFF + 1)				!012
C
	INTEGER*4 DSSPAD_OFF, DSSPRG_OFF, DSSPRF_OFF, DSSREF_OFF
	PARAMETER(DSSPAD_OFF = DSSSAL_OFF + 1)				!013
	PARAMETER(DSSPRG_OFF = DSSPAD_OFF + 1)				!014
	PARAMETER(DSSPRF_OFF = DSSPRG_OFF + 1)				!015
	PARAMETER(DSSREF_OFF = DSSPRF_OFF + 1)				!016
C
	INTEGER*4 DSSWON_OFF, DSSPOL_OFF, DSSTPL_OFF, DSSTBK_OFF
	PARAMETER(DSSWON_OFF = DSSREF_OFF + 1)				!017
	PARAMETER(DSSPOL_OFF = DSSWON_OFF + 1)				!018
	PARAMETER(DSSTPL_OFF = DSSPOL_OFF + 2)				!020
	PARAMETER(DSSTBK_OFF = DSSTPL_OFF + 1)				!021
C
	INTEGER*4 DSSBRK_OFF, DSSABW_OFF, DSSODS_OFF, DSSWIN_OFF
	PARAMETER(DSSBRK_OFF = DSSTBK_OFF + 1)				!022
	PARAMETER(DSSABW_OFF = DSSBRK_OFF + 2)				!024
	PARAMETER(DSSODS_OFF = DSSABW_OFF + 1)				!025
	PARAMETER(DSSWIN_OFF = DSSODS_OFF + 1)				!026
C
	INTEGER*4 DSSHLD_OFF, DSSTAX_OFF, DSSOTX_OFF
	PARAMETER(DSSHLD_OFF = DSSWIN_OFF + 6)				!031
	PARAMETER(DSSTAX_OFF = DSSHLD_OFF + 6)				!037
	PARAMETER(DSSOTX_OFF = DSSTAX_OFF + 1)
C
	INTEGER*4 DSSMID_OFF, DSSUTX_OFF, DSSHST_OFF, DSSORM_OFF
	PARAMETER(DSSMID_OFF = DSSOTX_OFF + NUMTOT)			!039
	PARAMETER(DSSUTX_OFF = DSSMID_OFF + NUMTOT)			!041
	PARAMETER(DSSHST_OFF = DSSUTX_OFF + NUMTOT)			!043
	PARAMETER(DSSORM_OFF = DSSHST_OFF + 1)				!044
C
	INTEGER*4 DSSMNM_OFF, DSSSNM_OFF, DSSREV_OFF, DSSTYP_OFF
        INTEGER*4 DSSDES_OFF, DSSEST_OFF
	PARAMETER(DSSMNM_OFF = DSSORM_OFF + NUMTOT)			!046
	PARAMETER(DSSDES_OFF = DSSMNM_OFF + SSNMS_LEN/4)                !053 
	PARAMETER(DSSSNM_OFF = DSSDES_OFF + SSDES_LEN/4)		!074
	PARAMETER(DSSEST_OFF = DSSSNM_OFF + SSNMS_LEN/4*3)              !095
	PARAMETER(DSSREV_OFF = DSSEST_OFF + 3)				!098
	PARAMETER(DSSTYP_OFF = DSSREV_OFF + 1)				!099
C
	INTEGER*4 DSSPFN_OFF, DSSPOF_OFF, DSSPRC_OFF, DSSSPR_OFF
	PARAMETER(DSSPFN_OFF = DSSTYP_OFF + 1)				!100
	PARAMETER(DSSPOF_OFF = DSSPFN_OFF + 5)				!105
	PARAMETER(DSSPRC_OFF = DSSPOF_OFF + 5)				!110
	PARAMETER(DSSSPR_OFF = DSSPRC_OFF + 1)				!111
C
        INTEGER*4 DSSWRO_OFF, DSSWRA_OFF, DSSWPO_OFF, DSSWPA_OFF
        INTEGER*4 DSSWPR_OFF, DSSECD_OFF, DSSECT_OFF
        PARAMETER(DSSWRO_OFF = DSSSPR_OFF + 1)                          !112
        PARAMETER(DSSWRA_OFF = DSSWRO_OFF + 2*NUMTOT)                   !116
        PARAMETER(DSSWPO_OFF = DSSWRA_OFF + 2*NUMTOT)                   !120
        PARAMETER(DSSWPA_OFF = DSSWPO_OFF + 2*NUMTOT)                   !124
        PARAMETER(DSSWPR_OFF = DSSWPA_OFF + 2*NUMTOT)                   !128
        PARAMETER(DSSECD_OFF = DSSWPR_OFF + 2*NUMTOT)
        PARAMETER(DSSECT_OFF = DSSECD_OFF + 3)
	INTEGER*4 DSSLAT_OFF
	PARAMETER(DSSLAT_OFF =  DSSECT_OFF + 3)

        INTEGER*4 DSSFRE_OFF
        PARAMETER(DSSFRE_OFF = DSSLAT_OFF + NUMLAT)
C
C CALCULATE FREE SPACE BASED ON MACHINE DEPENDENT SECTOR SIZES.
C SHOULD ALLWAYS ADD UP TO A EVEN SECTOR SIZE BYTES (VAX 512).
C NOTE! IN CALCULATIONS, FRE_OFF-2 IS DONE TO TAKE CARE OF WHEN THE DATA SIZE
C       ENDS ON A EVEN SECTOR SIZE.
C
        INTEGER*4 DSSSEC_VAX, DSSSEC_NUM
        PARAMETER (DSSSEC_VAX = SECSIZE/4)
	PARAMETER (DSSSEC_NUM = NINT(FLOAT((DSSFRE_OFF-2)/DSSSEC_VAX)+.5))
C
        INTEGER*4 DSSLEN, DSSSEC
        PARAMETER(DSSLEN = DSSSEC_VAX * DSSSEC_NUM)        ! even sector size
        PARAMETER(DSSSEC = DSSLEN/64)
C
        INTEGER*4 DSSREC(DSSLEN)
C
	EQUIVALENCE(DSSSTS,DSSREC(DSSSTS_OFF))
	EQUIVALENCE(DSSWEK,DSSREC(DSSWEK_OFF))
	EQUIVALENCE(DSSDAT,DSSREC(DSSDAT_OFF))
	EQUIVALENCE(DSSDRW,DSSREC(DSSDRW_OFF))
	EQUIVALENCE(DSSBSD,DSSREC(DSSBSD_OFF))
	EQUIVALENCE(DSSESD,DSSREC(DSSESD_OFF))
	EQUIVALENCE(DSSPUP,DSSREC(DSSPUP_OFF))
	EQUIVALENCE(DSSUPD,DSSREC(DSSUPD_OFF))
	EQUIVALENCE(DSSCTM,DSSREC(DSSCTM_OFF))
	EQUIVALENCE(DSSTIM,DSSREC(DSSTIM_OFF))
	EQUIVALENCE(DSSSER,DSSREC(DSSSER_OFF))
	EQUIVALENCE(DSSSAL,DSSREC(DSSSAL_OFF))
	EQUIVALENCE(DSSPAD,DSSREC(DSSPAD_OFF))
	EQUIVALENCE(DSSPRG,DSSREC(DSSPRG_OFF))
	EQUIVALENCE(DSSPRF,DSSREC(DSSPRF_OFF))
	EQUIVALENCE(DSSREF,DSSREC(DSSREF_OFF))
	EQUIVALENCE(DSSWON,DSSREC(DSSWON_OFF))
	EQUIVALENCE(DSSPOL,DSSREC(DSSPOL_OFF))
	EQUIVALENCE(DSSTPL,DSSREC(DSSTPL_OFF))
	EQUIVALENCE(DSSTBK,DSSREC(DSSTBK_OFF))
	EQUIVALENCE(DSSBRK,DSSREC(DSSBRK_OFF))
	EQUIVALENCE(DSSABW,DSSREC(DSSABW_OFF))
	EQUIVALENCE(DSSODS,DSSREC(DSSODS_OFF))
	EQUIVALENCE(DSSWIN,DSSREC(DSSWIN_OFF))
	EQUIVALENCE(DSSHLD,DSSREC(DSSHLD_OFF))
	EQUIVALENCE(DSSTAX,DSSREC(DSSTAX_OFF))
	EQUIVALENCE(DSSOTX,DSSREC(DSSOTX_OFF))
	EQUIVALENCE(DSSMID,DSSREC(DSSMID_OFF))
	EQUIVALENCE(DSSUTX,DSSREC(DSSUTX_OFF))
	EQUIVALENCE(DSSHST,DSSREC(DSSHST_OFF))
	EQUIVALENCE(DSSORM,DSSREC(DSSORM_OFF))
	EQUIVALENCE(DSSMNM,DSSREC(DSSMNM_OFF))
	EQUIVALENCE(DSSDES,DSSREC(DSSDES_OFF))
	EQUIVALENCE(DSSSNM,DSSREC(DSSSNM_OFF))
	EQUIVALENCE(DSSEST,DSSREC(DSSEST_OFF))
	EQUIVALENCE(DSSREV,DSSREC(DSSREV_OFF))
	EQUIVALENCE(DSSTYP,DSSREC(DSSTYP_OFF))
	EQUIVALENCE(DSSPFN,DSSREC(DSSPFN_OFF))
	EQUIVALENCE(DSSPOF,DSSREC(DSSPOF_OFF))
        EQUIVALENCE(DSSPRC,DSSREC(DSSPRC_OFF))
	EQUIVALENCE(DSSSPR,DSSREC(DSSSPR_OFF))
        EQUIVALENCE(DSSWRO,DSSREC(DSSWRO_OFF))
        EQUIVALENCE(DSSWRA,DSSREC(DSSWRA_OFF))
        EQUIVALENCE(DSSWPO,DSSREC(DSSWPO_OFF))
        EQUIVALENCE(DSSWPA,DSSREC(DSSWPA_OFF))
        EQUIVALENCE(DSSWPR,DSSREC(DSSWPR_OFF))
        EQUIVALENCE(DSSECD,DSSREC(DSSECD_OFF))
        EQUIVALENCE(DSSECT,DSSREC(DSSECT_OFF))
        EQUIVALENCE(DSSLAT,DSSREC(DSSLAT_OFF))
