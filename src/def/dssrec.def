C DSSREC.DEF
C
C V02 30-JUN-2000 UXN DSSLAT added.
C V01 ??-???-???? RXK Initial release. 
C
C
C SUPERSCORE GAME DATA FILE RECORD LAYOUT (DISK)
C
C
C     DSSSTS                         ; GAME STATUS
C     DSSWEK                         ; WEEK NUMBER
C     DSSDAT                         ; DRAW DATE
C     DSSDRW                         ; DRAW NUMBER
C     DSSBSD                         ; BEGINNING SALES DATE
C     DSSESD                         ; ENDING SALES DATE
C     DSSPUP                         ; LAST PRUGE UPDATE  (FILE ONLY)
C     DSSUPD                         ; LAST FILE UPDATE   (FILE ONLY)
C     DSSCTM                         ; ACTUAL TIME WHEN CLOSED
C     DSSTIM                         ; TIME WHEN GAME SHOULD CLOSE
C     DSSSER                         ; LAST SERIAL NUMBER
C     DSSSAL                         ; TOTAL SALES
C     DSSPAD                         ; PRIZES PAID + REFUNDS
C     DSSPRG                         ; PRIZES PURGED
C     DSSPRF                         ; PAID REFUNDS
C     DSSREF                         ; TOTAL REFUND AMOUNT
C     DSSWON                         ; PRIZES WON
C     DSSPOL(2)                      ; ROLL POOL
C     DSSTPL                         ; TOTAL PRIZE POOL
C     DSSTBK                         ; TOTAL BREAKAGE
C     DSSBRK(2)                      ; WINNING ROUNDING POT
C     DSSABW                         ; AMOUNT BET ON WINNING SCORE
C     DSSODS                         ; WINNING ODDS
C     DSSWIN(2,3)                    ; WINNING RESULTS
C     DSSHLD(2,3)                    ; WINNING RESULTS HOLD
C     DSSTAX                         ; WINNING TAXES
C     DSSOTX(NUMTOT)                 ; OVER TAX LIMIT
C     DSSMID(NUMTOT)                 ; >REDMIN <REDSSX
C     DSSUTX(NUMTOT)                 ; UNDER TAX LIMIT
C     DSSHST                         ; HIGHEST WINNER FOR THIS GAME
C     DSSORM(NUMTOT)                 ; OVER REDSSX
C     DSSMNM(SSNMS_LEN/4)            ; MASTER EVENT NAME
C     DSSDES(SSDES_LEN/4)            ; EVENT DESCRIPTION
C     DSSSNM(SSNMS_LEN/4,3)          ; EVENT NAMES
C     DSSEST(3)                      ; EVENT STATUS'S
C     DSSREV                         ; REV #
C     DSSTYP                         ; GAME TYPE 1=SOC. 2=HOCK.
C     DSSPFN(5)                      ; POOL FILE NAME
C     DSSPOF(5)                      ; POOL OVERFLOW FILE NAME
C     DSSPRC                         ; MINIMUM STAKE
C     DSSSPR                         ; POOL PERCENTAGE
C     DSSWRO(NUMTOT,2)               ; WINNING: COUNT,AMOUNT REFUNDS ONLY
C     DSSWRA(NUMTOT,2)               ; WINNING: COUNT,AMOUNT REFUNDS ALTOGETHER
C     DSSWPO(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES ONLY
C     DSSWPA(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES ALTOGETHER
C     DSSWPR(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES AND REFUNDS
C                                    ; (1=PRWON,2=PRPAD)
C     DSSECD(3)                      ; EVENT CLOSE DATE
C     DSSECT(3)                      ; EVENT CLOSE TIME
C     DSSLAT(NUMLAT)                 ; EVENT STARTED BEFORE GAME WAS CLOSED
C                                    ; TICKETS PLAYED LATER SHOULD BE REFUNDED.
C
C
C SUPERSCORE GAME FILE PARAMETERS
C
C+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
