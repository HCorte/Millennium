C
C
        INTEGER*4 OMAXGAM
        PARAMETER(OMAXGAM=10)
C
	INTEGER*4 ORDFPOL_OFF
	INTEGER*4 ORDFADD_OFF
	INTEGER*4 ORDFUSE_OFF
	INTEGER*4 ORDFUDW_OFF
	INTEGER*4 ORDFADW_OFF
	INTEGER*4 ORDFADR_OFF
	INTEGER*4 ORDFUSD_OFF
	INTEGER*4 ORDFDRW_OFF
        INTEGER*4 ORDF_WRFTAB_OFF
        INTEGER*4 ORDF_WRFCUD_OFF
	INTEGER*4 ORDF_SPTPOLDIV_OFF
	INTEGER*4 ORDFFRE_OFF

	PARAMETER( ORDFPOL_OFF = 1)
	PARAMETER( ORDFADD_OFF = ORDFPOL_OFF +OMAXGAM)
	PARAMETER( ORDFUSE_OFF = ORDFADD_OFF +OMAXGAM)
	PARAMETER( ORDFUDW_OFF = ORDFUSE_OFF +OMAXGAM)
	PARAMETER( ORDFADW_OFF = ORDFUDW_OFF +OMAXGAM)
	PARAMETER( ORDFADR_OFF = ORDFADW_OFF +OMAXGAM)
	PARAMETER( ORDFUSD_OFF = ORDFADR_OFF +OMAXGAM)
	PARAMETER( ORDFDRW_OFF = ORDFUSD_OFF +OMAXGAM)
        PARAMETER( ORDF_WRFTAB_OFF = ORDFDRW_OFF +OMAXGAM)
        PARAMETER( ORDF_WRFCUD_OFF = ORDF_WRFTAB_OFF +OMAXGAM*10)
	PARAMETER( ORDF_SPTPOLDIV_OFF = ORDF_WRFCUD_OFF +OMAXGAM)
	PARAMETER( ORDFFRE_OFF = ORDF_SPTPOLDIV_OFF + NUMSPT*SPGDIV)
C
        INTEGER*4 ORDFLEN
        PARAMETER(ORDFLEN = ORDFFRE_OFF-1)
C
        INTEGER*4 OIRDFSEC
        PARAMETER(OIRDFSEC = ORDFLEN/128 + 1)
C
        INTEGER*4 ORDFSEC
        PARAMETER(ORDFSEC = OIRDFSEC*2)
C
        INTEGER*4 ORDFFRESIZ
        PARAMETER(ORDFFRESIZ = ORDFSEC * 64 - ORDFLEN)
C
        INTEGER*4 ORDFREC(ORDFLEN+ORDFFRESIZ)
C
	INTEGER*4 ORDFPOL(OMAXGAM)
	INTEGER*4 ORDFADD(OMAXGAM)
	INTEGER*4 ORDFUSE(OMAXGAM)
	INTEGER*4 ORDFUDW(OMAXGAM)
	INTEGER*4 ORDFADW(OMAXGAM)
	INTEGER*4 ORDFADR(OMAXGAM)
	INTEGER*4 ORDFUSD(OMAXGAM)
	INTEGER*4 ORDFDRW(OMAXGAM)
        INTEGER*4 ORDF_WRFTAB(10,OMAXGAM)
        INTEGER*4 ORDF_WRFCUD(OMAXGAM)
        INTEGER*4 ORDF_SPTPOLDIV(NUMSPT,SPGDIV)
	INTEGER*4 ORDFFRE(ORDFFRESIZ)

	EQUIVALENCE (ORDFPOL(1),ORDFREC(ORDFPOL_OFF))
	EQUIVALENCE (ORDFADD(1),ORDFREC(ORDFADD_OFF))
	EQUIVALENCE (ORDFUSE(1),ORDFREC(ORDFUSE_OFF))
	EQUIVALENCE (ORDFUDW(1),ORDFREC(ORDFUDW_OFF))
	EQUIVALENCE (ORDFADW(1),ORDFREC(ORDFADW_OFF))
	EQUIVALENCE (ORDFADR(1),ORDFREC(ORDFADR_OFF))
	EQUIVALENCE (ORDFUSD(1),ORDFREC(ORDFUSD_OFF))
	EQUIVALENCE (ORDFDRW(1),ORDFREC(ORDFDRW_OFF))
        EQUIVALENCE (ORDF_WRFTAB(1,1),ORDFREC(ORDF_WRFTAB_OFF))
        EQUIVALENCE (ORDF_WRFCUD(1),ORDFREC(ORDF_WRFCUD_OFF))
        EQUIVALENCE (ORDF_SPTPOLDIV(1,1),ORDFREC(ORDF_SPTPOLDIV_OFF))
	EQUIVALENCE (ORDFFRE(1),ORDFREC(ORDFFRE_OFF))
