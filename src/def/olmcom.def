        COMMON /OLMCOM/
     *    OLMS_ATTACHSTS,
     *    OLMS_ATTACHDAT,
     *    OLMS_ATTACHTIM,
     *    OLMS_DETACHFLG,
     *    OLMS_DETACHDAT,
     *    OLMS_DETACHTIM
C
        INTEGER*4 OLMS_ATTACHSTS                                               !ATTACH STATUS (COMOLM IS OR IS NOT ATTACHED TO MESSAGEQ SERVER IN OLIMPO SYSTEM)
        INTEGER*4 OLMS_ATTACHDAT(8)                                             !LAST DATE ATTACHED TO MESSAGEQ SERVER HOSTED IN OLIMPO SYSTEM
        INTEGER*4 OLMS_ATTACHTIM(2)                                             !LAST TIME ATTACHED TO MESSAGEQ SERVER HOSTED IN OLIMPO SYSTEM (HH24:MI:SS)
        INTEGER*4 OLMS_DETACHFLG                                                !TIME LAST DETACH FLAG (A DETACH HAS OCCURRED)
        INTEGER*4 OLMS_DETACHDAT(8)                                             !LAST DATE DETACHED FROM MESSAGEQ SERVER HOSTED IN OLIMPO SYSTEM
        INTEGER*4 OLMS_DETACHTIM(2)                                             !LAST TIME DETACHED FROM MESSAGEQ SERVER HOSTED IN OLIMPO SYSTEM (HH24:MI:SS)


        COMMON /OLMCOM/
     *    OLMS_TOTOKYPUT,
     *    OLMS_TOTOKYGET,
     *    OLMS_TOTERRPUT,
     *    OLMS_TOTERRGET,
     *    OLMS_TOTTMOPUT,
     *    OLMS_GETMESSUC,
     *    OLMS_PUTMESSUC,
     *    OLMS_GETMESFAI,
     *    OLMS_PUTMESFAI,
     *    OLMS_GETTERFAI
C     *    OLMS_TOTNOMGET
C
        INTEGER*4 OLMS_TOTOKYPUT                                                !TOTAL # OF MESSAGES SUCCESSFULLY SENT TO OLIMPO SYSTEM
        INTEGER*4 OLMS_TOTOKYGET                                                !TOTAL # OF MESSAGES SUCCESSFULLY RECEIVED FROM OLIMPO SYSTEM
        INTEGER*4 OLMS_TOTERRPUT                                                !TOTAL # OF ERRORS IN PUTTING MESSAGES TO OLIMPO SYSTEM (ERROR WHILE PUTTING MESSAGES)
        INTEGER*4 OLMS_TOTERRGET                                                !TOTAL # OF ERRORS IN GETTING MESSAGES FROM OLIMPO SYSTEM (ERROR WHILE GETTING MESSAGES)
        INTEGER*4 OLMS_TOTTMOPUT                                                !TOTAL # OF MESSAGES NOT SENT TO OLIMPO SYSTEM BECAUSE OF A TIME OUT ERROR IN PUTTING THE MESSAGE        
        INTEGER*8 OLMS_GETMESSUC, OLMS_PUTMESSUC
        INTEGER*8 OLMS_GETMESFAI, OLMS_PUTMESFAI
        INTEGER*8 OLMS_GETTERFAI
C        INTEGER*4 OLMS_TOTNOMGET                                                !TOTAL # OF NO MORE MESSAGE IN MESSAGEQ THAT COMOLM GET THIS STATE USEFULL TO DETECT IF OLIMPO NOT PUTTING MESSAGES
C
C MESSAGEQ STATUS
C
        INTEGER*4 PAMS__NOMOREMSG
        PARAMETER (PAMS__NOMOREMSG = 139756347)
        INTEGER*4 PAMS__SUCCESS
        PARAMETER (PAMS__SUCCESS = 1)
        INTEGER*4 PAMS__DECLARED
        PARAMETER (PAMS__DECLARED = 139761972)
        INTEGER*4 PAMS__NOLINK
        PARAMETER (PAMS__NOLINK = 139759394)
        INTEGER*4 PAMS__LINK_DOWN
        PARAMETER (PAMS__LINK_DOWN = 139759266)
        INTEGER*4 PAMS__NOTACTIVE
        PARAMETER (PAMS__NOTACTIVE = 139759474)
        INTEGER*4 PAMS__NETLINKLOST
        PARAMETER (PAMS__NETLINKLOST = 139759730)
        INTEGER*4 PAMS__STOPPED
        PARAMETER (PAMS__STOPPED = 139759962)
        INTEGER*4 PAMS__NETNOLINK
        PARAMETER (PAMS__NETNOLINK = 139762140)
        INTEGER*4 PAMS__FATAL
        PARAMETER (PAMS__FATAL = 139762156)
        INTEGER*4 PAMS__NETERROR 
        PARAMETER (PAMS__NETERROR = 139759722)
        INTEGER*4 PAMS__STUB
        PARAMETER (PAMS__STUB = 139757248)
        INTEGER*4 PAMS__TIMEOUT
        PARAMETER (PAMS__TIMEOUT = 139757256)
C