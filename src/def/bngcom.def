C BNGCOM.DEF
C  
C V08 17-JAN-00 RXK Added new fields for more phases.
C V07 17-JUN-97 RXK Bingo Fullhouse changes.
C V06 11-NOV-94 HXK Removed BNGCLB, BNGNMS which were mistakenly left in
C V05 23-OCT-94 HXK Added new param, redimensioned others.
C V04 17-OCT-94 HXK Adjusted BNGMAT field
C V03 15-OCT-94 HXK Added new field for Winsel,fixed tstgam bugs
C V02 06-OCT-94 HXK Added Subgames to divisions
C V01 02-OCT-94 HXK Initial revision.
C  
C BINGO GAME COMMON
C
C     BNGSTS(NUMBGO)                 !STATUS
C     BNGCTM(NUMBGO)                 !ACTUAL TIME WHEN CLOSED
C     BNGTIM(NUMBGO)                 !TIME WHEN GAME SHOULD CLOSE
C     BNGDRW(NUMBGO)                 !DRAW NUMBER
C     BNGBSD(NUMBGO)                 !SPORT BEGGINING DRAW DATE
C     BNGESD(NUMBGO)                 !SPORT ENDING DRAW DATE
C     BNGPUP(NUMBGO)                 !LAST PURGE UPDATE
C     BNGUPD(NUMBGO)                 !LAST FILE UPDATE
C     BNGDAT(DATLEN,NUMBGO)          !DRAW DATES
C     BNGADV(NUMADV,NUMBGO)          !ADVANCE DRAW DATES
C     BNGSAL(BGOENT,NUMBGO)          !SALES DATA
C     BNGSHV(BGODIV,BGOSUB,NUMBGO)          !SHARE VALUES
C     BNGSHR(BGODIV,BGOSUB,NUMBGO)          !SHARES
C     BNGPOL(BGODIV,BGOSUB,NUMBGO)          !POOL CARRIED OVER
C     BNGPAD(BGODIV,BGOSUB,NUMBGO)          !AMOUNT PAID
C     BNGPRG(BGODIV,BGOSUB,NUMBGO)          !AMOUNT PURGED
C     BNGANU(BGODIV,BGOSUB,NUMBGO)          !ANNUITY PRIZES
C     BNGBRK(BGODIV,BGOSUB,NUMBGO)          !BREAKAGE AMOUNT BY POOL
C     BNGOSV(BGODIV,BGOSUB,NUMBGO)          !OLD SHARE VALUES
C     BNGFRZ(BGODIV,BGOSUB,NUMBGO)          !DIVISION PAYOUT FROZEN FLAGS
C     BNGWIN(BGONBR,NUMBGO)          !WINNING RESULTS
C     BNGHLD(BGONBR,NUMBGO)          !WINNING RESULTS HOLD
C     BNGWAB(BGONAB,NUMBGO)          !WINNING RESULTS (A,B)
C     BNGHAB(BGONAB,NUMBGO)          !WINNING RESULTS HOLD (A,B)
C     BNGWLN(BGONLN,NUMBGO)          !WINNING RESULTS (LUCKY)
C     BNGHLN(BGONLN,NUMBGO)          !WINNING RESULTS HOLD (LUCKY)
C     BNGFAC(NUMBGO)                 !ANNUITY FACTOR
C     BNGCUT(NUMBGO)                 !ANNUITY CUT-IN
C     BNGTAX(NUMBGO)                 !LOTTERY TAX
C     BNGRES(2,NUMBGO)               !RESERVE POOL
C     BNGAPL(NUMBGO)                 !ADDITIONAL POOL
C     BNGMIN(NUMBGO)                 !MINIMUM POOL
C     BNGSER(NUMBGO)                 !LAST SERIAL NUMBER
C     BNGOPA(NUMBGO)                 !OVER-RIDE POOL AMOUNT
C     BNGPRC(NUMBGO)                 !PRICE/SINGLE COMBINATION
C     BNGMAX(NUMBGO)                 !NUMBER OF EVENTS (ROWS)
C     BNGMLT(NUMBGO)                 !MULTI-DRAW ENABLE FLAG
C     BNGMDS(MAXMLTD_AVL,NUMBGO)     !MULTI-DRAW SELECTED TABLE (52/4 WORDS)
C     BNGDIV(BGOSUB,NUMBGO)          !# OF DIVISIONS
C     BNGMAT(BGOMAXMAP+2+BGOCOL*BGOROW,BGOSUB,NUMBGO) !DIVISION MATCH TABLE
C                    !(row bitmaps + worst + second worst + entire 5x5 panel)
C     BNGPER(BGODIV,BGOSUB,NUMBGO)          !DIVISION PERCENTAGES
C     BNGSPR(NUMBGO)                 !SALES PERCENTAGE
C     BNGTSR(BGODIV,BGOSUB,NUMBGO)          !TOTAL SHARE COUNT
C     BNGREV(NUMBGO)                 !SPORT REVISION NUMBER
C     BNGASH(BGODIV,BGOSUB,NUMBGO)          !ADDITIONAL POOL BY DIV.
C     BNGFRG(NUMBGO)                 !FLAG FOR FREE GAME (PRICE = 0.00)
C     BNGWRF(POSTED,NUMBGO)          !WIN RESERV FUND
C     BNGBST(NUMBGO)                 !BEST MATCH FLAG (ADJUST DIVISIONS)
C     BNGBAL(MAXDRW,NUMBGO)          !BALANCE SALES INFO. BY DRAW
C     BNGSED(NUMBGO)                 !FULL HOUSE SEED
C     BNGBAS(NUMBGO)                 !LUCKY NUMBER BASE
C     BNGPHS(BGOPHS,NUMBGO)          !#'s DRAWN PER PHASE (FH)
C     BNGPHSH(BGOPHS,NUMBGO)         !#'s DRAWN PER PHASE (FH) HOLD
C     BNG1ST(BGO1ST,NUMBGO)          !#'s DRAWN FOR 1st BINGO,DOUBLE,etc. (FH)
C     BNGWST(NUMBGO)                 !#'s MATCHED FOR WORST WIN (FH)
C     BNGWSTH(NUMBGO)                !#'s MATCHED FOR WORST WIN (FH) HOLD
C     BNGMAP(BGOMAXMAP,BGOSUB,NUMBGO)      !MAX BITMAPS TO WIN ON
C     BNGMPF(BGOMAXMAP,BGOSUB,NUMBGO)      !WIN ALLOWED FOR BITMAP OVERLAP FLAG
C     BNGAMP(BGODIV,BGOMAXMAP,BGOSUB,NUMBGO) !# OF ADDITIONAL BITMAPS FOR DIVS 
C     BNGPMT(3,BGOPHS,NUMBGO)        !# OF (1=BITMAPS,2=NUMBERS,3=FIGURES)
C                                    !TO MATCH FOR A PHASE TO COMPLETE
C     BNGLOB(NUMBGO)                 !NUMBER OF LAST BINGO WITH AB BOARDS
C     BNGWS2(NUMBGO)                 !#'s MATCHED FOR SECOND WORST WIN (FH)
C     BNGWS2H(NUMBGO)                !#'s MATCHED FOR SECOND WORST WIN (FH) HOLD
C     BNGNDF(BGODIV,NUMBGO)          !#'s DRAWN FOR FIRST MATCH (FH)
C     BNGNDFH(BGODIV,NUMBGO)         !#'s DRAWN FOR FIRST MATCH (FH) HOLD
C     BNGFST(BGODIV,BGOSUB,NUMBGO)   !FLAGS SET FOR DIVISIONS OF FIRST MATCH
C     BNGOTH(BGODIV,BGOSUB,NUMBGO)   !OTHER DIV WHICH WINS WITH SAME BITMAPS
C     BNGDNR(BGODIV,NUMBGO)          !DIV NUMBERS IN LOTTERY 1..20 SCALE (FH) 
C     BNGNSP(NUMBGO)                 !NUMBER OF SUBPHASES(INCL.FULLHOUSE)
C     BNGSPH(BGOSPH,NUMBGO)          !#'s DRAWN PER SUBPHASE (FH)
C     BNGSPHH(BGOSPH,NUMBGO          !#'s DRAWN PER SUBPHASE (FH) HOLD
C
C     BNGFRE(CALCULATED)             !FREE SPACE
C
C
C
