        INTEGER*4 BNGSTS_OFF, BNGCTM_OFF, BNGTIM_OFF, BNGDRW_OFF
        PARAMETER(BNGSTS_OFF=1)
        PARAMETER(BNGCTM_OFF=BNGSTS_OFF+NUMBGO)
        PARAMETER(BNGTIM_OFF=BNGCTM_OFF+NUMBGO)
        PARAMETER(BNGDRW_OFF=BNGTIM_OFF+NUMBGO)

        INTEGER*4 BNGBSD_OFF, BNGESD_OFF, BNGPUP_OFF, BNGUPD_OFF
        PARAMETER(BNGBSD_OFF=BNGDRW_OFF+NUMBGO)
        PARAMETER(BNGESD_OFF=BNGBSD_OFF+NUMBGO)
        PARAMETER(BNGPUP_OFF=BNGESD_OFF+NUMBGO)
        PARAMETER(BNGUPD_OFF=BNGPUP_OFF+NUMBGO)

        INTEGER*4 BNGDAT_OFF, BNGADV_OFF, BNGSAL_OFF, BNGSHV_OFF
        PARAMETER(BNGDAT_OFF=BNGUPD_OFF+NUMBGO)
        PARAMETER(BNGADV_OFF=BNGDAT_OFF+DATLEN*NUMBGO)
        PARAMETER(BNGSAL_OFF=BNGADV_OFF+NUMADV*NUMBGO)
        PARAMETER(BNGSHV_OFF=BNGSAL_OFF+BGOENT*NUMBGO)

        INTEGER*4 BNGSHR_OFF, BNGPOL_OFF, BNGPAD_OFF, BNGPRG_OFF
        PARAMETER(BNGSHR_OFF=BNGSHV_OFF+BGODIV*BGOSUB*NUMBGO)
        PARAMETER(BNGPOL_OFF=BNGSHR_OFF+BGODIV*BGOSUB*NUMBGO)
        PARAMETER(BNGPAD_OFF=BNGPOL_OFF+BGODIV*BGOSUB*NUMBGO)
        PARAMETER(BNGPRG_OFF=BNGPAD_OFF+BGODIV*BGOSUB*NUMBGO)

        INTEGER*4 BNGANU_OFF, BNGBRK_OFF, BNGOSV_OFF, BNGFRZ_OFF
        PARAMETER(BNGANU_OFF=BNGPRG_OFF+BGODIV*BGOSUB*NUMBGO)
        PARAMETER(BNGBRK_OFF=BNGANU_OFF+BGODIV*BGOSUB*NUMBGO)
        PARAMETER(BNGOSV_OFF=BNGBRK_OFF+BGODIV*BGOSUB*NUMBGO)
        PARAMETER(BNGFRZ_OFF=BNGOSV_OFF+BGODIV*BGOSUB*NUMBGO)

        INTEGER*4 BNGWIN_OFF, BNGHLD_OFF
        PARAMETER(BNGWIN_OFF=BNGFRZ_OFF+BGODIV*BGOSUB*NUMBGO)
        PARAMETER(BNGHLD_OFF=BNGWIN_OFF+BGONBR*NUMBGO)

        INTEGER*4 BNGWAB_OFF, BNGHAB_OFF, BNGWLN_OFF, BNGHLN_OFF
        PARAMETER(BNGWAB_OFF=BNGHLD_OFF+BGONBR*NUMBGO)
        PARAMETER(BNGHAB_OFF=BNGWAB_OFF+BGONAB*NUMBGO)
        PARAMETER(BNGWLN_OFF=BNGHAB_OFF+BGONAB*NUMBGO)
        PARAMETER(BNGHLN_OFF=BNGWLN_OFF+BGONLN*NUMBGO)

        INTEGER*4 BNGFAC_OFF, BNGCUT_OFF
        PARAMETER(BNGFAC_OFF=BNGHLN_OFF+BGONLN*NUMBGO)
        PARAMETER(BNGCUT_OFF=BNGFAC_OFF+NUMBGO)

        INTEGER*4 BNGTAX_OFF, BNGRES_OFF, BNGAPL_OFF, BNGMIN_OFF
        PARAMETER(BNGTAX_OFF=BNGCUT_OFF+NUMBGO)
        PARAMETER(BNGRES_OFF=BNGTAX_OFF+NUMBGO)
        PARAMETER(BNGAPL_OFF=BNGRES_OFF+2*NUMBGO)
        PARAMETER(BNGMIN_OFF=BNGAPL_OFF+NUMBGO)

        INTEGER*4 BNGSER_OFF, BNGOPA_OFF, BNGPRC_OFF, BNGMAX_OFF
        PARAMETER(BNGSER_OFF=BNGMIN_OFF+NUMBGO)
        PARAMETER(BNGOPA_OFF=BNGSER_OFF+NUMBGO)
        PARAMETER(BNGPRC_OFF=BNGOPA_OFF+NUMBGO)
        PARAMETER(BNGMAX_OFF=BNGPRC_OFF+NUMBGO)

        INTEGER*4 BNGMLT_OFF, BNGDIV_OFF, BNGMAT_OFF, BNGPER_OFF
        INTEGER*4 BNGMDS_OFF
        PARAMETER(BNGMLT_OFF=BNGMAX_OFF+NUMBGO)
        PARAMETER(BNGMDS_OFF=BNGMLT_OFF+NUMBGO)
        PARAMETER(BNGDIV_OFF=BNGMDS_OFF+(MAXMLTD_AVL/4)*NUMBGO)
        PARAMETER(BNGMAT_OFF=BNGDIV_OFF+BGOSUB*NUMBGO)
        PARAMETER(BNGPER_OFF=BNGMAT_OFF+
     *                       (BGOMAXMAP+2+BGOCOL*BGOROW)*BGOSUB*NUMBGO)

        INTEGER*4 BNGSPR_OFF, BNGTSR_OFF, BNGREV_OFF
        PARAMETER(BNGSPR_OFF=BNGPER_OFF+BGODIV*BGOSUB*NUMBGO)
        PARAMETER(BNGTSR_OFF=BNGSPR_OFF+NUMBGO)
        PARAMETER(BNGREV_OFF=BNGTSR_OFF+BGODIV*BGOSUB*NUMBGO)

        INTEGER*4 BNGASH_OFF, BNGFRG_OFF, BNGWRF_OFF
        PARAMETER(BNGASH_OFF = BNGREV_OFF+NUMBGO)
        PARAMETER(BNGFRG_OFF = BNGASH_OFF + BGODIV*BGOSUB*NUMBGO)
        PARAMETER(BNGWRF_OFF = BNGFRG_OFF + NUMBGO)

        INTEGER*4 BNGBST_OFF, BNGBAL_OFF
        PARAMETER(BNGBST_OFF = BNGWRF_OFF + POSTED*NUMBGO)
        PARAMETER(BNGBAL_OFF = BNGBST_OFF + NUMBGO)
C
        INTEGER*4 BNGSED_OFF, BNGBAS_OFF, BNGPHS_OFF, BNGPHSH_OFF
        INTEGER*4 BNG1ST_OFF
        PARAMETER(BNGSED_OFF  = BNGBAL_OFF + MAXDRW*NUMBGO)
        PARAMETER(BNGBAS_OFF  = BNGSED_OFF + NUMBGO)
        PARAMETER(BNGPHS_OFF  = BNGBAS_OFF + NUMBGO)
        PARAMETER(BNGPHSH_OFF  = BNGPHS_OFF + BGOPHS*NUMBGO)
        PARAMETER(BNG1ST_OFF  = BNGPHSH_OFF + BGOPHS*NUMBGO)
C
        INTEGER*4 BNGWST_OFF, BNGMAP_OFF, BNGMPF_OFF, BNGAMP_OFF
        INTEGER*4 BNGWSTH_OFF
        PARAMETER(BNGWST_OFF  = BNG1ST_OFF + BGO1ST*NUMBGO)
        PARAMETER(BNGWSTH_OFF  = BNGWST_OFF + NUMBGO)
        PARAMETER(BNGMAP_OFF  = BNGWSTH_OFF + NUMBGO)
        PARAMETER(BNGMPF_OFF  = BNGMAP_OFF + BGOMAXMAP*BGOSUB*NUMBGO)
        PARAMETER(BNGAMP_OFF  = BNGMPF_OFF + BGOMAXMAP*BGOSUB*NUMBGO)

       
        INTEGER*4 BNGPMT_OFF
        PARAMETER(BNGPMT_OFF  = BNGAMP_OFF + BGODIV*BGOMAXMAP*
     *                                       BGOSUB*NUMBGO)
        INTEGER*4 BNGLOB_OFF
        INTEGER*4 BNGWS2_OFF, BNGWS2H_OFF, BNGNDF_OFF, BNGNDFH_OFF 
        PARAMETER(BNGLOB_OFF = BNGPMT_OFF + 3*BGOSUB*NUMBGO)
        PARAMETER(BNGWS2_OFF = BNGLOB_OFF + NUMBGO)
        PARAMETER(BNGWS2H_OFF = BNGWS2_OFF + NUMBGO)
        PARAMETER(BNGNDF_OFF = BNGWS2H_OFF + NUMBGO)
        PARAMETER(BNGNDFH_OFF = BNGNDF_OFF + BGODIV*NUMBGO)

        INTEGER*4  BNGFST_OFF, BNGOTH_OFF, BNGDNR_OFF
        PARAMETER(BNGFST_OFF = BNGNDFH_OFF + BGODIV*NUMBGO)
        PARAMETER(BNGOTH_OFF = BNGFST_OFF + BGODIV*BGOSUB*NUMBGO)
        PARAMETER(BNGDNR_OFF = BNGOTH_OFF + BGODIV*BGOSUB*NUMBGO)

        INTEGER*4 BNGNSP_OFF, BNGSPH_OFF, BNGSPHH_OFF
        PARAMETER(BNGNSP_OFF = BNGDNR_OFF + BGODIV*NUMBGO)
        PARAMETER(BNGSPH_OFF = BNGNSP_OFF + NUMBGO)
        PARAMETER(BNGSPHH_OFF = BNGSPH_OFF + BGOSPH*NUMBGO)

        INTEGER*4  BNGFRE_OFF
        PARAMETER(BNGFRE_OFF = BNGSPHH_OFF + BGOSPH*NUMBGO)

        INTEGER*4 BNGSEC_SIZE, BNGSEC_NUM, BNGLEN
        PARAMETER(BNGSEC_SIZE = 2048)
        PARAMETER(BNGSEC_NUM = NINT(FLOAT((BNGFRE_OFF-2)/
     *                         BNGSEC_SIZE)+.5))
        PARAMETER(BNGLEN = BNGSEC_SIZE * BNGSEC_NUM)


        COMMON /BNGCOM/
     *   BNGSTS(NUMBGO),
     *   BNGCTM(NUMBGO),
     *   BNGTIM(NUMBGO),
     *   BNGDRW(NUMBGO),
     *   BNGBSD(NUMBGO),
     *   BNGESD(NUMBGO),
     *   BNGPUP(NUMBGO),
     *   BNGUPD(NUMBGO),
     *   BNGDAT(DATLEN,NUMBGO),
     *   BNGADV(NUMADV,NUMBGO),
     *   BNGSAL(BGOENT,NUMBGO)
        COMMON /BNGCOM/
     *   BNGSHV(BGODIV,BGOSUB,NUMBGO),
     *   BNGSHR(BGODIV,BGOSUB,NUMBGO),
     *   BNGPOL(BGODIV,BGOSUB,NUMBGO),
     *   BNGPAD(BGODIV,BGOSUB,NUMBGO),
     *   BNGPRG(BGODIV,BGOSUB,NUMBGO),
     *   BNGANU(BGODIV,BGOSUB,NUMBGO),
     *   BNGBRK(BGODIV,BGOSUB,NUMBGO),
     *   BNGOSV(BGODIV,BGOSUB,NUMBGO),
     *   BNGFRZ(BGODIV,BGOSUB,NUMBGO)
        COMMON /BNGCOM/
     *   BNGWIN(BGONBR,NUMBGO),
     *   BNGHLD(BGONBR,NUMBGO),
     *   BNGWAB(BGONAB,NUMBGO),
     *   BNGHAB(BGONAB,NUMBGO),
     *   BNGWLN(BGONLN,NUMBGO),
     *   BNGHLN(BGONLN,NUMBGO)
        COMMON /BNGCOM/
     *   BNGFAC(NUMBGO),
     *   BNGCUT(NUMBGO),
     *   BNGTAX(NUMBGO),
     *   BNGRES(2,NUMBGO),
     *   BNGAPL(NUMBGO),
     *   BNGMIN(NUMBGO)
        COMMON /BNGCOM/
     *   BNGSER(NUMBGO),
     *   BNGOPA(NUMBGO),
     *   BNGPRC(NUMBGO),
     *   BNGMAX(NUMBGO),
     *   BNGMLT(NUMBGO),
     *   BNGMDS(MAXMLTD_AVL,NUMBGO),
     *   BNGDIV(BGOSUB,NUMBGO),
     *   BNGMAT(BGOMAXMAP+2+BGOCOL*BGOROW,BGOSUB,NUMBGO),
     *   BNGPER(BGODIV,BGOSUB,NUMBGO)
        COMMON /BNGCOM/
     *   BNGSPR(NUMBGO),
     *   BNGTSR(BGODIV,BGOSUB,NUMBGO),
     *   BNGREV(NUMBGO),
     *   BNGASH(BGODIV,BGOSUB,NUMBGO),
     *   BNGFRG(NUMBGO),
     *   BNGWRF(POSTED,NUMBGO),
     *   BNGBST(NUMBGO),
     *   BNGBAL(MAXDRW,NUMBGO)
        COMMON /BNGCOM/
     *   BNGSED(NUMBGO),
     *   BNGBAS(NUMBGO),
     *   BNGPHS(BGOPHS,NUMBGO),
     *   BNGPHSH(BGOPHS,NUMBGO),
     *   BNG1ST(BGO1ST,NUMBGO),
     *   BNGWST(NUMBGO),
     *   BNGWSTH(NUMBGO)
        COMMON /BNGCOM/
     *   BNGMAP(BGOMAXMAP,BGOSUB,NUMBGO),
     *   BNGMPF(BGOMAXMAP,BGOSUB,NUMBGO),
     *   BNGAMP(BGODIV,BGOMAXMAP,BGOSUB,NUMBGO),
     *   BNGPMT(3,BGOPHS,NUMBGO),
     *   BNGLOB(NUMBGO),
     *   BNGWS2(NUMBGO),
     *   BNGWS2H(NUMBGO),
     *   BNGNDF(BGODIV,NUMBGO),
     *   BNGNDFH(BGODIV,NUMBGO),
     *   BNGFST(BGODIV,BGOSUB,NUMBGO),
     *   BNGOTH(BGODIV,BGOSUB,NUMBGO),
     *   BNGDNR(BGODIV,NUMBGO),
     *   BNGNSP(NUMBGO),
     *   BNGSPH(BGOSPH,NUMBGO),
     *   BNGSPHH(BGOSPH,NUMBGO),
     *   BNGFRE(BNGLEN - BNGFRE_OFF + 1)
C
        INTEGER*4 BNGSTS, BNGCTM, BNGTIM, BNGDRW, BNGBSD, BNGESD
        INTEGER*4 BNGPUP, BNGUPD, BNGDAT, BNGADV, BNGSAL, BNGSHV
        INTEGER*4 BNGSHR, BNGPOL, BNGPAD, BNGPRG, BNGANU, BNGBRK
        INTEGER*4 BNGOSV, BNGFRZ, BNGWIN, BNGHLD, BNGWAB, BNGHAB
        INTEGER*4 BNGWLN, BNGHLN, BNGFAC, BNGCUT
        INTEGER*4 BNGTAX, BNGRES, BNGAPL, BNGMIN, BNGSER, BNGOPA
        INTEGER*4 BNGPRC, BNGMAX, BNGMLT, BNGDIV, BNGMAT, BNGPER
        INTEGER*4 BNGSPR, BNGTSR, BNGREV, BNGASH
        INTEGER*4 BNGFRG, BNGWRF, BNGBST, BNGBAL
        INTEGER*4 BNGSED, BNGBAS, BNGPHS, BNGPHSH, BNG1ST
        INTEGER*4 BNGWST, BNGWSTH, BNGMAP
        INTEGER*4 BNGMPF, BNGAMP, BNGPMT, BNGLOB, BNGWS2, BNGWS2H
        INTEGER*4 BNGNDF, BNGNDFH, BNGFST, BNGOTH, BNGDNR
        INTEGER*4 BNGNSP, BNGSPH, BNGSPHH
        INTEGER*4 BNGFRE
C
        BYTE      BNGMDS
C
C EQUIVALENCE FOR CHECKPOINT
C
        INTEGER*4 BNGCSB
        PARAMETER(BNGCSB= BNGSEC_NUM)
C
        INTEGER*4 BNGBLK(2048,BNGCSB)
        EQUIVALENCE(BNGBLK(1,1),BNGSTS(1))
C
C
        INTEGER*4        FRST_BNGCOM(2)
        EQUIVALENCE     (FRST_BNGCOM, BNGSTS)
        INTEGER*4        LAST_BNGCOM
        EQUIVALENCE     (LAST_BNGCOM, BNGBLK(2048,BNGCSB))
C
        VOLATILE /BNGCOM/
C
