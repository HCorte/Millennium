        LOGICAL GVT_COMPRESSED_FORMAT   !GVT IS IN COMPRESSED FORMAT
        PARAMETER (GVT_COMPRESSED_FORMAT=.FALSE.)
C
	INTEGER*4  HRESET, MNEWS, URESET, SRESET, RRESET
        INTEGER*4  GVTNEWS,TVNEWS, TXTBEG, DLLBEG
	PARAMETER (HRESET=1)	! HARD RESET MESSAGE
	PARAMETER (MNEWS=2)	! NEWS MESSAGE ... ONLINE TERMINALS ONLY
	PARAMETER (URESET=3)	! ULTIMATE RESET MESSAGE
	PARAMETER (SRESET=4)	! SOFT RESET MESSAGE
        PARAMETER (RRESET=5)    ! RESTART RESET MESSAGE
	PARAMETER (GVTNEWS=7)   ! NEWS MESSAGE ... GTV TERMINALS ONLY
        PARAMETER (TVNEWS=9)    ! NEWS MESSAGE FOR TV MONITOR
        PARAMETER (TXTBEG=10)   ! BEGINNING OF TEXT MESSAGES
	PARAMETER (DLLBEG=1500)	! DOWNLINE LOAD FILES BEGIN MESSAGE
C
C       CHANGES IN CODE REQUIRES THAT LOAD NAMES BE ZERO IF NOT
C       DEFINED
C
	INTEGER*4 FCLTIN(MAXFCL),FCLNAM(MAXFCL)
	DATA FCLTIN/Z00,Z00,Z00,Z00,Z00,				!V06
     *	            Z00,Z00,Z00,Z00,Z00/
	DATA FCLNAM/10*0/
C
        INTEGER*4 ACLNAM(MAXFCL)
        DATA ACLNAM/10*0/
C
	INTEGER*4 MCPROM(MAXMCP),MCPNAM(MAXMCP)
        DATA MCPROM/Z00000000,          !ROM ID A6**			
     *              Z00000000,          !ROM ID A7**			!V06
     *              Z00000000,          !ROM ID &C**			!V06
     *              Z00000000,          !ROM ID &B**			!V06
     *	            Z00000000,
     *	            Z00000000,
     *	            Z00000000,
     *	            Z00000000,
     *	            Z00000000,
     *	            Z00000000/
	DATA MCPNAM/10*0/
C
	INTEGER*2 SFTROM(MAXSFT)
	INTEGER*4 SFTNAM(MAXSFT)
	INTEGER*4 SFT_LIST(MAXSFT)
C
	DATA SFTROM/Z0000,		! ROM ID (T2DD) 128K DIAL GVT
     *	            Z0000,		! ROM ID (T3AG) 128K ASYNC GVT
     *	            Z0000,              ! ROM ID (T6BD) 384K (IOWA) DIAL GVT
     *	            Z0000,              ! ROM ID (T3CG) 384K DIAL & ASYNC GVT
     *	            Z0000,
     *	            Z0000,
     *	            Z0000,
     *	            Z0000,
     *	            Z0000,
     *	            Z0000/
	DATA SFTNAM/10*0/
	DATA SFT_LIST/10*0/
C
	INTEGER*2 GVTROM(MAXGVT)
	INTEGER*4 GVTNAM(MAXGVT)
        INTEGER*4 GVT_LIST(MAXGVT)
	DATA GVTROM/Z0000,		! ROM ID (T2DD) 128K DIAL GVT
     *	            Z0000,		! ROM ID (T3AG) 128K ASYNC GVT
     *	            Z0000,              ! ROM ID (T6BD) 384K (IOWA) DIAL GVT
     *	            Z0000,              ! ROM ID (T3CG) 384K DIAL & ASYNC GVT
     *	            Z0000,
     *	            Z0000,
     *	            Z0000,
     *	            Z0000,
     *	            Z0000,
     *	            Z0000/
	DATA GVTNAM/10*0/
	DATA GVT_LIST/10*0/
C
C
C       START OF GAME TEXT
C
        INTEGER*4 GAMTXT(MAXGAM)
        DATA GAMTXT/020,040,060,080,100,
     *            120,140,160,180,200,
     *            220,240,260,280,300,
     *            320,340,360,380,400,
     *            420,440,460,480,500,
     *            520,540,560,580,600,
     *            620,640,660,680,700,
     *            720,740,760,780,800,
     *            820,840,860,880,900,
     *            920,940,960,980,1000/

	COMMON /GAMTXT/ GAMTXT

C
C	RECORD SIZES DEFINED FOR DIFFERENT LOADS
C
	INTEGER*4	APPCSIZ, APPISIZ,
     *			GVTCSIZ, GVTISIZ,
     *			MCPCSIZ, MCPISIZ,
     *			SFTCSIZ, SFTISIZ,
     *			ACLCSIZ, ACLISIZ
	PARAMETER      (ACLCSIZ = 276)
	PARAMETER      (ACLISIZ = (ACLCSIZ + 3) / 4)
	PARAMETER      (APPCSIZ = 276)
	PARAMETER      (APPISIZ = (APPCSIZ + 3) / 4)
	PARAMETER      (GVTCSIZ = 268)
	PARAMETER      (GVTISIZ = (GVTCSIZ + 3) / 4)
	PARAMETER      (MCPCSIZ = 268)
	PARAMETER      (MCPISIZ = (MCPCSIZ + 3) / 4)
	PARAMETER      (SFTCSIZ = 263)
	PARAMETER      (SFTISIZ = (SFTCSIZ + 3) / 4)
C
C	LENGTH OF SEG0 SEGMENTS
C
	INTEGER*4	FCLSEG0,
     *                  GVTSEG0,
     *                  MCPSEG0,
     *                  SFTSEG0
C
        PARAMETER      (FCLSEG0 = 34)
        PARAMETER      (GVTSEG0 = 20)
        PARAMETER      (MCPSEG0 = 30) !!!18)
        PARAMETER      (SFTSEG0 = 12)
C
C
C       SUBTYPE FOR DOWNLOAD MESSAGES
C
        INTEGER*4 DLL_SUBTYP_LOOPBACK, DLL_SUBTYP_MCP,
     *            DLL_SUBTYP_BGROUND_APP, DLL_SUBTYP_BGROUND_DRIVER,
     *            DLL_SUBTYP_OLD_APP, DLL_SUBTYP_OLD_DRIVER,
     *            DLL_SUBTYP_ACL, DLL_SUBTYP_LOAD_DONE
C
        PARAMETER (DLL_SUBTYP_MCP=1)
        PARAMETER (DLL_SUBTYP_LOOPBACK=6)       !LOOPBACK TYPE MESSAGE
C                                               !SUPPORTED BY MCP
        PARAMETER (DLL_SUBTYP_ACL=8)            !ACL LOAD
        PARAMETER (DLL_SUBTYP_BGROUND_APP=9)    !MCP SUPPORT FOR BACKGROUND -APP
        PARAMETER (DLL_SUBTYP_BGROUND_DRIVER=10)!MCP SUPPORT FOR BACKGROUND-DRVR
        PARAMETER (DLL_SUBTYP_LOAD_DONE=11)     !LOAD DONE AS IN UK !!!!!!!!!
        PARAMETER (DLL_SUBTYP_OLD_APP=13)       !OLD STYLE, APPLICATION REQUEST
        PARAMETER (DLL_SUBTYP_OLD_DRIVER=14)    !OLD STYLE, DRIVER REQUEST
C
C       TYPES FOR DOWNLOAD MESSAGES
C
        INTEGER*4 DLL_TYPE_TERMINAL, DLL_TYPE_GVT, DLL_TYPE_GVT_SOFTL
        PARAMETER (DLL_TYPE_GVT_SOFTL=4)        !GVT SOFTLOADER
        PARAMETER (DLL_TYPE_TERMINAL=5)         !LOTTERY TERMINAL DOWNLOAD
        PARAMETER (DLL_TYPE_GVT=15)             !GVT APPLICATION LOAD
C
        INTEGER*4 DLL_SUBTYP_GVT_COMPRESSED
        PARAMETER (DLL_SUBTYP_GVT_COMPRESSED=2)!COMPRESSED LOAD FOR GVT
C

        INTEGER*4 ALTURA_ROM, ALTURA_TIN        !V16
        COMMON /PRMDLLCOM/ FCLTIN,FCLNAM,MCPROM,MCPNAM,ACLNAM,
