C
C SET OLD BINGO PARAMETERS
        INTEGER*4 OBGODIV, OBGOSUB, OBGONAB, OBGONLN, OBGOMAXMAP
        INTEGER*4 OBGOENT, OBGONBR, OBGOCOL, OBGOROW, OBGOPHS
        INTEGER*4 OBGO1ST

        PARAMETER(OBGODIV=8)
        PARAMETER(OBGOSUB=3)
        PARAMETER(OBGONAB=25)
        PARAMETER(OBGONLN=10)
        PARAMETER(OBGOMAXMAP=15)
        PARAMETER(OBGOENT=25)
        PARAMETER(OBGONBR=75)
        PARAMETER(OBGOCOL=5)
        PARAMETER(OBGOROW=5)
        PARAMETER(OBGOPHS=2)
        PARAMETER(OBGO1ST=4)
C
C RECORD OFFSET PARAMETERS
C
        INTEGER*4 ODBNSTS,ODBNCTM,ODBNTIM,ODBNDRW,ODBNBSD,ODBNESD
        INTEGER*4 ODBNPUP,ODBNUPD,ODBNDAT(DATLEN),ODBNADV(NUMADV)
        INTEGER*4 ODBNSAL(OBGOENT)
        INTEGER*4 ODBNSHV(OBGODIV,OBGOSUB),ODBNSHR(OBGODIV,OBGOSUB)
        INTEGER*4 ODBNPOL(OBGODIV,OBGOSUB)
        INTEGER*4 ODBNPAD(OBGODIV,OBGOSUB),ODBNPRG(OBGODIV,OBGOSUB)
        INTEGER*4 ODBNANU(OBGODIV,OBGOSUB),ODBNBRK(OBGODIV,OBGOSUB)
        INTEGER*4 ODBNOSV(OBGODIV,OBGOSUB)
        INTEGER*4 ODBNFRZ(OBGODIV,OBGOSUB),ODBNWIN(OBGONBR),ODBNHLD(OBGONBR)
        INTEGER*4 ODBNWAB(OBGONAB),ODBNHAB(OBGONAB),ODBNWLN(OBGONLN)
        INTEGER*4 ODBNHLN(OBGONLN)
        INTEGER*4 ODBNFAC,ODBNCUT
        INTEGER*4 ODBNTAX,ODBNRES(2),ODBNAPL,ODBNMIN,ODBNSER,ODBNOPA,ODBNPRC
        INTEGER*4 ODBNMAX,ODBNMLT
        BYTE      ODBNMDS(MAXMLTD_AVL)
        INTEGER*4 ODBNDIV(OBGOSUB)
        INTEGER*4 ODBNMAT(OBGOMAXMAP+1+OBGOCOL*OBGOROW,OBGOSUB)
        INTEGER*4 ODBNPER(OBGODIV,OBGOSUB)
        INTEGER*4 ODBNSPR,ODBNTSR(OBGODIV,OBGOSUB),ODBNREV
        INTEGER*4 ODBNASH(OBGODIV,OBGOSUB),ODBNFRG,ODBNWRF(POSTED),ODBNBST
        INTEGER*4 ODBNBAL(MAXDRW),ODBNSED,ODBNBAS,ODBNPHS(OBGOPHS)
        INTEGER*4 ODBNPHSH(OBGOPHS),ODBN1ST(OBGO1ST),ODBNWST,ODBNWSTH
        INTEGER*4 ODBNMAP(OBGOMAXMAP,OBGOSUB)
        INTEGER*4 ODBNMPF(OBGOMAXMAP,OBGOSUB)
        INTEGER*4 ODBNAMP(OBGODIV,OBGOMAXMAP,OBGOSUB)
        INTEGER*4 ODBNPMT(2,OBGOPHS)
C
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
        INTEGER*4 ODBNSTS_OFF, ODBNCTM_OFF, ODBNTIM_OFF, ODBNDRW_OFF
        PARAMETER(ODBNSTS_OFF = 1)                                      !0001
        PARAMETER(ODBNCTM_OFF = ODBNSTS_OFF + 1)                                !0002
        PARAMETER(ODBNTIM_OFF = ODBNCTM_OFF + 1)                                !0003
        PARAMETER(ODBNDRW_OFF = ODBNTIM_OFF + 1)                                !0004
C
        INTEGER*4 ODBNBSD_OFF, ODBNESD_OFF, ODBNPUP_OFF, ODBNUPD_OFF
        PARAMETER(ODBNBSD_OFF = ODBNDRW_OFF + 1)                                !0005
        PARAMETER(ODBNESD_OFF = ODBNBSD_OFF + 1)                                !0006
        PARAMETER(ODBNPUP_OFF = ODBNESD_OFF + 1)                                !0007
        PARAMETER(ODBNUPD_OFF = ODBNPUP_OFF + 1)                                !0008   
C
        INTEGER*4 ODBNDAT_OFF, ODBNADV_OFF, ODBNSAL_OFF, ODBNSHV_OFF
        PARAMETER(ODBNDAT_OFF = ODBNUPD_OFF + 1)                                !0009
        PARAMETER(ODBNADV_OFF = ODBNDAT_OFF + DATLEN)                   !0029
        PARAMETER(ODBNSAL_OFF = ODBNADV_OFF + NUMADV)                   !0036
        PARAMETER(ODBNSHV_OFF = ODBNSAL_OFF + OBGOENT)                  !0061
C
        INTEGER*4 ODBNSHR_OFF, ODBNPOL_OFF, ODBNPAD_OFF, ODBNPRG_OFF
        PARAMETER(ODBNSHR_OFF = ODBNSHV_OFF + OBGODIV*OBGOSUB)
        PARAMETER(ODBNPOL_OFF = ODBNSHR_OFF + OBGODIV*OBGOSUB)  !0077
        PARAMETER(ODBNPAD_OFF = ODBNPOL_OFF + OBGODIV*OBGOSUB)  !0085
        PARAMETER(ODBNPRG_OFF = ODBNPAD_OFF + OBGODIV*OBGOSUB)  !0093
C
        INTEGER*4 ODBNANU_OFF, ODBNBRK_OFF, ODBNOSV_OFF, ODBNFRZ_OFF
        PARAMETER(ODBNANU_OFF = ODBNPRG_OFF + OBGODIV*OBGOSUB)  !0101
        PARAMETER(ODBNBRK_OFF = ODBNANU_OFF + OBGODIV*OBGOSUB)  !0109
        PARAMETER(ODBNOSV_OFF = ODBNBRK_OFF + OBGODIV*OBGOSUB)  !0117
        PARAMETER(ODBNFRZ_OFF = ODBNOSV_OFF + OBGODIV*OBGOSUB)  !0125
C
        INTEGER*4 ODBNWIN_OFF, ODBNHLD_OFF
        PARAMETER(ODBNWIN_OFF = ODBNFRZ_OFF + OBGODIV*OBGOSUB)          !0133
        PARAMETER(ODBNHLD_OFF = ODBNWIN_OFF + OBGONBR)                  !0146
C
        INTEGER*4 ODBNWAB_OFF, ODBNHAB_OFF, ODBNWLN_OFF, ODBNHLN_OFF
        PARAMETER(ODBNWAB_OFF = ODBNHLD_OFF + OBGONBR)                  !
        PARAMETER(ODBNHAB_OFF = ODBNWAB_OFF + OBGONAB)                     !
        PARAMETER(ODBNWLN_OFF = ODBNHAB_OFF + OBGONAB)                     !
        PARAMETER(ODBNHLN_OFF = ODBNWLN_OFF + OBGONLN)                     !
C
        INTEGER*4 ODBNFAC_OFF, ODBNCUT_OFF
        PARAMETER(ODBNFAC_OFF = ODBNHLN_OFF + OBGONLN)
        PARAMETER(ODBNCUT_OFF = ODBNFAC_OFF + 1)                                !0160
C
        INTEGER*4 ODBNTAX_OFF, ODBNRES_OFF, ODBNAPL_OFF, ODBNMIN_OFF
        PARAMETER(ODBNTAX_OFF = ODBNCUT_OFF + 1)                                !0161
        PARAMETER(ODBNRES_OFF = ODBNTAX_OFF + 1)                                !0162
        PARAMETER(ODBNAPL_OFF = ODBNRES_OFF + 2)                                !0164
        PARAMETER(ODBNMIN_OFF = ODBNAPL_OFF + 1)                                !0165
C
        INTEGER*4 ODBNSER_OFF, ODBNOPA_OFF, ODBNPRC_OFF, ODBNMAX_OFF
        PARAMETER(ODBNSER_OFF = ODBNMIN_OFF + 1)                                !0166
        PARAMETER(ODBNOPA_OFF = ODBNSER_OFF + 1)                                !0167
        PARAMETER(ODBNPRC_OFF = ODBNOPA_OFF + 1)                                !0168
        PARAMETER(ODBNMAX_OFF = ODBNPRC_OFF + 1)                                !0169
C
        INTEGER*4 ODBNMLT_OFF, ODBNMDS_OFF, ODBNDIV_OFF, ODBNMAT_OFF
        PARAMETER(ODBNMLT_OFF = ODBNMAX_OFF + 1)                                !0170
        PARAMETER(ODBNMDS_OFF = ODBNMLT_OFF + 1)                                !0171
        PARAMETER(ODBNDIV_OFF = ODBNMDS_OFF + (MAXMLTD_AVL/4))          !0184
        PARAMETER(ODBNMAT_OFF = ODBNDIV_OFF + OBGOSUB)
C
        INTEGER*4 ODBNPER_OFF, ODBNSPR_OFF, ODBNTSR_OFF, ODBNREV_OFF
        PARAMETER(ODBNPER_OFF = ODBNMAT_OFF + 
     *                         (OBGOMAXMAP+1+OBGOCOL*OBGOROW)*OBGOSUB)
        PARAMETER(ODBNSPR_OFF = ODBNPER_OFF + OBGODIV*OBGOSUB)
        PARAMETER(ODBNTSR_OFF = ODBNSPR_OFF + 1)                                !0202
        PARAMETER(ODBNREV_OFF = ODBNTSR_OFF + OBGODIV*OBGOSUB)          !0210
C
        INTEGER*4 ODBNASH_OFF, ODBNFRG_OFF
        PARAMETER(ODBNASH_OFF = ODBNREV_OFF + 1)                                !0340
        PARAMETER(ODBNFRG_OFF = ODBNASH_OFF + OBGODIV*OBGOSUB)  !0348
C
        INTEGER*4 ODBNWRF_OFF, ODBNBST_OFF, ODBNBAL_OFF
        PARAMETER(ODBNWRF_OFF = ODBNFRG_OFF + 1)                                !0349
        PARAMETER(ODBNBST_OFF = ODBNWRF_OFF + POSTED)                   !0359
        PARAMETER(ODBNBAL_OFF = ODBNBST_OFF + 1)                                !0360
C
        INTEGER*4 ODBNSED_OFF, ODBNBAS_OFF, ODBNPHS_OFF
        INTEGER*4 ODBNPHSH_OFF,ODBN1ST_OFF
        PARAMETER(ODBNSED_OFF = ODBNBAL_OFF + MAXDRW)                
        PARAMETER(ODBNBAS_OFF = ODBNSED_OFF + 1)                     
        PARAMETER(ODBNPHS_OFF = ODBNBAS_OFF + 1)
        PARAMETER(ODBNPHSH_OFF= ODBNPHS_OFF + OBGOPHS)
        PARAMETER(ODBN1ST_OFF = ODBNPHSH_OFF + OBGOPHS)               

        INTEGER*4 ODBNWST_OFF, ODBNWSTH_OFF, ODBNMAP_OFF, ODBNMPF_OFF
        INTEGER*4 ODBNAMP_OFF
        PARAMETER(ODBNWST_OFF = ODBN1ST_OFF + OBGO1ST)
        PARAMETER(ODBNWSTH_OFF= ODBNWST_OFF + 1)
        PARAMETER(ODBNMAP_OFF = ODBNWSTH_OFF + 1)                     !0380
        PARAMETER(ODBNMPF_OFF = ODBNMAP_OFF + OBGOMAXMAP*OBGOSUB)  
        PARAMETER(ODBNAMP_OFF = ODBNMPF_OFF + OBGOMAXMAP*OBGOSUB)
        
        INTEGER*4 ODBNPMT_OFF, ODBNFRE_OFF
        PARAMETER(ODBNPMT_OFF = ODBNAMP_OFF + OBGODIV*OBGOMAXMAP*OBGOSUB)
        PARAMETER(ODBNFRE_OFF = ODBNPMT_OFF + 2*OBGOPHS)
C
C CALCULATE FREE SPACE BASED ON MACHINE DEPENDENT SECTOR SIZES.
C SHOULD ALLWAYS ADD UP TO A EVEN SECTOR SIZE BYTES (VAX 512).
C NOTE! IN CALCULATIONS, FRE_OFF-2 IS DONE TO TAKE CARE OF WHEN THE DATA SIZE
C       ENDS ON A EVEN SECTOR SIZE.
C
        INTEGER*4 ODBNSEC_VAX, ODBNSEC_NUM
        PARAMETER (ODBNSEC_VAX = SECSIZE/4)
        PARAMETER (ODBNSEC_NUM = 
     *             NINT(FLOAT((ODBNFRE_OFF-2)/ODBNSEC_VAX)+.5))
C
        INTEGER*4 ODBNLEN, ODBNSEC
        PARAMETER (ODBNLEN = ODBNSEC_VAX*ODBNSEC_NUM)         !EVEN SEC SIZE
        PARAMETER (ODBNSEC = ODBNLEN / 64)
C
        INTEGER*4 ODBNREC(ODBNLEN)      
C
        EQUIVALENCE(ODBNSTS,ODBNREC(ODBNSTS_OFF))
        EQUIVALENCE(ODBNCTM,ODBNREC(ODBNCTM_OFF))
        EQUIVALENCE(ODBNTIM,ODBNREC(ODBNTIM_OFF))
        EQUIVALENCE(ODBNDRW,ODBNREC(ODBNDRW_OFF))
        EQUIVALENCE(ODBNBSD,ODBNREC(ODBNBSD_OFF))
        EQUIVALENCE(ODBNESD,ODBNREC(ODBNESD_OFF))
        EQUIVALENCE(ODBNPUP,ODBNREC(ODBNPUP_OFF))
        EQUIVALENCE(ODBNUPD,ODBNREC(ODBNUPD_OFF))
        EQUIVALENCE(ODBNDAT,ODBNREC(ODBNDAT_OFF))
        EQUIVALENCE(ODBNADV,ODBNREC(ODBNADV_OFF))
        EQUIVALENCE(ODBNSAL,ODBNREC(ODBNSAL_OFF))
        EQUIVALENCE(ODBNSHV,ODBNREC(ODBNSHV_OFF))
        EQUIVALENCE(ODBNSHR,ODBNREC(ODBNSHR_OFF))
        EQUIVALENCE(ODBNPOL,ODBNREC(ODBNPOL_OFF))
        EQUIVALENCE(ODBNPAD,ODBNREC(ODBNPAD_OFF))
        EQUIVALENCE(ODBNPRG,ODBNREC(ODBNPRG_OFF))
        EQUIVALENCE(ODBNANU,ODBNREC(ODBNANU_OFF))
        EQUIVALENCE(ODBNBRK,ODBNREC(ODBNBRK_OFF))
        EQUIVALENCE(ODBNOSV,ODBNREC(ODBNOSV_OFF))
        EQUIVALENCE(ODBNFRZ,ODBNREC(ODBNFRZ_OFF))
        EQUIVALENCE(ODBNWIN,ODBNREC(ODBNWIN_OFF))
        EQUIVALENCE(ODBNHLD,ODBNREC(ODBNHLD_OFF))
        EQUIVALENCE(ODBNWAB,ODBNREC(ODBNWAB_OFF))
        EQUIVALENCE(ODBNHAB,ODBNREC(ODBNHAB_OFF))
        EQUIVALENCE(ODBNWLN,ODBNREC(ODBNWLN_OFF))
        EQUIVALENCE(ODBNHLN,ODBNREC(ODBNHLN_OFF))
        EQUIVALENCE(ODBNFAC,ODBNREC(ODBNFAC_OFF))
        EQUIVALENCE(ODBNCUT,ODBNREC(ODBNCUT_OFF))
        EQUIVALENCE(ODBNTAX,ODBNREC(ODBNTAX_OFF))
        EQUIVALENCE(ODBNRES,ODBNREC(ODBNRES_OFF))
        EQUIVALENCE(ODBNAPL,ODBNREC(ODBNAPL_OFF))
        EQUIVALENCE(ODBNMIN,ODBNREC(ODBNMIN_OFF))
        EQUIVALENCE(ODBNSER,ODBNREC(ODBNSER_OFF))
        EQUIVALENCE(ODBNOPA,ODBNREC(ODBNOPA_OFF))
        EQUIVALENCE(ODBNPRC,ODBNREC(ODBNPRC_OFF))
        EQUIVALENCE(ODBNMAX,ODBNREC(ODBNMAX_OFF))
        EQUIVALENCE(ODBNMLT,ODBNREC(ODBNMLT_OFF))
        EQUIVALENCE(ODBNMDS,ODBNREC(ODBNMDS_OFF))
        EQUIVALENCE(ODBNDIV,ODBNREC(ODBNDIV_OFF))
        EQUIVALENCE(ODBNMAT,ODBNREC(ODBNMAT_OFF))
        EQUIVALENCE(ODBNPER,ODBNREC(ODBNPER_OFF))
        EQUIVALENCE(ODBNSPR,ODBNREC(ODBNSPR_OFF))
        EQUIVALENCE(ODBNTSR,ODBNREC(ODBNTSR_OFF))
        EQUIVALENCE(ODBNREV,ODBNREC(ODBNREV_OFF))
        EQUIVALENCE(ODBNASH,ODBNREC(ODBNASH_OFF))
        EQUIVALENCE(ODBNFRG,ODBNREC(ODBNFRG_OFF))
        EQUIVALENCE(ODBNWRF,ODBNREC(ODBNWRF_OFF))
        EQUIVALENCE(ODBNBST,ODBNREC(ODBNBST_OFF))
        EQUIVALENCE(ODBNBAL,ODBNREC(ODBNBAL_OFF))
        EQUIVALENCE(ODBNSED,ODBNREC(ODBNSED_OFF))
        EQUIVALENCE(ODBNBAS,ODBNREC(ODBNBAS_OFF))
        EQUIVALENCE(ODBNPHS,ODBNREC(ODBNPHS_OFF))
        EQUIVALENCE(ODBNPHSH,ODBNREC(ODBNPHSH_OFF))
        EQUIVALENCE(ODBN1ST,ODBNREC(ODBN1ST_OFF))
        EQUIVALENCE(ODBNWST,ODBNREC(ODBNWST_OFF))
        EQUIVALENCE(ODBNWSTH,ODBNREC(ODBNWSTH_OFF))
        EQUIVALENCE(ODBNMAP,ODBNREC(ODBNMAP_OFF))
        EQUIVALENCE(ODBNMPF,ODBNREC(ODBNMPF_OFF))
        EQUIVALENCE(ODBNAMP,ODBNREC(ODBNAMP_OFF))
        EQUIVALENCE(ODBNPMT,ODBNREC(ODBNPMT_OFF))
