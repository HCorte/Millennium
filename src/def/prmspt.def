        INTEGER*4 NUMSPT
        PARAMETER(NUMSPT=6)            !NUMBER OF SPORT GAMES
C
        INTEGER*4 SPGENT, SPGDIV, SPGNBR, SPGSYS, SPXMAX
        PARAMETER(SPGENT=25)           !NUMBER OF SALES ENTRIES
        PARAMETER(SPGDIV=8)            !MAXIMUM NUMBER OF DIVISIONS
        PARAMETER(SPGNBR=17)           !MAXIMUM WINNING NUMBERS DRAWN
        PARAMETER(SPGSYS=110)          !MAXIMUM SYSTEM BET DEFINITIONS
        PARAMETER(SPXMAX=0)            !MAXIMUM TEXT MESSAGES
C
C NAME FIELD LENGTH PARAMETERS
C
        INTEGER*4 SPNMS_LEN
        PARAMETER (SPNMS_LEN=16)       !LENGTH IN BYTES OF NAME FIELD
C
C NAME FILED LENGTH PARAMETERS
C
        INTEGER*4 SPEVN_LEN
        PARAMETER (SPEVN_LEN=16)       ! EVENT NAME LENGTH IN BYTES
C
C
C WINNING MARK WHEN THE DRAW HAS BEEN CANCELLED
C
C  1 = 1 --> WIN HOME
C  2 = X --> EQUAL
C  3 = C --> EVENT CANCELLED
C  4 = 2 --> WIN AWAY
C
       INTEGER * 4 WINNUM_CANEVENT
C
       PARAMETER(WINNUM_CANEVENT = 3)
