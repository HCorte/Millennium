	INTEGER*4 NUMSTR, MAXSTRRW, MAXSTRTI
	PARAMETER(NUMSTR=6)             ! NUMBER OF SUPER TRIPLE GAMES
	PARAMETER(MAXSTRRW=18)          ! MAX NUMBER OF COMPETITOR ROWS
	PARAMETER(MAXSTRTI=24)	        ! MAX NUMBER OF TIES

C NAME FILED LNGTH PARAMETERS

	INTEGER*4 STRENM_LEN, STRDES_LEN , STRNMS_LEN, STRTVC_LEN
	PARAMETER (STRENM_LEN=28)	! EVENT NAME LENGTH IN BYTES
	PARAMETER (STRDES_LEN=3*28)     ! EVENT DESCRIPTION LENGTH IN BYTES     
	PARAMETER (STRNMS_LEN=16)	! ROW NAME LENGTH IN BYTES
	PARAMETER (STRTVC_LEN= 4)       ! LENGTH IN BYTES OF TC-CHANEL NAME.

C SUPER TRIPLE MAXIMUM NUMBER OF TEXT MESSAGES

	INTEGER*4 STRTXMAX
	PARAMETER(STRTXMAX=3)           ! MAXIMUM TEXT MESSAGES

	INTEGER*4 STRMAXD		
	PARAMETER(STRMAXD=30)		! # OF ODDS TO DISPLAY.
