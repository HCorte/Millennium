        INTEGER*4 OMAXGAM
        PARAMETER (OMAXGAM=10)
C
	   INTEGER*4 OLD_ASFACT, OLD_ASFTKC, OLD_ASFSRV, OLD_ASFPADU, OLD_ASFPADP
	   INTEGER*4 OLD_ASFSCMU, OLD_ASFSCMP, OLD_ASFVCMU, OLD_ASFVCMP
	   INTEGER*4 OLD_ASFWCMU, OLD_ASFWCMP, OLD_ASFWCNT, OLD_ASFWAMT
	   PARAMETER (OLD_ASFACT=1)    !DAYS ACTIVE                (ASFINV)
	   PARAMETER (OLD_ASFTKC=2)    !TICKET CHARGE              (ASFINV)
	   PARAMETER (OLD_ASFSRV=3)    !SERVICE CHARGE             (ASFINV)
	   PARAMETER (OLD_ASFSCMU=4)   !SALES COM (UNITS)          (ASFINV)
	   PARAMETER (OLD_ASFSCMP=5)   !SALES COM (PENNIES)        (ASFINV)
	   PARAMETER (OLD_ASFVCMU=6)   !VALIDATION COM  (UNITS)    (ASFINV)
	   PARAMETER (OLD_ASFVCMP=7)   !VALIDATION COM (PENNNIES)  (ASFINV)
	   PARAMETER (OLD_ASFWCMU=8)   !WINNERS COM (UNITS)        (ASFINV)
           PARAMETER (OLD_ASFWCMP=9)   !WINNERS COM (PENNIES)      (ASFINV)
	   PARAMETER (OLD_ASFPADU=10)  !PAYMENTS (UNITS)           (ASFINV)
	   PARAMETER (OLD_ASFPADP=11)  !PAYMENTS (PENNIES)         (ASFINV)
C
	   INTEGER*4  OLD_ASFADJU, OLD_ASFADJP, OLD_ASFSTR, OLD_ASFEND
           INTEGER*4  OLD_ASFBALU, OLD_ASFBALP
	   INTEGER*4  OLD_ASFDUEU, OLD_ASFDUEP, OLD_ASFBTWU, OLD_ASFBTWC
	   INTEGER*4  OLD_ASFOFFPAY, OLD_ASFCRAMT
	   PARAMETER (OLD_ASFADJU=12) !ADJUSTMENTS (UNITS)  (ASFINV)
	   PARAMETER (OLD_ASFADJP=13) !ADJUSTMENTS (PENNIES)(ASFINV)
	   PARAMETER (OLD_ASFSTR=14)  !START DATE           (ASFINV)
	   PARAMETER (OLD_ASFEND=15)  !END DATE             (ASFINV)
	   PARAMETER (OLD_ASFBALU=16) !BALANCE (UNITS)      (ASFINV)
	   PARAMETER (OLD_ASFBALP=17) !BALANCE (PENNIES)    (ASFINV)
	   PARAMETER (OLD_ASFDUEU=18) !AMOUNT DUE (UNITS)   (ASFINV)
	   PARAMETER (OLD_ASFDUEP=19) !AMOUNT DUE (PENNIES) (ASFINV)
	   PARAMETER (OLD_ASFWCNT=20) !WINNERS COUNT        (ASFINV)
	   PARAMETER (OLD_ASFWAMT=21) !WINNERS AMOUNT       (ASFINV)
	   PARAMETER (OLD_ASFBTWU=22) !BTW TAX (UNITS)      (ASFINV)
	   PARAMETER (OLD_ASFBTWC=23) !BTW TAX (PENNIES)    (ASFINV)
	   PARAMETER (OLD_ASFOFFPAY=24) !OFFLINE VALIDATIONS(ASFINV)
	   PARAMETER (OLD_ASFCRAMT =25) !TOTAL AMOUNT WON FOR BEING A "CENTRAL DE RECEPCAO"(ASFINV)
C
C          OFFSETS 26-30 AVAILABLE FOR USE
C

C SALES BY DURATION IS KEPT IN THE BILLING TABLE     (ASFBIL) AFTER THE
C REGULAR VALUES. NO PARAMETERS ARE DEFINED EXCEPT 
C THE STARTING OFFSET.
C
	INTEGER*4 OLD_ASFWKOFF
	PARAMETER (OLD_ASFWKOFF = AGAMLEN+1)
C
C LEDGER TABLE OFFSETS
C
	INTEGER*4  OLD_LGRCOD, OLD_LGRCDC, OLD_LGRAMTU, OLD_LGRAMTP, OLD_LGRINF
	PARAMETER (OLD_LGRCOD=1)  !CODE                  (ASFLGR)
	PARAMETER (OLD_LGRCDC=2)  !CDC DATE              (ASFLGR)
	PARAMETER (OLD_LGRAMTU=3) !AMOUNT (UNITS)        (ASFLGR)
	PARAMETER (OLD_LGRAMTP=4) !AMOUNT (PENNIES)      (ASFLGR)
	PARAMETER (OLD_LGRINF=5)  !INFORMATIONAL         (ASFLGR)
C                            BYTE 1= WEEK NUMBER
C                            BYTE 2= YEAR
C                            BYTE 3= CHANGE TYPE (0,1,2)
C                            BYTE 4= GAME TYPE
C
C INSTANT TICKET INVOICE OFFSETS
C
        INTEGER*4 OLD_ASFITSCNT, OLD_ASFITSAMT
        INTEGER*4 OLD_ASFITVCNT, OLD_ASFITVAMT, OLD_ASFITBCNT, OLD_ASFITBAMT
        INTEGER*4 OLD_ASFITRAMT, OLD_ASFITSCM, OLD_ASFITPCM
        INTEGER*4 OLD_ASFITADJ, OLD_ASFITDUE
	   PARAMETER (OLD_ASFITSCNT=1)   !PACKS SOLD COUNT          (asfitinv)
	   PARAMETER (OLD_ASFITSAMT=2)   !PACKS SOLD AMOUNT         (asfitinv)
	   PARAMETER (OLD_ASFITVCNT=3)   !VALIDATIONS COUNT         (asfitinv)  
	   PARAMETER (OLD_ASFITVAMT=4)   !VALIDATIONS AMOUNT        (asfitinv)  
	   PARAMETER (OLD_ASFITBCNT=5)   !BANK VALIDATIONS COUNT    (asfitinv)  
	   PARAMETER (OLD_ASFITBAMT=6)   !BANK VALIDATIONS AMOUNT   (asfitinv)  
	   PARAMETER (OLD_ASFITRAMT=7)   !RETURNS AMOUNT            (asfitinv)  
	   PARAMETER (OLD_ASFITSCM=8)    !SALES COM                 (asfitinv)
	   PARAMETER (OLD_ASFITPCM=9)    !PRIZE COM                 (asfitinv)
	   PARAMETER (OLD_ASFITADJ=10)   !ADJUSTMENT AMOUNT         (asfitinv)
	   PARAMETER (OLD_ASFITDUE=11)   !TOTAL AMOUNT DUE          (asfitinv)
C
C          OFFSETS 12-15 AVAILABLE FOR USE IN ASFITINV
C
C
C RECORD OFFSET PARAMETERS
C
	INTEGER*4 OLD_ASFLOK_OFF, OLD_ASFINF_OFF, OLD_ASFDAT_OFF, OLD_ASFDAY_OFF
	INTEGER*4 OLD_ASFBIL_OFF, OLD_ASFINV_OFF, OLD_ASFLGR_OFF, OLD_ASFGFL_OFF
	INTEGER*4 OLD_ASFWCT_OFF, OLD_ASFSC1_OFF, OLD_ASFSC2_OFF, OLD_ASFYTD_OFF
	INTEGER*4 OLD_ASFYTDINV_OFF, OLD_ASFGUT_OFF, OLD_ASFHWN_OFF
	INTEGER*4 OLD_ASFSPE_OFF, OLD_ASFMIS_OFF
	INTEGER*4 OLD_ASFDNM_OFF, OLD_ASFFRE_OFF
	INTEGER*4 OLD_ASFITINV_OFF, OLD_ASFITGSAL_OFF
	INTEGER*4 OLD_ASFGVT_OFF, OLD_ASFGVTIM_OFF
	INTEGER*4 OLD_ASFNCDC_OFF, OLD_ASFLCDC_OFF
C
	PARAMETER (OLD_ASFLOK_OFF = 1)					!   1
	PARAMETER (OLD_ASFINF_OFF = OLD_ASFLOK_OFF + 1)				!   2
	PARAMETER (OLD_ASFDAT_OFF = OLD_ASFINF_OFF + (ALENGTH/4))		! 192      ! 130
	PARAMETER (OLD_ASFDAY_OFF = OLD_ASFDAT_OFF + (2*ANUMDAY))		! 
	PARAMETER (OLD_ASFBIL_OFF = OLD_ASFDAY_OFF + (AGAMLEN*OMAXGAM*ANUMDAY))	!
	PARAMETER (OLD_ASFINV_OFF = OLD_ASFBIL_OFF +
     *		  ((AGAMLEN+MAXMLTD_SEL)*OMAXGAM*2))			!
	PARAMETER (OLD_ASFLGR_OFF = OLD_ASFINV_OFF + (AINVLEN*2))		!
	PARAMETER (OLD_ASFGFL_OFF = OLD_ASFLGR_OFF + (6*15))			!
	PARAMETER (OLD_ASFWCT_OFF = OLD_ASFGFL_OFF + OMAXGAM)			!
	PARAMETER (OLD_ASFSC1_OFF = OLD_ASFWCT_OFF + 1)				!
	PARAMETER (OLD_ASFSC2_OFF = OLD_ASFSC1_OFF + 1)				!
	PARAMETER (OLD_ASFYTD_OFF = OLD_ASFSC2_OFF + 1)				!
	PARAMETER (OLD_ASFYTDINV_OFF = OLD_ASFYTD_OFF + (AGAMLEN*OMAXGAM*2))	!
	PARAMETER (OLD_ASFGUT_OFF = OLD_ASFYTDINV_OFF + (AINVLEN*2))		!
	PARAMETER (OLD_ASFHWN_OFF = OLD_ASFGUT_OFF + GUTLEN)			!
	PARAMETER (OLD_ASFSPE_OFF = OLD_ASFHWN_OFF + 2)                         !
	PARAMETER (OLD_ASFMIS_OFF = OLD_ASFSPE_OFF + (ASPELEN*OMAXGAM*ANUMDAY))  !
	PARAMETER (OLD_ASFDNM_OFF = OLD_ASFMIS_OFF + (AMISLEN*2*ANUMDAY))	!
	PARAMETER (OLD_ASFITINV_OFF = OLD_ASFDNM_OFF + 1)			!
	PARAMETER (OLD_ASFITGSAL_OFF = OLD_ASFITINV_OFF + AITINVLEN)		!
	PARAMETER (OLD_ASFGVT_OFF = OLD_ASFITGSAL_OFF + 2*AITGAM)		!
	PARAMETER (OLD_ASFGVTIM_OFF = OLD_ASFGVT_OFF + 1)			!
	PARAMETER (OLD_ASFNCDC_OFF = OLD_ASFGVTIM_OFF + 1)			!
	PARAMETER (OLD_ASFLCDC_OFF = OLD_ASFNCDC_OFF + 1)			!
	PARAMETER (OLD_ASFFRE_OFF = OLD_ASFLCDC_OFF + 1)			! 7346    !7284
C
C CALCULATE FREE SPACE BASED ON MACHINE DEPENDENT SECTOR SIZES.
C SHOULD ALLWAYS ADD UP TO A EVEN SECTOR SIZE BYTES (VAX 512).
C NOTE! IN CALCULATIONS, FRE_OFF-2 IS DONE TO TAKE CARE OF WHEN THE DATA SIZE
C       ENDS ON A EVEN SECTOR SIZE.
C
	INTEGER*4 OLD_ASFSEC_VAX, OLD_ASFSEC_NUM
	PARAMETER (OLD_ASFSEC_VAX = SECSIZE/4)
	PARAMETER (OLD_ASFSEC_NUM = NINT(FLOAT((OLD_ASFFRE_OFF-2)/OLD_ASFSEC_VAX)+.5))
C
	INTEGER*4 OLD_ASFLEN , OLD_ASFSEC
	PARAMETER (OLD_ASFLEN = OLD_ASFSEC_VAX*OLD_ASFSEC_NUM)		!EVEN SEC SIZE.
	PARAMETER (OLD_ASFSEC = OLD_ASFLEN / 64)
C
	INTEGER*4 OLD_ASFREC(OLD_ASFLEN)
C
	INTEGER*4 OLD_ASFINF(ALENGTH/4)
	INTEGER*4 OLD_ASFDAT(2,ANUMDAY)
	INTEGER*4 OLD_ASFDAY(AGAMLEN,OMAXGAM,ANUMDAY)
	INTEGER*4 OLD_ASFBIL(AGAMLEN+MAXMLTD_SEL,OMAXGAM,2)
	INTEGER*4 OLD_ASFINV(AINVLEN,2)
	INTEGER*4 OLD_ASFLGR(6,15)
	INTEGER*4 OLD_ASFGFL(OMAXGAM)
	INTEGER*4 OLD_ASFYTD(AGAMLEN,OMAXGAM,2)
	INTEGER*4 OLD_ASFYTDINV(AINVLEN,2)
	INTEGER*4 OLD_ASFGUT(GUTLEN)
	INTEGER*4 OLD_ASFHWN(2)
        INTEGER*4 OLD_ASFSPE(ASPELEN,OMAXGAM,ANUMDAY)
        INTEGER*4 OLD_ASFMIS(AMISLEN,2,ANUMDAY)
	CHARACTER OLD_ASFBYT(ALENGTH)
	INTEGER*4 OLD_ASFWCT, OLD_ASFSC1, OLD_ASFSC2, OLD_ASFDNM, OLD_ASFLOK

        INTEGER*4 OLD_ASFITINV(AITINVLEN)
        INTEGER*4 OLD_ASFITGSAL(2,AITGAM)
	INTEGER*4 OLD_ASFGVT, OLD_ASFGVTIM, OLD_ASFNCDC, OLD_ASFLCDC

	EQUIVALENCE (OLD_ASFREC(OLD_ASFLOK_OFF),OLD_ASFLOK)
	EQUIVALENCE (OLD_ASFREC(OLD_ASFINF_OFF),OLD_ASFINF(1),OLD_ASFBYT(1))
	EQUIVALENCE (OLD_ASFREC(OLD_ASFDAT_OFF),OLD_ASFDAT(1,1))
	EQUIVALENCE (OLD_ASFREC(OLD_ASFDAY_OFF),OLD_ASFDAY(1,1,1))
	EQUIVALENCE (OLD_ASFREC(OLD_ASFBIL_OFF),OLD_ASFBIL(1,1,1))
	EQUIVALENCE (OLD_ASFREC(OLD_ASFINV_OFF),OLD_ASFINV(1,1))
	EQUIVALENCE (OLD_ASFREC(OLD_ASFLGR_OFF),OLD_ASFLGR(1,1))
	EQUIVALENCE (OLD_ASFREC(OLD_ASFGFL_OFF),OLD_ASFGFL(1))
	EQUIVALENCE (OLD_ASFREC(OLD_ASFWCT_OFF),OLD_ASFWCT)
	EQUIVALENCE (OLD_ASFREC(OLD_ASFSC1_OFF),OLD_ASFSC1)
	EQUIVALENCE (OLD_ASFREC(OLD_ASFSC2_OFF),OLD_ASFSC2)
	EQUIVALENCE (OLD_ASFREC(OLD_ASFYTD_OFF),OLD_ASFYTD(1,1,1))
	EQUIVALENCE (OLD_ASFREC(OLD_ASFYTDINV_OFF),OLD_ASFYTDINV(1,1))
	EQUIVALENCE (OLD_ASFREC(OLD_ASFGUT_OFF),OLD_ASFGUT(1))
	EQUIVALENCE (OLD_ASFREC(OLD_ASFHWN_OFF),OLD_ASFHWN(1))
        EQUIVALENCE (OLD_ASFREC(OLD_ASFSPE_OFF),OLD_ASFSPE(1,1,1))
        EQUIVALENCE (OLD_ASFREC(OLD_ASFMIS_OFF),OLD_ASFMIS(1,1,1))
	EQUIVALENCE (OLD_ASFREC(OLD_ASFDNM_OFF),OLD_ASFDNM)
	EQUIVALENCE (OLD_ASFREC(OLD_ASFITINV_OFF),OLD_ASFITINV(1))
	EQUIVALENCE (OLD_ASFREC(OLD_ASFITGSAL_OFF),OLD_ASFITGSAL(1,1))
	EQUIVALENCE (OLD_ASFREC(OLD_ASFGVT_OFF),OLD_ASFGVT)
	EQUIVALENCE (OLD_ASFREC(OLD_ASFGVTIM_OFF),OLD_ASFGVTIM)
	EQUIVALENCE (OLD_ASFREC(OLD_ASFNCDC_OFF),OLD_ASFNCDC)
	EQUIVALENCE (OLD_ASFREC(OLD_ASFLCDC_OFF),OLD_ASFLCDC)
C
