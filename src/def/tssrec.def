C TSSREC.DEF
C $Log:   GXAFXT:[GOLS]TSSREC.DEV  $
C  
C
C SUPERSCORE GAME DATA FILE RECORD LAYOUT (DISK)
C
C
C     TSSSTS                         ; GAME STATUS
C     TSSWEK                         ; WEEK NUMBER
C     TSSDAT                         ; DRAW DATE
C     TSSDRW                         ; DRAW NUMBER
C     TSSBSD                         ; BEGINNING SALES DATE
C     TSSESD                         ; ENDING SALES DATE
C     TSSPUP                         ; LAST PRUGE UPDATE  (FILE ONLY)
C     TSSUPD                         ; LAST FILE UPDATE   (FILE ONLY)
C     TSSCTM                         ; ACTUAL TIME WHEN CLOSED
C     TSSTIM                         ; TIME WHEN GAME SHOULD CLOSE
C     TSSSER                         ; LAST SERIAL NUMBER
C     TSSSAL                         ; TOTAL SALES
C     TSSPAD                         ; PRIZES PAID + REFUNDS
C     TSSPRG                         ; PRIZES PURGED
C     TSSPRF                         ; PAID REFUNDS
C     TSSREF                         ; TOTAL REFUND AMOUNT
C     TSSWON                         ; PRIZES WON
C     TSSPOL(2)                      ; ROLL POOL
C     TSSTPL                         ; TOTAL PRIZE POOL
C     TSSTBK                         ; TOTAL BREAKAGE
C     TSSBRK(2)                      ; WINNING ROUNDING POT
C     TSSABW                         ; AMOUNT BET ON WINNING SCORE
C     TSSODS                         ; WINNING ODDS
C     TSSWIN(2,3)                    ; WINNING RESULTS
C     TSSHLD(2,3)                    ; WINNING RESULTS HOLD
C     TSSTAX                         ; WINNING TAXES
C     TSSOTX(NUMTOT)                 ; OVER TAX LIMIT
C     TSSMID(NUMTOT)                 ; >REDMIN <RETSSX
C     TSSUTX(NUMTOT)                 ; UNDER TAX LIMIT
C     TSSHST                         ; HIGHEST WINNER FOR THIS GAME
C     TSSORM(NUMTOT)                 ; OVER RETSSX
C     TSSMNM(SSNMS_LEN/4)            ; MASTER EVENT NAME
C     TSSDES(SSDES_LEN/4)            ; EVENT DESCRIPTION
C     TSSSNM(SSNMS_LEN/4,3)          ; EVENT NAMES
C     TSSEST(3)                      ; EVENT STATUS'S
C     TSSREV                         ; REV #
C     TSSTYP                         ; GAME TYPE 1=SOC. 2=HOCK.
C     TSSPFN(5)                      ; POOL FILE NAME
C     TSSPOF(5)                      ; POOL OVERFLOW FILE NAME
C     TSSPRC                         ; MINIMUM STAKE
C     TSSSPR                         ; POOL PERCENTAGE
C     TSSWRO(NUMTOT,2)               ; WINNING: COUNT,AMOUNT REFUNDS ONLY
C     TSSWRA(NUMTOT,2)               ; WINNING: COUNT,AMOUNT REFUNDS ALTOGETHER
C     TSSWPO(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES ONLY
C     TSSWPA(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES ALTOGETHER
C     TSSWPR(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES AND REFUNDS
C                                    ; (1=PRWON,2=PRPAD)
C
C SCORE GAME FILE PARAMETERS
C
C
C
C+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
C This item is the property of GTECH Corporation, Providence, Rhode
C Island, and contains confidential and trade secret information. It
C may not be transferred from the custody or control of GTECH except
C as authorized in writing by an officer of GTECH. Neither this item
