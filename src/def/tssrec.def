C reproduced, published, or disclosed, in whole or in part, and
C directly or indirectly, except as expressly authorized by an
C officer of GTECH, pursuant to written agreement.
C
C Copyright 1997 GTECH Corporation. All rights reserved.
C+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
C
C
	INTEGER*4 TSSSTS,TSSWEK,TSSDAT,TSSBSD,TSSESD,TSSCTM,TSSTIM
	INTEGER*4 TSSSAL,TSSPAD,TSSPRG,TSSREF,TSSWON,TSSPOL(2),TSSTPL
	INTEGER*4 TSSTBK,TSSBRK(2),TSSABW,TSSODS,TSSWIN(2,3),TSSHLD(2,3)
	INTEGER*4 TSSTAX,TSSOTX(NUMTOT)
	INTEGER*4 TSSMID(NUMTOT),TSSUTX(NUMTOT),TSSHST,TSSORM(NUMTOT)
	INTEGER*4 TSSMNM(SSNMS_LEN/4),TSSSNM(SSNMS_LEN/4,3),TSSPFN(5)
	INTEGER*4 TSSPOF(5),TSSEST(3)
	INTEGER*4 TSSDRW,TSSSER,TSSPRF
	INTEGER*4 TSSREV,TSSTYP,TSSPRC,TSSPUP,TSSUPD
	INTEGER*4 TSSSPR,TSSDES(SSDES_LEN/4)
        INTEGER*4 TSSWRO(NUMTOT,2),TSSWRA(NUMTOT,2),TSSWPO(NUMTOT,2)
        INTEGER*4 TSSWPA(NUMTOT,2),TSSWPR(NUMTOT,2)
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
	INTEGER*4 TSSSTS_OFF, TSSWEK_OFF, TSSDAT_OFF, TSSDRW_OFF
	PARAMETER(TSSSTS_OFF = 1)					!001
	PARAMETER(TSSWEK_OFF = TSSSTS_OFF + 1)				!002
	PARAMETER(TSSDAT_OFF = TSSWEK_OFF + 1)				!003
	PARAMETER(TSSDRW_OFF = TSSDAT_OFF + 1)				!004
C
	INTEGER*4 TSSBSD_OFF, TSSESD_OFF, TSSPUP_OFF, TSSUPD_OFF
	PARAMETER(TSSBSD_OFF = TSSDRW_OFF + 1)				!005
	PARAMETER(TSSESD_OFF = TSSBSD_OFF + 1)				!006
	PARAMETER(TSSPUP_OFF = TSSESD_OFF + 1)				!007
	PARAMETER(TSSUPD_OFF = TSSPUP_OFF + 1)				!008
C
	INTEGER*4 TSSCTM_OFF, TSSTIM_OFF, TSSSER_OFF, TSSSAL_OFF
	PARAMETER(TSSCTM_OFF = TSSUPD_OFF + 1)				!009
	PARAMETER(TSSTIM_OFF = TSSCTM_OFF + 1)				!010
	PARAMETER(TSSSER_OFF = TSSTIM_OFF + 1)				!011
	PARAMETER(TSSSAL_OFF = TSSSER_OFF + 1)				!012
C
	INTEGER*4 TSSPAD_OFF, TSSPRG_OFF, TSSPRF_OFF, TSSREF_OFF
	PARAMETER(TSSPAD_OFF = TSSSAL_OFF + 1)				!013
	PARAMETER(TSSPRG_OFF = TSSPAD_OFF + 1)				!014
	PARAMETER(TSSPRF_OFF = TSSPRG_OFF + 1)				!015
	PARAMETER(TSSREF_OFF = TSSPRF_OFF + 1)				!016
C
	INTEGER*4 TSSWON_OFF, TSSPOL_OFF, TSSTPL_OFF, TSSTBK_OFF
	PARAMETER(TSSWON_OFF = TSSREF_OFF + 1)				!017
	PARAMETER(TSSPOL_OFF = TSSWON_OFF + 1)				!018
	PARAMETER(TSSTPL_OFF = TSSPOL_OFF + 2)				!020
	PARAMETER(TSSTBK_OFF = TSSTPL_OFF + 1)				!021
C
	INTEGER*4 TSSBRK_OFF, TSSABW_OFF, TSSODS_OFF, TSSWIN_OFF
	PARAMETER(TSSBRK_OFF = TSSTBK_OFF + 1)				!022
	PARAMETER(TSSABW_OFF = TSSBRK_OFF + 2)				!024
	PARAMETER(TSSODS_OFF = TSSABW_OFF + 1)				!025
	PARAMETER(TSSWIN_OFF = TSSODS_OFF + 1)				!026
C
	INTEGER*4 TSSHLD_OFF, TSSTAX_OFF, TSSOTX_OFF
	PARAMETER(TSSHLD_OFF = TSSWIN_OFF + 6)				!031
	PARAMETER(TSSTAX_OFF = TSSHLD_OFF + 6)				!037
	PARAMETER(TSSOTX_OFF = TSSTAX_OFF + 1)
C
	INTEGER*4 TSSMID_OFF, TSSUTX_OFF, TSSHST_OFF, TSSORM_OFF
	PARAMETER(TSSMID_OFF = TSSOTX_OFF + NUMTOT)			!039
	PARAMETER(TSSUTX_OFF = TSSMID_OFF + NUMTOT)			!041
	PARAMETER(TSSHST_OFF = TSSUTX_OFF + NUMTOT)			!043
	PARAMETER(TSSORM_OFF = TSSHST_OFF + 1)				!044
C
	INTEGER*4 TSSMNM_OFF, TSSSNM_OFF, TSSREV_OFF, TSSTYP_OFF
        INTEGER*4 TSSDES_OFF, TSSEST_OFF
	PARAMETER(TSSMNM_OFF = TSSORM_OFF + NUMTOT)			!046
	PARAMETER(TSSDES_OFF = TSSMNM_OFF + SSNMS_LEN/4)                !053 
	PARAMETER(TSSSNM_OFF = TSSDES_OFF + SSDES_LEN/4)		!074
	PARAMETER(TSSEST_OFF = TSSSNM_OFF + SSNMS_LEN/4*3)              !095
	PARAMETER(TSSREV_OFF = TSSEST_OFF + 3)				!098
	PARAMETER(TSSTYP_OFF = TSSREV_OFF + 1)				!099
C
	INTEGER*4 TSSPFN_OFF, TSSPOF_OFF, TSSPRC_OFF, TSSSPR_OFF
	PARAMETER(TSSPFN_OFF = TSSTYP_OFF + 1)				!100
	PARAMETER(TSSPOF_OFF = TSSPFN_OFF + 5)				!105
	PARAMETER(TSSPRC_OFF = TSSPOF_OFF + 5)				!110
	PARAMETER(TSSSPR_OFF = TSSPRC_OFF + 1)				!111
C
        INTEGER*4 TSSWRO_OFF, TSSWRA_OFF, TSSWPO_OFF, TSSWPA_OFF
        INTEGER*4 TSSWPR_OFF
        PARAMETER(TSSWRO_OFF = TSSSPR_OFF + 1)                          !112
        PARAMETER(TSSWRA_OFF = TSSWRO_OFF + 2*NUMTOT)                   !116
        PARAMETER(TSSWPO_OFF = TSSWRA_OFF + 2*NUMTOT)                   !120
        PARAMETER(TSSWPA_OFF = TSSWPO_OFF + 2*NUMTOT)                   !124
        PARAMETER(TSSWPR_OFF = TSSWPA_OFF + 2*NUMTOT)                   !128

        INTEGER*4 TSSFRE_OFF
        PARAMETER(TSSFRE_OFF = TSSWPR_OFF + 2*NUMTOT)                   !129
C
C CALCULATE FREE SPACE BASED ON MACHINE DEPENDENT SECTOR SIZES.
C SHOULD ALLWAYS ADD UP TO A EVEN SECTOR SIZE BYTES (VAX 512).
C NOTE! IN CALCULATIONS, FRE_OFF-2 IS DONE TO TAKE CARE OF WHEN THE DATA SIZE
C       ENDS ON A EVEN SECTOR SIZE.
C
        INTEGER*4 TSSSEC_VAX, TSSSEC_NUM
        PARAMETER (TSSSEC_VAX = SECSIZE/4)
	PARAMETER (TSSSEC_NUM = NINT(FLOAT((TSSFRE_OFF-2)/TSSSEC_VAX)+.5))
C
        INTEGER*4 TSSLEN, TSSSEC
        PARAMETER(TSSLEN = TSSSEC_VAX * TSSSEC_NUM)        ! even sector size
        PARAMETER(TSSSEC = TSSLEN/64)
C
        INTEGER*4 TSSREC(TSSLEN)
C
	EQUIVALENCE(TSSSTS,TSSREC(TSSSTS_OFF))
	EQUIVALENCE(TSSWEK,TSSREC(TSSWEK_OFF))
	EQUIVALENCE(TSSDAT,TSSREC(TSSDAT_OFF))
	EQUIVALENCE(TSSDRW,TSSREC(TSSDRW_OFF))
	EQUIVALENCE(TSSBSD,TSSREC(TSSBSD_OFF))
	EQUIVALENCE(TSSESD,TSSREC(TSSESD_OFF))
	EQUIVALENCE(TSSPUP,TSSREC(TSSPUP_OFF))
	EQUIVALENCE(TSSUPD,TSSREC(TSSUPD_OFF))
	EQUIVALENCE(TSSCTM,TSSREC(TSSCTM_OFF))
	EQUIVALENCE(TSSTIM,TSSREC(TSSTIM_OFF))
	EQUIVALENCE(TSSSER,TSSREC(TSSSER_OFF))
	EQUIVALENCE(TSSSAL,TSSREC(TSSSAL_OFF))
	EQUIVALENCE(TSSPAD,TSSREC(TSSPAD_OFF))
	EQUIVALENCE(TSSPRG,TSSREC(TSSPRG_OFF))
	EQUIVALENCE(TSSPRF,TSSREC(TSSPRF_OFF))
	EQUIVALENCE(TSSREF,TSSREC(TSSREF_OFF))
	EQUIVALENCE(TSSWON,TSSREC(TSSWON_OFF))
	EQUIVALENCE(TSSPOL,TSSREC(TSSPOL_OFF))
	EQUIVALENCE(TSSTPL,TSSREC(TSSTPL_OFF))
	EQUIVALENCE(TSSTBK,TSSREC(TSSTBK_OFF))
	EQUIVALENCE(TSSBRK,TSSREC(TSSBRK_OFF))
	EQUIVALENCE(TSSABW,TSSREC(TSSABW_OFF))
	EQUIVALENCE(TSSODS,TSSREC(TSSODS_OFF))
	EQUIVALENCE(TSSWIN,TSSREC(TSSWIN_OFF))
	EQUIVALENCE(TSSHLD,TSSREC(TSSHLD_OFF))
	EQUIVALENCE(TSSTAX,TSSREC(TSSTAX_OFF))
	EQUIVALENCE(TSSOTX,TSSREC(TSSOTX_OFF))
	EQUIVALENCE(TSSMID,TSSREC(TSSMID_OFF))
	EQUIVALENCE(TSSUTX,TSSREC(TSSUTX_OFF))
	EQUIVALENCE(TSSHST,TSSREC(TSSHST_OFF))
	EQUIVALENCE(TSSORM,TSSREC(TSSORM_OFF))
	EQUIVALENCE(TSSMNM,TSSREC(TSSMNM_OFF))
	EQUIVALENCE(TSSDES,TSSREC(TSSDES_OFF))
	EQUIVALENCE(TSSSNM,TSSREC(TSSSNM_OFF))
	EQUIVALENCE(TSSEST,TSSREC(TSSEST_OFF))
	EQUIVALENCE(TSSREV,TSSREC(TSSREV_OFF))
	EQUIVALENCE(TSSTYP,TSSREC(TSSTYP_OFF))
	EQUIVALENCE(TSSPFN,TSSREC(TSSPFN_OFF))
	EQUIVALENCE(TSSPOF,TSSREC(TSSPOF_OFF))
        EQUIVALENCE(TSSPRC,TSSREC(TSSPRC_OFF))
	EQUIVALENCE(TSSSPR,TSSREC(TSSSPR_OFF))
        EQUIVALENCE(TSSWRO,TSSREC(TSSWRO_OFF))
        EQUIVALENCE(TSSWRA,TSSREC(TSSWRA_OFF))
        EQUIVALENCE(TSSWPO,TSSREC(TSSWPO_OFF))
        EQUIVALENCE(TSSWPA,TSSREC(TSSWPA_OFF))
        EQUIVALENCE(TSSWPR,TSSREC(TSSWPR_OFF))

