	INTEGER*4  X2FEMES_MESLEN, X2FEMES_PROTID, X2FEMES_PROTID_X2X
	PARAMETER (X2FEMES_MESLEN=1)    !MESSAGE LENGTH
	PARAMETER (X2FEMES_PROTID=3)    !PROTOCOL ID
	   PARAMETER (X2FEMES_PROTID_X2X=1) !THIS PROTOCOL
C
	INTEGER*4  X2FEMES_HOST_ID, X2FEMES_HOST_ID_PROCESS_MASK
	INTEGER*4  X2FEMES_HOST_ID_EB_FLAG
	PARAMETER (X2FEMES_HOST_ID=4)   !HOST ID, USED BY RELAY
	   PARAMETER (X2FEMES_HOST_ID_PROCESS_MASK='0000000F'X)!
C                              ;PROCESS NUMBER MASK
	   PARAMETER (X2FEMES_HOST_ID_EB_FLAG='00000080'X) !END OF
C                              ;BROADCAST FLAG
C
C                                      ;0 IF .NON.RELAY
	INTEGER*4  X2FEMES_MESTYP, X2FEMES_MESTYP_UP
	INTEGER*4  X2FEMES_MESTYP_DOWN, X2FEMES_MESTYP_CMD
	INTEGER*4  X2FEMES_MESTYP_ACK, X2FEMES_MESTYP_ERR
	INTEGER*4  X2FEMES_MESTYP_ALARM
	PARAMETER (X2FEMES_MESTYP=5)      !MESSAGE TYPE
	   PARAMETER (X2FEMES_MESTYP_UP=1) !UPLINE MESSAGE
	   PARAMETER (X2FEMES_MESTYP_DOWN=2) !DOWNLINE MESSAGE
	   PARAMETER (X2FEMES_MESTYP_CMD=3)   !COMMAND/STATUS MESS
	   PARAMETER (X2FEMES_MESTYP_ACK=66)        !DELIVERY ACK
	   PARAMETER (X2FEMES_MESTYP_ERR=130) !ERROR MESSAGE
	   PARAMETER (X2FEMES_MESTYP_ALARM=240) !ALARM MESSAGE
C
C
	INTEGER*4  X2FEMES_HEADLEN, X2FEMES_DELIVERY
	INTEGER*4  X2FEMES_DELIVERY_ESCAPE
	PARAMETER (X2FEMES_HEADLEN=6)    !HEADER LENGTH IN BYTES
	PARAMETER (X2FEMES_DELIVERY=7)   !DELIVERY ERROR CODE FOR ERROR
C                                         MESSAGE
        PARAMETER (X2FEMES_DELIVERY_ESCAPE='FF'X)   !USE 4 BYTE ERR CODE
C                                                   ;AFTER ADDRESS FIELD
C
	INTEGER*4  X2FEMES_FLAGS
	INTEGER*4  X2FEMES_FLAGS_ND, X2FEMES_FLAGS_ER
	INTEGER*4  X2FEMES_FLAGS_RC, X2FEMES_FLAGS_DVS
	INTEGER*4  X2FEMES_FLAGS_DA, X2FEMES_FLAGS_RESPONCE
	PARAMETER (X2FEMES_FLAGS=7)      !FLAGS
	   PARAMETER (X2FEMES_FLAGS_ND='00000080'X) !DOWNLINE, NO DATA
	   PARAMETER (X2FEMES_FLAGS_ER='00000040'X) !DOWN, REPORT ERR
	   PARAMETER (X2FEMES_FLAGS_DA='00000020'X) !DOWN,DELIVERY ACK
	   PARAMETER (X2FEMES_FLAGS_RC='00000010'X) !DOWN,REVERSE CHARGE
	   PARAMETER (X2FEMES_FLAGS_DVS='00000008'X)!DOWN,VER. SEQ.
	   PARAMETER (X2FEMES_FLAGS_RESPONCE=0)     !DOWN, DEFAULT
        INTEGER*4 X2FEMES_FLAGS_VS, X2FEMES_FLAGS_AA
        INTEGER*4 X2FEMES_FLAGS_EVS
           PARAMETER (X2FEMES_FLAGS_VS='00000080'X) !UPLINE, VER SEQ.
           PARAMETER (X2FEMES_FLAGS_AA='00000040'X) !UPLINE, ALT ADDR
           PARAMETER (X2FEMES_FLAGS_EVS='00000020'X) !UPLINE,EXT. VER SEQ
C
C
        INTEGER*4  X2FEMES_NET_TYPE, X2FEMES_NET_TYPE_UNDEF
        INTEGER*4  X2FEMES_NET_TYPE_X21, X2FEMES_NET_TYPE_X25SVC
        INTEGER*4  X2FEMES_NET_TYPE_X25PVC, X2FEMES_NET_TYPE_X28PAD
        INTEGER*4  X2FEMES_NET_TYPE_GTDIAL, X2FEMES_NET_TYPE_ASYPVC
        INTEGER*4  X2FEMES_NET_TYPE_USATPVC
        PARAMETER (X2FEMES_NET_TYPE = 7)             !DOWN NETWORK ADDRESS TYPE
           PARAMETER(X2FEMES_NET_TYPE_UNDEF=0)       !UNDEFINED
           PARAMETER(X2FEMES_NET_TYPE_X21 = 1)       !X21 SWITCHED CIRCUIT
           PARAMETER(X2FEMES_NET_TYPE_X25SVC=2)      !X25 SVC
           PARAMETER(X2FEMES_NET_TYPE_X25PVC=3)      !X25 PVC
           PARAMETER(X2FEMES_NET_TYPE_X28PAD=4)      !X28 PAD SERVER
           PARAMETER(X2FEMES_NET_TYPE_GTDIAL=5)      !GSC DIAL
           PARAMETER(X2FEMES_NET_TYPE_ASYPVC=6)      !ASYNC PVC
           PARAMETER(X2FEMES_NET_TYPE_USATPVC=7)     !USAT PVC
	INTEGER*4  X2FEMES_CONNCTL
	INTEGER*4  X2FEMES_RESPONCE, X2FEMES_CALL, X2FEMES_UNCCALL
	INTEGER*4  X2FEMES_FASTSEL, X2FEMES_FEIDCO
	PARAMETER (X2FEMES_CONNCTL=8)       !CONNECTION CONTROL UPLINE
	   PARAMETER (X2FEMES_RESPONCE=0)          !RESPONCE MODE
	   PARAMETER (X2FEMES_CALL='00000010'X)    !CALL, IF CONNECTED
	   PARAMETER (X2FEMES_UNCCALL='00000020'X) !UNCONDITIONAL CALL
	   PARAMETER (X2FEMES_FASTSEL='00000030'X) !FAST SELECT CALL
	   PARAMETER (X2FEMES_FEIDCO='00000040'X) !USE FE ID TO CONNECT
C
C
	   INTEGER*4  X2FEMES_CONDDISC, X2FEMES_UNCDDISC
	   INTEGER*4  X2FEMES_FASTDISC
	   PARAMETER (X2FEMES_CONDDISC=1)       !CONDITIONAL DISCONNECT
	   PARAMETER (X2FEMES_UNCDDISC=2)       !UNCONDITIONAL DISCN
	   PARAMETER (X2FEMES_FASTDISC=3)       !FAST SELECT DISCONNECT
C
        INTEGER*4  X2FEMES_MAX_CONNTYPE
           PARAMETER (X2FEMES_MAX_CONNTYPE = 10)
	INTEGER*4  X2FEMES_CONNTYP, X2FEMES_CONTYP_MASK
	INTEGER*4  X2FEMES_CONTYP_NONE, X2FEMES_CONTYP_X21
	INTEGER*4  X2FEMES_CONTYP_X25SVC, X2FEMES_CONTYP_X25PVC
	INTEGER*4  X2FEMES_CONTYP_X28PAD, X2FEMES_CONTYP_X25FAST
        INTEGER*4  X2FEMES_CONTYP_ASYPVC, X2FEMES_CONTYP_X25_X32
        INTEGER*4  X2FEMES_CONTYP_X25_DIAL, X2FEMES_CONTYP_GTECH_DIAL
        INTEGER*4  X2FEMES_CONTYP_USAT_PVC
	INTEGER*4  X2FEMES_CONTYP_ICOC
	PARAMETER (X2FEMES_CONNTYP=8)    !UPLINE, CONNECTION TYPE
	   PARAMETER (X2FEMES_CONTYP_MASK='0000007F'X) !ONLY THESE BITS
	   PARAMETER (X2FEMES_CONTYP_NONE=0)          !NOT TYPE SENT
	   PARAMETER (X2FEMES_CONTYP_X21=1)           !X21 TYPE
	   PARAMETER (X2FEMES_CONTYP_X25SVC=2)        !X25, SVC
	   PARAMETER (X2FEMES_CONTYP_X25PVC=3)        !X25, PVC
	   PARAMETER (X2FEMES_CONTYP_X28PAD=4)        !X28, PAD
	   PARAMETER (X2FEMES_CONTYP_X25FAST=5)       !X25, FAST SELECT
           PARAMETER (X2FEMES_CONTYP_ASYPVC=6)        !ASYNC PVC
           PARAMETER (X2FEMES_CONTYP_X25_X32=7)       !X.25 THRU X.32
           PARAMETER (X2FEMES_CONTYP_X25_DIAL=8)      !X.25 THRU DIAL UP
           PARAMETER (X2FEMES_CONTYP_GTECH_DIAL=9)    !GTECH DIAL UP
           PARAMETER (X2FEMES_CONTYP_USAT_PVC=10)     !USAT PVC
	   PARAMETER (X2FEMES_CONTYP_ICOC='00000080'X) !SET FOR INCALL
C
C Only 3 bits in downline station data available for Network address type.
C The following table, indexed by connection type above, allows cross
C reference of Connection type with available Network address types.
C
           INTEGER*4 X2FEMES_NET_TYPE_XREF(0:X2FEMES_MAX_CONNTYPE)
	   COMMON/X2FEMES_NET_TYPE_XREF/ X2FEMES_NET_TYPE_XREF
           DATA      X2FEMES_NET_TYPE_XREF/
     *               X2FEMES_NET_TYPE_UNDEF,X2FEMES_NET_TYPE_X21,
     *               X2FEMES_NET_TYPE_X25SVC,X2FEMES_NET_TYPE_X25PVC,
     *               X2FEMES_NET_TYPE_X28PAD,X2FEMES_NET_TYPE_UNDEF,
     *               X2FEMES_NET_TYPE_ASYPVC,X2FEMES_NET_TYPE_UNDEF,
     *               X2FEMES_NET_TYPE_UNDEF,X2FEMES_NET_TYPE_GTDIAL,
     *               X2FEMES_NET_TYPE_UNDEF/
C
	INTEGER*4  X2FEMES_CONN_ID, X2FEMES_ADRLEN, X2FEMES_ADR
	PARAMETER (X2FEMES_CONN_ID=9)               !CONNECTION ID
	PARAMETER (X2FEMES_ADRLEN=12)               !ADRESS LENGTH
	PARAMETER (X2FEMES_ADR=13)                  !ADDRESS
        INTEGER*4 X2FEMES_VER_LEN, X2FEMES_ALT_ADDLEN,X2FEMES_EVS_LEN
        PARAMETER (X2FEMES_VER_LEN=2)               !VERIFICATION SEQ LENGTH
        PARAMETER (X2FEMES_ALT_ADDLEN=1)            !ALT ADDRESS LENGTH
        PARAMETER (X2FEMES_EVS_LEN=1)              !EVS LENGTH FIELD
C
C     ALARM/LOG/COMMAND/STATUS DATA CONTINUATION AREA
C
	INTEGER*4  X2FEMES_FORMAT, X2FEMES_FORM_X2X, X2FEMES_FORM_ASCII
	PARAMETER (X2FEMES_FORMAT=6)    ! FORMAT ID
	   PARAMETER (X2FEMES_FORM_X2X=1)
           PARAMETER (X2FEMES_FORM_ASCII=1)
C
	INTEGER*4  X2FEMES_MESCOD, X2FEMES_ALM, X2FEMES_CR
	INTEGER*4  X2FEMES_MESCOD_CONSTS, X2FEMES_MESCOD_TEXT
	PARAMETER (X2FEMES_MESCOD=7)    ! MESSAGE CODE
	   PARAMETER (X2FEMES_ALM='00008000'X)  !ALARM FLAG
	   PARAMETER (X2FEMES_CR='00008000'X)   !COMMAND RESPONCE
	   PARAMETER (X2FEMES_MESCOD_CONSTS=1)  !CONNECTION STATUS
           PARAMETER (X2FEMES_MESCOD_TEXT=2)    !ASCII MESSAGE
C
	INTEGER*4  X2FEMES_DATALEN, X2FEMES_MAXDATALEN
	PARAMETER (X2FEMES_DATALEN=9)     ! LOG DATA LENGTH
	   PARAMETER (X2FEMES_MAXDATALEN=128) !UP TO 128 BYTES
C
C FRONT END COMMAND/ALARM CODE 1 - CONNECTION STATUS AND/OR
C DISCONNECTION ALARM.
C
	INTEGER*4  X2FEMES_CONSTS_LINE, X2FEMES_CONSTS_CONID
	INTEGER*4  X2FEMES_CONSTS_ABSTIM, X2FEMES_CONSTS_DURTIM
C
	INTEGER*4  X2FEMES_CONSTS_ESTTIM
	PARAMETER (X2FEMES_CONSTS_LINE=11)          !HDLC LINE UTILIZED
	PARAMETER (X2FEMES_CONSTS_CONID=12)         !CONNECTION ID
	PARAMETER (X2FEMES_CONSTS_ABSTIM=15)        !ABSOLUTE TIME CONN
	PARAMETER (X2FEMES_CONSTS_DURTIM=19)        !CONNECTION DURATION
	PARAMETER (X2FEMES_CONSTS_ESTTIM=23)        !ESTABLISHMENT TIME
C
	INTEGER*4  X2FEMES_CONSTS_TERCOD, X2FEMES_CONSTS_TERCOD_IC
	INTEGER*4  X2FEMES_CONSTS_TERCOD_UP
	PARAMETER (X2FEMES_CONSTS_TERCOD=25)        !TERMINATION REASON
	   PARAMETER (X2FEMES_CONSTS_TERCOD_IC='00000080'X) !INCALL
	   PARAMETER (X2FEMES_CONSTS_TERCOD_UP='00000040'X) !STILL UP
C
	INTEGER*4  X2FEMES_CONSTS_DIACOD, X2FEMES_CONSTS_NETERR
	INTEGER*4  X2FEMES_CONSTS_ADRLEN, X2FEMES_CONSTS_STNADD
	INTEGER*4  X2FEMES_CONSTS_MSGRCV, X2FEMES_CONSTS_MSGSND
        INTEGER*4  X2FEMES_CONSTS_X32ADRLEN, X2FEMES_CONSTS_X32ADDR
	PARAMETER (X2FEMES_CONSTS_DIACOD=26)        !DIAGNOSTICS CODE
	PARAMETER (X2FEMES_CONSTS_NETERR=27)        !NETWORK ERROR CODE
	PARAMETER (X2FEMES_CONSTS_ADRLEN=28)        !STN ADDRESS LENGTH
	PARAMETER (X2FEMES_CONSTS_STNADD=29)        !STATION ADDRESS
	PARAMETER (X2FEMES_CONSTS_MSGSND=37)        !MESSAGES SENT
	PARAMETER (X2FEMES_CONSTS_MSGRCV=39)        !MESSAGES RECEIVED
        PARAMETER (X2FEMES_CONSTS_X32ADRLEN=41)     !X32 ADDRESS LENGTH
        PARAMETER (X2FEMES_CONSTS_X32ADDR=42)       !X32 ADDRESS 
C
        INTEGER*4  X2FEMES_CONSTS_CONTYPE           !LEN OF CONN. TYPE
        INTEGER*4  X2FEMES_CONSTS_EVS               !LEN OF EVS. FIELD
        PARAMETER  (X2FEMES_CONSTS_CONTYPE=1)
        PARAMETER  (X2FEMES_CONSTS_EVS=1)
