C
C OFFSET PARAMETERS
C
	INTEGER*4 LPASTSOFF,LPAEMISOFF,LPANUMTCKOFF,LPANUMSEROFF
	INTEGER*4 LPAPRCOFF
	PARAMETER(LPASTSOFF     = 1)
	PARAMETER(LPAEMISOFF    = LPASTSOFF    + PAGEMI*NUMPAS)
	PARAMETER(LPANUMTCKOFF  = LPAEMISOFF   + PAGEMI*NUMPAS)
	PARAMETER(LPANUMSEROFF  = LPANUMTCKOFF + PAGEMI*NUMPAS)
	PARAMETER(LPAPRCOFF     = LPANUMSEROFF + PAGEMI*NUMPAS)

	INTEGER*4 LPAESDOFF,LPATIMOFF,LPACTMOFF,LPAPLANOFF
	INTEGER*4 LPAREDAMTOFF,LPADIVOFF,LPAPRGCDCOFF,LPAEMTOFF,LPAWSEROFF
	PARAMETER(LPAESDOFF     = LPAPRCOFF    + PAGEMI*NUMPAS)
	PARAMETER(LPATIMOFF     = LPAESDOFF    + PAGEMI*NUMPAS)
	PARAMETER(LPACTMOFF     = LPATIMOFF    + PAGEMI*NUMPAS)
	PARAMETER(LPAPLANOFF    = LPACTMOFF    + PAGEMI*NUMPAS)
	PARAMETER(LPAPRGCDCOFF  = LPAPLANOFF   + PAGEMI*NUMPAS)
	PARAMETER(LPAREDAMTOFF  = LPAPRGCDCOFF + PAGEMI*NUMPAS)
	PARAMETER(LPADIVOFF     = LPAREDAMTOFF + PAGEMI*NUMPAS)
	PARAMETER(LPAEMTOFF     = LPADIVOFF    + PAGEMI*NUMPAS)
	PARAMETER(LPAWSEROFF    = LPAEMTOFF    + PAGEMI*NUMPAS)

	INTEGER*4 LPAWNUMOFF,LPAWINOFF
	PARAMETER(LPAWNUMOFF    = LPAWSEROFF   + PAGEMI*NUMPAS)
	PARAMETER(LPAWINOFF     = LPAWNUMOFF   + PAGDIV*PAGEMI*NUMPAS)

	INTEGER*4 LPASHROFF,LPASHVOFF
	INTEGER*4 LPAPADOFF,LPATYPOFF,LPADIGOFF,LPAIDNUMOFF
	PARAMETER(LPASHROFF     = LPAWINOFF    + PAGNBR*PAGDIV*PAGEMI*NUMPAS)
	PARAMETER(LPASHVOFF     = LPASHROFF    + PAGDIV*PAGEMI*NUMPAS)
        PARAMETER(LPAPADOFF     = LPASHVOFF    + 2*PAGDIV*PAGEMI*NUMPAS)
	PARAMETER(LPATYPOFF     = LPAPADOFF    + PAGDIV*PAGEMI*NUMPAS)
	PARAMETER(LPADIGOFF     = LPATYPOFF    + PAGDIV*PAGEMI*NUMPAS)
	PARAMETER(LPAIDNUMOFF   = LPADIGOFF    + PAGDIV*PAGEMI*NUMPAS)

	INTEGER*4 LPAEXSHROFF,LPAEXSHVOFF
	PARAMETER(LPAEXSHROFF   = LPAIDNUMOFF  + PAGDIV*PAGEMI*NUMPAS)
	PARAMETER(LPAEXSHVOFF   = LPAEXSHROFF  + PAGEDV*PAGEMI*NUMPAS)

	INTEGER*4 LPACHGCDCOFF, LPABSDOFF, LPAEXPADOFF
	PARAMETER (LPAEXPADOFF  = LPAEXSHVOFF  + 2*PAGEDV*PAGEMI*NUMPAS)
	PARAMETER (LPACHGCDCOFF = LPAEXPADOFF  + PAGEDV*PAGEMI*NUMPAS)
	PARAMETER (LPABSDOFF    = LPACHGCDCOFF + PAGEMI*NUMPAS)

	INTEGER*4 LPAPRGOFF,LPAPUPOFF,LPARETCNTOFF,LPANOFFRAOFF
	PARAMETER (LPAPRGOFF    = LPABSDOFF    + PAGEMI*NUMPAS)
	PARAMETER (LPAPUPOFF    = LPAPRGOFF    + PAGDIV*PAGEMI*NUMPAS)
	PARAMETER (LPARETCNTOFF = LPAPUPOFF    + PAGEMI*NUMPAS)
        PARAMETER (LPANOFFRAOFF = LPARETCNTOFF + PAGEMI*NUMPAS)

	INTEGER*4 LPATODPAYOFF,LPATOPAYAMTOFF,LPARETAFTAMTOFF,LPAMAXDAYPAYOFF
        PARAMETER (LPATODPAYOFF    = LPANOFFRAOFF    + PAGEMI*NUMPAS)
        PARAMETER (LPATOPAYAMTOFF  = LPATODPAYOFF    + PAGEMI*NUMPAS)
        PARAMETER (LPARETAFTAMTOFF = LPATOPAYAMTOFF  + PAGEMI*NUMPAS)
        PARAMETER (LPAMAXDAYPAYOFF = LPARETAFTAMTOFF + PAGEMI*NUMPAS)

	INTEGER*4 LPADRAWOFF
        PARAMETER (LPADRAWOFF      = LPAMAXDAYPAYOFF + PAGEMI*NUMPAS)
C====== ePassive
	INTEGER*4 LPAFINOFF
        PARAMETER (LPAFINOFF       = LPADRAWOFF      + PAGEMI*NUMPAS)
        
	INTEGER*4 LPADIVPLNOFF
        PARAMETER (LPADIVPLNOFF    = LPAFINOFF       + NUMTOT*NUMFIN*PAGEMI*NUMPAS)    
        
	INTEGER*4 LPALITDRWOFF
        PARAMETER (LPALITDRWOFF    = LPADIVPLNOFF    + NUMTOT*(PAGDIV+PAGEDV)*PAGEMI*NUMPAS)                  
C====== ePassive        
	INTEGER*4 LPARECLEN
        PARAMETER (LPARECLEN       = LPALITDRWOFF    + 6*PAGEMI*NUMPAS)
C
C LOGICAL RECORD DEFINITION
C
	INTEGER*4 LPAREC(LPARECLEN)
C
C GAME FILE VARIABLE DEFINITIONS
C
	INTEGER*4 LPASTS(PAGEMI,NUMPAS)		!STATUS
	INTEGER*4 LPAEMIS(PAGEMI,NUMPAS)	!EMISSION
	INTEGER*4 LPANUMTCK(PAGEMI,NUMPAS)	!NUMBER OF TICKETS/EMISSION
	INTEGER*4 LPANUMSER(PAGEMI,NUMPAS)	!NUMBER OF SERIES/EMISSION
	INTEGER*4 LPAPRC(PAGEMI,NUMPAS)		!BASE PRICE
	INTEGER*4 LPAESD(PAGEMI,NUMPAS)		!END EMISSION DATE
	INTEGER*4 LPATIM(PAGEMI,NUMPAS)		!CLOSE TIME
	INTEGER*4 LPACTM(PAGEMI,NUMPAS)		!ACTUAL CLOSE TIME
	INTEGER*4 LPAPLAN(PAGEMI,NUMPAS)	!EMISSION PLAN
	INTEGER*4 LPAPRGCDC(PAGEMI,NUMPAS)	!PURGING CDC OF THIS EMISSION
	INTEGER*4 LPAREDAMT(PAGEMI,NUMPAS)	!REDEMPTION AMOUNT
	INTEGER*4 LPADIV(PAGEMI,NUMPAS)		!NUMBER OF DIVISIONS
	INTEGER*4 LPAEMT(PAGEMI,NUMPAS)		!EMISSION TYPE (POP/CLAS/EXTRA)
	INTEGER*4 LPAWSER(PAGEMI,NUMPAS)	!WINNING SERIE
	INTEGER*4 LPAWNUM(PAGDIV,PAGEMI,NUMPAS)	!NUMBER OF WIN. NO.
	INTEGER*4 LPAWIN(PAGNBR,PAGDIV,PAGEMI,NUMPAS) !WINNING NUMBERS
	INTEGER*4 LPASHR(PAGDIV,PAGEMI,NUMPAS)	!SHARES
	INTEGER*8 LPASHV(PAGDIV,PAGEMI,NUMPAS)	!SHARE VALUE
	INTEGER*4 LPAPAD(PAGDIV,PAGEMI,NUMPAS)	!PRIZES PAID(COUNT)	
	INTEGER*4 LPATYP(PAGDIV,PAGEMI,NUMPAS)	!PRIZE TYPE
	INTEGER*4 LPADIG(PAGDIV,PAGEMI,NUMPAS)	!# OF DIGITS
	INTEGER*4 LPAIDNUM(PAGDIV,PAGEMI,NUMPAS)!CROSS REFERENCE
	INTEGER*4 LPAEXSHR(PAGEDV,PAGEMI,NUMPAS)!EXTRA SHARES
	INTEGER*8 LPAEXSHV(PAGEDV,PAGEMI,NUMPAS)!EXTRA SHARE VALUE
	INTEGER*4 LPAEXPAD(PAGEDV,PAGEMI,NUMPAS)!EXTRA PRIZES PAID (COUNT)
	INTEGER*4 LPACHGCDC(PAGEMI,NUMPAS)	!LAST CHANGE DATE
	INTEGER*4 LPABSD(PAGEMI,NUMPAS)		!BEGIN SALES DATE
	INTEGER*4 LPAPRG(PAGDIV,PAGEMI,NUMPAS)	!PRIZES PURGED
	INTEGER*4 LPAPUP(PAGEMI,NUMPAS)		!LAST PURGE UPDATE
	INTEGER*4 LPARETCNT(PAGEMI,NUMPAS)      !RETURN TICKETS COUNT
	INTEGER*4 LPANOFFRA(PAGEMI,NUMPAS)	!# OF TICKET FRACTIONS
	INTEGER*4 LPATODPAY(PAGEMI,NUMPAS)	!FLAG FOR PRIZES PAID TODAY
	INTEGER*4 LPATOPAYAMT(PAGEMI,NUMPAS)	!AMT FOR PRIZES THAT CAN BE PAID
	INTEGER*4 LPARETAFTAMT(PAGEMI,NUMPAS)	!AMT FOR RETURNED TCKS AFTER DRW
	INTEGER*4 LPAMAXDAYPAY(PAGEMI,NUMPAS)	!MAXIMUM # OF DAYS THAT PRIZES CAN BE PAID BY AN AGENT
                                                !(AFTER THIS, ONLY SCML PRIVILEGED TERMINAL CAN PAY THE PRIZE)
	INTEGER*4 LPADRAW(PAGEMI,NUMPAS)	!SCML DRAW NUMBER
C====== ePassive	
	INTEGER*4 LPAFIN(NUMTOT,NUMFIN,PAGEMI,NUMPAS)	        !Activity matrix by type or tras
	INTEGER*4 LPADIVPLN(NUMTOT,PAGDIV+PAGEDV,PAGEMI,NUMPAS) !Plano real by prize ticket paid
	INTEGER*4 LPALITDRW(6,PAGEMI,NUMPAS)    !Draw short description
C====== ePassive	
C
C EQUIVALENCES
C       
	EQUIVALENCE(LPASTS,LPAREC(LPASTSOFF))
	EQUIVALENCE(LPAEMIS,LPAREC(LPAEMISOFF))
	EQUIVALENCE(LPANUMTCK,LPAREC(LPANUMTCKOFF))
	EQUIVALENCE(LPANUMSER,LPAREC(LPANUMSEROFF))
	EQUIVALENCE(LPAPRC,LPAREC(LPAPRCOFF))
	EQUIVALENCE(LPAESD,LPAREC(LPAESDOFF))
	EQUIVALENCE(LPATIM,LPAREC(LPATIMOFF))
	EQUIVALENCE(LPACTM,LPAREC(LPACTMOFF))
	EQUIVALENCE(LPAPLAN,LPAREC(LPAPLANOFF))
	EQUIVALENCE(LPAPRGCDC,LPAREC(LPAPRGCDCOFF))
	EQUIVALENCE(LPAREDAMT,LPAREC(LPAREDAMTOFF))
	EQUIVALENCE(LPADIV,LPAREC(LPADIVOFF))
	EQUIVALENCE(LPAEMT,LPAREC(LPAEMTOFF))
	EQUIVALENCE(LPAWSER,LPAREC(LPAWSEROFF))
	EQUIVALENCE(LPAWNUM,LPAREC(LPAWNUMOFF))
	EQUIVALENCE(LPAWIN,LPAREC(LPAWINOFF))
	EQUIVALENCE(LPASHR,LPAREC(LPASHROFF))
	EQUIVALENCE(LPASHV,LPAREC(LPASHVOFF))
	EQUIVALENCE(LPAPAD,LPAREC(LPAPADOFF))
	EQUIVALENCE(LPATYP,LPAREC(LPATYPOFF))
	EQUIVALENCE(LPADIG,LPAREC(LPADIGOFF))
	EQUIVALENCE(LPAIDNUM,LPAREC(LPAIDNUMOFF))
	EQUIVALENCE(LPAEXSHR,LPAREC(LPAEXSHROFF))
	EQUIVALENCE(LPAEXSHV,LPAREC(LPAEXSHVOFF))
	EQUIVALENCE(LPAEXPAD,LPAREC(LPAEXPADOFF))
	EQUIVALENCE(LPACHGCDC,LPAREC(LPACHGCDCOFF))
	EQUIVALENCE(LPABSD,LPAREC(LPABSDOFF))
	EQUIVALENCE(LPAPRG,LPAREC(LPAPRGOFF))
	EQUIVALENCE(LPAPUP,LPAREC(LPAPUPOFF))
	EQUIVALENCE(LPARETCNT,LPAREC(LPARETCNTOFF))
	EQUIVALENCE(LPANOFFRA,LPAREC(LPANOFFRAOFF))
	EQUIVALENCE(LPATODPAY,LPAREC(LPATODPAYOFF))
	EQUIVALENCE(LPATOPAYAMT,LPAREC(LPATOPAYAMTOFF))
	EQUIVALENCE(LPARETAFTAMT,LPAREC(LPARETAFTAMTOFF))
	EQUIVALENCE(LPAMAXDAYPAY,LPAREC(LPAMAXDAYPAYOFF))
	EQUIVALENCE(LPADRAW,LPAREC(LPADRAWOFF))
C====== ePassive	
	EQUIVALENCE(LPAFIN,LPAREC(LPAFINOFF))
	EQUIVALENCE(LPADIVPLN,LPAREC(LPADIVPLNOFF))		
	EQUIVALENCE(LPALITDRW,LPAREC(LPALITDRWOFF))		
C====== ePassive	
C
C END OF LPAREC.DEF
C
