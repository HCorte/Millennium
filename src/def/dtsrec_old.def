C TOTO SELECT DATE FILE RECORD LAYOUT (DISK)
C
C
C     ODTSSTS				! GAME STATUS                     1
C     ODTSWEK				! WEEK NUMBER                     1
C     ODTSDAT(MAXSRW)			! DRAW DATE                      40
C     ODTSDRW				! DRAW NUMBER                     1
C     ODTSBSD				! BEGINNING SALES DATE            1
C     ODTSESD				! ENDING SALES DATE               1
C     ODTSPUP				! LAST PURGE UPDATE   (FILE ONLY) 1
C     ODTSUPD				! LAST FILE UPDATE    (FILE ONLY) 1
C     ODTSCTM(MAXSRW)			! ACTUAL TIME WHEN CLOSED        40
C     ODTSTIM(MAXSRW)			! TIME WHEN GAME SHOULD CLOSE    40
C     ODTSSER				! LAST SERIAL NUMBER              1
C     ODTSSAL				! TOTAL SALES                     1
C     ODTSPAD				! PRIZES PAID + REFUNDS           1
C     ODTSPRG				! PRIZES PURGED                   1
C     ODTSPRF				! PAID REFUNDS                    1
C     ODTSREF				! TOTAL REFUND AMOUNT             1
C     ODTSWON				! PRIZES WON                      1
C     ODTSWBD(OTSLDIV,NUMTOT)		! WON BY DIVISION COUNT,AMOUNT    8
C     ODTSRBD(OTSLDIV,NUMTOT)		! REFUNDS BY DIVISION COUNT,AMOUNT8
C     ODTSPBD(OTSLDIV,NUMTOT)		! PAID BY DIVISION COUNT, AMOUNT  8
C     ODTSTBD(OTSLDIV)			! WINNING TAX BY DIVISION         4
C     ODTSSBD(OTSLDIV,NUMTOT)		! SALES BY DIVISION CNT,AMT       8
C     ODTSODS(3,MAXSRW)			! TOTOSELECT ODDS BY ROW (1,2,3)120
C     ODTSWIN(MAXSRW)			! WINNING RESULTS                40
C     ODTSHLD(MAXSRW)			! RESULTS HOLD                   40
C     ODTSTAX				! WINNING TAXES                   1
C     ODTSOTX(NUMTOT)			! OVER TAX LIMIT                  2
C     ODTSMID(NUMTOT)			! >REDMIN <REDMAX                 2
C     ODTSUTX(NUMTOT)			! UNDER TAX LIMIT                 2
C     ODTSHST				! HIGHEST WINNER FOR THIS GAME    1
C     ODTSORM(NUMTOT)			! OVER REDMAX                     2
C     ODTSNMS(TNMS_LEN/4,2,MAXSRW)	! TEAM NAMES (HOME,AWAY)        320
C     ODTSSTA(MAXSRW)			! ROW STATUS                     40
C     ODTSREV				! REV #                           1
C     ODTSPFN(5)				! POOL FILE NAME                  5
C     ODTSTOP(2,30)			! TOP 30 COMBINATIONS            60
C     ODTSRWS				! ACTUAL NUMBER OF ROWS USED      1
C     ODTSCMB				! NUMBER OF PLAYED COMB. TODAY    1
C     ODTSPRC				! BASE PRICE                      1
C     ODTSDTE				! EVENT DRAW DATE	          1
C     ODTSTVC(TTVC_LEN/4,MAXSRW)		! TV-CHANEL NAME		  4
C     ODTSMDS(MAXMLTD_AVL)	        ! MULTI DRAW SELECTED TABLE
C                                         (52 BYTE / 4 = 13)
C     ODTSWTOP(2,100)			! TOP 100 LIABILITIES           100
C
