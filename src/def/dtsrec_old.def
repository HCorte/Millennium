	PARAMETER (OTSLDIV=4)
C
	INTEGER*4 ODTSSTS,ODTSWEK,ODTSDRW
	INTEGER*4 ODTSDAT(MAXSRW),ODTSBSD
	INTEGER*4 ODTSESD,ODTSPUP
	INTEGER*4 ODTSUPD,ODTSCTM(MAXSRW)
	INTEGER*4 ODTSTIM(MAXSRW),ODTSSER
	INTEGER*4 ODTSSAL,ODTSPAD
	INTEGER*4 ODTSPRG,ODTSPRF
	INTEGER*4 ODTSREF,ODTSWON
	INTEGER*4 ODTSWBD(OTSLDIV,NUMTOT),ODTSRBD(OTSLDIV,NUMTOT)
	INTEGER*4 ODTSPBD(OTSLDIV,NUMTOT),ODTSTBD(OTSLDIV)
	INTEGER*4 ODTSSBD(OTSLDIV,NUMTOT),ODTSODS(3,MAXSRW)
	INTEGER*4 ODTSWIN(MAXSRW),ODTSHLD(MAXSRW)
	INTEGER*4 ODTSTAX,ODTSOTX(NUMTOT)
	INTEGER*4 ODTSMID(NUMTOT),ODTSUTX(NUMTOT)
	INTEGER*4 ODTSHST,ODTSORM(NUMTOT)
	INTEGER*4 ODTSNMS(TNMS_LEN/4,2,MAXSRW)
	INTEGER*4 ODTSSTA(MAXSRW),ODTSREV
	INTEGER*4 ODTSPFN(5),ODTSTOP(2,30),ODTSRWS
	INTEGER*4 ODTSCMB,ODTSPRC,ODTSDTE
	INTEGER*4 ODTSTVC(TTVC_LEN/4,MAXSRW)
	INTEGER*4 ODTSWTOP(2,100)
C
	BYTE	  ODTSMDS(MAXMLTD_AVL)
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
	INTEGER*4 ODTSSTS_OFF, ODTSWEK_OFF, ODTSDAT_OFF, ODTSDRW_OFF
	PARAMETER(ODTSSTS_OFF = 1)					!001
	PARAMETER(ODTSWEK_OFF = ODTSSTS_OFF + 1)				!002
	PARAMETER(ODTSDAT_OFF = ODTSWEK_OFF + 1)				!003
	PARAMETER(ODTSDRW_OFF = ODTSDAT_OFF + MAXSRW)			!043
C
	INTEGER*4 ODTSBSD_OFF, ODTSESD_OFF, ODTSPUP_OFF, ODTSUPD_OFF
	PARAMETER(ODTSBSD_OFF = ODTSDRW_OFF + 1)				!044
	PARAMETER(ODTSESD_OFF = ODTSBSD_OFF + 1)				!045
	PARAMETER(ODTSPUP_OFF = ODTSESD_OFF + 1)				!046
	PARAMETER(ODTSUPD_OFF = ODTSPUP_OFF + 1)				!047
C
	INTEGER*4 ODTSCTM_OFF, ODTSTIM_OFF, ODTSSER_OFF, ODTSSAL_OFF
	PARAMETER(ODTSCTM_OFF = ODTSUPD_OFF + 1)				!048
	PARAMETER(ODTSTIM_OFF = ODTSCTM_OFF + MAXSRW)			!088
	PARAMETER(ODTSSER_OFF = ODTSTIM_OFF + MAXSRW)			!128
	PARAMETER(ODTSSAL_OFF = ODTSSER_OFF + 1)				!129
C
	INTEGER*4 ODTSPAD_OFF, ODTSPRG_OFF, ODTSPRF_OFF, ODTSREF_OFF
	PARAMETER(ODTSPAD_OFF = ODTSSAL_OFF + 1)				!130
	PARAMETER(ODTSPRG_OFF = ODTSPAD_OFF + 1)				!131
	PARAMETER(ODTSPRF_OFF = ODTSPRG_OFF + 1)				!132
	PARAMETER(ODTSREF_OFF = ODTSPRF_OFF + 1)				!133
C
	INTEGER*4 ODTSWON_OFF, ODTSWBD_OFF, ODTSRBD_OFF, ODTSPBD_OFF
	PARAMETER(ODTSWON_OFF = ODTSREF_OFF + 1)				!134
	PARAMETER(ODTSWBD_OFF = ODTSWON_OFF + 1)				!135
	PARAMETER(ODTSRBD_OFF = ODTSWBD_OFF + (OTSLDIV*NUMTOT))		!139
	PARAMETER(ODTSPBD_OFF = ODTSRBD_OFF + (OTSLDIV*NUMTOT))		!143
C
	INTEGER*4 ODTSTBD_OFF, ODTSSBD_OFF, ODTSODS_OFF, ODTSWIN_OFF
	PARAMETER(ODTSTBD_OFF = ODTSPBD_OFF + (OTSLDIV*NUMTOT))		!147
	PARAMETER(ODTSSBD_OFF = ODTSTBD_OFF + OTSLDIV)			!151
	PARAMETER(ODTSODS_OFF = ODTSSBD_OFF + (OTSLDIV*NUMTOT))		!155
	PARAMETER(ODTSWIN_OFF = ODTSODS_OFF + 3*MAXSRW)			!275
C
	INTEGER*4 ODTSHLD_OFF, ODTSTAX_OFF, ODTSOTX_OFF, ODTSMID_OFF
	PARAMETER(ODTSHLD_OFF = ODTSWIN_OFF + MAXSRW)			!315
	PARAMETER(ODTSTAX_OFF = ODTSHLD_OFF + MAXSRW)			!355
	PARAMETER(ODTSOTX_OFF = ODTSTAX_OFF + 1)				!356
	PARAMETER(ODTSMID_OFF = ODTSOTX_OFF + NUMTOT)			!358
C
	INTEGER*4 ODTSUTX_OFF, ODTSHST_OFF, ODTSORM_OFF, ODTSNMS_OFF
	PARAMETER(ODTSUTX_OFF = ODTSMID_OFF + NUMTOT)			!360
	PARAMETER(ODTSHST_OFF = ODTSUTX_OFF + NUMTOT)			!362
	PARAMETER(ODTSORM_OFF = ODTSHST_OFF + 1)				!363
	PARAMETER(ODTSNMS_OFF = ODTSORM_OFF + NUMTOT)			!365
C
	INTEGER*4 ODTSSTA_OFF, ODTSREV_OFF, ODTSPFN_OFF, ODTSTOP_OFF,ODTSWTOP_OFF
	PARAMETER(ODTSSTA_OFF = ODTSNMS_OFF + ((TNMS_LEN/4)*2*MAXSRW))	!685
	PARAMETER(ODTSREV_OFF = ODTSSTA_OFF + MAXSRW)			!725
	PARAMETER(ODTSPFN_OFF = ODTSREV_OFF + 1)				!726
	PARAMETER(ODTSTOP_OFF = ODTSPFN_OFF + 5)				!731
C
	INTEGER*4 ODTSRWS_OFF, ODTSCMB_OFF, ODTSPRC_OFF, ODTSDTE_OFF
	PARAMETER(ODTSRWS_OFF = ODTSTOP_OFF + (2*30))			!791
	PARAMETER(ODTSCMB_OFF = ODTSRWS_OFF + 1)				!792
	PARAMETER(ODTSPRC_OFF = ODTSCMB_OFF + 1)				!793
	PARAMETER(ODTSDTE_OFF = ODTSPRC_OFF + 1)				!794
C
	INTEGER*4 ODTSTVC_OFF, ODTSMDS_OFF
	PARAMETER(ODTSTVC_OFF = ODTSDTE_OFF + 1)				!795
	PARAMETER(ODTSMDS_OFF = ODTSTVC_OFF + (TTVC_LEN/4)*MAXSRW)	!796
	PARAMETER(ODTSWTOP_OFF = ODTSMDS_OFF + (MAXMLTD_AVL/4))		!809
C
	INTEGER*4 ODTSFRE_OFF
	PARAMETER(ODTSFRE_OFF = ODTSWTOP_OFF + (2*100))				!731
C
C CALCULATE FREE SPACE BASED ON MACHINE DEPENDENT SECTOR SIZES.
C SHOULD ALLWAYS ADD UP TO A EVEN SECTOR SIZE BYTES (VAX 512).
C NOTE! IN CALCULATIONS, FRE_OFF-2 IS DONE TO TAKE CARE OF WHEN THE DATA SIZE
C       ENDS ON A EVEN SECTOR SIZE.
C
        INTEGER*4 ODTSSEC_VAX, ODTSSEC_NUM
        PARAMETER (ODTSSEC_VAX = SECSIZE/4)
	PARAMETER (ODTSSEC_NUM = NINT(FLOAT((ODTSFRE_OFF-2)/ODTSSEC_VAX)+.5))
C
	INTEGER*4 ODTSLEN, ODTSSEC
	PARAMETER(ODTSLEN = ODTSSEC_VAX * ODTSSEC_NUM)        ! even sector size
	PARAMETER(ODTSSEC = ODTSLEN/64)
C
	INTEGER*4 ODTSREC(ODTSLEN)
C
	EQUIVALENCE(ODTSSTS,ODTSREC(ODTSSTS_OFF))
	EQUIVALENCE(ODTSWEK,ODTSREC(ODTSWEK_OFF))
	EQUIVALENCE(ODTSDAT,ODTSREC(ODTSDAT_OFF))
	EQUIVALENCE(ODTSDRW,ODTSREC(ODTSDRW_OFF))
	EQUIVALENCE(ODTSBSD,ODTSREC(ODTSBSD_OFF))
	EQUIVALENCE(ODTSESD,ODTSREC(ODTSESD_OFF))
	EQUIVALENCE(ODTSPUP,ODTSREC(ODTSPUP_OFF))
	EQUIVALENCE(ODTSUPD,ODTSREC(ODTSUPD_OFF))
	EQUIVALENCE(ODTSCTM,ODTSREC(ODTSCTM_OFF))
	EQUIVALENCE(ODTSTIM,ODTSREC(ODTSTIM_OFF))
	EQUIVALENCE(ODTSSER,ODTSREC(ODTSSER_OFF))
	EQUIVALENCE(ODTSSAL,ODTSREC(ODTSSAL_OFF))
	EQUIVALENCE(ODTSPAD,ODTSREC(ODTSPAD_OFF))
	EQUIVALENCE(ODTSPRG,ODTSREC(ODTSPRG_OFF))
	EQUIVALENCE(ODTSPRF,ODTSREC(ODTSPRF_OFF))
	EQUIVALENCE(ODTSREF,ODTSREC(ODTSREF_OFF))
	EQUIVALENCE(ODTSWON,ODTSREC(ODTSWON_OFF))
	EQUIVALENCE(ODTSWBD,ODTSREC(ODTSWBD_OFF))
	EQUIVALENCE(ODTSRBD,ODTSREC(ODTSRBD_OFF))
	EQUIVALENCE(ODTSPBD,ODTSREC(ODTSPBD_OFF))
	EQUIVALENCE(ODTSTBD,ODTSREC(ODTSTBD_OFF))
	EQUIVALENCE(ODTSSBD,ODTSREC(ODTSSBD_OFF))
	EQUIVALENCE(ODTSODS,ODTSREC(ODTSODS_OFF))
	EQUIVALENCE(ODTSWIN,ODTSREC(ODTSWIN_OFF))
	EQUIVALENCE(ODTSHLD,ODTSREC(ODTSHLD_OFF))
	EQUIVALENCE(ODTSTAX,ODTSREC(ODTSTAX_OFF))
	EQUIVALENCE(ODTSOTX,ODTSREC(ODTSOTX_OFF))
	EQUIVALENCE(ODTSMID,ODTSREC(ODTSMID_OFF))
	EQUIVALENCE(ODTSUTX,ODTSREC(ODTSUTX_OFF))
	EQUIVALENCE(ODTSHST,ODTSREC(ODTSHST_OFF))
	EQUIVALENCE(ODTSORM,ODTSREC(ODTSORM_OFF))
	EQUIVALENCE(ODTSNMS,ODTSREC(ODTSNMS_OFF))
	EQUIVALENCE(ODTSSTA,ODTSREC(ODTSSTA_OFF))
	EQUIVALENCE(ODTSREV,ODTSREC(ODTSREV_OFF))
	EQUIVALENCE(ODTSPFN,ODTSREC(ODTSPFN_OFF))
	EQUIVALENCE(ODTSTOP,ODTSREC(ODTSTOP_OFF))
	EQUIVALENCE(ODTSRWS,ODTSREC(ODTSRWS_OFF))
	EQUIVALENCE(ODTSCMB,ODTSREC(ODTSCMB_OFF))
        EQUIVALENCE(ODTSPRC,ODTSREC(ODTSPRC_OFF))
	EQUIVALENCE(ODTSDTE,ODTSREC(ODTSDTE_OFF))
	EQUIVALENCE(ODTSTVC,ODTSREC(ODTSTVC_OFF))
	EQUIVALENCE(ODTSMDS,ODTSREC(ODTSMDS_OFF))
	EQUIVALENCE(ODTSWTOP,ODTSREC(ODTSWTOP_OFF))
