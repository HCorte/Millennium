	INTEGER*4  X2TDBH_BLKCHK, X2TDBH_TDBH
	PARAMETER (X2TDBH_BLKCHK=1)       !BLOCK CHECKSUM STRING
	   PARAMETER (X2TDBH_TDBH='TDBH') !MUST BE SET TO
C
	INTEGER*4  X2TDBH_BLKSEQ, X2TDBH_DSAP, X2TDBH_SSAP
	INTEGER*4  X2TDBH_BLKSIZ
	PARAMETER (X2TDBH_BLKSEQ=5)       !BLOCK SEQUENCE NO
	PARAMETER (X2TDBH_DSAP=7)          !DESTINATION SAP
	PARAMETER (X2TDBH_SSAP=8)          !SOURCE SAP
	PARAMETER (X2TDBH_BLKSIZ=9)        !BLOCK SIZE
C
	INTEGER*4  X2TDBH_FLAGS, X2TDBHF_ONLINE, X2TDBHF_ICE
	INTEGER*4  X2TDBHF_OCE, X2TDBHF_EIC, X2TDBHF_EOC
	INTEGER*4  X2TDBHF_DEFAULT
	PARAMETER (X2TDBH_FLAGS=11)        !FLAGS
	   PARAMETER (X2TDBHF_ONLINE='00000080'X) !ON LINE STAT
	   PARAMETER (X2TDBHF_ICE='00000040'X) !IN CALLS ENABLE
	   PARAMETER (X2TDBHF_OCE='00000020'X) !OUT CALL ENABLE
C
	   PARAMETER (X2TDBHF_EIC='00000004'X) !ENABLE IN CALLS
	   PARAMETER (X2TDBHF_EOC='00000002'X) !ENABLE OUT CALL
C
	   PARAMETER (X2TDBHF_DEFAULT=X2TDBHF_ONLINE+
     *	     X2TDBHF_EIC+X2TDBHF_EOC+
     *	     X2TDBHF_ICE+X2TDBHF_OCE )
C
	INTEGER*4  X2TDBH_BLKTYP, X2TDBHT_DATA, X2TDBHT_SYNC_REQ
	INTEGER*4  X2TDBHT_SYNC_CONF, X2TDBHT_XOFF, X2TDBHT_XON
	INTEGER*4  X2TDBHT_MAINTENANCE_REQ, X2TDBHT_MAINTENANCE_RESP
	INTEGER*4  X2TDBHT_RESET, X2TDBHT_RESET_CONF, X2TDBHT_BEGIN
	INTEGER*4  X2TDBHT_BEGIN_CONF, X2TDBHT_LOOPBACK
	PARAMETER (X2TDBH_BLKTYP=12)      !BLOCK TYPE
	   PARAMETER (X2TDBHT_DATA=1)       !DATA MODE
	   PARAMETER (X2TDBHT_SYNC_REQ=4) !SYNCHRONIZE REQUEST
	   PARAMETER (X2TDBHT_SYNC_CONF=5) !SYNCHRONIZE CONFIRM
	   PARAMETER (X2TDBHT_XOFF=8)       !FLOW CONTROL (STOP)
	   PARAMETER (X2TDBHT_XON=9)        !FLOW CONTROL (START)
	   PARAMETER (X2TDBHT_MAINTENANCE_REQ=16) !MAINTENANCE REQUEST
	   PARAMETER (X2TDBHT_MAINTENANCE_RESP=17)!MAINTENANCE RESPONCE
	   PARAMETER (X2TDBHT_RESET=128)    !RESET COMMAND
	   PARAMETER (X2TDBHT_RESET_CONF=129) !RESET CONFIRM
	   PARAMETER (X2TDBHT_BEGIN=240)    !BEGIN
	   PARAMETER (X2TDBHT_BEGIN_CONF=241)!BEGIN CONFIRMATION
	   PARAMETER (X2TDBHT_LOOPBACK=255)   !LOOPBACK PACKET
C
	INTEGER*4  X2TDBH_DATAOFF, X2TDBH_MSGCNT, X2TDBH_FE_ID
	INTEGER*4  X2TDBH_CNFSUM, X2TDBH_FEAV, X2TDBH_DATA_LOCATION
	PARAMETER (X2TDBH_DATAOFF=13)          !DATA OFFSET
	PARAMETER (X2TDBH_MSGCNT=14)            !MESSAGE COUNT
	PARAMETER (X2TDBH_FE_ID=15)          !FRONT END ID
	PARAMETER (X2TDBH_CNFSUM=16)          !CONFIGURATION CHECKSUM
	PARAMETER (X2TDBH_FEAV=17)            !FRONT END AVAILABILITY
	PARAMETER (X2TDBH_DATA_LOCATION=21)   !OFFSET FOR FIRST DATA
C
C     MAINTENANCE MESSAGE OFFSETS (REQUEST)
C     OFFSETS START IN THE DATA AREA
C
	INTEGER*4  X2TDBHM_OUT_RETRY_LIMIT, X2TDBHM_OUT_RETRY_INTERVAL
	INTEGER*4  X2TDBHM_OUT_DURATION_TIMOUT, X2TDBHM_MAX_SIZE
	INTEGER*4  X2TDBHM_IN_DURATION_TIMOUT, X2TDBHM_MAX_CNT
CV02	INTEGER*4  X2TDBHM_SEND_INTERVAL, X2TDBHM_FLOW_TIMOUT
	INTEGER*4  X2TDBHM_SEND_INTERVAL, X2TDBHM_MAINT_OPTIONS		!V02
	INTEGER*4  X2TDBHM_CENTRAL_TIME, X2TDBHM_MAINTENANCE_TYPE_REQ
	PARAMETER (X2TDBHM_OUT_RETRY_LIMIT=1) !OUTCALL RETRY LIMIT
	PARAMETER (X2TDBHM_OUT_RETRY_INTERVAL=5)!OUTCALL RETRY INTERVLL
	PARAMETER (X2TDBHM_OUT_DURATION_TIMOUT=9) !OUTCALL XTIMOUT
	PARAMETER (X2TDBHM_IN_DURATION_TIMOUT=13)  !INCALL TIMOUT
	PARAMETER (X2TDBHM_MAX_SIZE=17)        !MAX TRANSPORT BLK SIZE
	PARAMETER (X2TDBHM_MAX_CNT=21)          !MAX TRANSPORT BLK CNT
	PARAMETER (X2TDBHM_SEND_INTERVAL=25)    !SEND INTERVAL
CV02	PARAMETER (X2TDBHM_FLOW_TIMOUT=29)      !CONTROL FLOW TIMEOUT
	PARAMETER (X2TDBHM_MAINT_OPTIONS=29)    !OPTIONS FIELD		!V02
	PARAMETER (X2TDBHM_CENTRAL_TIME=33)     !TIME IN CENTRAL
	PARAMETER (X2TDBHM_MAINTENANCE_TYPE_REQ=37)!MAINTENANCE TYPE
C                                             ;REQUEST
C
	INTEGER*4  X2TDBHM_STATION_DISCONNECT
	INTEGER*4  X2TDBHM_STATION_DISCONNECT_ON
	INTEGER*4  X2TDBHM_STATION_DISCONNECT_OFF
	INTEGER*4  X2TDBHM_CENTRAL_SYSID
	INTEGER*4  X2TDBHM_STATION_DISCONNECT_TIMO
	PARAMETER (X2TDBHM_STATION_DISCONNECT=38) !SEND DISCONNECT MESS
	   PARAMETER (X2TDBHM_STATION_DISCONNECT_ON=1)  !ALWAYS SEND
	   PARAMETER (X2TDBHM_STATION_DISCONNECT_OFF=2) !DO NOT SEND
	PARAMETER (X2TDBHM_CENTRAL_SYSID=40)    !ID OF CENTRAL SYSTEM
	PARAMETER (X2TDBHM_STATION_DISCONNECT_TIMO  =39) !TIMEOUT
C                           ;TO DISCONNECT AFTER SAP DISCONNECT
C
C     MAINTENANCE OFFSETS RESPONSE
C
	INTEGER*4  X2TDBHM_FE_TYPE, X2TDBHM_FE_TYPE_GTX
	INTEGER*4  X2TDBHM_FE_TYPE_JUPITER_DAUGHTE
	INTEGER*4  X2TDBHM_FE_TYPE_JUPITER, X2TDBHM_FE_TYPE_STRATUS
	INTEGER*4  X2TDBH_FE_TYPE_DIAL_UP, X2TDBH_FE_TYPE_GTX386
	PARAMETER (X2TDBHM_FE_TYPE=1)            !FE TYPE
	   PARAMETER (X2TDBHM_FE_TYPE_GTX=1)     !PC/AT BASED FE
	   PARAMETER (X2TDBHM_FE_TYPE_JUPITER_DAUGHTE =2)!JUPITER FE
	   PARAMETER (X2TDBHM_FE_TYPE_JUPITER=3) !JUPITER FE
	   PARAMETER (X2TDBHM_FE_TYPE_STRATUS=4) !STRATUS FE
	   PARAMETER (X2TDBH_FE_TYPE_DIAL_UP=5)  !DIAL UP (JUPITER) FE
	   PARAMETER (X2TDBH_FE_TYPE_GTX386=6)   !GTX 386
C
	INTEGER*4  X2TDBHM_LINK_CNT, X2TDBHM_LINK_TYPE
	INTEGER*4  X2TDBHM_LINK_TYPE_802_3, X2TDBHM_MAXPORTS
	INTEGER*4  X2TDBHM_STAT_OFFSET, X2TDBHM_LINE_ENTRY_LEN
	INTEGER*4  X2TDBHM_TOTAL_CAPACITY, X2TDBHM_SUBNETWORK	!V02
	PARAMETER (X2TDBHM_LINK_CNT=2)        !LINK COUNT
	PARAMETER (X2TDBHM_LINK_TYPE=3)       !LINK TYPE
	   PARAMETER (X2TDBHM_LINK_TYPE_802_3=1) !802.3 TYPE CONNECTION
	PARAMETER (X2TDBHM_MAXPORTS=4)        !MAX NO OF PORTS
	PARAMETER (X2TDBHM_STAT_OFFSET=5)     !STATS OFFSET
	PARAMETER (X2TDBHM_LINE_ENTRY_LEN=6)  !LINE ENTRY LENGTH
	PARAMETER (X2TDBHM_TOTAL_CAPACITY=7)  !TOTAL SAP CAPACITY
C
	INTEGER*4  X2TDBHM_MSG_TYPE, X2TDBHM_MSG_TYPE_0
	INTEGER*4  X2TDBHM_MSG_TYPE_1, X2TDBHM_MSG_TYPE_2
	INTEGER*4  X2TDBHM_MSG_TYPE_3
	PARAMETER (X2TDBHM_MSG_TYPE=9)        !MAINTENANCE MESSAGE TYPE
	   PARAMETER (X2TDBHM_MSG_TYPE_0=0)  !TYPE 0
	   PARAMETER (X2TDBHM_MSG_TYPE_1=1)  !TYPE 1
	   PARAMETER (X2TDBHM_MSG_TYPE_2=2)  !TYPE 2
	   PARAMETER (X2TDBHM_MSG_TYPE_3=3)  !TYPE 3
C
	PARAMETER (X2TDBHM_SUBNETWORK=13)       !SUBNETWORK NUMBER - V02

	INTEGER*4  X2TDBHM_PORT_STATUS, X2TDBHM_PORT_FE_ID
	PARAMETER (X2TDBHM_PORT_STATUS=1)     !PORT STATUS
	PARAMETER (X2TDBHM_PORT_FE_ID=2)      !PORT FE ID
C
	INTEGER*4  X2NPLX
	PARAMETER (X2NPLX='NPLX')             !N PLEX COMMS
C
