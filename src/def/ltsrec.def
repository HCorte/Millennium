C
C LOGICAL TOTO SELECT GAME RECORD
C
C
C GAME VARIABLE ARRAYS
C
C     LTSSTS(NUMTSL)                    ; GAME STATUS
C     LTSWEK(NUMTSL)                    ; WEEK NUMBER
C     LTSDAT(MAXSRW,NUMTSL)             ; DRAW DATE
C     LTSDRW(NUMTSL)                    ; DRAW NUMBER
C     LTSBSD(NUMTSL)                    ; BEGINNING SALES DATE
C     LTSESD(NUMTSL)                    ; ENDING SALES DATE
C     LTSPUP(NUMTSL)                    ; LAST PURGE UPDATE   (FILE ONLY)
C     LTSUPD(NUMTSL)                    ; LAST FILE UPDATE    (FILE ONLY)
C     LTSCTM(MAXSRW,NUMTSL)             ; ACTUAL TIME WHEN CLOSED
C     LTSTIM(MAXSRW,NUMTSL)             ; TIME WHEN GAME SHOULD CLOSE
C     LTSBRK(NUMTSL)                    ; BREAKAGE AMOUNT 
C     LTSSAL(NUMTSL)                    ; TOTAL SALES
C     LTSPAD(NUMTSL)                    ; PRIZES PAID + REFUNDS
C     LTSPRG(NUMTSL)                    ; PRIZES PURGED
C     LTSPRF(NUMTSL)                    ; PAID REFUNDS
C     LTSREF(NUMTSL)                    ; TOTAL REFUND AMOUNT
C     LTSWON(NUMTSL)                    ; PRIZES WON
C     LTSWBD(TSLDIV,NUMTOT,NUMTSL)      ; WON BY DIVISION COUNT, AMOUNT
C     LTSRBD(TSLDIV,NUMTOT,NUMTSL)      ; REFUNDS BY DIVISION COUNT,AMOUNT
C     LTSPBD(TSLDIV,NUMTOT,NUMTSL)      ; PAID BY DIVISION COUNT,AMOUNT
C     LTSTBD(TSLDIV,NUMTSL)             ; WINNING TAX BY DIVISION
C     LTSSBD(TSLDIV,NUMTOT,NUMTSL)      ; SALES BY DIVISION COUNT,AMOUNT
C     LTSODS(3,MAXSRW,NUMTSL)           ; TOTOSELECT ODDS BY ROW (1,2,3)
C     LTSWIN(MAXSRW,NUMTSL)             ; WINNING RESULTS
C     LTSHLD(MAXSRW,NUMTSL)             ; RESULTS HOLD
C     LTSTAX(NUMTSL)                    ; WINNING TAXES
C     LTSOTX(NUMTOT,NUMTSL)             ; OVER TAX LIMIT
C     LTSMID(NUMTOT,NUMTSL)             ; >REDMIN <REDMAX
C     LTSUTX(NUMTOT,NUMTSL)             ; UNDER TAX LIMIT
C     LTSHST(NUMTSL)                    ; HIGHEST WINNER FOR THIS GAME
C     LTSORM(NUMTOT,NUMTSL)             ;  OVER REDMAX
C     LTSNMS(TNMS_LEN/4,2,MAXSRW,NUMTSL); TOTOSELECT TEAM NAMES (HOME,AWAY)
C     LTSSTA(MAXSRW,NUMTSL)             ; TOTOSELECT ROW STATUS
C     LTSREV(NUMTSL)                    ; REV #
C     LTSPFN(5,NUMTSL)                  ; TOTOSELECT POOL FILE NAME
C     LTSTOP(2,30,NUMTSL)               ; TOP 30 COMBINATIONS PLAYED
C     LTSRWS(NUMTSL)                    ; ACTUAL ROWS CONFIGURED
C     LTSCMB(NUMTSL)                    ; NUMBER OF COMB. PLAYED TODAY
C     LTSPRC(NUMTSL)                    ; BASE PRICE
C     LTSDTE(NUMTSL)                    ; EVENT DRAW DATE
C     LTSTVC(TTVC_LEN/4,MAXSRW,NUMTSL)  ; TV-CHANEL NAME
C     LTSMDS(MAXMLTD_AVL,NUMTSL)        ; MULTI DRAW SELECTED TABLE
C                                         (52 BYTE / 4 = 13)
C     LTSWTOP(2,100,NUMTSL)             ; TOP 100 LIABILITIES
C     LTSROWTYP(MAXSRW,NUMTSL)          ; 1 - SINGLE ROW (CAN BE PLAYED ALONE)
C                                       ; 2 - DOUBLE ROW (2 ROWS NEEDED)
C                                       ; 3 - REGULAR ROW (3-6 ROWS NEEDED)
C
C LOGICAL GAME FILE VARIABLE OFFSETS
C
C
C+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
