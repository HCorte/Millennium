C+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
C
        INTEGER*4 LTSSTS_OFF, LTSWEK_OFF, LTSDAT_OFF, LTSDRW_OFF
        INTEGER*4 LTSBSD_OFF, LTSESD_OFF, LTSPUP_OFF, LTSUPD_OFF
        INTEGER*4 LTSCTM_OFF, LTSTIM_OFF, LTSBRK_OFF, LTSSAL_OFF
        PARAMETER(LTSSTS_OFF=1)
        PARAMETER(LTSWEK_OFF=LTSSTS_OFF+NUMTSL)
        PARAMETER(LTSDAT_OFF=LTSWEK_OFF+NUMTSL)
        PARAMETER(LTSDRW_OFF=LTSDAT_OFF+MAXSRW*NUMTSL)
        PARAMETER(LTSBSD_OFF=LTSDRW_OFF+NUMTSL)
        PARAMETER(LTSESD_OFF=LTSBSD_OFF+NUMTSL)
        PARAMETER(LTSPUP_OFF=LTSESD_OFF+NUMTSL)
        PARAMETER(LTSUPD_OFF=LTSPUP_OFF+NUMTSL)
        PARAMETER(LTSCTM_OFF=LTSUPD_OFF+NUMTSL)
        PARAMETER(LTSTIM_OFF=LTSCTM_OFF+MAXSRW*NUMTSL)
        PARAMETER(LTSBRK_OFF=LTSTIM_OFF+MAXSRW*NUMTSL)
        PARAMETER(LTSSAL_OFF=LTSBRK_OFF+NUMTSL)
C
        INTEGER*4 LTSPAD_OFF, LTSPRG_OFF, LTSPRF_OFF, LTSREF_OFF
        INTEGER*4 LTSWON_OFF, LTSWBD_OFF, LTSRBD_OFF, LTSPBD_OFF
        INTEGER*4 LTSTBD_OFF, LTSSBD_OFF, LTSODS_OFF, LTSWIN_OFF
        PARAMETER(LTSPAD_OFF=LTSSAL_OFF+NUMTSL)
        PARAMETER(LTSPRG_OFF=LTSPAD_OFF+NUMTSL)
        PARAMETER(LTSPRF_OFF=LTSPRG_OFF+NUMTSL)
        PARAMETER(LTSREF_OFF=LTSPRF_OFF+NUMTSL)
        PARAMETER(LTSWON_OFF=LTSREF_OFF+NUMTSL)
        PARAMETER(LTSWBD_OFF=LTSWON_OFF+NUMTSL)
        PARAMETER(LTSRBD_OFF=LTSWBD_OFF+TSLDIV*NUMTOT*NUMTSL)
        PARAMETER(LTSPBD_OFF=LTSRBD_OFF+TSLDIV*NUMTOT*NUMTSL)
        PARAMETER(LTSTBD_OFF=LTSPBD_OFF+TSLDIV*NUMTOT*NUMTSL)
        PARAMETER(LTSSBD_OFF=LTSTBD_OFF+TSLDIV*NUMTSL)
        PARAMETER(LTSODS_OFF=LTSSBD_OFF+TSLDIV*NUMTOT*NUMTSL)
        PARAMETER(LTSWIN_OFF=LTSODS_OFF+MAXSRW*3*NUMTSL)
C
        INTEGER*4 LTSHLD_OFF, LTSTAX_OFF, LTSOTX_OFF, LTSMID_OFF
        INTEGER*4 LTSUTX_OFF, LTSHST_OFF, LTSORM_OFF, LTSNMS_OFF
        INTEGER*4 LTSSTA_OFF, LTSREV_OFF, LTSPFN_OFF, LTSTOP_OFF
        INTEGER*4 LTSWTOP_OFF
        PARAMETER(LTSHLD_OFF=LTSWIN_OFF+MAXSRW*NUMTSL)
        PARAMETER(LTSTAX_OFF=LTSHLD_OFF+MAXSRW*NUMTSL)
        PARAMETER(LTSOTX_OFF=LTSTAX_OFF+NUMTSL)
        PARAMETER(LTSMID_OFF=LTSOTX_OFF+NUMTOT*NUMTSL)
        PARAMETER(LTSUTX_OFF=LTSMID_OFF+NUMTOT*NUMTSL)
        PARAMETER(LTSHST_OFF=LTSUTX_OFF+NUMTOT*NUMTSL)
        PARAMETER(LTSORM_OFF=LTSHST_OFF+NUMTSL)
        PARAMETER(LTSNMS_OFF=LTSORM_OFF+NUMTOT*NUMTSL)
        PARAMETER(LTSSTA_OFF=LTSNMS_OFF+(TNMS_LEN/4)*2*MAXSRW*NUMTSL)
        PARAMETER(LTSREV_OFF=LTSSTA_OFF+MAXSRW*NUMTSL)
        PARAMETER(LTSPFN_OFF=LTSREV_OFF+NUMTSL)
        PARAMETER(LTSTOP_OFF=LTSPFN_OFF+5*NUMTSL)
C
        INTEGER*4 LTSRWS_OFF, LTSCMB_OFF, LTSPRC_OFF  
        INTEGER*4 LTSDTE_OFF, LTSTVC_OFF, LTSMDS_OFF
        PARAMETER(LTSRWS_OFF = LTSTOP_OFF+2*30*NUMTSL)
        PARAMETER(LTSCMB_OFF = LTSRWS_OFF+NUMTSL)
        PARAMETER(LTSPRC_OFF = LTSCMB_OFF+NUMTSL)
        PARAMETER(LTSDTE_OFF = LTSPRC_OFF+NUMTSL)
        PARAMETER(LTSTVC_OFF = LTSDTE_OFF + NUMTSL)
        PARAMETER(LTSMDS_OFF = LTSTVC_OFF + (TTVC_LEN/4)*MAXSRW*NUMTSL)
        PARAMETER(LTSWTOP_OFF = LTSMDS_OFF+(MAXMLTD_AVL/4)*NUMTSL)
C
C MAXSRW*NUMTSL MUST BE DIVIDABLE BY 4. LTSROWTYP IS A BYTE ARRAY.
C
        INTEGER*4 LTSROWTYP_OFF
        PARAMETER(LTSROWTYP_OFF = LTSWTOP_OFF+2*100*NUMTSL)
C
        INTEGER*4 LTSRECLEN
        PARAMETER(LTSRECLEN = LTSROWTYP_OFF+MAXSRW*NUMTSL/4-1)
C
C LOGICAL RECORD DEFINITION
C
        INTEGER*4 LTSREC(LTSRECLEN)
C
C GAME FILE VARIABLE DEFINITIONS
C
        INTEGER*4 LTSSTS(NUMTSL),LTSWEK(NUMTSL),LTSDRW(NUMTSL)
        INTEGER*4 LTSDAT(MAXSRW,NUMTSL),LTSBSD(NUMTSL)
        INTEGER*4 LTSESD(NUMTSL),LTSPUP(NUMTSL)
        INTEGER*4 LTSUPD(NUMTSL),LTSCTM(MAXSRW,NUMTSL)
        INTEGER*4 LTSTIM(MAXSRW,NUMTSL),LTSBRK(NUMTSL)
        INTEGER*4 LTSSAL(NUMTSL),LTSPAD(NUMTSL)
        INTEGER*4 LTSPRG(NUMTSL),LTSPRF(NUMTSL)
        INTEGER*4 LTSREF(NUMTSL),LTSWON(NUMTSL)
        INTEGER*4 LTSWBD(TSLDIV,NUMTOT,NUMTSL)
        INTEGER*4 LTSRBD(TSLDIV,NUMTOT,NUMTSL)
        INTEGER*4 LTSPBD(TSLDIV,NUMTOT,NUMTSL)
        INTEGER*4 LTSTBD(TSLDIV,NUMTSL)
        INTEGER*4 LTSSBD(TSLDIV,NUMTOT,NUMTSL),LTSODS(3,MAXSRW,NUMTSL)
        INTEGER*4 LTSWIN(MAXSRW,NUMTSL),LTSHLD(MAXSRW,NUMTSL)
        INTEGER*4 LTSTAX(NUMTSL),LTSOTX(NUMTOT,NUMTSL)
        INTEGER*4 LTSMID(NUMTOT,NUMTSL),LTSUTX(NUMTOT,NUMTSL)
        INTEGER*4 LTSHST(NUMTSL),LTSORM(NUMTOT,NUMTSL)
        INTEGER*4 LTSNMS(TNMS_LEN/4,2,MAXSRW,NUMTSL)
        INTEGER*4 LTSSTA(MAXSRW,NUMTSL),LTSREV(NUMTSL)
        INTEGER*4 LTSPFN(5,NUMTSL),LTSTOP(2,30,NUMTSL)
        INTEGER*4 LTSRWS(NUMTSL),LTSCMB(NUMTSL),LTSPRC(NUMTSL)
        INTEGER*4 LTSDTE(NUMTSL)
        INTEGER*4 LTSTVC(TTVC_LEN/4,MAXSRW,NUMTSL)
        INTEGER*4 LTSWTOP(2,100,NUMTSL)
C
        BYTE      LTSMDS(MAXMLTD_AVL,NUMTSL)
        BYTE      LTSROWTYP(MAXSRW,NUMTSL)
C
        EQUIVALENCE(LTSSTS,LTSREC(LTSSTS_OFF))
        EQUIVALENCE(LTSWEK,LTSREC(LTSWEK_OFF))
        EQUIVALENCE(LTSDAT,LTSREC(LTSDAT_OFF))
        EQUIVALENCE(LTSDRW,LTSREC(LTSDRW_OFF))
        EQUIVALENCE(LTSBSD,LTSREC(LTSBSD_OFF))
        EQUIVALENCE(LTSESD,LTSREC(LTSESD_OFF))
        EQUIVALENCE(LTSPUP,LTSREC(LTSPUP_OFF))
        EQUIVALENCE(LTSUPD,LTSREC(LTSUPD_OFF))
        EQUIVALENCE(LTSCTM,LTSREC(LTSCTM_OFF))
        EQUIVALENCE(LTSTIM,LTSREC(LTSTIM_OFF))
        EQUIVALENCE(LTSBRK,LTSREC(LTSBRK_OFF))
        EQUIVALENCE(LTSSAL,LTSREC(LTSSAL_OFF))
        EQUIVALENCE(LTSPAD,LTSREC(LTSPAD_OFF))
        EQUIVALENCE(LTSPRG,LTSREC(LTSPRG_OFF))
        EQUIVALENCE(LTSPRF,LTSREC(LTSPRF_OFF))
        EQUIVALENCE(LTSREF,LTSREC(LTSREF_OFF))
        EQUIVALENCE(LTSWON,LTSREC(LTSWON_OFF))
        EQUIVALENCE(LTSWBD,LTSREC(LTSWBD_OFF))
        EQUIVALENCE(LTSRBD,LTSREC(LTSRBD_OFF))
        EQUIVALENCE(LTSPBD,LTSREC(LTSPBD_OFF))
        EQUIVALENCE(LTSTBD,LTSREC(LTSTBD_OFF))
        EQUIVALENCE(LTSSBD,LTSREC(LTSSBD_OFF))
        EQUIVALENCE(LTSODS,LTSREC(LTSODS_OFF))
        EQUIVALENCE(LTSWIN,LTSREC(LTSWIN_OFF))
        EQUIVALENCE(LTSHLD,LTSREC(LTSHLD_OFF))
        EQUIVALENCE(LTSTAX,LTSREC(LTSTAX_OFF))
        EQUIVALENCE(LTSOTX,LTSREC(LTSOTX_OFF))
        EQUIVALENCE(LTSMID,LTSREC(LTSMID_OFF))
        EQUIVALENCE(LTSUTX,LTSREC(LTSUTX_OFF))
        EQUIVALENCE(LTSHST,LTSREC(LTSHST_OFF))
        EQUIVALENCE(LTSORM,LTSREC(LTSORM_OFF))
        EQUIVALENCE(LTSNMS,LTSREC(LTSNMS_OFF))
        EQUIVALENCE(LTSSTA,LTSREC(LTSSTA_OFF))
        EQUIVALENCE(LTSREV,LTSREC(LTSREV_OFF))
        EQUIVALENCE(LTSPFN,LTSREC(LTSPFN_OFF))
        EQUIVALENCE(LTSTOP,LTSREC(LTSTOP_OFF))
        EQUIVALENCE(LTSRWS,LTSREC(LTSRWS_OFF))
        EQUIVALENCE(LTSPRC,LTSREC(LTSPRC_OFF))
        EQUIVALENCE(LTSCMB,LTSREC(LTSCMB_OFF))
        EQUIVALENCE(LTSDTE,LTSREC(LTSDTE_OFF))
        EQUIVALENCE(LTSTVC,LTSREC(LTSTVC_OFF))
        EQUIVALENCE(LTSMDS,LTSREC(LTSMDS_OFF))
        EQUIVALENCE(LTSWTOP,LTSREC(LTSWTOP_OFF))
        EQUIVALENCE(LTSROWTYP,LTSREC(LTSROWTYP_OFF))
C
