        PARAMETER (DAYCRSOFF = DAYDISOFF + NUMTOT*MAXGAM)        !931
        PARAMETER (DAYIVALOFF= DAYCRSOFF + NUMCRS)
        PARAMETER (DAYICLMOFF= DAYIVALOFF + 1)
C
	INTEGER*4  DAYFREOFF
        PARAMETER (DAYFREOFF = DAYICLMOFF + 1)
C
        INTEGER*4  DAYLEN
        PARAMETER (DAYLEN = DAYFREOFF-1)
C
        INTEGER*4  IDAYSEC
        PARAMETER (IDAYSEC = DAYLEN/128 + 1)
C
        INTEGER*4  DAYSEC
        PARAMETER (DAYSEC = IDAYSEC*2)
C
        INTEGER*4  DAYFRESIZ
        PARAMETER (DAYFRESIZ = DAYSEC * 64 - DAYLEN)
C
C
	COMMON /CONCOM/
     *	     DAYSTS,
     *	     DAYCDC,
     *	     DAYYER,
     *	     DAYJUL,
     *	     DAYWEK,
     *	     DAYTYP(NUMTOT,NUMFIN,MAXGAM),
     *	     DAYDRW(MAXGAM),
     *	     DAYHDR(MAXGAM),
     *	     DAYVAL(MAXGAM),
     *       DAYSAL(MAXDRW,MAXGAM),
     *       DAYDIS(NUMTOT,MAXGAM),
     *       DAYCRS(NUMCRS),
     *       DAYIVAL,
     *       DAYICLM,
     *	     DAYFRE(DAYFRESIZ)
C
	INTEGER*4 DAYSTS,DAYCDC,DAYYER,DAYJUL,DAYWEK,DAYTYP
	INTEGER*4 DAYDRW,DAYHDR,DAYVAL,DAYSAL,DAYDIS
	INTEGER*4 DAYCRS,DAYIVAL,DAYICLM
	INTEGER*4 DAYFRE
C
C CONCOM FREE SPACE
C
        INTEGER*4 CONCOMLEN
        PARAMETER (CONCOMLEN= (CNTSEC*64)+(SCMSEC*64)+(DAYSEC*64) )
C
        INTEGER*4  CONCSB       !# OF CONCOM 2048 COMMON BLOCKS
        PARAMETER (CONCSB = CONCOMLEN/2048+1)
C
	INTEGER*4 CONBLK(2048,CONCSB)
C
        INTEGER*4 CONFRESIZ
        PARAMETER (CONFRESIZ = CONCSB*2048-CONCOMLEN)
C
        COMMON /CONCOM/ CONFRE(CONFRESIZ)
C
	INTEGER*4 CONFRE
C
	EQUIVALENCE (CONBLK(1),NXTSER)
C
C
	INTEGER*4	 FRST_CONCOM(2)
	EQUIVALENCE	(FRST_CONCOM, NXTSER)
	INTEGER*4	 LAST_CONCOM
	EQUIVALENCE	(LAST_CONCOM, CONFRE(CONFRESIZ))
C
	VOLATILE /CONCOM/
C
C END CONCOM.DEF
C
