	PARAMETER(LLTSTS_OFF=1)
	PARAMETER(LLTCTM_OFF=LLTSTS_OFF+NUMLTO)
	PARAMETER(LLTTIM_OFF=LLTCTM_OFF+NUMLTO)
	PARAMETER(LLTDRW_OFF=LLTTIM_OFF+NUMLTO)
