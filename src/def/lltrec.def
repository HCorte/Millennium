C     LLTSHV(LTGDIV,2,NUMLTO)               ;SHARE VALUES   (REGULAR,BONUS)
C     LLTSHR(LTGDIV,2,NUMLTO)               ;SHARES         (REGULAR,BONUS)
C     LLTPAD(LTGDIV,2,NUMLTO)               ;AMOUNT PAID    (REGULAR,BONUS)
C     LLTPRG(LTGDIV,2,NUMLTO)               ;AMOUNT PURGED  (REGULAR,BONUS)
C     LLTANU(LTGDIV,2,NUMLTO)               ;ANNUITY PRIZES (REGULAR,BONUS)
C     LLTMSR(LTGDIV,2,NUMLTO)               ;MAILSUB SHARES (REGULAR,BONUS)
C     LLTPOL(LTGDIV,NUMLTO)                 ;POOL CARRIED OVER
