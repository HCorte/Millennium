C     LLTHLD(LTGNBR,MAXBDR,NUMLTO)          ;WINNING NUMBERS HOLD
C     LLTBNM(LTGBON,MAXBDR,NUMLTO)          ;WINNING BONUS NUMBER(S)
C     LLTBHL(LTGBON,MAXBDR,NUMLTO)          ;BONUS NUMBER(S) HOLD
C     LLTPRP(NUMLTO)                        ;PERCENTAGE OF THE PROMISED AMOUNT
C     LLTPRN(NUMLTO)                        ;NUM. OF WINNERS FOR PROMISED AMOUNT
C     LLTTAX(NUMLTO)                        ;LOTTERY TAX
C     LLTRES(2,NUMLTO)                      ;RESERVE POOL
C     LLTAPL(NUMLTO)                        ;ADDITIONAL POOL
C     LLTMIN(NUMLTO)                        ;MINIMUM POOL
C     LLTSER(NUMLTO)                        ;LAST SERIAL NUMBER
C     LLTOPA(NUMLTO)                        ;OVER-RIDE POOL AMOUNT
C     LLTPRC(NUMLTO)                        ;PRICE/BOARD
C     LLTMAX(NUMLTO)                        ;HIGHEST # BET
C     LLTNUM(NUMLTO)                        ;# OF NUMBERS DRAWN
C     LLTBFL(NUMLTO)                        ;BONUS NUMBER ENABLE FLAG
C     LLTMLT(NUMLTO)                        ;MULTI-DRAW ENABLE FLAG
C     LLTDIV(NUMLTO)                        ;# OF DIVISIONS
C     LLTWTB(LTGNBR,LTGBON+1,LTGBET,NUMLTO) ;WINNING DIVISIONS TABLE
C     LLTPER(LTGDIV,NUMLTO)                 ;DIVISION PERCENTAGES
C     LLTSPR(NUMLTO)                        ;SALES PERCENTAGE
C     LLTTSR(LTGDIV,2,NUMLTO)               ;TOTAL SHARE COUNT (REGULAR,BONUS)
C     LLTREV(NUMLTO)                        ;LOTTO REVISION NUMBER
C     LLTBDR(NUMLTO)                        ;#BONUS DRAWS ENABLED
C     LLTLST(NUMLTO)                        ;LAST DRAWS STATUS
C     LLTLSV(LTGDIV,2,NUMLTO)               ;LAST DRAWS SHARE VALUES
C     LLTADW(NUMLTO)                        ;ADVANCE WAGERING FLAG
C     LLTLAN(LTGDIV,2,NUMLTO)               ;LAST DRAWS ANNUITY VALUES
C     LLTBET(LTGBET.NUMLTO)                 ;ACTIVE BET TYPES
C     LLTWED(2,NUMLTO)                      ;WEDNESDAY DRAW DATES
C     LLTPAW(NUMLTO)                        ;PERCENTAGE APPLIED TO WINS
C     LLTSTX(LTGDIV,2,NUMLTO)               ;SHARE TAX VALUE(REG,BONUS)
C     LLTASH(LTGDIV,NUMLTO)                 ;ADDITIONAL POOL BY DIV
C     LLTWRF(10,NUMLTO)                     ;WIN RESERVE FUND TABLE
C     LLTEST(NUMLTO)                        ;ESTIMATED VIKING JACKPOT
C     LLTMDS(MAXMLTD_AVL,NUMLTO)            ;MULTI DRAW SELECTED TABLE
C     LLTBAL(MAXDRW,NUMLTO)		    ;BALANCE SALES INFO. BY DRAW
C     LLTSPL(NUMLTO)		;SPECIAL POOL, ADDED TO TOTAL POOL -
C                                     NOT TO DIV1 POOL AS OTHER POOL MONEY
C     LLTLNM(MAXBDR,NUMLTO)                 ;WINNING LUCKY NUMBER      !V13...
C     LLTLHL(MAXBDR,NUMLTO)                 ;LUCKY NUMBER HOLD
C     LLTLNC(2,LTGDIV,NUMLTO)               ;LUCKY NUMBER DIVISION CONVERSION TABLE
C     LLTLFL(NUMLTO)                        ;LUCKY NUMBER ENABLE FLAG  !...V13
C     LLTLDV(NUMLTO)                        ;Lucky number refund division number  !...V13
C     LLTJPG(NUMLTO)                        ;JACKPOT POST GAME FLAG    !...V13 FJG
C     LLTCCC(NUMLTO)                        ;TOTOLOTO DRAW COUNTER
C     LLTFRE			;FREE SPACE
