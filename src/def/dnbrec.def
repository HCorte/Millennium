C
C NUMBERS DATA FILE RECORD LAYOUT (DISK)
C
C
C
C     DNBSTS                        ;STATUS
C     DNBCTM                        ;ACTUAL TIME WHEN CLOSED
C     DNBTIM                        ;TIME WHEN GAME SHOULD CLOSE
C     DNBDRW                        ;DRAW NUMBER
C     DNBBSD                        ;NUMBERS BEGINING DRAW DATE
C     DNBESD                        ;NUMBERS ENDING DRAW DATE
C     DNBPUP                        ;LAST PURGE UPDATE
C     DNBUPD                        ;LAST FILE UPDATE
C     DNBDAT(DATLEN)                ;DRAW DATES
C     DNBADV(NUMADV)                ;ADVANCE DRAW NUMBERS
C     DNBSAL(NUMTOT,NBGPOL)         ;TOTAL SALES DATA BY POOL
C     DNBASL(NUMTOT,NBGPOL)         ;ADVANCE SALES FOR THIS DRAW
C     DNBWON(NUMTOT,NBGPOL,2)       ;AMOUNT WON BY POOL      (REGULAR,BONUS)
C     DNBPAD(NUMTOT,NBGPOL,2)       ;AMOUNT PAID BY POOL     (REGULAR,BONUS)
C     DNBPRG(NUMTOT,NBGPOL,2)       ;AMOUNT PURGED BY POOL   (REGULAR,BONUS) 
C     DNBPRZ(NBGPOL,2)              ;PRIZE VALUES BY POOL    (REGULAR,BONUS)
C     DNBWIN(NBGPOL,MAXBDR)         ;WINNING NUMBERS BY POOL (REGULAR,BONUS)
C     DNBHLD(MAXBDR)                ;WINNING NUMBER HOLD
C     DNBLIM(3)                     ;LIABILITY LIMITS
C     DNBSER                        ;LAST SERIAL NUMBER
C     DNBMLT                        ;MULTI-DRAW FLAG
C     DNBOPT                        ;BETTING OPTIONS ENABLED
C     DNBTYP                        ;NUMBERS GAME TYPE
C     DNBPOL                        ;NUMBER OF PRIZE POOLS
C     DNBRES                        ;RESERVE POOL AMOUNT
C     DNBAPL                        ;ADDITIONAL POOL AMOUNT
C     DNBBRK                        ;BREAKAGE AMOUNT
C     DNBROL                        ;ROLLOVER POOL AMOUNT
C     DNBREV                        ;REVISION #
C     DNBMAX                        ;MAXIMUM BETS/TKT
C     DNBPRC                        ;BASE PRICE
C     DNBPER                        ;POOL PERCENTAGE
C     DNBBDR                        ;# OF BONUS DRAWS ENABLED
C     DNBTAX                        ;WINNERS TAX
C     DNBFRE(86)                    ;FREE SPACE
C
