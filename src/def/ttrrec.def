	INTEGER*4 TTRSTS,TTRWEK,TTRDAT,TTRBSD,TTRESD,TTRCTM,TTRTIM
	INTEGER*4 TTRSAL(NUMTOT),TTRPAD,TTRPRG,TTRREF,TTRWON,TTRPOL(2),TTRTPL
	INTEGER*4 TTRTBK,TTRBRK(2),TTRABW,TTRODS(MAXTRPTI),TTRWIN(4,3)
	INTEGER*4 TTRTAX,TTROTX(NUMTOT),TTRHLD(4,3)
	INTEGER*4 TTRMID(NUMTOT),TTRUTX(NUMTOT),TTRHST(MAXTRPTI),TTRORM(2)
	INTEGER*4 TTRMNM(TRPENM_LEN/4),TTRENM(TRPENM_LEN/4,3)
	INTEGER*4 TTRDES(TRPDES_LEN/4,3)
	INTEGER*4 TTRNMS(TRPNMS_LEN/4,MAXTRPRW,3)
	INTEGER*4 TTRREV,TTRPUP,TTRUPD,TTRSTA(MAXTRPRW,3),TTREST(3)
	INTEGER*4 TTREVD(3),TTREVT(3)
	INTEGER*4 TTRSBR(MAXTRPRW,3),TTRPFN(5),TTRERF,TTRTER,TTRDRW
	INTEGER*4 TTRSER,TTRPRF,TTRSPR,TTRPRC
	INTEGER*4 TTRRWS(3),TTRWCP(MAXTRPTI),TTRWBT(NUMTOT,MAXTRPTI)
        INTEGER*4 TTRWRO(NUMTOT,2),TTRWRA(NUMTOT,2),TTRWPO(NUMTOT,2)
        INTEGER*4 TTRWPA(NUMTOT,2),TTRWPR(NUMTOT,2)
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
	INTEGER*4 TTRSTS_OFF, TTRWEK_OFF, TTRDAT_OFF, TTRDRW_OFF
	PARAMETER(TTRSTS_OFF = 1)					!001
	PARAMETER(TTRWEK_OFF = TTRSTS_OFF + 1)				!002
	PARAMETER(TTRDAT_OFF = TTRWEK_OFF + 1)				!003
	PARAMETER(TTRDRW_OFF = TTRDAT_OFF + 1)				!004
C
	INTEGER*4 TTRBSD_OFF, TTRESD_OFF, TTRPUP_OFF, TTRUPD_OFF
	PARAMETER(TTRBSD_OFF = TTRDRW_OFF + 1)				!005
	PARAMETER(TTRESD_OFF = TTRBSD_OFF + 1)				!006
	PARAMETER(TTRPUP_OFF = TTRESD_OFF + 1)				!007
	PARAMETER(TTRUPD_OFF = TTRPUP_OFF + 1)				!008
C
	INTEGER*4 TTRCTM_OFF, TTRTIM_OFF, TTRSER_OFF, TTRSAL_OFF
	PARAMETER(TTRCTM_OFF = TTRUPD_OFF + 1)				!009
	PARAMETER(TTRTIM_OFF = TTRCTM_OFF + 1)				!010
	PARAMETER(TTRSER_OFF = TTRTIM_OFF + 1)				!011
	PARAMETER(TTRSAL_OFF = TTRSER_OFF + 1)			        !012
C
	INTEGER*4 TTRPAD_OFF, TTRPRG_OFF, TTRPRF_OFF, TTRREF_OFF
	PARAMETER(TTRPAD_OFF = TTRSAL_OFF + NUMTOT)	                !014
	PARAMETER(TTRPRG_OFF = TTRPAD_OFF + 1)				!015
	PARAMETER(TTRPRF_OFF = TTRPRG_OFF + 1)				!016
	PARAMETER(TTRREF_OFF = TTRPRF_OFF + 1)				!017
C
	INTEGER*4 TTRERF_OFF, TTRTER_OFF, TTRWON_OFF, TTRPOL_OFF
	PARAMETER(TTRERF_OFF = TTRREF_OFF + 1)				!018
	PARAMETER(TTRTER_OFF = TTRERF_OFF + 1)				!019
	PARAMETER(TTRWON_OFF = TTRTER_OFF + 1)				!020
	PARAMETER(TTRPOL_OFF = TTRWON_OFF + 1)				!021
C
	INTEGER*4 TTRTPL_OFF, TTRTBK_OFF, TTRBRK_OFF, TTRABW_OFF
	PARAMETER(TTRTPL_OFF = TTRPOL_OFF + 2)				!023
	PARAMETER(TTRTBK_OFF = TTRTPL_OFF + 1)				!024
	PARAMETER(TTRBRK_OFF = TTRTBK_OFF + 1)				!025
	PARAMETER(TTRABW_OFF = TTRBRK_OFF + 2)				!027
C
	INTEGER*4 TTRODS_OFF, TTRWIN_OFF, TTRHLD_OFF, TTRTAX_OFF
	PARAMETER(TTRODS_OFF = TTRABW_OFF + 1)				!028
	PARAMETER(TTRWIN_OFF = TTRODS_OFF + MAXTRPTI)			!094
	PARAMETER(TTRHLD_OFF = TTRWIN_OFF + 12)                         !106
	PARAMETER(TTRTAX_OFF = TTRHLD_OFF + 12)				!118

C
	INTEGER*4 TTROTX_OFF, TTRMID_OFF, TTRUTX_OFF, TTRHST_OFF
	PARAMETER(TTROTX_OFF = TTRTAX_OFF + 1)				!119
	PARAMETER(TTRMID_OFF = TTROTX_OFF + NUMTOT)			!121
	PARAMETER(TTRUTX_OFF = TTRMID_OFF + NUMTOT)			!123
	PARAMETER(TTRHST_OFF = TTRUTX_OFF + NUMTOT)			!125
C
	INTEGER*4 TTRORM_OFF, TTRMNM_OFF, TTRENM_OFF, TTRDES_OFF
        INTEGER*4 TTRNMS_OFF
	PARAMETER(TTRORM_OFF = TTRHST_OFF + MAXTRPTI)                   !189
	PARAMETER(TTRMNM_OFF = TTRORM_OFF + NUMTOT)			!191
	PARAMETER(TTRENM_OFF = TTRMNM_OFF + TRPENM_LEN/4)               !198
	PARAMETER(TTRDES_OFF = TTRENM_OFF + (TRPENM_LEN/4)*3)		!247
	PARAMETER(TTRNMS_OFF = TTRDES_OFF + (TRPDES_LEN/4)*3)		!268
C
	INTEGER*4 TTRSTA_OFF, TTRSBR_OFF, TTRREV_OFF, TTRPFN_OFF
	INTEGER*4 TTREST_OFF, TTREVD_OFF, TTREVT_OFF
	PARAMETER(TTRSTA_OFF = TTRNMS_OFF + (TRPNMS_LEN/4)*MAXTRPRW*3)	!412
	PARAMETER(TTREST_OFF = TTRSTA_OFF + MAXTRPRW*3)                 !460
	PARAMETER(TTREVD_OFF = TTREST_OFF + 3)				!463
	PARAMETER(TTREVT_OFF = TTREVD_OFF + 3)				!466
	PARAMETER(TTRSBR_OFF = TTREVT_OFF + 3)				!469
	PARAMETER(TTRREV_OFF = TTRSBR_OFF + MAXTRPRW*3)			!517
	PARAMETER(TTRPFN_OFF = TTRREV_OFF + 1)				!518
C
	INTEGER*4 TTRPRC_OFF, TTRSPR_OFF
	PARAMETER(TTRPRC_OFF = TTRPFN_OFF + 5)				!523
	PARAMETER(TTRSPR_OFF = TTRPRC_OFF + 1)				!524

        INTEGER*4 TTRWRO_OFF, TTRWRA_OFF, TTRWPO_OFF, TTRWPA_OFF
        INTEGER*4 TTRWPR_OFF, TTRRWS_OFF, TTRWCP_OFF
	INTEGER*4 TTRWBT_OFF
	PARAMETER(TTRRWS_OFF = TTRSPR_OFF + 1)				!525
	PARAMETER(TTRWCP_OFF = TTRRWS_OFF + 3)				!528
	PARAMETER(TTRWBT_OFF = TTRWCP_OFF + MAXTRPTI)			!592
        PARAMETER(TTRWRO_OFF = TTRWBT_OFF + NUMTOT*MAXTRPTI)		!720
        PARAMETER(TTRWRA_OFF = TTRWRO_OFF + 2*NUMTOT)			!724
        PARAMETER(TTRWPO_OFF = TTRWRA_OFF + 2*NUMTOT)			!728
        PARAMETER(TTRWPA_OFF = TTRWPO_OFF + 2*NUMTOT)			!732
        PARAMETER(TTRWPR_OFF = TTRWPA_OFF + 2*NUMTOT)			!736

	INTEGER*4 TTRFRE_OFF
        PARAMETER(TTRFRE_OFF = TTRWPR_OFF + 2*NUMTOT)

C
C CALCULATE FREE SPACE BASED ON MACHINE DEPENDENT SECTOR SIZES.
C SHOULD ALLWAYS ADD UP TO A EVEN SECTOR SIZE BYTES (VAX 512).
C NOTE! IN CALCULATIONS, FRE_OFF-2 IS DONE TO TAKE CARE OF WHEN THE DATA SIZE
C       ENDS ON A EVEN SECTOR SIZE.
C
        INTEGER*4 TTRSEC_VAX, TTRSEC_NUM
        PARAMETER (TTRSEC_VAX = SECSIZE/4)
	PARAMETER (TTRSEC_NUM = NINT(FLOAT((TTRFRE_OFF-2)/TTRSEC_VAX)+.5))
C
        INTEGER*4 TTRLEN, TTRSEC
        PARAMETER(TTRLEN = TTRSEC_VAX * TTRSEC_NUM)        ! even sector size
        PARAMETER(TTRSEC = TTRLEN/64)
C
        INTEGER*4 TTRREC(TTRLEN)
C
	EQUIVALENCE(TTRSTS,TTRREC(TTRSTS_OFF))
	EQUIVALENCE(TTRWEK,TTRREC(TTRWEK_OFF))
	EQUIVALENCE(TTRDAT,TTRREC(TTRDAT_OFF))
	EQUIVALENCE(TTRDRW,TTRREC(TTRDRW_OFF))
	EQUIVALENCE(TTRBSD,TTRREC(TTRBSD_OFF))
	EQUIVALENCE(TTRESD,TTRREC(TTRESD_OFF))
	EQUIVALENCE(TTRPUP,TTRREC(TTRPUP_OFF))
	EQUIVALENCE(TTRUPD,TTRREC(TTRUPD_OFF))
	EQUIVALENCE(TTRCTM,TTRREC(TTRCTM_OFF))
	EQUIVALENCE(TTRTIM,TTRREC(TTRTIM_OFF))
	EQUIVALENCE(TTRSER,TTRREC(TTRSER_OFF))
	EQUIVALENCE(TTRSAL,TTRREC(TTRSAL_OFF))
	EQUIVALENCE(TTRPAD,TTRREC(TTRPAD_OFF))
	EQUIVALENCE(TTRPRG,TTRREC(TTRPRG_OFF))
	EQUIVALENCE(TTRPRF,TTRREC(TTRPRF_OFF))
	EQUIVALENCE(TTRREF,TTRREC(TTRREF_OFF))
	EQUIVALENCE(TTRERF,TTRREC(TTRERF_OFF))
	EQUIVALENCE(TTRTER,TTRREC(TTRTER_OFF))
	EQUIVALENCE(TTRWON,TTRREC(TTRWON_OFF))
	EQUIVALENCE(TTRPOL,TTRREC(TTRPOL_OFF))
	EQUIVALENCE(TTRTPL,TTRREC(TTRTPL_OFF))
	EQUIVALENCE(TTRTBK,TTRREC(TTRTBK_OFF))
	EQUIVALENCE(TTRBRK,TTRREC(TTRBRK_OFF))
	EQUIVALENCE(TTRABW,TTRREC(TTRABW_OFF))
	EQUIVALENCE(TTRODS,TTRREC(TTRODS_OFF))
	EQUIVALENCE(TTRWIN,TTRREC(TTRWIN_OFF))
	EQUIVALENCE(TTRHLD,TTRREC(TTRHLD_OFF))
	EQUIVALENCE(TTRTAX,TTRREC(TTRTAX_OFF))
	EQUIVALENCE(TTROTX,TTRREC(TTROTX_OFF))
	EQUIVALENCE(TTRMID,TTRREC(TTRMID_OFF))
	EQUIVALENCE(TTRUTX,TTRREC(TTRUTX_OFF))
	EQUIVALENCE(TTRHST,TTRREC(TTRHST_OFF))
	EQUIVALENCE(TTRORM,TTRREC(TTRORM_OFF))
	EQUIVALENCE(TTRMNM,TTRREC(TTRMNM_OFF))
	EQUIVALENCE(TTRENM,TTRREC(TTRENM_OFF))
	EQUIVALENCE(TTRDES,TTRREC(TTRDES_OFF))
	EQUIVALENCE(TTRNMS,TTRREC(TTRNMS_OFF))
	EQUIVALENCE(TTRSTA,TTRREC(TTRSTA_OFF))
	EQUIVALENCE(TTREST,TTRREC(TTREST_OFF))
	EQUIVALENCE(TTREVD,TTRREC(TTREVD_OFF))
	EQUIVALENCE(TTREVT,TTRREC(TTREVT_OFF))
	EQUIVALENCE(TTRSBR,TTRREC(TTRSBR_OFF))
	EQUIVALENCE(TTRREV,TTRREC(TTRREV_OFF))
	EQUIVALENCE(TTRPFN,TTRREC(TTRPFN_OFF))
	EQUIVALENCE(TTRPRC,TTRREC(TTRPRC_OFF))
	EQUIVALENCE(TTRSPR,TTRREC(TTRSPR_OFF))
	EQUIVALENCE(TTRRWS,TTRREC(TTRRWS_OFF))
	EQUIVALENCE(TTRWCP,TTRREC(TTRWCP_OFF))
	EQUIVALENCE(TTRWBT,TTRREC(TTRWBT_OFF))
        EQUIVALENCE(TTRWRO,TTRREC(TTRWRO_OFF))
        EQUIVALENCE(TTRWRA,TTRREC(TTRWRA_OFF))
        EQUIVALENCE(TTRWPO,TTRREC(TTRWPO_OFF))
        EQUIVALENCE(TTRWPA,TTRREC(TTRWPA_OFF))
        EQUIVALENCE(TTRWPR,TTRREC(TTRWPR_OFF))
