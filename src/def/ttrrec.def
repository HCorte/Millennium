C TTRREC.DEF
C
C V02 10-MAY-1999 UXN TRIPLE CHANGED TO TRIO
C V01 23-DEC-1997 RXK Initial release.
C  
C  
C TODAY'S TRIO GAME DATA FILE RECORD LAYOUT (DISK)
C
C
C
C     TTRSTS                         ; GAME STATUS
C     TTRWEK                         ; WEEK NUMBER
C     TTRDAT                         ; DRAW DATE
C     TTRDRW                         ; DRAW NUMBER
C     TTRBSD                         ; BEGINNING SALES DATE
C     TTRESD                         ; ENDING SALES DATE
C     TTRPUP                         ; LAST PRUGE UPDATE  (FILE ONLY)
C     TTRUPD                         ; LAST FILE UPDATE   (FILE ONLY)
C     TTRCTM                         ; ACTUAL TIME WHEN CLOSED
C     TTRTIM                         ; TIME WHEN GAME SHOULD CLOSE
C     TTRSER                         ; LAST SERIAL NUMBER
C     TTRSAL(NUMTOT)                 ; TOTAL SALES (COUNT , AMOUNT)
C     TTRPAD                         ; PRIZES PAID + REFUNDS
C     TTRPRG                         ; PRIZES PURGED
C     TTRPRF                         ; PAID REFUNDS
C     TTRREF                         ; TOTAL REFUND AMOUNT
C     TTRERF                         ; EARLY REFUNDS PAID
C     TTRTER                         ; TOTAL EARLY REFUNDS (LIAB)
C     TTRWON                         ; PRIZES WON
C     TTRPOL(2)                      ; ROLL POOL
C     TTRTPL                         ; TOTAL PRIZE POOL
C     TTRTBK                         ; TOTAL BREAKAGE
C     TTRBRK(2)                      ; WINNING ROUNDING POT
C     TTRABW                         ; AMOUNT BET ON WINNING SCORE
C     TTRODS(MAXTRPTI)               ; WINNING ODDS
C     TTRWIN(4,3)                    ; WINNING RESULTS (MAX 4 WINNERS)
C     TTRHLD(4,3)                    ; WINNING RESULTS HOLD (MAX 4 WINNERS)
C     TTRTAX                         ; WINNING TAXES
C     TTROTX(NUMTOT)                 ; OVER TAX LIMIT
C     TTRMID(NUMTOT)                 ; >REDMIN <REDMAX
C     TTRUTX(NUMTOT)                 ; UNDER TAX LIMIT
C     TTRHST(MAXTRPTI)               ; HIGHEST WINNER PER WINING COMB
C     TTRORM(NUMTOT)                 ; OVER REDMAX
C     TTRMNM(TRPENM_LEN/4)           ; MASTER EVENT NAME
C     TTRENM(TRPENM_LEN/4,3)         ; EVENT NAME
C     TTRDES(TRPDES_LEN/4,3)         ; EVENT DESCRIPTION
C     TTRNMS(TRPNMS_LEN/4,MAXTRPRW,3); ROW NAMES
C     TTRSTA(MAXTRPRW,3)             ; ROW STATUS'S
C     TTREST(3)			     ; EVENT STATUS
C     TTREVD(3)			     ; EVENT BEGIN DATE
C     TTRETT(3)			     ; EVENT BEGIN TIME
C     TTRSBR(MAXTRPRW,3)             ; SALES BY ROW TABLE
C     TTRREV                         ; REV #
C     TTRPFN(5)                      ; POOL FILE NAME
C     TTRPRC                         ; BASE PRICE
C     TTRSPR                         ; POOL PERCENTAGE
C     TTRRWS(3)			     ; ACTUAL # OF ROWS USED.
C     TTRWCP(MAXTRPTI)		     ; # WINNING COUPONS PLAYED
C     TTRWBT(NUMTOT,MAXTRPTI)        ; CNT, AMT WINNERS PLAYED
C     TTRWRO(NUMTOT,2)               ; WINNING: COUNT,AMOUNT REFUNDS ONLY
C     TTRWRA(NUMTOT,2)               ; WINNING: COUNT,AMOUNT REFUNDS ALTOGETHER
C     TTRWPO(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES ONLY
C     TTRWPA(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES ALTOGETHER
C     TTRWPR(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES AND REFUNDS
C                                    ; (1=PRWON,2=PRPAD)
C
C WINNERS GAME FILE PARAMETERS
C
C
C
