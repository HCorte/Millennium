C
C SPORT DATA FILE RECORD LAYOUT (DISK)
C
C
C
C     ODSPSTS                         ;STATUS
C     ODSPCTM                         ;ACTUAL TIME WHEN CLOSED
C     ODSPTIM                         ;TIME WHEN GAME SHOULD CLOSE
C     ODSPDRW                         ;DRAW NUMBER
C     ODSPBSD                         ;SPORT BEGGINING DRAW DATE
C     ODSPESD                         ;SPORT ENDING DRAW DATE
C     ODSPPUP                         ;LAST PURGE UPDATE
C     ODSPUPD                         ;LAST FILE UPDATE
C     ODSPDAT(DATLEN)                 ;DRAW DATES
C     ODSPADV(NUMADV)                 ;ADVANCE DRAW DATES
C     ODSPSAL(SPGENT)                 ;SALES DATA
C     ODSPSHV(SPGDIV)                 ;SHARE VALUES
C     ODSPSHR(SPGDIV)                 ;SHARES
C     ODSPPOL(SPGDIV)                 ;POOL CARRIED OVER
C     ODSPPAD(SPGDIV)                 ;AMOUNT PAID
C     ODSPPRG(SPGDIV)                 ;AMOUNT PURGED
C     ODSPANU(SPGDIV)                 ;ANNUITY PRIZES
C     ODSPBRK(SPGDIV)                 ;BREAKAGE AMOUNT BY POOL
C     ODSPOSV(SPGDIV)                 ;OLD SHARE VALUES
C     ODSPFRZ(SPGDIV)                 ;DIVISION PAYOUT FROZEN FLAGS
C     ODSPWIN(OSPGNBR)                 ;WINNING RESULTS
C     ODSPHLD(OSPGNBR)                 ;WINNING RESULTS HOLD
C     ODSPPRP                         ;PERCENTAGE OF THE PROMISED AMOUNT
C     ODSPPRN                         ;MAX NUM. OF WINNERS FOR PROMISED AMOUNT,
C                                    ;IF ODSPPRN=-1, IT MEANS 'ERIKOISPOTTI'
C     ODSPTAX                         ;LOTTERY TAX
C     ODSPRES(2)                      ;RESERVE POOL
C     ODSPAPL                         ;ADDITIONAL POOL
C     ODSPMIN                         ;MINIMUM POOL
C     ODSPSER                         ;LAST SERIAL NUMBER
C     ODSPOPA                         ;OVER-RIDE POOL AMOUNT
C     ODSPPRC                         ;PRICE/SINGLE COMBINATION
C     ODSPMAX                         ;MAXIMUM NUMBER OF EVENTS (ROWS)
C     ODSPMLT                         ;MULTI-DRAW ENABLE FLAG
C     ODSPMDS(MAXMLTD_AVL)            ;MULTI-DRAW SELECTED TABLE (52/4 WORDS)
C     ODSPDIV                         ;# OF DIVISIONS
C     ODSPMAT(SPGDIV)                 ;DIVISION MATCH TABLE
C     ODSPPER(SPGDIV)                 ;DIVISION PERCENTAGES
C     ODSPSPR                         ;SALES PERCENTAGE
C     ODSPTSR(SPGDIV)                 ;TOTAL SHARE COUNT
C     ODSPREV                         ;SPORT REVISION NUMBER
C     ODSPNMS(SPNMS_LEN/4,2,OSPGNBR)   ;TEAM NAMES
C     ODSPCLB(25)                     ;CLUB VOTING COUNT
C     ODSPASH(SPGDIV)                 ;ADDITIONAL POOL BY DIV.
C     ODSPFRG                         ;FLAG FOR FREE GAME (PRICE = 0.00)
C     ODSPWRF(POSTED)                 ;WIN RESERVE FUND
C     ODSPBST                         ;BEST MATCH FLAG (ADJUST DIVISIONS)
C     ODSPBAL(MAXDRW)		     ;BALANCE SALES INFO. BY DRAW
C
C SPORT GAME FILE PARAMETERS
C
C
