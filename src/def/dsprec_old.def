        INTEGER*4 OSPGNBR
        PARAMETER (OSPGNBR = 13)

C
C RECORD OFFSET PARAMETERS
C
	INTEGER*4 ODSPSTS,ODSPCTM,ODSPTIM,ODSPDRW,ODSPBSD,ODSPESD
	INTEGER*4 ODSPPUP,ODSPUPD,ODSPDAT(DATLEN),ODSPSAL(SPGENT)
	INTEGER*4 ODSPSHV(SPGDIV),ODSPSHR(SPGDIV),ODSPPOL(SPGDIV)
	INTEGER*4 ODSPPAD(SPGDIV),ODSPPRG(SPGDIV),ODSPADV(NUMADV)
	INTEGER*4 ODSPANU(SPGDIV),ODSPBRK(SPGDIV),ODSPOSV(SPGDIV)
	INTEGER*4 ODSPFRZ(SPGDIV),ODSPWIN(OSPGNBR),ODSPHLD(OSPGNBR)
	INTEGER*4 ODSPPRP,ODSPPRN,ODSPMAT(SPGDIV),ODSPNMS(SPNMS_LEN/4,2,OSPGNBR)
	INTEGER*4 ODSPTAX,ODSPRES(2),ODSPAPL,ODSPMIN,ODSPSER,ODSPOPA,ODSPPRC
	INTEGER*4 ODSPMAX,ODSPMLT,ODSPDIV,ODSPCLB(25)
	INTEGER*4 ODSPPER(SPGDIV)
	INTEGER*4 ODSPSPR,ODSPTSR(SPGDIV),ODSPREV
	INTEGER*4 ODSPASH(SPGDIV),ODSPFRG,ODSPWRF(POSTED),ODSPBST
	INTEGER*4 ODSPBAL(MAXDRW)
C
	BYTE      ODSPMDS(MAXMLTD_AVL)
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
	INTEGER*4 ODSPSTS_OFF, ODSPCTM_OFF, ODSPTIM_OFF, ODSPDRW_OFF
	PARAMETER(ODSPSTS_OFF = 1)					!0001
	PARAMETER(ODSPCTM_OFF = ODSPSTS_OFF + 1)				!0002
	PARAMETER(ODSPTIM_OFF = ODSPCTM_OFF + 1)				!0003
	PARAMETER(ODSPDRW_OFF = ODSPTIM_OFF + 1)				!0004
C
	INTEGER*4 ODSPBSD_OFF, ODSPESD_OFF, ODSPPUP_OFF, ODSPUPD_OFF
	PARAMETER(ODSPBSD_OFF = ODSPDRW_OFF + 1)				!0005
	PARAMETER(ODSPESD_OFF = ODSPBSD_OFF + 1)				!0006
	PARAMETER(ODSPPUP_OFF = ODSPESD_OFF + 1)				!0007
	PARAMETER(ODSPUPD_OFF = ODSPPUP_OFF + 1)				!0008	
C
	INTEGER*4 ODSPDAT_OFF, ODSPADV_OFF, ODSPSAL_OFF, ODSPSHV_OFF
	PARAMETER(ODSPDAT_OFF = ODSPUPD_OFF + 1)				!0009
	PARAMETER(ODSPADV_OFF = ODSPDAT_OFF + DATLEN)			!0029
	PARAMETER(ODSPSAL_OFF = ODSPADV_OFF + NUMADV)			!0036
	PARAMETER(ODSPSHV_OFF = ODSPSAL_OFF + SPGENT)			!0061
C
	INTEGER*4 ODSPSHR_OFF, ODSPPOL_OFF, ODSPPAD_OFF, ODSPPRG_OFF
	PARAMETER(ODSPSHR_OFF = ODSPSHV_OFF + SPGDIV)			!0069
	PARAMETER(ODSPPOL_OFF = ODSPSHR_OFF + SPGDIV)			!0077
	PARAMETER(ODSPPAD_OFF = ODSPPOL_OFF + SPGDIV)			!0085
	PARAMETER(ODSPPRG_OFF = ODSPPAD_OFF + SPGDIV)			!0093
C
	INTEGER*4 ODSPANU_OFF, ODSPBRK_OFF, ODSPOSV_OFF, ODSPFRZ_OFF
	PARAMETER(ODSPANU_OFF = ODSPPRG_OFF + SPGDIV)			!0101
	PARAMETER(ODSPBRK_OFF = ODSPANU_OFF + SPGDIV)			!0109
	PARAMETER(ODSPOSV_OFF = ODSPBRK_OFF + SPGDIV)			!0117
	PARAMETER(ODSPFRZ_OFF = ODSPOSV_OFF + SPGDIV)			!0125
C
	INTEGER*4 ODSPWIN_OFF, ODSPHLD_OFF, ODSPPRP_OFF, ODSPPRN_OFF
	PARAMETER(ODSPWIN_OFF = ODSPFRZ_OFF + SPGDIV)			!0133
	PARAMETER(ODSPHLD_OFF = ODSPWIN_OFF + OSPGNBR)			!0146
	PARAMETER(ODSPPRP_OFF = ODSPHLD_OFF + OSPGNBR)			!0159
	PARAMETER(ODSPPRN_OFF = ODSPPRP_OFF + 1)				!0160
C
	INTEGER*4 ODSPTAX_OFF, ODSPRES_OFF, ODSPAPL_OFF, ODSPMIN_OFF
	PARAMETER(ODSPTAX_OFF = ODSPPRN_OFF + 1)				!0161
	PARAMETER(ODSPRES_OFF = ODSPTAX_OFF + 1)				!0162
	PARAMETER(ODSPAPL_OFF = ODSPRES_OFF + 2)				!0164
	PARAMETER(ODSPMIN_OFF = ODSPAPL_OFF + 1)				!0165
C
	INTEGER*4 ODSPSER_OFF, ODSPOPA_OFF, ODSPPRC_OFF, ODSPMAX_OFF
	PARAMETER(ODSPSER_OFF = ODSPMIN_OFF + 1)				!0166
	PARAMETER(ODSPOPA_OFF = ODSPSER_OFF + 1)				!0167
	PARAMETER(ODSPPRC_OFF = ODSPOPA_OFF + 1)				!0168
	PARAMETER(ODSPMAX_OFF = ODSPPRC_OFF + 1)				!0169
C
	INTEGER*4 ODSPMLT_OFF, ODSPMDS_OFF, ODSPDIV_OFF, ODSPMAT_OFF
	PARAMETER(ODSPMLT_OFF = ODSPMAX_OFF + 1)				!0170
	PARAMETER(ODSPMDS_OFF = ODSPMLT_OFF + 1)				!0171
	PARAMETER(ODSPDIV_OFF = ODSPMDS_OFF + (MAXMLTD_AVL/4))		!0184
	PARAMETER(ODSPMAT_OFF = ODSPDIV_OFF + 1)				!0185
C
	INTEGER*4 ODSPPER_OFF, ODSPSPR_OFF, ODSPTSR_OFF, ODSPREV_OFF
	PARAMETER(ODSPPER_OFF = ODSPMAT_OFF + SPGDIV)			!0193
	PARAMETER(ODSPSPR_OFF = ODSPPER_OFF + SPGDIV)			!0201
	PARAMETER(ODSPTSR_OFF = ODSPSPR_OFF + 1)				!0202
	PARAMETER(ODSPREV_OFF = ODSPTSR_OFF + SPGDIV)			!0210
C
	INTEGER*4 ODSPNMS_OFF, ODSPCLB_OFF, ODSPASH_OFF, ODSPFRG_OFF
	PARAMETER(ODSPNMS_OFF = ODSPREV_OFF + 1)				!0211
	PARAMETER(ODSPCLB_OFF = ODSPNMS_OFF + ((SPNMS_LEN/4)*2*OSPGNBR))	!0315
	PARAMETER(ODSPASH_OFF = ODSPCLB_OFF + 25)				!0340
	PARAMETER(ODSPFRG_OFF = ODSPASH_OFF + SPGDIV)			!0348
C
	INTEGER*4 ODSPWRF_OFF, ODSPBST_OFF, ODSPBAL_OFF
	PARAMETER(ODSPWRF_OFF = ODSPFRG_OFF + 1)				!0349
	PARAMETER(ODSPBST_OFF = ODSPWRF_OFF + POSTED)			!0359
	PARAMETER(ODSPBAL_OFF = ODSPBST_OFF + 1)				!0360
C
	INTEGER*4 ODSPFRE_OFF
	PARAMETER(ODSPFRE_OFF = ODSPBAL_OFF + MAXDRW)                     !0380
C
C CALCULATE FREE SPACE BASED ON MACHINE DEPENDENT SECTOR SIZES.
C SHOULD ALLWAYS ADD UP TO A EVEN SECTOR SIZE BYTES (VAX 512).
C NOTE! IN CALCULATIONS, FRE_OFF-2 IS DONE TO TAKE CARE OF WHEN THE DATA SIZE
C       ENDS ON A EVEN SECTOR SIZE.
C
        INTEGER*4 ODSPSEC_VAX, ODSPSEC_NUM
        PARAMETER (ODSPSEC_VAX = SECSIZE/4)
	PARAMETER (ODSPSEC_NUM = NINT(FLOAT((ODSPFRE_OFF-2)/ODSPSEC_VAX)+.5))
C
        INTEGER*4 ODSPLEN, ODSPSEC
        PARAMETER (ODSPLEN = ODSPSEC_VAX*ODSPSEC_NUM)         !EVEN SEC SIZE
        PARAMETER (ODSPSEC = ODSPLEN / 64)
C
	INTEGER*4 ODSPREC(ODSPLEN)	
C
	EQUIVALENCE(ODSPSTS,ODSPREC(ODSPSTS_OFF))
	EQUIVALENCE(ODSPCTM,ODSPREC(ODSPCTM_OFF))
	EQUIVALENCE(ODSPTIM,ODSPREC(ODSPTIM_OFF))
	EQUIVALENCE(ODSPDRW,ODSPREC(ODSPDRW_OFF))
	EQUIVALENCE(ODSPBSD,ODSPREC(ODSPBSD_OFF))
	EQUIVALENCE(ODSPESD,ODSPREC(ODSPESD_OFF))
	EQUIVALENCE(ODSPPUP,ODSPREC(ODSPPUP_OFF))
	EQUIVALENCE(ODSPUPD,ODSPREC(ODSPUPD_OFF))
	EQUIVALENCE(ODSPDAT,ODSPREC(ODSPDAT_OFF))
	EQUIVALENCE(ODSPADV,ODSPREC(ODSPADV_OFF))
	EQUIVALENCE(ODSPSAL,ODSPREC(ODSPSAL_OFF))
	EQUIVALENCE(ODSPSHV,ODSPREC(ODSPSHV_OFF))
	EQUIVALENCE(ODSPSHR,ODSPREC(ODSPSHR_OFF))
	EQUIVALENCE(ODSPPOL,ODSPREC(ODSPPOL_OFF))
	EQUIVALENCE(ODSPPAD,ODSPREC(ODSPPAD_OFF))
	EQUIVALENCE(ODSPPRG,ODSPREC(ODSPPRG_OFF))
	EQUIVALENCE(ODSPANU,ODSPREC(ODSPANU_OFF))
	EQUIVALENCE(ODSPBRK,ODSPREC(ODSPBRK_OFF))
	EQUIVALENCE(ODSPOSV,ODSPREC(ODSPOSV_OFF))
	EQUIVALENCE(ODSPFRZ,ODSPREC(ODSPFRZ_OFF))
	EQUIVALENCE(ODSPWIN,ODSPREC(ODSPWIN_OFF))
	EQUIVALENCE(ODSPHLD,ODSPREC(ODSPHLD_OFF))
	EQUIVALENCE(ODSPPRP,ODSPREC(ODSPPRP_OFF))
	EQUIVALENCE(ODSPPRN,ODSPREC(ODSPPRN_OFF))
	EQUIVALENCE(ODSPTAX,ODSPREC(ODSPTAX_OFF))
	EQUIVALENCE(ODSPRES,ODSPREC(ODSPRES_OFF))
	EQUIVALENCE(ODSPAPL,ODSPREC(ODSPAPL_OFF))
	EQUIVALENCE(ODSPMIN,ODSPREC(ODSPMIN_OFF))
	EQUIVALENCE(ODSPSER,ODSPREC(ODSPSER_OFF))
	EQUIVALENCE(ODSPOPA,ODSPREC(ODSPOPA_OFF))
	EQUIVALENCE(ODSPPRC,ODSPREC(ODSPPRC_OFF))
	EQUIVALENCE(ODSPMAX,ODSPREC(ODSPMAX_OFF))
	EQUIVALENCE(ODSPMLT,ODSPREC(ODSPMLT_OFF))
	EQUIVALENCE(ODSPMDS,ODSPREC(ODSPMDS_OFF))
	EQUIVALENCE(ODSPDIV,ODSPREC(ODSPDIV_OFF))
	EQUIVALENCE(ODSPMAT,ODSPREC(ODSPMAT_OFF))
	EQUIVALENCE(ODSPPER,ODSPREC(ODSPPER_OFF))
	EQUIVALENCE(ODSPSPR,ODSPREC(ODSPSPR_OFF))
	EQUIVALENCE(ODSPTSR,ODSPREC(ODSPTSR_OFF))
	EQUIVALENCE(ODSPREV,ODSPREC(ODSPREV_OFF))
	EQUIVALENCE(ODSPNMS,ODSPREC(ODSPNMS_OFF))
	EQUIVALENCE(ODSPCLB,ODSPREC(ODSPCLB_OFF))
	EQUIVALENCE(ODSPASH,ODSPREC(ODSPASH_OFF))
	EQUIVALENCE(ODSPFRG,ODSPREC(ODSPFRG_OFF))
	EQUIVALENCE(ODSPWRF,ODSPREC(ODSPWRF_OFF))
	EQUIVALENCE(ODSPBST,ODSPREC(ODSPBST_OFF))
	EQUIVALENCE(ODSPBAL,ODSPREC(ODSPBAL_OFF))
