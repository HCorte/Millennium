	INTEGER*4 LSPSTS_OFF, LSPCTM_OFF, LSPTIM_OFF, LSPDRW_OFF
	PARAMETER(LSPSTS_OFF=1)
	PARAMETER(LSPCTM_OFF=LSPSTS_OFF+NUMSPT)
	PARAMETER(LSPTIM_OFF=LSPCTM_OFF+NUMSPT)
	PARAMETER(LSPDRW_OFF=LSPTIM_OFF+NUMSPT)
C
	INTEGER*4 LSPBSD_OFF, LSPESD_OFF, LSPPUP_OFF, LSPUPD_OFF
	PARAMETER(LSPBSD_OFF=LSPDRW_OFF+NUMSPT)
	PARAMETER(LSPESD_OFF=LSPBSD_OFF+NUMSPT)
	PARAMETER(LSPPUP_OFF=LSPESD_OFF+NUMSPT)
	PARAMETER(LSPUPD_OFF=LSPPUP_OFF+NUMSPT)
C
	INTEGER*4 LSPDAT_OFF, LSPADV_OFF, LSPSAL_OFF, LSPSHV_OFF
	PARAMETER(LSPDAT_OFF=LSPUPD_OFF+NUMSPT)
	PARAMETER(LSPADV_OFF=LSPDAT_OFF+DATLEN*NUMSPT)
	PARAMETER(LSPSAL_OFF=LSPADV_OFF+NUMADV*NUMSPT)
	PARAMETER(LSPSHV_OFF=LSPSAL_OFF+SPGENT*NUMSPT)
C
	INTEGER*4 LSPSHR_OFF, LSPPOL_OFF, LSPPAD_OFF, LSPPRG_OFF
	PARAMETER(LSPSHR_OFF=LSPSHV_OFF+SPGDIV*NUMSPT)
	PARAMETER(LSPPOL_OFF=LSPSHR_OFF+SPGDIV*NUMSPT)
	PARAMETER(LSPPAD_OFF=LSPPOL_OFF+SPGDIV*NUMSPT)
	PARAMETER(LSPPRG_OFF=LSPPAD_OFF+SPGDIV*NUMSPT)
C
	INTEGER*4 LSPAFD_OFF, LSPCSP_OFF, LSPOSV_OFF, LSPOSH_OFF
	PARAMETER(LSPAFD_OFF=LSPPRG_OFF+SPGDIV*NUMSPT)
	PARAMETER(LSPCSP_OFF=LSPAFD_OFF+SPGDIV*NUMSPT)
	PARAMETER(LSPOSV_OFF=LSPCSP_OFF+SPGDIV*NUMSPT)
	PARAMETER(LSPOSH_OFF=LSPOSV_OFF+SPGDIV*NUMSPT)
C
	INTEGER*4 LSPWIN_OFF, LSPHLD_OFF, LSPPRP_OFF, LSPPRN_OFF
	PARAMETER(LSPWIN_OFF=LSPOSH_OFF+SPGDIV*NUMSPT)
	PARAMETER(LSPHLD_OFF=LSPWIN_OFF+SPGNBR*NUMSPT)
	PARAMETER(LSPPRP_OFF=LSPHLD_OFF+SPGNBR*NUMSPT)
	PARAMETER(LSPPRN_OFF=LSPPRP_OFF+NUMSPT)
C
	INTEGER*4 LSPTAX_OFF, LSPRES_OFF, LSPAPL_OFF, LSPMIN_OFF
	PARAMETER(LSPTAX_OFF=LSPPRN_OFF+NUMSPT)
	PARAMETER(LSPRES_OFF=LSPTAX_OFF+NUMSPT)
	PARAMETER(LSPAPL_OFF=LSPRES_OFF+2*NUMSPT)
	PARAMETER(LSPMIN_OFF=LSPAPL_OFF+NUMSPT)
C
	INTEGER*4 LSPSER_OFF, LSPOPA_OFF, LSPPRC_OFF, LSPMAX_OFF
	PARAMETER(LSPSER_OFF=LSPMIN_OFF+NUMSPT)
	PARAMETER(LSPOPA_OFF=LSPSER_OFF+NUMSPT)
	PARAMETER(LSPPRC_OFF=LSPOPA_OFF+NUMSPT)
	PARAMETER(LSPMAX_OFF=LSPPRC_OFF+NUMSPT)
C
	INTEGER*4 LSPMLT_OFF, LSPDIV_OFF, LSPMAT_OFF, LSPPER_OFF
        INTEGER*4 LSPMDS_OFF
	PARAMETER(LSPMLT_OFF=LSPMAX_OFF+NUMSPT)
        PARAMETER(LSPMDS_OFF=LSPMLT_OFF+NUMSPT)
	PARAMETER(LSPDIV_OFF=LSPMDS_OFF+(MAXMLTD_AVL/4)*NUMSPT)
	PARAMETER(LSPMAT_OFF=LSPDIV_OFF+NUMSPT)
	PARAMETER(LSPPER_OFF=LSPMAT_OFF+SPGDIV*NUMSPT)
C
	INTEGER*4 LSPSPR_OFF, LSPTSR_OFF, LSPREV_OFF, LSPNMS_OFF
	PARAMETER(LSPSPR_OFF=LSPPER_OFF+SPGDIV*NUMSPT)
	PARAMETER(LSPTSR_OFF=LSPSPR_OFF+NUMSPT)
	PARAMETER(LSPREV_OFF=LSPTSR_OFF+SPGDIV*NUMSPT)
	PARAMETER(LSPNMS_OFF=LSPREV_OFF+NUMSPT)
C
	INTEGER*4 LSPCLB_OFF, LSPASH_OFF, LSPFRG_OFF, LSPWRF_OFF
	PARAMETER(LSPCLB_OFF = LSPNMS_OFF+((SPNMS_LEN/4)*2*SPGNBR*NUMSPT))
	PARAMETER(LSPASH_OFF = LSPCLB_OFF + 25*NUMSPT)
	PARAMETER(LSPFRG_OFF = LSPASH_OFF + SPGDIV*NUMSPT)
	PARAMETER(LSPWRF_OFF = LSPFRG_OFF + NUMSPT)
C
	INTEGER*4 LSPBST_OFF, LSPBAL_OFF, LSPEVN_OFF
	PARAMETER(LSPBST_OFF = LSPWRF_OFF + POSTED*NUMSPT)
	PARAMETER(LSPBAL_OFF = LSPBST_OFF + NUMSPT)
	PARAMETER(LSPEVN_OFF = LSPBAL_OFF + MAXDRW*NUMSPT)
C
        INTEGER*4 LSPEXT_OFF
        INTEGER*4 LSPRGT_OFF
        INTEGER*4 LSPROD_OFF
        PARAMETER(LSPEXT_OFF = LSPEVN_OFF + (SPEVN_LEN/4)*NUMSPT)
        PARAMETER(LSPRGT_OFF = LSPEXT_OFF + NUMSPT)
        PARAMETER(LSPROD_OFF = LSPRGT_OFF + 2 * NUMSPT)
C
        INTEGER * 4 LSPECD_OFF
        INTEGER * 4 LSPMCE_OFF
        INTEGER * 4 LSPDCD_OFF
        INTEGER * 4 LSPRWD_OFF
C
        PARAMETER(LSPECD_OFF = LSPROD_OFF + 2 * NUMSPT)
        PARAMETER(LSPMCE_OFF = LSPECD_OFF + SPGNBR * NUMSPT)
        PARAMETER(LSPDCD_OFF = LSPMCE_OFF + NUMSPT)
        PARAMETER(LSPRWD_OFF = LSPDCD_OFF + NUMSPT)
C
	INTEGER*4 LSPRECLEN
	PARAMETER(LSPRECLEN  = (LSPRWD_OFF + NUMSPT) - 1)
C
C LOGICAL RECORD DEFINITION
C
	INTEGER*4 LSPREC(LSPRECLEN)
C
C GAME FILE VARIABLE DEFINITIONS
C
	INTEGER*4 LSPSTS(NUMSPT)
	INTEGER*4 LSPCTM(NUMSPT),LSPTIM(NUMSPT)
	INTEGER*4 LSPDRW(NUMSPT),LSPBSD(NUMSPT)
	INTEGER*4 LSPESD(NUMSPT),LSPDAT(DATLEN,NUMSPT)
	INTEGER*4 LSPPUP(NUMSPT),LSPUPD(NUMSPT),LSPADV(NUMADV,NUMSPT)
	INTEGER*4 LSPSAL(SPGENT,NUMSPT),LSPSHV(SPGDIV,NUMSPT)
	INTEGER*4 LSPSHR(SPGDIV,NUMSPT)
	INTEGER*4 LSPPOL(SPGDIV,NUMSPT)
        INTEGER*4 LSPPAD(SPGDIV,NUMSPT)
	INTEGER*4 LSPPRG(SPGDIV,NUMSPT)
	INTEGER*4 LSPAFD(SPGDIV,NUMSPT),LSPCSP(SPGDIV,NUMSPT)
	INTEGER*4 LSPOSV(SPGDIV,NUMSPT),LSPOSH(SPGDIV,NUMSPT)
	INTEGER*4 LSPWIN(SPGNBR,NUMSPT),LSPHLD(SPGNBR,NUMSPT)
	INTEGER*4 LSPPRP(NUMSPT),LSPPRN(NUMSPT)
	INTEGER*4 LSPTAX(NUMSPT),LSPRES(2,NUMSPT),LSPAPL(NUMSPT)
	INTEGER*4 LSPMIN(NUMSPT),LSPSER(NUMSPT)
	INTEGER*4 LSPOPA(NUMSPT),LSPPRC(NUMSPT)
	INTEGER*4 LSPMAX(NUMSPT),LSPCLB(25,NUMSPT)
	INTEGER*4 LSPMLT(NUMSPT),LSPNMS(SPNMS_LEN/4,2,SPGNBR,NUMSPT)
	INTEGER*4 LSPDIV(NUMSPT),LSPMAT(SPGDIV,NUMSPT)
	INTEGER*4 LSPPER(SPGDIV,NUMSPT),LSPSPR(NUMSPT)
	INTEGER*4 LSPTSR(SPGDIV,NUMSPT)
	INTEGER*4 LSPREV(NUMSPT)
        INTEGER*4 LSPASH(SPGDIV,NUMSPT),LSPFRG(NUMSPT)
	INTEGER*4 LSPWRF(POSTED,NUMSPT),LSPBST(NUMSPT)
	INTEGER*4 LSPBAL(MAXDRW,NUMSPT)
	INTEGER*4 LSPEVN(SPEVN_LEN/4,NUMSPT)
        INTEGER*4 LSPEXT(NUMSPT)
        INTEGER*4 LSPRGT(2,NUMSPT)
	INTEGER*4 LSPROD(2,NUMSPT)
        INTEGER*4 LSPECD(SPGNBR, NUMSPT)
        INTEGER*4 LSPMCE(NUMSPT)
        INTEGER*4 LSPDCD(NUMSPT)
        INTEGER*4 LSPRWD(NUMSPT)
C
	BYTE	  LSPMDS(MAXMLTD_AVL,NUMSPT)
C
C
	EQUIVALENCE(LSPSTS,LSPREC(LSPSTS_OFF))
	EQUIVALENCE(LSPCTM,LSPREC(LSPCTM_OFF))
	EQUIVALENCE(LSPTIM,LSPREC(LSPTIM_OFF))
	EQUIVALENCE(LSPDRW,LSPREC(LSPDRW_OFF))
	EQUIVALENCE(LSPBSD,LSPREC(LSPBSD_OFF))
	EQUIVALENCE(LSPESD,LSPREC(LSPESD_OFF))
	EQUIVALENCE(LSPPUP,LSPREC(LSPPUP_OFF))
	EQUIVALENCE(LSPUPD,LSPREC(LSPUPD_OFF))
	EQUIVALENCE(LSPDAT,LSPREC(LSPDAT_OFF))
	EQUIVALENCE(LSPADV,LSPREC(LSPADV_OFF))
	EQUIVALENCE(LSPSAL,LSPREC(LSPSAL_OFF))
