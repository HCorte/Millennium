	EQUIVALENCE(LSPPOL,LSPREC(LSPPOL_OFF))
	EQUIVALENCE(LSPPAD,LSPREC(LSPPAD_OFF))
	EQUIVALENCE(LSPPRG,LSPREC(LSPPRG_OFF))
	EQUIVALENCE(LSPAFD,LSPREC(LSPAFD_OFF))
	EQUIVALENCE(LSPCSP,LSPREC(LSPCSP_OFF))
	EQUIVALENCE(LSPOSV,LSPREC(LSPOSV_OFF))
	EQUIVALENCE(LSPOSH,LSPREC(LSPOSH_OFF))
	EQUIVALENCE(LSPWIN,LSPREC(LSPWIN_OFF))
	EQUIVALENCE(LSPHLD,LSPREC(LSPHLD_OFF))
	EQUIVALENCE(LSPPRP,LSPREC(LSPPRP_OFF))
	EQUIVALENCE(LSPPRN,LSPREC(LSPPRN_OFF))
	EQUIVALENCE(LSPTAX,LSPREC(LSPTAX_OFF))
	EQUIVALENCE(LSPRES,LSPREC(LSPRES_OFF))
	EQUIVALENCE(LSPAPL,LSPREC(LSPAPL_OFF))
	EQUIVALENCE(LSPMIN,LSPREC(LSPMIN_OFF))
	EQUIVALENCE(LSPSER,LSPREC(LSPSER_OFF))
	EQUIVALENCE(LSPOPA,LSPREC(LSPOPA_OFF))
	EQUIVALENCE(LSPPRC,LSPREC(LSPPRC_OFF))
	EQUIVALENCE(LSPMAX,LSPREC(LSPMAX_OFF))
	EQUIVALENCE(LSPMLT,LSPREC(LSPMLT_OFF))
        EQUIVALENCE(LSPMDS,LSPREC(LSPMDS_OFF))
	EQUIVALENCE(LSPDIV,LSPREC(LSPDIV_OFF))
	EQUIVALENCE(LSPMAT,LSPREC(LSPMAT_OFF))
	EQUIVALENCE(LSPPER,LSPREC(LSPPER_OFF))
	EQUIVALENCE(LSPSPR,LSPREC(LSPSPR_OFF))
