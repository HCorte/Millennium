C LOGICAL SPORT GAME RECORD
C
C GAME VARIABLE ARRAYS
C
C     LSPSTS(NUMSPT)                 ;STATUS
C     LSPCTM(NUMSPT)                 ;ACTUAL TIME WHEN CLOSED
C     LSPTIM(NUMSPT)                 ;TIME WHEN GAME SHOULD CLOSE
C     LSPDRW(NUMSPT)                 ;DRAW NUMBER
C     LSPBSD(NUMSPT)                 ;SPORT BEGGINING DRAW DATE
C     LSPESD(NUMSPT)                 ;SPORT ENDING DRAW DATE
C     LSPPUP(NUMSPT)                 ;LAST PURGE UPDATE
C     LSPUPD(NUMSPT)                 ;LAST FILE UPDATE
C     LSPDAT(DATLEN,NUMSPT)          ;DRAW DATES
C     LSPADV(NUMADV,NUMKIK)          ;ADVANCE DRAW DATES
C     LSPSAL(SPGENT,NUMSPT)          ;SALES DATA
C     LSPSHV(SPGDIV,NUMSPT)          ;SHARE VALUES
C     LSPSHR(SPGDIV,NUMSPT)          ;SHARES
C     LSPPOL(SPGDIV,NUMSPT)          ;POOL CARRIED OVER
C     LSPPAD(SPGDIV,NUMSPT)          ;AMOUNT PAID
C     LSPPRG(SPGDIV,NUMSPT)          ;AMOUNT PURGED
C     LSPAFD(SPGDIV,NUMSPT)          ;AMOUNTS FROM OTHERS DRAWS/GAMES (EXTRA)
C     LSPCSP(SPGDIV,NUMSPT)          ;CALCULATED SHARE CALCULATIONS POOLS
C     LSPOSV(SPGDIV,NUMSPT)          ;OFF LINE AMOUNT TO PRIZES BY DIVISION
C     LSPOSH(SPGDIV,NUMSPT)          ;OFF LINE SHARE
C     LSPWIN(SPGNBR,NUMSPT)          ;WINNING RESULTS
C     LSPHLD(SPGNBR,NUMSPT)          ;WINNING RESULTS HOLD
C     LSPPRP(NUMSPT)                 ;PERCENTAGE OF THE PROMISED AMOUNT
C     LSPPRN(NUMSPT)                 ;NUM. OF WEEKS WITHOUT DIV1 WINNERS
C     LSPTAX(NUMSPT)                 ;LOTTERY TAX
C     LSPRES(2,NUMSPT)               ;RESERVE POOL
C     LSPAPL(NUMSPT)                 ;ADDITIONAL POOL
C     LSPMIN(NUMSPT)                 ;MINIMUM POOL
C     LSPSER(NUMSPT)                 ;LAST SERIAL NUMBER
C     LSPOPA(NUMSPT)                 ;OVER-RIDE POOL AMOUNT
C     LSPPRC(NUMSPT)                 ;PRICE/SINGLE COMBINATION
C     LSPMAX(NUMSPT)                 ;MAXIMUM NUMBER OF EVENTS (ROWS)
C     LSPMLT(NUMSPT)                 ;MULTI-DRAW ENABLE FLAG
C     LSPMDS(MAXMLTD_AVL,NUMSPT)     ;MULTI-DRAW SELECTED TABLE (52/4 WORDS)
C     LSPDIV(NUMSPT)                 ;# OF DIVISIONS
C     LSPMAT(SPGDIV,NUMSPT)          ;DIVISION MATCH TABLE
C     LSPPER(SPGDIV,NUMSPT)          ;DIVISION PERCENTAGES
C     LSPSPR(NUMSPT)                 ;SALES PERCENTAGE
C     LSPTSR(SPGDIV,NUMSPT)          ;TOTAL SHARE COUNT
C     LSPREV(NUMSPT)                 ;SPORT REVISION NUMBER
C     LSPNMS(SPNMS_LEN/4,2,SPGNBR,NUMSPT);TEAM NAMES
C     LSPCLB(25,NUMSPT)              ;CLUB VOTING COUNTS
C     LSPASH(SPGDIV,NUMSPT)	     ;ADDITIONAL POL BY DIV.
C     LSPFRG(NUMSPT)		     ;NUMBER OF RESULTS GAMES
C     LSPWRF(POSTED,NUMSPT)	     ;WIN RESERVE FUND
C     LSPBST(NUMSPT)                 ;BEST MATCH FLAG (ADJUST DIVISIONS)
C     LSPBAL(MAXDRW,NUMSPT)	     ;BALANCE SALES INFO. BY DRAW
C     LSPEVN(SPEVN_LEN/4,NUMSPT)     ;EVENT NAME
C     LSPEXT(NUMSPT)                 ;EXTRAORDINARY DRAW
C     LSPRGT(2,NUMSPT)               ;WHERE ROLLOVER GO TO (1:GAME, 2:DRW)
C     LSPROD(2,NUMSPT)               ;WHERE ROLLOVER CAME FROM (1:GAME, 2:DRW)
C     LSPECD(SPGNBR, NUMSPT)         ;EVENT CANCEL DATE AND TIME
C     LSPMCE(NUMSPT)                 ;MAXIMUN NUMBER OF EVENTS CANCELLED TO CANCEL THE DRAW
C     LSPDCD(NUMSPT)                 ;DRAW CANCEL DATE AND TIME
C     LSPRWD(NUMSPT)                 ;REFUND WINNING DIVISION
C
C LOGICAL GAME FILE VARIABLE FIELD OFFSETS
C
