        INTEGER*4 LBNSTS_OFF, LBNCTM_OFF, LBNTIM_OFF, LBNDRW_OFF
        PARAMETER(LBNSTS_OFF=1)
        PARAMETER(LBNCTM_OFF=LBNSTS_OFF+NUMBGO)
        PARAMETER(LBNTIM_OFF=LBNCTM_OFF+NUMBGO)
        PARAMETER(LBNDRW_OFF=LBNTIM_OFF+NUMBGO)
C
        INTEGER*4 LBNBSD_OFF, LBNESD_OFF, LBNPUP_OFF, LBNUPD_OFF
        PARAMETER(LBNBSD_OFF=LBNDRW_OFF+NUMBGO)
        PARAMETER(LBNESD_OFF=LBNBSD_OFF+NUMBGO)
        PARAMETER(LBNPUP_OFF=LBNESD_OFF+NUMBGO)
        PARAMETER(LBNUPD_OFF=LBNPUP_OFF+NUMBGO)
C
        INTEGER*4 LBNDAT_OFF, LBNADV_OFF, LBNSAL_OFF, LBNSHV_OFF
        PARAMETER(LBNDAT_OFF=LBNUPD_OFF+NUMBGO)
        PARAMETER(LBNADV_OFF=LBNDAT_OFF+DATLEN*NUMBGO)
        PARAMETER(LBNSAL_OFF=LBNADV_OFF+NUMADV*NUMBGO)
        PARAMETER(LBNSHV_OFF=LBNSAL_OFF+BGOENT*NUMBGO)
C
        INTEGER*4 LBNSHR_OFF, LBNPOL_OFF, LBNPAD_OFF, LBNPRG_OFF
        PARAMETER(LBNSHR_OFF=LBNSHV_OFF+BGODIV*BGOSUB*NUMBGO)
        PARAMETER(LBNPOL_OFF=LBNSHR_OFF+BGODIV*BGOSUB*NUMBGO)
        PARAMETER(LBNPAD_OFF=LBNPOL_OFF+BGODIV*BGOSUB*NUMBGO)
        PARAMETER(LBNPRG_OFF=LBNPAD_OFF+BGODIV*BGOSUB*NUMBGO)
C
        INTEGER*4 LBNANU_OFF, LBNBRK_OFF, LBNOSV_OFF, LBNFRZ_OFF
        PARAMETER(LBNANU_OFF=LBNPRG_OFF+BGODIV*BGOSUB*NUMBGO)
        PARAMETER(LBNBRK_OFF=LBNANU_OFF+BGODIV*BGOSUB*NUMBGO)
        PARAMETER(LBNOSV_OFF=LBNBRK_OFF+BGODIV*BGOSUB*NUMBGO)
        PARAMETER(LBNFRZ_OFF=LBNOSV_OFF+BGODIV*BGOSUB*NUMBGO)
C
        INTEGER*4 LBNWIN_OFF, LBNHLD_OFF
        PARAMETER(LBNWIN_OFF=LBNFRZ_OFF+BGODIV*BGOSUB*NUMBGO)
        PARAMETER(LBNHLD_OFF=LBNWIN_OFF+BGONBR*NUMBGO)
C
        INTEGER*4 LBNWAB_OFF, LBNHAB_OFF, LBNWLN_OFF, LBNHLN_OFF
        PARAMETER(LBNWAB_OFF=LBNHLD_OFF+BGONBR*NUMBGO)
        PARAMETER(LBNHAB_OFF=LBNWAB_OFF+BGONAB*NUMBGO)
        PARAMETER(LBNWLN_OFF=LBNHAB_OFF+BGONAB*NUMBGO)
        PARAMETER(LBNHLN_OFF=LBNWLN_OFF+BGONLN*NUMBGO)
C
        INTEGER*4 LBNFAC_OFF, LBNCUT_OFF
        PARAMETER(LBNFAC_OFF=LBNHLN_OFF+BGONLN*NUMBGO)
        PARAMETER(LBNCUT_OFF=LBNFAC_OFF+NUMBGO)
C
        INTEGER*4 LBNTAX_OFF, LBNRES_OFF, LBNAPL_OFF, LBNMIN_OFF
        PARAMETER(LBNTAX_OFF=LBNCUT_OFF+NUMBGO)
        PARAMETER(LBNRES_OFF=LBNTAX_OFF+NUMBGO)
        PARAMETER(LBNAPL_OFF=LBNRES_OFF+2*NUMBGO)
        PARAMETER(LBNMIN_OFF=LBNAPL_OFF+NUMBGO)
C
        INTEGER*4 LBNSER_OFF, LBNOPA_OFF, LBNPRC_OFF, LBNMAX_OFF
        PARAMETER(LBNSER_OFF=LBNMIN_OFF+NUMBGO)
        PARAMETER(LBNOPA_OFF=LBNSER_OFF+NUMBGO)
        PARAMETER(LBNPRC_OFF=LBNOPA_OFF+NUMBGO)
        PARAMETER(LBNMAX_OFF=LBNPRC_OFF+NUMBGO)
C
        INTEGER*4 LBNMLT_OFF, LBNMDS_OFF, LBNDIV_OFF, LBNMAT_OFF
        INTEGER*4 LBNPER_OFF
        PARAMETER(LBNMLT_OFF=LBNMAX_OFF+NUMBGO)
        PARAMETER(LBNMDS_OFF=LBNMLT_OFF+NUMBGO)
        PARAMETER(LBNDIV_OFF=LBNMDS_OFF+(MAXMLTD_AVL/4)*NUMBGO)
        PARAMETER(LBNMAT_OFF=LBNDIV_OFF+BGOSUB*NUMBGO)
        PARAMETER(LBNPER_OFF=LBNMAT_OFF+
     *                       (BGOMAXMAP+2+BGOCOL*BGOROW)*BGOSUB*NUMBGO)
        INTEGER*4 LBNSPR_OFF, LBNTSR_OFF, LBNREV_OFF
        PARAMETER(LBNSPR_OFF=LBNPER_OFF+BGODIV*BGOSUB*NUMBGO)
        PARAMETER(LBNTSR_OFF=LBNSPR_OFF+NUMBGO)
        PARAMETER(LBNREV_OFF=LBNTSR_OFF+BGODIV*BGOSUB*NUMBGO)

        INTEGER*4 LBNASH_OFF, LBNFRG_OFF, LBNWRF_OFF
        PARAMETER(LBNASH_OFF = LBNREV_OFF + NUMBGO)
        PARAMETER(LBNFRG_OFF = LBNASH_OFF + BGODIV*BGOSUB*NUMBGO)
        PARAMETER(LBNWRF_OFF = LBNFRG_OFF + NUMBGO)
C
        INTEGER*4 LBNBST_OFF, LBNBAL_OFF
        PARAMETER(LBNBST_OFF = LBNWRF_OFF + POSTED*NUMBGO)
        PARAMETER(LBNBAL_OFF = LBNBST_OFF + NUMBGO)
C
        INTEGER*4 LBNSED_OFF, LBNBAS_OFF, LBNPHS_OFF, LBN1ST_OFF
        INTEGER*4 LBNPHSH_OFF
        PARAMETER(LBNSED_OFF  = LBNBAL_OFF + MAXDRW*NUMBGO)
        PARAMETER(LBNBAS_OFF  = LBNSED_OFF + NUMBGO)
        PARAMETER(LBNPHS_OFF  = LBNBAS_OFF + NUMBGO)
        PARAMETER(LBNPHSH_OFF  = LBNPHS_OFF + BGOPHS*NUMBGO)
        PARAMETER(LBN1ST_OFF  = LBNPHSH_OFF + BGOPHS*NUMBGO)
C
        INTEGER*4 LBNWST_OFF, LBNMAP_OFF, LBNMPF_OFF, LBNAMP_OFF
        INTEGER*4 LBNWSTH_OFF
        PARAMETER(LBNWST_OFF  = LBN1ST_OFF + BGO1ST*NUMBGO)
        PARAMETER(LBNWSTH_OFF  = LBNWST_OFF + NUMBGO)
        PARAMETER(LBNMAP_OFF  = LBNWSTH_OFF + NUMBGO)
        PARAMETER(LBNMPF_OFF  = LBNMAP_OFF + BGOMAXMAP*BGOSUB*NUMBGO)
        PARAMETER(LBNAMP_OFF  = LBNMPF_OFF + BGOMAXMAP*BGOSUB*NUMBGO)
C
        INTEGER*4 LBNPMT_OFF
        PARAMETER(LBNPMT_OFF  = LBNAMP_OFF + 
     *                          BGODIV*BGOMAXMAP*BGOSUB*NUMBGO)
        INTEGER*4 LBNLOB_OFF
        INTEGER*4 LBNWS2_OFF, LBNWS2H_OFF, LBNNDF_OFF, LBNNDFH_OFF
        PARAMETER(LBNLOB_OFF  = LBNPMT_OFF + 3*BGOPHS*NUMBGO)
        PARAMETER(LBNWS2_OFF  = LBNLOB_OFF + NUMBGO)
        PARAMETER(LBNWS2H_OFF  = LBNWS2_OFF + NUMBGO)
        PARAMETER(LBNNDF_OFF  = LBNWS2H_OFF + NUMBGO)
        PARAMETER(LBNNDFH_OFF  = LBNNDF_OFF + BGODIV*NUMBGO)
C
        INTEGER*4 LBNFST_OFF, LBNOTH_OFF, LBNDNR_OFF
        PARAMETER(LBNFST_OFF  = LBNNDFH_OFF + BGODIV*NUMBGO)
        PARAMETER(LBNOTH_OFF  = LBNFST_OFF + BGODIV*BGOSUB*NUMBGO)
        PARAMETER(LBNDNR_OFF  = LBNOTH_OFF + BGODIV*BGOSUB*NUMBGO)

        INTEGER*4 LBNNSP_OFF, LBNSPH_OFF, LBNSPHH_OFF 
        PARAMETER(LBNNSP_OFF  = LBNDNR_OFF + BGODIV*NUMBGO) 
        PARAMETER(LBNSPH_OFF  = LBNNSP_OFF + NUMBGO) 
        PARAMETER(LBNSPHH_OFF = LBNSPH_OFF + BGOSPH*NUMBGO) 

        INTEGER*4 LBNRECLEN
        PARAMETER(LBNRECLEN   = LBNSPHH_OFF + BGOSPH*NUMBGO-1)
C
C LOGICAL RECORD DEFINITION
C
        INTEGER*4 LBNREC(LBNRECLEN)
C
C GAME FILE VARIABLE DEFINITIONS
C
        INTEGER*4 LBNSTS(NUMBGO)
