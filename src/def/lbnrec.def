C LOGICAL BINGO GAME RECORD
C
C GAME VARIABLE ARRAYS
C
C     LBNSTS(NUMBGO)                 !STATUS
C     LBNCTM(NUMBGO)                 !ACTUAL TIME WHEN CLOSED
C     LBNTIM(NUMBGO)                 !TIME WHEN GAME SHOULD CLOSE
C     LBNDRW(NUMBGO)                 !DRAW NUMBER
C     LBNBSD(NUMBGO)                 !BINGO BEGGINING DRAW DATE
C     LBNESD(NUMBGO)                 !BINGO ENDING DRAW DATE
C     LBNPUP(NUMBGO)                 !LAST PURGE UPDATE
C     LBNUPD(NUMBGO)                 !LAST FILE UPDATE
C     LBNDAT(DATLEN,NUMBGO)          !DRAW DATES
C     LBNADV(NUMADV,NUMBGO)          !ADVANCE DRAW DATES
C     LBNSAL(BGOENT,NUMBGO)          !SALES DATA
C     LBNSHV(BGODIV,BGOSUB,NUMBGO)          !SHARE VALUES
C     LBNSHR(BGODIV,BGOSUB,NUMBGO)          !SHARES
C     LBNPOL(BGODIV,BGOSUB,NUMBGO)          !POOL CARRIED OVER
C     LBNPAD(BGODIV,BGOSUB,NUMBGO)          !AMOUNT PAID
C     LBNPRG(BGODIV,BGOSUB,NUMBGO)          !AMOUNT PURGED
C     LBNANU(BGODIV,BGOSUB,NUMBGO)          !ANNUITY PRIZES
C     LBNBRK(BGODIV,BGOSUB,NUMBGO)          !BREAKAGE AMOUNT BY POOL
C     LBNOSV(BGODIV,BGOSUB,NUMBGO)          !OLD SHARE VALUES
C     LBNFRZ(BGODIV,BGOSUB,NUMBGO)          !DIVISION PAYOUT FROZEN FLAGS
C     LBNWIN(BGONBR,NUMBGO)          !WINNING RESULTS
C     LBNHLD(BGONBR,NUMBGO)          !WINNING RESULTS HOLD
C     LBNWAB(BGONAB,NUMBGO)          !WINNING RESULTS (A,B)
C     LBNHAB(BGONAB,NUMBGO)          !WINNING RESULTS HOLD (A,B)
C     LBNWLN(BGONLN,NUMBGO)          !WINNING RESULTS (LUCKY)
C     LBNHLN(BGONLN,NUMBGO)          !WINNING RESULTS HOLD (LUCKY)
C     LBNFAC(NUMBGO)                 !ANNUITY FACTOR
C     LBNCUT(NUMBGO)                 !ANNUITY CUT-IN
C     LBNTAX(NUMBGO)                 !LOTTERY TAX
C     LBNRES(2,NUMBGO)               !RESERVE POOL
C     LBNAPL(NUMBGO)                 !ADDITIONAL POOL
C     LBNMIN(NUMBGO)                 !MINIMUM POOL
C     LBNSER(NUMBGO)                 !LAST SERIAL NUMBER
C     LBNOPA(NUMBGO)                 !OVER-RIDE POOL AMOUNT
C     LBNPRC(NUMBGO)                 !PRICE/SINGLE COMBINATION
C     LBNMAX(NUMBGO)                 !MAXIMUM NUMBER OF EVENTS (ROWS)
C     LBNMLT(NUMBGO)                 !MULTI-DRAW ENABLE FLAG
C     LBNMDS(MAXMLTD_AVL,NUMBGO)     !MULTI-DRAW SELECTED TABLE (52/4 WORDS)
C     LBNDIV(BGOSUB,NUMBGO)          !# OF DIVISIONS
C     LBNMAT(BGOMAXMAP+2+BGOCOL*BGOROW,BGOSUB,NUMBGO) !DIVISION MATCH TABLE
C                    !(row bitmaps + worst + second worst + entire 5x5 panel)
C     LBNPER(BGODIV,BGOSUB,NUMBGO)          !DIVISION PERCENTAGES
C     LBNSPR(NUMBGO)                 !SALES PERCENTAGE
C     LBNTSR(BGODIV,BGOSUB,NUMBGO)          !TOTAL SHARE COUNT
C     LBNREV(NUMBGO)                 !BINGO REVISION NUMBER
C     LBNASH(BGODIV,BGOSUB,NUMBGO)           !ADDITIONAL POL BY DIV.
C     LBNFRG(NUMBGO)                 !FLAG FOR FREE GAME (PRICE = 0.00)
C     LBNWRF(POSTED,NUMBGO)          !WIN RESERVE FUND
C     LBNBST(NUMBGO)                 !BEST MATCH FLAG (ADJUST DIVISIONS)
C     LBNBAL(MAXDRW,NUMBGO)          !BALANCE SALES INFO. BY DRAW
C     LBNSED(NUMBGO)                 !FULL HOUSE SEED
C     LBNBAS(NUMBGO)                 !LUCKY NUMBER BASE
C     LBNPHS(BGOPHS,NUMBGO)          !#'s DRAWN PER PHASE (FH)
C     LBNPHSH(BGOPHS,NUMBGO)         !#'s DRAWN PER PHASE (FH) HOLD
C     LBN1ST(BGO1ST,NUMBGO)          !#'s DRAWN FOR 1st BINGO,DOUBLE,etc. (FH)
C     LBNWST(NUMBGO)                 !#'s MATCHED FOR WORST WIN (FH)
C     LBNWSTH(NUMBGO)                !#'s MATCHED FOR WORST WIN (FH) HOLD
C     LBNMAP(BGOMAXMAP,BGOSUB,NUMBGO)      !MAX BITMAPS TO WIN ON
C     LBNMPF(BGOMAXMAP,BGOSUB,NUMBGO)      !WIN ALLOWED FOR BITMAP OVERLAP FLAG
C     LBNAMP(BGODIV,BGOMAXMAP,BGOSUB,NUMBGO) !# OF ADDITIONAL BITMAPS FOR DIVS WON
C     LBNPMT(3,BGOPHS,NUMBGO)        !# OF (1=BITMAPS,2=NUMBERS,3=FIGURES)
C                                    !MATCH A PHASE TO COMPLETE
C     LBNLOB(NUMBGO)                 !NUMBER OF LAST BINGO WITH AB BOARDS
C     LBNWS2(NUMBGO)                 !#'s MATCHED FOR SECOND WORST WIN (FH)
C     LBNWS2H(NUMBGO)                !#'s MATCHED FOR SECOND WORST WIN (FH) HOLD
C     LBNNDF(BGODIV,NUMBGO)          !#'s DRAWN FOR FIRST MATCH (FH) 
C     LBNNDFH(BGODIV,NUMBGO)         !#'s DRAWN FOR FIRST MATCH (FH) HOLD
C     LBNFST(BGODIV,BGOSUB,NUMBGO)   !FLAGS SET FOR DIVISIONS OF FIRST MATCH
C     LBNOTH(BGODIV,BGOSUB,NUMBGO)   !OTHER DIV WHICH WINS WITH SAME BITMAPS
C     LBNDNR(BGODIV,NUMBGO)          !DIV NUMBERS IN LOTTERY 1..20 SCALE (FH)
C     LBNNSP(NUMBGO)                 !NUMBER OF SUBPHASES(INCL.FULLHOUSE)
C     LBNSPH(BGOSPH,NUMBGO)          !#'s DRAWN PER SUBPHASE (FH)
C     LBNSPHH(BGOSPH,NUMBGO          !#'s DRAWN PER SUBPHASE (FH) HOLD
C
C LOGICAL GAME FILE VARIABLE FIELD OFFSETS
C
C
