C V06 01-JUN-1993 GXA Adjusted dimension of DWITVC.
C V05 17-JUN-1993 GXA Added Multi-Draw Selected table.
C V04 16-JUN-1993 GXA Released for Finland Dec Conversion / Oddset.
C V03 21-JAN-1993 DAB Initial Release
C                     Based on Netherlands Bible, 12/92, and Comm 1/93 update
C                     DEC Baseline
C V02 07-OCT-1991 MTK INITAL RELEASE FOR NETHERLANDS
C V01 01-AUG-1990 XXX RELEASED FOR VAX
C
C WINNERS TIP GAME DATA FILE RECORD LAYOUT (DISK)
C
C
C
C     DWISTS                         ; GAME STATUS
C     DWIWEK                         ; WEEK NUMBER
C     DWIDAT                         ; DRAW DATE
C     DWIDRW                         ; DRAW NUMBER
C     DWIBSD                         ; BEGINNING SALES DATE
C     DWIESD                         ; ENDING SALES DATE
C     DWIPUP                         ; LAST PRUGE UPDATE  (FILE ONLY)
C     DWIUPD                         ; LAST FILE UPDATE   (FILE ONLY)
C     DWICTM                         ; ACTUAL TIME WHEN CLOSED
C     DWITIM                         ; TIME WHEN GAME SHOULD CLOSE
C     DWISER                         ; LAST SERIAL NUMBER
C     DWISAL                         ; TOTAL SALES
C     DWIPAD                         ; PRIZES PAID + REFUNDS
C     DWIPRG                         ; PRIZES PURGED
C     DWIPRF                         ; PAID REFUNDS
C     DWIREF                         ; TOTAL REFUND AMOUNT
C     DWIERF                         ; EARLY REFUNDS PAID
C     DWITER                         ; TOTAL EARLY REFUNDS (LIAB)
C     DWIWON                         ; PRIZES WON
C     DWIPOL(2)                      ; ROLL POOL
C     DWITPL                         ; TOTAL PRIZE POOL
C     DWITBK                         ; TOTAL BREAKAGE
C     DWIBRK(2)                      ; WINNING ROUNDING POT
C     DWIABW                         ; AMOUNT BET ON WINNING SCORE
C     DWIODS(4)                      ; WINNING ODDS
C     DWIWIN(4)                      ; WINNING RESULTS
C     DWIHLD(4)                      ; WINNING RESULTS HOLD
C     DWITAX                         ; WINNING TAXES
C     DWIOTX(NUMTOT)                 ; OVER TAX LIMIT
C     DWIMID(NUMTOT)                 ; >REDMIN <REDMAX
C     DWIUTX(NUMTOT)                 ; UNDER TAX LIMIT
C     DWIHST                         ; HIGHEST WINNER FOR THIS GAME
C     DWIORM(NUMTOT)                 ; OVER REDMAX
C     DWIENM(WENM_LEN/4)             ; EVENT NAME
C     DWIDES(WDES_LEN/4)             ; GAME DESCRIPTION
C     DWINMS(WNMS_LEN/4,MAXWRW)      ; ROW NAMES
C     DWISTA(MAXWRW)                 ; ROW STATUS 1 THROUGH MAXTRW
C     DWISBR(MAXWRW)                 ; SALES BY ROW TABLE
C     DWIREV                         ; REV #
C     DWIPFN(5)                      ; POOL FILE NAME
C     DWIRTM(MAXWRW)                 ; ROW CLOSE TIMES
C     DWIPRC                         ; BASE PRICE
C     DWISPR                         ; POOL PERCENTAGE
C     DWITVC(WTVC_LEN/4)    	     ; TV-CHANEL NAME
C     DWIMDS(MAXMLTD_AVL)	     ; MULTI DRAW SELECTED TABLE
C                                      (52 BYTE / 4 = 13)
C     DWIWRO(NUMTOT,2)               ; WINNING: COUNT,AMOUNT REFUNDS ONLY
C     DWIWRA(NUMTOT,2)               ; WINNING: COUNT,AMOUNT REFUNDS ALTOGETHER
C     DWIWPO(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES ONLY
C     DWIWPA(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES ALTOGETHER
C     DWIWPR(NUMTOT,2)               ; WINNING: COUNT,AMOUNT PRIZES AND REFUNDS
C                                    ; (1=PRWON,2=PRPAD)
C     DWILAT(NUMLAT)                 ; EVENT STARTED BEFORE GAME WAS CLOSED
C                                    ; TICKETS PLAYED LATER SHOULD BE REFUNDED.
C
C WINNERS GAME FILE PARAMETERS
C
C
C
