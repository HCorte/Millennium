	INTEGER*4 DWISTS,DWIWEK,DWIDAT,DWIBSD,DWIESD,DWICTM,DWITIM
	INTEGER*4 DWISAL,DWIPAD,DWIPRG,DWIREF,DWIWON,DWIPOL(2),DWITPL
	INTEGER*4 DWITBK,DWIBRK(2),DWIABW,DWIODS(4),DWIWIN(4)
	INTEGER*4 DWITAX,DWIOTX(NUMTOT),DWIHLD(4)
	INTEGER*4 DWIMID(NUMTOT),DWIUTX(NUMTOT),DWIHST,DWIORM(2)
	INTEGER*4 DWIENM(WENM_LEN/4)
	INTEGER*4 DWIDES(WDES_LEN/4)
	INTEGER*4 DWINMS(WNMS_LEN/4,MAXWRW)
	INTEGER*4 DWIREV,DWIPUP,DWIUPD,DWISTA(MAXWRW)
	INTEGER*4 DWISBR(MAXWRW),DWIPFN(5),DWIERF,DWITER,DWIDRW
	INTEGER*4 DWIRTM(MAXWRW),DWISER,DWIPRF,DWISPR,DWIPRC
	INTEGER*4 DWITVC(WTVC_LEN/4)
C
	BYTE	  DWIMDS(MAXMLTD_AVL)
C
        INTEGER*4 DWIWRO(NUMTOT,2),DWIWRA(NUMTOT,2),DWIWPO(NUMTOT,2)
        INTEGER*4 DWIWPA(NUMTOT,2),DWIWPR(NUMTOT,2)       
	INTEGER*4 DWILAT(NUMLAT)
C
C OFFSET PARAMETERS AND EQUIVALENCES
C
	INTEGER*4 DWISTS_OFF, DWIWEK_OFF, DWIDAT_OFF, DWIDRW_OFF
	PARAMETER(DWISTS_OFF = 1)					!001
	PARAMETER(DWIWEK_OFF = DWISTS_OFF + 1)				!002
	PARAMETER(DWIDAT_OFF = DWIWEK_OFF + 1)				!003
	PARAMETER(DWIDRW_OFF = DWIDAT_OFF + 1)				!004
C
	INTEGER*4 DWIBSD_OFF, DWIESD_OFF, DWIPUP_OFF, DWIUPD_OFF
	PARAMETER(DWIBSD_OFF = DWIDRW_OFF + 1)				!005
	PARAMETER(DWIESD_OFF = DWIBSD_OFF + 1)				!006
	PARAMETER(DWIPUP_OFF = DWIESD_OFF + 1)				!007
	PARAMETER(DWIUPD_OFF = DWIPUP_OFF + 1)				!008
C
	INTEGER*4 DWICTM_OFF, DWITIM_OFF, DWISER_OFF, DWISAL_OFF
	PARAMETER(DWICTM_OFF = DWIUPD_OFF + 1)				!009
	PARAMETER(DWITIM_OFF = DWICTM_OFF + 1)				!010
	PARAMETER(DWISER_OFF = DWITIM_OFF + 1)				!011
	PARAMETER(DWISAL_OFF = DWISER_OFF + 1)				!012
C
	INTEGER*4 DWIPAD_OFF, DWIPRG_OFF, DWIPRF_OFF, DWIREF_OFF
	PARAMETER(DWIPAD_OFF = DWISAL_OFF + 1)				!013
	PARAMETER(DWIPRG_OFF = DWIPAD_OFF + 1)				!014
	PARAMETER(DWIPRF_OFF = DWIPRG_OFF + 1)				!015
	PARAMETER(DWIREF_OFF = DWIPRF_OFF + 1)				!016
C
	INTEGER*4 DWIERF_OFF, DWITER_OFF, DWIWON_OFF, DWIPOL_OFF
	PARAMETER(DWIERF_OFF = DWIREF_OFF + 1)				!017
	PARAMETER(DWITER_OFF = DWIERF_OFF + 1)				!018
	PARAMETER(DWIWON_OFF = DWITER_OFF + 1)				!019
	PARAMETER(DWIPOL_OFF = DWIWON_OFF + 1)				!020
C
	INTEGER*4 DWITPL_OFF, DWITBK_OFF, DWIBRK_OFF, DWIABW_OFF
	PARAMETER(DWITPL_OFF = DWIPOL_OFF + 2)				!022
	PARAMETER(DWITBK_OFF = DWITPL_OFF + 1)				!023
	PARAMETER(DWIBRK_OFF = DWITBK_OFF + 1)				!024
	PARAMETER(DWIABW_OFF = DWIBRK_OFF + 2)				!026
C
	INTEGER*4 DWIODS_OFF, DWIWIN_OFF, DWIHLD_OFF, DWITAX_OFF
	PARAMETER(DWIODS_OFF = DWIABW_OFF + 1)				!027
	PARAMETER(DWIWIN_OFF = DWIODS_OFF + MAXWTI)			!031
	PARAMETER(DWIHLD_OFF = DWIWIN_OFF + MAXWTI)			!035
	PARAMETER(DWITAX_OFF = DWIHLD_OFF + MAXWTI)			!039
C
	INTEGER*4 DWIOTX_OFF, DWIMID_OFF, DWIUTX_OFF, DWIHST_OFF
	PARAMETER(DWIOTX_OFF = DWITAX_OFF + 1)				!040
	PARAMETER(DWIMID_OFF = DWIOTX_OFF + NUMTOT)			!042
	PARAMETER(DWIUTX_OFF = DWIMID_OFF + NUMTOT)			!044
	PARAMETER(DWIHST_OFF = DWIUTX_OFF + NUMTOT)			!046
C
	INTEGER*4 DWIORM_OFF, DWIENM_OFF, DWIDES_OFF, DWINMS_OFF
	PARAMETER(DWIORM_OFF = DWIHST_OFF + 1)				!047
	PARAMETER(DWIENM_OFF = DWIORM_OFF + NUMTOT)			!049
	PARAMETER(DWIDES_OFF = DWIENM_OFF + (WENM_LEN/4))		!056
	PARAMETER(DWINMS_OFF = DWIDES_OFF + (WDES_LEN/4))		!086
C
	INTEGER*4 DWISTA_OFF, DWISBR_OFF, DWIREV_OFF, DWIPFN_OFF
	PARAMETER(DWISTA_OFF = DWINMS_OFF + (WNMS_LEN/4)*MAXWRW)	!230
	PARAMETER(DWISBR_OFF = DWISTA_OFF + MAXWRW)			!266
	PARAMETER(DWIREV_OFF = DWISBR_OFF + MAXWRW)			!302
	PARAMETER(DWIPFN_OFF = DWIREV_OFF + 1)				!303
C
	INTEGER*4 DWIRTM_OFF, DWIPRC_OFF, DWISPR_OFF, DWITVC_OFF
	PARAMETER(DWIRTM_OFF = DWIPFN_OFF + 5)				!308
	PARAMETER(DWIPRC_OFF = DWIRTM_OFF + MAXWRW)			!344
	PARAMETER(DWISPR_OFF = DWIPRC_OFF + 1)				!345
	PARAMETER(DWITVC_OFF = DWISPR_OFF + 1)				!346 
C
	INTEGER*4 DWIMDS_OFF
	PARAMETER(DWIMDS_OFF = DWITVC_OFF + (WTVC_LEN/4))		!347
C
        INTEGER*4 DWIWRO_OFF, DWIWRA_OFF, DWIWPO_OFF, DWIWPA_OFF
        INTEGER*4 DWIWPR_OFF
        PARAMETER(DWIWRO_OFF = DWIMDS_OFF + (MAXMLTD_AVL/4))
        PARAMETER(DWIWRA_OFF = DWIWRO_OFF + 2*NUMTOT)
        PARAMETER(DWIWPO_OFF = DWIWRA_OFF + 2*NUMTOT)
        PARAMETER(DWIWPA_OFF = DWIWPO_OFF + 2*NUMTOT)
        PARAMETER(DWIWPR_OFF = DWIWPA_OFF + 2*NUMTOT) 
	INTEGER*4 DWILAT_OFF
	PARAMETER(DWILAT_OFF = DWIWPR_OFF + 2*NUMTOT)

	INTEGER*4 DWIFRE_OFF
	PARAMETER(DWIFRE_OFF = DWILAT_OFF + NUMLAT)
C
C CALCULATE FREE SPACE BASED ON MACHINE DEPENDENT SECTOR SIZES.
C SHOULD ALLWAYS ADD UP TO A EVEN SECTOR SIZE BYTES (VAX 512).
C NOTE! IN CALCULATIONS, FRE_OFF-2 IS DONE TO TAKE CARE OF WHEN THE DATA SIZE
C       ENDS ON A EVEN SECTOR SIZE.
C
        INTEGER*4 DWISEC_VAX, DWISEC_NUM
        PARAMETER (DWISEC_VAX = SECSIZE/4)
	PARAMETER (DWISEC_NUM = NINT(FLOAT((DWIFRE_OFF-2)/DWISEC_VAX)+.5))
C
        INTEGER*4 DWILEN, DWISEC
        PARAMETER(DWILEN = DWISEC_VAX * DWISEC_NUM)        ! even sector size
        PARAMETER(DWISEC = DWILEN/64)
C
        INTEGER*4 DWIREC(DWILEN)
C
	EQUIVALENCE(DWISTS,DWIREC(DWISTS_OFF))
	EQUIVALENCE(DWIWEK,DWIREC(DWIWEK_OFF))
	EQUIVALENCE(DWIDAT,DWIREC(DWIDAT_OFF))
	EQUIVALENCE(DWIDRW,DWIREC(DWIDRW_OFF))
	EQUIVALENCE(DWIBSD,DWIREC(DWIBSD_OFF))
	EQUIVALENCE(DWIESD,DWIREC(DWIESD_OFF))
	EQUIVALENCE(DWIPUP,DWIREC(DWIPUP_OFF))
	EQUIVALENCE(DWIUPD,DWIREC(DWIUPD_OFF))
	EQUIVALENCE(DWICTM,DWIREC(DWICTM_OFF))
	EQUIVALENCE(DWITIM,DWIREC(DWITIM_OFF))
	EQUIVALENCE(DWISER,DWIREC(DWISER_OFF))
	EQUIVALENCE(DWISAL,DWIREC(DWISAL_OFF))
	EQUIVALENCE(DWIPAD,DWIREC(DWIPAD_OFF))
	EQUIVALENCE(DWIPRG,DWIREC(DWIPRG_OFF))
	EQUIVALENCE(DWIPRF,DWIREC(DWIPRF_OFF))
	EQUIVALENCE(DWIREF,DWIREC(DWIREF_OFF))
	EQUIVALENCE(DWIERF,DWIREC(DWIERF_OFF))
	EQUIVALENCE(DWITER,DWIREC(DWITER_OFF))
	EQUIVALENCE(DWIWON,DWIREC(DWIWON_OFF))
	EQUIVALENCE(DWIPOL,DWIREC(DWIPOL_OFF))
	EQUIVALENCE(DWITPL,DWIREC(DWITPL_OFF))
	EQUIVALENCE(DWITBK,DWIREC(DWITBK_OFF))
	EQUIVALENCE(DWIBRK,DWIREC(DWIBRK_OFF))
	EQUIVALENCE(DWIABW,DWIREC(DWIABW_OFF))
	EQUIVALENCE(DWIODS,DWIREC(DWIODS_OFF))
	EQUIVALENCE(DWIWIN,DWIREC(DWIWIN_OFF))
	EQUIVALENCE(DWIHLD,DWIREC(DWIHLD_OFF))
	EQUIVALENCE(DWITAX,DWIREC(DWITAX_OFF))
	EQUIVALENCE(DWIOTX,DWIREC(DWIOTX_OFF))
	EQUIVALENCE(DWIMID,DWIREC(DWIMID_OFF))
	EQUIVALENCE(DWIUTX,DWIREC(DWIUTX_OFF))
	EQUIVALENCE(DWIHST,DWIREC(DWIHST_OFF))
	EQUIVALENCE(DWIORM,DWIREC(DWIORM_OFF))
	EQUIVALENCE(DWIENM,DWIREC(DWIENM_OFF))
	EQUIVALENCE(DWIDES,DWIREC(DWIDES_OFF))
	EQUIVALENCE(DWINMS,DWIREC(DWINMS_OFF))
	EQUIVALENCE(DWISTA,DWIREC(DWISTA_OFF))
	EQUIVALENCE(DWISBR,DWIREC(DWISBR_OFF))
	EQUIVALENCE(DWIREV,DWIREC(DWIREV_OFF))
	EQUIVALENCE(DWIPFN,DWIREC(DWIPFN_OFF))
	EQUIVALENCE(DWIRTM,DWIREC(DWIRTM_OFF))
	EQUIVALENCE(DWIPRC,DWIREC(DWIPRC_OFF))
	EQUIVALENCE(DWISPR,DWIREC(DWISPR_OFF))
	EQUIVALENCE(DWITVC,DWIREC(DWITVC_OFF))
	EQUIVALENCE(DWIMDS,DWIREC(DWIMDS_OFF))
        EQUIVALENCE(DWIWRO,DWIREC(DWIWRO_OFF))
        EQUIVALENCE(DWIWRA,DWIREC(DWIWRA_OFF))
        EQUIVALENCE(DWIWPO,DWIREC(DWIWPO_OFF))
        EQUIVALENCE(DWIWPA,DWIREC(DWIWPA_OFF))
        EQUIVALENCE(DWIWPR,DWIREC(DWIWPR_OFF))
        EQUIVALENCE(DWILAT,DWIREC(DWILAT_OFF))
